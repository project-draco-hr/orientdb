{
  do {
    lockRead.lock();
    if (!channelRead) {
      channelRead=true;
      currentStatus=readByte();
      currentTxId=readInt();
    }
    if (currentTxId == iRequesterId)     break;
    lockRead.unlock();
synchronized (this) {
      try {
        wait(1000);
      }
 catch (      InterruptedException e) {
      }
    }
  }
 while (true);
  handleStatus(currentStatus,currentTxId);
  return iRequesterId;
}
