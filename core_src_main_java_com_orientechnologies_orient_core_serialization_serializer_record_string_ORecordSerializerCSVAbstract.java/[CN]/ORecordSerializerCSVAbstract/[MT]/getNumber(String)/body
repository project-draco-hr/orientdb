{
  boolean integer=true;
  char c;
  for (int index=0; index < iValue.length(); ++index) {
    c=iValue.charAt(index);
    if (c < '0' || c > '9')     if ((index == 0 && (c == '+' || c == '-')))     continue;
 else     if (c == DECIMAL_SEPARATOR)     integer=false;
 else {
      return OType.STRING;
    }
  }
  return integer ? OType.LONG : OType.DOUBLE;
}
