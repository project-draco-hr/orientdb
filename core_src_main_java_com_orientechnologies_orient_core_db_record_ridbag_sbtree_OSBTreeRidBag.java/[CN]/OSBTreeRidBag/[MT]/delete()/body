{
  final ORecordSerializationContext context=ORecordSerializationContext.getContext();
  if (context != null && rootPointer != null)   context.push(new ORidBagDeleteSerializationOperation(rootPointer));
  rootPointer=null;
  changedValues.clear();
  newEntries.clear();
  size=0;
  changeListeners.clear();
}
