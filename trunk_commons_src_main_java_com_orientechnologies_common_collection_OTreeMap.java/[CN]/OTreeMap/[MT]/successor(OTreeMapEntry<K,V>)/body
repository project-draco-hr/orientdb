{
  if (t == null)   return null;
  OTreeMapEntry<K,V> p=null;
  if (t.getRight() != null) {
    p=t.getRight();
    while (p.getLeft() != null)     p=p.getLeft();
  }
 else {
    p=t.getParent();
    while (p != null && t == p.getRight()) {
      t=p;
      p=p.getParent();
    }
  }
  return p;
}
