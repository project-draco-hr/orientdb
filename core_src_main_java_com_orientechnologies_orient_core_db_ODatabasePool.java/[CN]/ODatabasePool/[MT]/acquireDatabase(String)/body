{
  final String url=iURL.substring(0,iURL.lastIndexOf(":"));
  OResourcePool<String,DB> pool=pools.get(url);
  if (pool == null) {
synchronized (pools) {
      if (pool == null) {
        pool=new OResourcePool<String,DB>(maxSize,this);
        pools.put(url,pool);
      }
    }
  }
  return pool.getResource(iURL,timeout);
}
