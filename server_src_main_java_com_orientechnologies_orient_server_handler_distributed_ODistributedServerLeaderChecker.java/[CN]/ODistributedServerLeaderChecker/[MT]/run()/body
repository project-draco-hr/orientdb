{
  final long time=System.currentTimeMillis() - manager.getLastHeartBeat();
  if (time > heartBeatDelay) {
    OLogManager.instance().warn(this,"No heartbeat message has been received from the Leader node (last was %dms ago). Trying to became the new Leader...",time);
    cancel();
    manager.becameLeader(true);
    return;
  }
}
