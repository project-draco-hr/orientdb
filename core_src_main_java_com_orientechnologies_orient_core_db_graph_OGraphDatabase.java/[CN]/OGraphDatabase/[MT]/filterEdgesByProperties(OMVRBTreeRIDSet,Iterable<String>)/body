{
  if (autoLock)   acquireReadLock(null);
  try {
    if (iPropertyNames == null)     if (iEdges != null)     return Collections.unmodifiableSet(iEdges);
 else     return Collections.emptySet();
    final OMVRBTreeRIDSet result=new OMVRBTreeRIDSet();
    if (iEdges != null)     for (    OIdentifiable item : iEdges) {
      final ODocument doc=(ODocument)item;
      for (      String propName : iPropertyNames) {
        if (doc.containsField(propName))         result.add(item);
      }
    }
    return result;
  }
  finally {
    if (autoLock)     releaseReadLock(null);
  }
}
