{
  try {
    if (!storage.deleteRecord(id,iClusterId,iPosition,iVersion))     throw new ORecordNotFoundException("The record with id '" + iClusterId + ":"+ iPosition+ "' was not found");
  }
 catch (  Exception e) {
    OLogManager.instance().exception("Error on deleting record #%d in cluster '%s'",e,ODatabaseException.class,iPosition,storage.getPhysicalClusterNameById(iClusterId));
  }
}
