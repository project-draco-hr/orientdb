{
  if (iNetwork == null)   return;
  try {
    iNetwork.flush();
  }
 catch (  IOException e) {
    try {
      iNetwork.close();
    }
 catch (    Exception e2) {
    }
 finally {
synchronized (networkPool) {
        networkPool.remove(iNetwork);
      }
    }
    throw e;
  }
 finally {
    iNetwork.getLockWrite().unlock();
    if (debug)     System.out.println("<- req: " + getSessionId());
synchronized (networkPool) {
      networkPool.notifyAll();
    }
  }
}
