{
  int freeSpace=0;
  for (int level=0; level <= maxLevel; level++) {
    int count=0;
    int pointer=freeListHeader[level];
    while (pointer != NULL_POINTER) {
      count++;
      pointer=next(pointer);
    }
    freeSpace+=(1 << level) * count;
  }
  freeSpace*=minChunkSize;
  return freeSpace;
}
