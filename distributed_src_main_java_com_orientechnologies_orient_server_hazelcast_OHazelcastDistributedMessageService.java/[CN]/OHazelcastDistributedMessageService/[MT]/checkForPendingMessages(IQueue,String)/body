{
  final int queueSize=iQueue.size();
  if (queueSize > 0) {
    ODistributedServerLog.warn(this,manager.getLocalNodeName(),null,DIRECTION.NONE,"Found %d messages in queue '%s'. Clearing queue now...",queueSize,iQueueName);
    iQueue.clear();
  }
 else   ODistributedServerLog.info(this,manager.getLocalNodeName(),null,DIRECTION.NONE,"Found no previous messages in queue '%s'",iQueueName);
}
