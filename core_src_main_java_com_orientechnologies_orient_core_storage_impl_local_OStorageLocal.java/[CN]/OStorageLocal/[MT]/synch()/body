{
  checkOpeness();
  final long timer=OProfiler.getInstance().startChrono();
  final boolean locked=acquireExclusiveLock();
  try {
    if (physicalClusters != null)     for (    OCluster cluster : physicalClusters)     cluster.synch();
    if (dataSegments != null)     for (    ODataLocal data : dataSegments)     data.synch();
  }
  finally {
    releaseExclusiveLock(locked);
    OProfiler.getInstance().stopChrono("OStorageLocal.synch",timer);
  }
}
