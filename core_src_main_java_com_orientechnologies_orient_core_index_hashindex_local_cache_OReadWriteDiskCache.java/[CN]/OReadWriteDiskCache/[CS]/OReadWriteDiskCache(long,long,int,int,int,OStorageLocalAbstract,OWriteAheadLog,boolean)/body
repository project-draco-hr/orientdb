{
  this.filePages=new HashMap<Long,Set<Long>>();
  maxSize=normalizeMemory(readCacheMaxMemory,pageSize);
  this.writeCache=new OWOWCache(syncOnPageFlush,pageSize,writeGroupTTL,writeAheadLog,pageFlushInterval,normalizeMemory(writeCacheMaxMemory,pageSize),storageLocal);
  K_IN=maxSize >> 2;
  K_OUT=maxSize >> 1;
  am=new LRUList();
  a1out=new LRUList();
  a1in=new LRUList();
  syncObject=new Object();
}
