{
  enable=false;
synchronized (this) {
    clusters.clear();
    cache.clear();
  }
  configuration.field("enabled",true);
  try {
    updateCfgOnDisk();
  }
 catch (  IOException e) {
    throw OException.wrapException(new OConfigurationException("Cannot write Command Cache Cache configuration to file '" + CONFIG_FILE + "'"),e);
  }
  return this;
}
