{
  Object result=getDistributedDatabaseMap().get(new ODistributedRecordId(name,iRequesterId,new ORecordId(iClusterId,iPosition),-1));
  if (result instanceof ORawBuffer)   return (ORawBuffer)result;
 else   if (result instanceof AddressAwareException)   throw new ODistributedException("Error on reading record from the cluster: " + ((AddressAwareException)result).getAddress(),((AddressAwareException)result).getException());
  return null;
}
