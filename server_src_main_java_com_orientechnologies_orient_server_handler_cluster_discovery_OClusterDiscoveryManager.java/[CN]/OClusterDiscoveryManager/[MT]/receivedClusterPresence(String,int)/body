{
  final String key=iServerAddress + ":" + iServerPort;
synchronized (nodes) {
    if (nodes.containsKey(key))     return;
    final OClusterNode node=new OClusterNode(this,iServerAddress,iServerPort);
    nodes.put(key,node);
    OLogManager.instance().warn(this,"Discovered new cluster node %s. Trying to connect...",key);
    try {
      node.connect(networkTimeoutConnectionSlaves);
      node.startSynchronization();
    }
 catch (    IOException e) {
      OLogManager.instance().error(this,"Can't connect to cluster slave node: %s",key);
    }
  }
}
