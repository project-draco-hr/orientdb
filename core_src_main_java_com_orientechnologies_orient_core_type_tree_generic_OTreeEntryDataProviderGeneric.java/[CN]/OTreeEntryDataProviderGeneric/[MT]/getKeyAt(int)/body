{
  K k=keys[iIndex];
  if (k == null)   try {
    OProfiler.getInstance().updateCounter("OMVRBTreeEntryP.unserializeKey",1);
    k=(K)keyFromStream(iIndex);
    if (iIndex == 0 || iIndex == size || treeDataProvider.keepKeysInMemory)     keys[iIndex]=k;
  }
 catch (  IOException e) {
    OLogManager.instance().error(this,"Can't lazy load the key #" + iIndex + " in tree node "+ this,e,OSerializationException.class);
  }
  return k;
}
