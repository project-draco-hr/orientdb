{
  this.prefetchSize=prefetchSize;
  OSBTreeBonsai<OIdentifiable,Integer> tree=loadTree();
  try {
    if (tree.size() == 0) {
      this.preFetchedValues=null;
      return;
    }
    firstKey=tree.firstKey();
  }
  finally {
    releaseTree();
  }
  this.preFetchedValues=new LinkedList<Map.Entry<OIdentifiable,Integer>>();
  prefetchData(true);
}
