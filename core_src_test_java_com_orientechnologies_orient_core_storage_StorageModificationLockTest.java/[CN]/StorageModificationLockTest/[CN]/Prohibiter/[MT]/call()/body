{
  countDownLatch.await();
  for (int n=0; n < CYCLES_COUNT; n++) {
    modificationLock.prohibitModifications();
    long beforeModification=counter.get();
    Thread.sleep(50);
    if (n % 10 == 0)     System.out.println("After prohibit  modifications " + beforeModification + " before allow modifications "+ counter.get());
    Assert.assertEquals(beforeModification,counter.get());
    modificationLock.allowModifications();
    Thread.sleep(50);
  }
  return null;
}
