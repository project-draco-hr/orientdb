{
  if (converted || !convertToRecord)   return;
  final Object o=list.get(iIndex);
  final ODatabaseRecord database=ODatabaseRecordThreadLocal.INSTANCE.get();
  if (o != null) {
    if (o instanceof ORID)     list.set(iIndex,database.getDatabaseOwner().getUserObjectByRecord((ORecordInternal<?>)database.load((ORID)o,fetchPlan),fetchPlan));
 else     if (o instanceof ODocument)     list.set(iIndex,database.getDatabaseOwner().getUserObjectByRecord((ORecordInternal<?>)o,fetchPlan));
  }
}
