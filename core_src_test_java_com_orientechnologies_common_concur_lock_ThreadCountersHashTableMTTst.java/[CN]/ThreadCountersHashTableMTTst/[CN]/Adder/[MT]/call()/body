{
  latch.await();
  try {
    while (!stop) {
      final Thread thread=new Thread();
      threadCounters.insert(thread);
      addedThreads.add(thread);
      threadsCounter.getAndIncrement();
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
    throw new RuntimeException(e);
  }
  return null;
}
