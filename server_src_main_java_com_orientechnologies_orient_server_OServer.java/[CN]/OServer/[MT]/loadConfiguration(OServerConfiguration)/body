{
  try {
    configuration=iConfiguration;
    contextConfiguration=new OContextConfiguration();
    if (iConfiguration.properties != null)     for (    OServerEntryConfiguration prop : iConfiguration.properties)     contextConfiguration.setValue(prop.name,prop.value);
    loadStorages();
    loadUsers();
  }
 catch (  IOException e) {
    OLogManager.instance().error(this,"Error on reading server configuration.",OConfigurationException.class);
  }
}
