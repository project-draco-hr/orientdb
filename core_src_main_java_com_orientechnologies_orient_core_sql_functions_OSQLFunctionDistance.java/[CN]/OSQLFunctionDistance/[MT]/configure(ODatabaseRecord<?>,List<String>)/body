{
  String paramName;
  Object paramValue;
  for (int i=0; i < iParameters.size(); ++i) {
    paramName=iParameters.get(i);
    paramValue=OSQLHelper.parseValue(iDatabase,paramName);
    if (paramValue == OSQLHelper.VALUE_NOT_PARSED)     configuredFields[i]=(String)paramName;
 else     configuredValues[i]=Math.toRadians(Double.parseDouble(paramValue.toString()));
  }
}
