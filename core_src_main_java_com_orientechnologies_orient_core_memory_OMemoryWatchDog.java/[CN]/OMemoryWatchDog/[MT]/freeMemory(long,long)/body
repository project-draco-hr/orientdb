{
  final long dateLastGC=System.currentTimeMillis();
  if (dateLastGC - lastGC > minimalTimeAmount * 1000) {
    lastGC=dateLastGC;
    System.gc();
    if (iDelayTime > 0)     try {
      Thread.sleep(iDelayTime);
    }
 catch (    InterruptedException e) {
      Thread.currentThread().interrupt();
    }
  }
}
