{
  for (int i=0; i < configuredParameters.length; ++i) {
    if (configuredParameters[i] instanceof OSQLFilterItemField)     runtimeParameters[i]=((OSQLFilterItemField)configuredParameters[i]).getValue(o);
 else     if (configuredParameters[i] instanceof OSQLFunctionRuntime)     runtimeParameters[i]=((OSQLFunctionRuntime)configuredParameters[i]).execute(o,iRequester);
  }
  final Object functionResult=function.execute(o,runtimeParameters,iRequester);
  return transformValue(o,functionResult);
}
