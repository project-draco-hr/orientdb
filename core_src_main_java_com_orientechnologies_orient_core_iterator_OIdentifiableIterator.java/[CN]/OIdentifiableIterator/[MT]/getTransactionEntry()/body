{
  final long physicalRecordTobrowse=current.clusterPosition > -2 && lastClusterPosition > -1 ? lastClusterPosition - current.clusterPosition : 0;
  if (physicalRecordTobrowse == 0 && txEntries != null) {
    currentTxEntryPosition++;
    if (currentTxEntryPosition >= txEntries.size())     throw new NoSuchElementException();
 else     return txEntries.get(currentTxEntryPosition).getRecord();
  }
  return null;
}
