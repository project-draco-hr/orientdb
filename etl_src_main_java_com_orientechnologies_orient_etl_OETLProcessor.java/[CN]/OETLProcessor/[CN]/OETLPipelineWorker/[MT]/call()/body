{
  pipeline.begin();
  pipeline.getDocumentDatabase();
  while (!queue.isEmpty() || !extractionFinished.get()) {
    try {
      final OExtractedItem content=queue.poll();
      if (content != null)       pipeline.execute(content);
    }
 catch (    OException e) {
      throw e;
    }
  }
  return Boolean.TRUE;
}
