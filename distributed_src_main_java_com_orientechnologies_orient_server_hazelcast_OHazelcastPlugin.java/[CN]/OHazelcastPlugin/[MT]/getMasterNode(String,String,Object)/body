{
  String masterNode=getDatabaseClusterConfiguration(iDatabaseName,iClusterName).field("master");
  if (masterNode == null) {
    OLogManager.instance().warn(this,"DISTRIBUTED found wrong configuration for database '%s': cannot find the 'master' field for the cluster '%s'. '$auto' will be used",iDatabaseName,iClusterName);
    masterNode="$auto";
  }
 else   if (!masterNode.equalsIgnoreCase("$auto"))   return masterNode;
  return getNodeId(hazelcastInstance.getPartitionService().getPartition(iKey).getOwner());
}
