{
  OCluster localCluster=getCluster();
  for (int i=0; i < KEYS_COUNT; i++)   Assert.assertNotNull(localCluster.addPhysicalPosition(new OPhysicalPosition(i)));
  for (int i=0; i < KEYS_COUNT; i++) {
    if (i % 3 == 0)     localCluster.removePhysicalPosition(i);
    if (i % 2 == 0)     Assert.assertNotNull(localCluster.addPhysicalPosition(new OPhysicalPosition(KEYS_COUNT + i)));
  }
  for (int i=0; i < KEYS_COUNT; i++) {
    if (i % 3 == 0)     Assert.assertNull(localCluster.getPhysicalPosition(new OPhysicalPosition(i)));
 else     Assert.assertNotNull(localCluster.getPhysicalPosition(new OPhysicalPosition(i)));
    if (i % 2 == 0)     Assert.assertNotNull(localCluster.getPhysicalPosition(new OPhysicalPosition(KEYS_COUNT + i)));
  }
}
