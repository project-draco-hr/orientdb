{
  checkConnection();
  if (iClusterName == null)   return -1;
  if (Character.isDigit(iClusterName.charAt(0)))   return Integer.parseInt(iClusterName);
  boolean locked=lock.acquireSharedLock();
  try {
    final Integer id=clustersIds.get(iClusterName.toLowerCase());
    if (id == null)     return -1;
    return id;
  }
  finally {
    lock.releaseSharedLock(locked);
  }
}
