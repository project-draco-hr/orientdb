{
  OSBTree<OIdentifiable,Boolean> tree=new OSBTree<OIdentifiable,Boolean>(".sbt",1,true);
  tree.create(FILE_ID,-1,OLinkSerializer.INSTANCE,OBooleanSerializer.INSTANCE,(OStorageLocalAbstract)ODatabaseRecordThreadLocal.INSTANCE.get().getStorage());
  treeCache.put(tree.getRootIndex(),tree);
  return tree;
}
