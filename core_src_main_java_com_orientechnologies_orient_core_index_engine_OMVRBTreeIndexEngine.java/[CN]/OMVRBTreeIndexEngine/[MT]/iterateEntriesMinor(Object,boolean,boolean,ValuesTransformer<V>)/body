{
  acquireExclusiveLock();
  try {
    if (ascSortOrder)     return new OMBRBTreeIndexCursor(map.headMap(toKey,isInclusive).entrySet().iterator(),transformer);
    return new OMBRBTreeIndexCursor(map.headMap(toKey,isInclusive).descendingMap().entrySet().iterator(),transformer);
  }
  finally {
    releaseExclusiveLock();
  }
}
