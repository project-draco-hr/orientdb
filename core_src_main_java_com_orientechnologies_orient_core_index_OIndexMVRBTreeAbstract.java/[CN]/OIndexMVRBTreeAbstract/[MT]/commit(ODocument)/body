{
  if (iDocument == null)   return;
  acquireExclusiveLock();
  try {
    if ((Boolean)iDocument.field("clear"))     clear();
    for (    Entry<String,Object> entry : iDocument) {
      final Object key=entry.getKey();
      final int operation=(Integer)((ODocument)entry.getValue()).field("o");
      final OIdentifiable value=((ODocument)entry.getValue()).field("v");
      if (operation == OPERATION.PUT.ordinal())       put(key,value);
 else       if (operation == OPERATION.REMOVE.ordinal())       remove(key,value);
    }
  }
  finally {
    releaseExclusiveLock();
  }
}
