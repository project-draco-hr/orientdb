{
  ODatabaseBinary db=null;
  try {
    db=OSharedBinaryDatabase.acquireDatabase(database.getName() + ":admin:admin");
    super.commitChanges(db);
  }
 catch (  InterruptedException e) {
    e.printStackTrace();
  }
 finally {
    if (db != null)     OSharedBinaryDatabase.releaseDatabase(db);
  }
}
