{
  stateLock.acquireReadLock();
  try {
    if (iExclusiveLock)     dataLock.acquireExclusiveLock();
 else     dataLock.acquireSharedLock();
    try {
      return iCallable.call();
    }
 catch (    RuntimeException e) {
      throw e;
    }
catch (    Exception e) {
      throw new OException("Error on nested call in lock",e);
    }
 finally {
      if (iExclusiveLock)       dataLock.releaseExclusiveLock();
 else       dataLock.releaseSharedLock();
    }
  }
  finally {
    stateLock.releaseReadLock();
  }
}
