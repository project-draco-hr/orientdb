{
  checkForRebuild();
  final OType[] types=getDefinition().getTypes();
  if (types.length == 1) {
    rangeFrom=OType.convert(rangeFrom,types[0].getDefaultJavaType());
    rangeTo=OType.convert(rangeTo,types[0].getDefaultJavaType());
  }
  acquireSharedLock();
  try {
    indexEngine.getEntriesBetween(rangeFrom,rangeTo,inclusive,MultiValuesTransformer.INSTANCE,new OIndexEngine.EntriesResultListener(){
      @Override public boolean addResult(      ODocument entry){
        return indexEntriesResultListener.addResult(entry);
      }
    }
);
  }
  finally {
    releaseSharedLock();
  }
}
