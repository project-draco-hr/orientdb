{
  File file=new File("temp");
  if (!file.exists()) {
    try {
      file.createNewFile();
    }
 catch (    IOException ioe) {
      OLogManager.instance().error(this,"Error creating history file.",ioe,"");
    }
  }
  BufferedWriter writer=new BufferedWriter(new FileWriter(file,true));
  writer.append(ivalue);
  writer.newLine();
  writer.flush();
  writer.close();
}
