{
  int pageOffset=OWALPage.RECORDS_OFFSET;
  int maxPageOffset=page.getFilledUpTo();
  while (pageOffset < maxPageOffset) {
    if (page.recordTail(pageOffset))     pageOffset+=page.getSerializedRecordSize(pageOffset);
 else     break;
  }
  if (pageOffset == maxPageOffset)   return -1;
  return pageOffset;
}
