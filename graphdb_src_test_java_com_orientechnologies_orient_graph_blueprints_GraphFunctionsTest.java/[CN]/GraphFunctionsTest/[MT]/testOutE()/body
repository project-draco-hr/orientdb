{
  int found;
  found=0;
  for (  Edge v : (Iterable<Edge>)graph.command(new OCommandSQL("select flatten( outE() ) from " + v1.getId())).execute())   found++;
  Assert.assertEquals(found,2);
  found=0;
  for (  Edge v : (Iterable<Edge>)graph.command(new OCommandSQL("select flatten( outE('SubEdge') ) from " + v1.getId())).execute())   found++;
  Assert.assertEquals(found,1);
  found=0;
  for (  Edge v : (Iterable<Edge>)graph.command(new OCommandSQL("select flatten( outE('dddd') ) from " + v1.getId())).execute())   found++;
  Assert.assertEquals(found,0);
  found=0;
  for (  Edge v : (Iterable<Edge>)graph.command(new OCommandSQL("select flatten( outE() ) from " + v2.getId())).execute())   found++;
  Assert.assertEquals(found,0);
  found=0;
  for (  Edge v : (Iterable<Edge>)graph.command(new OCommandSQL("select flatten( outE() ) from " + v3.getId())).execute())   found++;
  Assert.assertEquals(found,0);
}
