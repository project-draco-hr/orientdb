{
  database.open("admin","admin");
  int i=0;
  Animal a;
  for (OObjectIteratorCluster<Animal> iterator=database.browseCluster("Animal"); iterator.hasNext(); ) {
    a=iterator.next();
    if (i % 2 == 0)     Assert.assertEquals(a.getLocation(),"Spain");
 else     Assert.assertEquals(a.getLocation(),"Italy");
    Assert.assertEquals(a.getPrice(),i + 100f);
    i++;
  }
  database.close();
}
