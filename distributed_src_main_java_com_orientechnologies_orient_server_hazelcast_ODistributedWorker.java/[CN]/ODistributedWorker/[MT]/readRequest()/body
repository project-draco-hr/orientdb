{
  ODistributedRequest req=nextMessage();
  if (manager.isOffline())   waitNodeIsOnline();
  final String senderNodeName=manager.getNodeNameById(req.getSenderNodeId());
  if (ODistributedServerLog.isDebugEnabled())   ODistributedServerLog.debug(this,manager.getLocalNodeName(),senderNodeName,DIRECTION.IN,"Processing request=%s sourceNode=%s",req,senderNodeName);
  return req;
}
