{
  OLogManager.instance().warn(this,"CONFLICT against node %s DELETE record %s (cannot be deleted on other node)",iRemoteNode,iCurrentRID);
  if (!existConflictsForRecord(iCurrentRID)) {
    final ODocument doc=createConflictDocument(ORecordOperation.DELETED,iCurrentRID,iRemoteNode);
    doc.save();
  }
}
