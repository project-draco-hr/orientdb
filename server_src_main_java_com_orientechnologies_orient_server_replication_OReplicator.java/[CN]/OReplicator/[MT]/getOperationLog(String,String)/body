{
synchronized (this) {
    if (manager.itsMe(iNodeId))     return localLogs.get(iDatabaseName);
    final ODistributedNode node=nodes.get(iNodeId);
    if (node != null) {
      final ODistributedDatabaseInfo db=node.getDatabase(iDatabaseName);
      if (db != null)       return db.getLog();
    }
    return null;
  }
}
