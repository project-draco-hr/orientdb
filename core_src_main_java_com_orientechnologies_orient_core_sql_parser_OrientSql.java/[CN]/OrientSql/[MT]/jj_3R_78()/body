{
  if (jj_scan_token(CREATE))   return true;
  if (jj_scan_token(INDEX))   return true;
  if (jj_3R_221())   return true;
  Token xsp;
  xsp=jj_scanpos;
  if (jj_3_132()) {
    jj_scanpos=xsp;
    if (jj_3R_222())     return true;
  }
  xsp=jj_scanpos;
  if (jj_3_135()) {
    jj_scanpos=xsp;
    if (jj_3R_223())     return true;
  }
  return false;
}
