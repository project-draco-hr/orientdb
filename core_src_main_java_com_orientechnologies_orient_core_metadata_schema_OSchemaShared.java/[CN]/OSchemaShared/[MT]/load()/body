{
  getDatabase().getStorage().callInLock(new Callable<Object>(){
    @Override public Object call() throws Exception {
      getDatabase();
      ((ORecordId)document.getIdentity()).fromString(getDatabase().getStorage().getConfiguration().schemaRecordId);
      reload("*:-1 index:0");
      return null;
    }
  }
,true);
  return this;
}
