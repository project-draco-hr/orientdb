{
  Set<OGraphEdge> temp=outEdges != null ? outEdges.get() : null;
  if (temp == null) {
    temp=new HashSet<OGraphEdge>();
    if (iEdgeLabel == null)     outEdges=new SoftReference<Set<OGraphEdge>>(temp);
    final Set<Object> docs=document.field(OGraphDatabase.VERTEX_FIELD_OUT);
    if (docs != null) {
      ODocument doc;
      for (      Object o : docs) {
        if (o instanceof ODocument)         doc=(ODocument)o;
 else         doc=database.getRecordById((ORID)o);
        if (iEdgeLabel != null && !iEdgeLabel.equals(doc.field(OGraphDatabase.LABEL)))         continue;
        temp.add((OGraphEdge)database.getUserObjectByRecord(doc,null));
      }
    }
  }
 else   if (iEdgeLabel != null) {
    HashSet<OGraphEdge> filtered=new HashSet<OGraphEdge>();
    for (    OGraphEdge e : temp) {
      if (iEdgeLabel.equals(e.getLabel()))       filtered.add(e);
    }
    temp=filtered;
  }
  return temp;
}
