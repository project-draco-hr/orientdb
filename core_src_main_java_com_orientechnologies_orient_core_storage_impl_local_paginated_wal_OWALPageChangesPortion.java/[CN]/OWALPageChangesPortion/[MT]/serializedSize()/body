{
  if (pageChunks == null) {
    return OShortSerializer.SHORT_SIZE;
  }
  int offset=0;
  offset+=OShortSerializer.SHORT_SIZE;
  for (int i=0; i < pageChunks.length; i++) {
    if (pageChunks[i] != null) {
      for (int j=0; j < PORTION_SIZE; j++) {
        if (pageChunks[i][j] != null) {
          offset+=OShortSerializer.SHORT_SIZE;
          offset+=CHUNK_SIZE;
        }
      }
    }
  }
  return offset;
}
