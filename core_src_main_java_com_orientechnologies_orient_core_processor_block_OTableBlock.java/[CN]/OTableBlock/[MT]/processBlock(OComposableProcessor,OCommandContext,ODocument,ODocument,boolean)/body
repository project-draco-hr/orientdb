{
  if (!(iConfig instanceof ODocument))   throw new OProcessException("Content in not a JSON");
  final ODocument table=new ODocument();
  header=getRequiredField(iContext,iConfig,"header");
  if (isBlock(header))   header=delegate("header",iManager,header,iContext,iOutput,iReadOnly);
  table.field("header",header);
  body=getRequiredField(iContext,iConfig,"body");
  if (isBlock(body))   body=delegate("body",iManager,body,iContext,iOutput,iReadOnly);
  table.field("body",body);
  footer=getRequiredField(iContext,iConfig,"footer");
  if (isBlock(footer))   footer=delegate("footer",iManager,footer,iContext,iOutput,iReadOnly);
  table.field("footer",footer);
  return table;
}
