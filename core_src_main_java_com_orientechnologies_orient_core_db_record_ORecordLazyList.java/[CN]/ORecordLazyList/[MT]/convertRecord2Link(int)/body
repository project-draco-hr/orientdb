{
  if (status == MULTIVALUE_STATUS.ALL_RIDS || database == null)   return true;
  final Object o=super.get(iIndex);
  if (o != null) {
    if (o instanceof ORecord<?> && !((ORecord<?>)o).isDirty())     try {
      super.set(iIndex,((ORecord<?>)o).getIdentity());
      return true;
    }
 catch (    ORecordNotFoundException e) {
    }
 else     if (o instanceof ORID)     return true;
  }
  return false;
}
