{
  String buildDirectory=System.getProperty("buildDirectory",".");
  if (buildDirectory == null)   buildDirectory=".";
  databaseDocumentTx=new ODatabaseDocumentTx("local:" + buildDirectory + "/notUniqueLucene");
  if (databaseDocumentTx.exists()) {
    databaseDocumentTx.open("admin","admin");
    databaseDocumentTx.drop();
  }
  databaseDocumentTx.create();
  hashIndex=databaseDocumentTx.getMetadata().getIndexManager().createIndex("hashIndex","NOTUNIQUE",new OSimpleKeyIndexDefinition(OType.STRING),new int[0],null,null,"LUCENE");
}
