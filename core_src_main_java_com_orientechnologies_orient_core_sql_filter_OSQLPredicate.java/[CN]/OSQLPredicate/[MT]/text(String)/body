{
  if (iText == null)   throw new OCommandSQLParsingException("Query text is null");
  try {
    parserText=iText;
    parserTextUpperCase=upperCase(parserText);
    parserSetCurrentPosition(0);
    parserSkipWhiteSpaces();
    rootCondition=(OSQLFilterCondition)extractConditions(null);
    optimize();
  }
 catch (  OQueryParsingException e) {
    if (e.getText() == null)     throw new OQueryParsingException("Error on parsing query",parserText,parserGetCurrentPosition(),e);
    throw e;
  }
catch (  Throwable t) {
    throw new OQueryParsingException("Error on parsing query",parserText,parserGetCurrentPosition(),t);
  }
  return this;
}
