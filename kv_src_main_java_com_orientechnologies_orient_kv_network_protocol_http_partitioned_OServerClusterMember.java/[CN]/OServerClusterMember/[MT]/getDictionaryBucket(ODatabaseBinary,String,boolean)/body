{
  ORecordBytes record=iDatabase.getDictionary().get(iName);
  OTreeMapPersistent<String,String> bucket;
  if (record == null) {
    if (iAsynchMode)     bucket=new OTreeMapPersistentAsynch<String,String>(iDatabase,DEFAULT_CLUSTER_NAME,OStreamSerializerString.INSTANCE,OStreamSerializerString.INSTANCE);
 else     bucket=new OTreeMapPersistent<String,String>(iDatabase,DEFAULT_CLUSTER_NAME,OStreamSerializerString.INSTANCE,OStreamSerializerString.INSTANCE);
    bucket.save();
    iDatabase.getDictionary().put(iName,bucket.getRecord());
  }
 else {
    if (iAsynchMode)     bucket=new OTreeMapPersistentAsynch<String,String>(iDatabase,DEFAULT_CLUSTER_NAME,record.getIdentity());
 else     bucket=new OTreeMapPersistent<String,String>(iDatabase,DEFAULT_CLUSTER_NAME,record.getIdentity());
    bucket.load();
  }
  return bucket;
}
