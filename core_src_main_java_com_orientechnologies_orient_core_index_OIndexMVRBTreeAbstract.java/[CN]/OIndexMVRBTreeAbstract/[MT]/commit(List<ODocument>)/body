{
  if (iEntries == null)   return;
  acquireExclusiveLock();
  try {
    for (    ODocument entry : iEntries) {
      final int status=(Integer)entry.field("s");
      if (status == OTransactionIndexEntry.STATUSES.CLEAR.ordinal())       clear();
 else       if (status == OTransactionIndexEntry.STATUSES.PUT.ordinal())       put(entry.field("k"),(OIdentifiable)entry.field("v"));
 else       if (status == OTransactionIndexEntry.STATUSES.REMOVE.ordinal())       remove(entry.field("k"),(OIdentifiable)entry.field("v"));
    }
  }
  finally {
    releaseExclusiveLock();
  }
}
