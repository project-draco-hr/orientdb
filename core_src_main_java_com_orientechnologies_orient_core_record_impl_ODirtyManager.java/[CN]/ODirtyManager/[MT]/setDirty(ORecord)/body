{
  if (overrider != null)   overrider.setDirty(record);
 else {
    if (record.getIdentity().isNew()) {
      if (newRecord == null)       newRecord=Collections.newSetFromMap(new IdentityHashMap<ORecord,Boolean>());
      newRecord.add(record);
    }
 else {
      if (updateRecord == null)       updateRecord=Collections.newSetFromMap(new IdentityHashMap<ORecord,Boolean>());
      updateRecord.add(record);
    }
  }
}
