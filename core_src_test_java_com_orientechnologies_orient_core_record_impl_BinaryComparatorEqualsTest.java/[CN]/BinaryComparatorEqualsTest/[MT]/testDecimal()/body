{
  Assert.assertTrue(comparator.isEqual(field(OType.DECIMAL,new BigDecimal(10)),field(OType.DECIMAL,new BigDecimal(10))));
  Assert.assertFalse(comparator.isEqual(field(OType.DECIMAL,new BigDecimal(10)),field(OType.DECIMAL,new BigDecimal(11))));
  Assert.assertFalse(comparator.isEqual(field(OType.DECIMAL,new BigDecimal(10)),field(OType.DECIMAL,new BigDecimal(9))));
}
