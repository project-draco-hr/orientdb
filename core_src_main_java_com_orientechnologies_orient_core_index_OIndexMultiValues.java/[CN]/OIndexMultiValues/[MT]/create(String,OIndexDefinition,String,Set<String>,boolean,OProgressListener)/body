{
  final OStreamSerializer serializer;
  if (ODefaultIndexFactory.SBTREEBONSAI_VALUE_CONTAINER.equals(valueContainerAlgorithm))   serializer=OStreamSerializerSBTreeIndexRIDContainer.INSTANCE;
 else   serializer=OStreamSerializerListRID.INSTANCE;
  return (OIndexMultiValues)super.create(name,indexDefinition,clusterIndexName,clustersToIndex,rebuild,progressListener,serializer);
}
