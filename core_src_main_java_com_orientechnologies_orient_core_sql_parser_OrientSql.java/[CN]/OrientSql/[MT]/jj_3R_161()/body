{
  if (jj_scan_token(ORDER))   return true;
  if (jj_scan_token(BY))   return true;
  Token xsp;
  xsp=jj_scanpos;
  if (jj_3R_215()) {
    jj_scanpos=xsp;
    if (jj_3R_216()) {
      jj_scanpos=xsp;
      if (jj_scan_token(51))       return true;
    }
  }
  xsp=jj_scanpos;
  if (jj_3R_217())   jj_scanpos=xsp;
  while (true) {
    xsp=jj_scanpos;
    if (jj_3R_218()) {
      jj_scanpos=xsp;
      break;
    }
  }
  return false;
}
