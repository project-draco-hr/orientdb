{
  acquireWriteLock();
  try {
    if (headerDirty || dirty) {
      headerDirty=dirty=false;
      try {
        channel.force(false);
      }
 catch (      IOException e) {
        OLogManager.instance().warn(this,"Error during flush of file %s. Data may be lost in case of power failure.",getName());
      }
    }
  }
  finally {
    releaseWriteLock();
  }
}
