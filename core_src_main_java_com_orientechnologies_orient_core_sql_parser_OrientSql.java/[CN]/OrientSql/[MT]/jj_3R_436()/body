{
  Token xsp;
  xsp=jj_scanpos;
  if (jj_3R_508()) {
    jj_scanpos=xsp;
    if (jj_3R_509()) {
      jj_scanpos=xsp;
      if (jj_3R_510()) {
        jj_scanpos=xsp;
        if (jj_3R_511())         return true;
      }
    }
  }
  if (jj_scan_token(COLON))   return true;
  if (jj_3R_73())   return true;
  while (true) {
    xsp=jj_scanpos;
    if (jj_3R_512()) {
      jj_scanpos=xsp;
      break;
    }
  }
  return false;
}
