{
  if (_fieldValues != null) {
    Object fieldValue;
    for (    Map.Entry<String,Object> entry : _fieldValues.entrySet()) {
      fieldValue=entry.getValue();
      if (fieldValue instanceof ORecord<?>)       _fieldValues.put(entry.getKey(),((ORecord<?>)fieldValue).getIdentity());
 else       if (fieldValue instanceof ORecordLazyMultiValue)       ((ORecordLazyMultiValue)fieldValue).convertRecords2Links();
    }
  }
  return this;
}
