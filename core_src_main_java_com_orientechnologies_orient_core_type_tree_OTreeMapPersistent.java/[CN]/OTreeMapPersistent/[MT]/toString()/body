{
  final int currPageIndex=pageIndex;
  final StringBuilder buffer=new StringBuilder();
  buffer.append("size=");
  buffer.append(size);
  buffer.append(" ");
  buffer.append(getFirstEntry().getFirstKey());
  buffer.append("-");
  buffer.append(getLastEntry().getLastKey());
  pageIndex=currPageIndex;
  return buffer.toString();
}
