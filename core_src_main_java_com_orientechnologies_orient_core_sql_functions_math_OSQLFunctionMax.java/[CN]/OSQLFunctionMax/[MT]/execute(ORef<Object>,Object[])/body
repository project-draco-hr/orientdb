{
  Number value=(Number)iParameters[0];
  if (value != null && value instanceof Number) {
    if (iContext.value == null)     iContext.value=value;
 else {
      Number contextValue=getContextValue(iContext,value.getClass());
      if (contextValue instanceof Integer) {
        if (((Integer)contextValue).compareTo((Integer)value) > 0)         iContext.value=value;
      }
 else       if (contextValue instanceof Long) {
        if (((Long)contextValue).compareTo((Long)value) > 0)         iContext.value=value;
      }
 else       if (contextValue instanceof Float) {
        if (((Float)contextValue).compareTo((Float)value) > 0)         iContext.value=value;
      }
 else       if (contextValue instanceof Double) {
        if (((Double)contextValue).compareTo((Double)value) > 0)         iContext.value=value;
      }
    }
  }
  return null;
}
