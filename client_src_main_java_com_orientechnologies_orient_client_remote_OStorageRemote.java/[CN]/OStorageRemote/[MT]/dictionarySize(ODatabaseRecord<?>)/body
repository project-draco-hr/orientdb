{
  checkConnection();
  do {
    boolean locked=lock.acquireExclusiveLock();
    try {
      try {
        beginRequest(OChannelBinaryProtocol.REQUEST_DICTIONARY_SIZE);
      }
  finally {
        endRequest();
      }
      try {
        beginResponse();
        return network.readInt();
      }
  finally {
        endResponse();
      }
    }
 catch (    Exception e) {
      if (handleException("Error on getting size of database's dictionary",e))       break;
    }
 finally {
      lock.releaseExclusiveLock(locked);
    }
  }
 while (true);
  return -1;
}
