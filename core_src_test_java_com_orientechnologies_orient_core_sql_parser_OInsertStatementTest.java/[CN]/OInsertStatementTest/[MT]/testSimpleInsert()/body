{
  checkRightSyntax("insert into Foo (a) values (1)");
  checkRightSyntax("insert into Foo (a) values ('1')");
  checkRightSyntax("insert into Foo (a) values (\"1\")");
  checkRightSyntax("insert into Foo (a,b) values (1, 2)");
  checkRightSyntax("insert into Foo (a,b) values ('1', '2')");
  checkRightSyntax("insert into Foo (a,b) values (\"1\", \"2\")");
}
