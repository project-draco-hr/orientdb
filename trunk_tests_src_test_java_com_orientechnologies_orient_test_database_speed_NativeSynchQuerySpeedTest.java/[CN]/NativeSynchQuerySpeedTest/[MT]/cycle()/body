{
  result=database.query(new ONativeSynchQuery<ORecordVObject,OQueryContextNativeSchema<ORecordVObject>>("Animal",new OQueryContextNativeSchema<ORecordVObject>()){
    @Override public boolean filter(    OQueryContextNativeSchema<ORecordVObject> iRecord){
      return iRecord.column("race").like("Euro%").and().column("race").like("%an").and().column("id").toInt().eq(10).go();
    }
  }
).execute();
}
