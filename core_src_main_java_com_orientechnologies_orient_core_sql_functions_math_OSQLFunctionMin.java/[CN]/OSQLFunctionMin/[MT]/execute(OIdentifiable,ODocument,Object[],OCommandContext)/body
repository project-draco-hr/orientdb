{
  if (iParameters[0] == null || !(iParameters[0] instanceof Comparable<?>))   return null;
  if (iParameters.length == 1) {
    final Comparable<Object> value=(Comparable<Object>)iParameters[0];
    if (context == null)     context=value;
 else     if (context.compareTo(value) > 0)     context=value;
    return null;
  }
 else {
    Object min=null;
    for (int i=0; i < iParameters.length; ++i) {
      if (min == null || ((Comparable)iParameters[i]).compareTo(min) < 0)       min=iParameters[i];
    }
    return min;
  }
}
