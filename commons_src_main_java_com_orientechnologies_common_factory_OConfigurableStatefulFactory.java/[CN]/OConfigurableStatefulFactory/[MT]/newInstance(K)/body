{
  final Class<? extends V> instance=registry.get(iKey);
  if (instance != null) {
    try {
      return instance.newInstance();
    }
 catch (    Exception e) {
      throw new OException(String.format("Error on creating new instance of class '%s' registered in factory with key '%s'",instance,iKey),e);
    }
  }
  return defaultClass;
}
