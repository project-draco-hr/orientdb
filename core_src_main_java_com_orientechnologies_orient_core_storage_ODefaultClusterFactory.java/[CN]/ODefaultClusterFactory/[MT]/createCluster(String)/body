{
  if (iType.equalsIgnoreCase(OClusterLocal.TYPE))   return new OClusterLocal();
 else   if (iType.equalsIgnoreCase(OClusterMemory.TYPE))   return new OClusterMemory();
 else   OLogManager.instance().exception("Cluster type '" + iType + "' is not supported. Supported types are: "+ Arrays.toString(TYPES),null,OStorageException.class);
  return null;
}
