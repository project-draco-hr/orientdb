{
  if (record == null || record.isDirty() || this == pTree.getRoot())   return 0;
  boolean entryPoint=false;
  for (  OTreeMapEntryPersistent<K,V> e : pTree.entryPoints)   if (e == this) {
    entryPoint=true;
    break;
  }
  if (!entryPoint) {
    keys=null;
    values=null;
    serializedKeys=null;
    serializedValues=null;
    record=null;
    tree=pTree=null;
  }
  return disconnectLinked() + 1;
}
