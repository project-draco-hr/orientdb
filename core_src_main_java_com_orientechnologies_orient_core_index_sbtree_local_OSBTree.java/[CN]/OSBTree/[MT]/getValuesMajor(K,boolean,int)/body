{
  final List<V> result=new ArrayList<V>();
  loadValuesMajor(key,inclusive,new RangeResultListener<K,V>(){
    @Override public boolean addResult(    OSBTreeBucket.SBTreeEntry<K,V> entry){
      result.add(entry.value);
      if (maxValuesToFetch > -1 && result.size() >= maxValuesToFetch)       return false;
      return true;
    }
  }
);
  return result;
}
