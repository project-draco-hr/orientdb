{
  Vertex v=graph.addVertex("class:TestVertex");
  try {
    graph.getEdge(v.getId()).getClass();
    Assert.fail();
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    graph.getEdgesOfClass("TestVertex").iterator().next().getClass();
    Assert.fail();
  }
 catch (  IllegalArgumentException e) {
  }
}
