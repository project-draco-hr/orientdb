{
  final Object[] result;
  final ODistributedServerManager dServer=getDistributedServerManager();
  int executedTasks=0;
  try {
    result=new Object[tasks.size()];
    ODistributedServerLog.warn(this,dServer.getLocalNodeId(),getNodeSource(),DIRECTION.IN,"****** BEGIN EXECUTING ALIGNMENT BLOCK db=%s tasks=%d ******",databaseName,tasks.size());
    for (; executedTasks < tasks.size(); ++executedTasks) {
      final OAbstractRemoteTask<?> task=tasks.get(executedTasks);
      result[executedTasks]=task.call();
    }
  }
  finally {
    ODistributedServerLog.warn(this,dServer.getLocalNodeId(),getNodeSource(),DIRECTION.IN,"****** END EXECUTING ALIGNMENT BLOCK db=%s tasks=%d/%d ******",databaseName,executedTasks,tasks.size());
  }
  return result;
}
