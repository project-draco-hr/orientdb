{
  try {
    float growFactor=Float.parseFloat(stringValue);
    if (growFactor < 1)     throw new OStorageException(ATTRIBUTES.RECORD_OVERFLOW_GROW_FACTOR + " can not be less than 1");
    config.recordOverflowGrowFactor=growFactor;
    storageLocal.getConfiguration().update();
  }
 catch (  NumberFormatException nfe) {
    throw new OStorageException("Invalid value for cluster attribute " + ATTRIBUTES.RECORD_OVERFLOW_GROW_FACTOR + " was passed ["+ stringValue+ "].",nfe);
  }
}
