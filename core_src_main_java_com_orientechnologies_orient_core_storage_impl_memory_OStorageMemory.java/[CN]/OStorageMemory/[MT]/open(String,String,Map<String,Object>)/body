{
  addUser();
  if (status == STATUS.OPEN)   return;
  lock.acquireExclusiveLock();
  try {
    if (!exists())     throw new OStorageException("Cannot open the storage '" + name + "' because it does not exist in path: "+ url);
    status=STATUS.OPEN;
  }
  finally {
    lock.releaseExclusiveLock();
  }
}
