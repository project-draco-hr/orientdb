{
switch (iOperation) {
case OTransactionRecordEntry.CREATED:
    if (iResponse != iRecord.getIdentity().getClusterPosition())     conflictResolver.handleCreateConflict(OTransactionRecordEntry.CREATED,iRequestType,iRecord,iResponse);
  break;
case OTransactionRecordEntry.UPDATED:
if ((int)iResponse != iRecord.getVersion()) conflictResolver.handleUpdateConflict(OTransactionRecordEntry.UPDATED,iRequestType,iRecord,iRecord.getVersion(),(int)iResponse);
break;
case OTransactionRecordEntry.DELETED:
if ((int)iResponse == 0) conflictResolver.handleDeleteConflict(OTransactionRecordEntry.DELETED,iRequestType,iRecord);
break;
}
}
