{
  final OSQLSynchQuery<ODocument> query=new OSQLSynchQuery<ODocument>("select roles.include('name') from OUser");
  List<ODocument> resultset=database.query(query);
  Assert.assertEquals(resultset.size(),1);
  for (  ODocument d : resultset) {
    Assert.assertEquals(d.fields(),1);
    Assert.assertTrue(d.containsField("name"));
  }
}
