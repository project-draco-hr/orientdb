{
  sleep(1000);
synchronized (LOCK) {
    OrientBaseGraph graph=new OrientGraph(CLIENT_ORIENT_URL_MAIN);
    try {
      Vertex v1=graph.addVertex("VertexType1",(String)null);
      graph.commit();
      vertex1Id=v1.getId();
      Vertex v2=graph.addVertex("VertexType1",(String)null);
      graph.commit();
      vertex2Id=v2.getId();
      v1.setProperty("p1","v1");
      graph.commit();
      v2.setProperty("p2","v2");
      graph.commit();
      pause();
    }
 catch (    Throwable e) {
      if (exceptionInThread == null) {
        exceptionInThread=e;
      }
    }
 finally {
      OLogManager.instance().info(this,"Shutting down");
      graph.shutdown();
    }
  }
}
