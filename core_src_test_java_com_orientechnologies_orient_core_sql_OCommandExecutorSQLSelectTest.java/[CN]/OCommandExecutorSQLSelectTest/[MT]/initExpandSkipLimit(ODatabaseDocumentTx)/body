{
  db.getMetadata().getSchema().createClass("ExpandSkipLimit");
  for (int i=0; i < 5; i++) {
    ODocument doc=new ODocument("ExpandSkipLimit");
    doc.field("nnum",i);
    doc.save();
    ODocument parent=new ODocument("ExpandSkipLimit");
    parent.field("parent",true);
    parent.field("num",i);
    parent.field("linked",doc);
    parent.save();
  }
}
