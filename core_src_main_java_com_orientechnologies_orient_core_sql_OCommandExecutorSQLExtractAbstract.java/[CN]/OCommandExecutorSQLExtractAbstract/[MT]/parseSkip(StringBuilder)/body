{
  if (!word.toString().equals(KEYWORD_SKIP))   return -1;
  currentPos=OSQLHelper.nextWord(text,textUpperCase,currentPos,word,true);
  try {
    skip=Integer.parseInt(word.toString());
  }
 catch (  Exception e) {
    throw new OCommandSQLParsingException("Invalid SKIP value setted to '" + word + "' but it should be a valid positive integer. Example: SKIP 10",text,currentPos);
  }
  if (skip < 0)   throw new OCommandSQLParsingException("Invalid SKIP value setted to the negative number '" + word + "'. Only positive numbers are valid. Example: SKIP 10",text,currentPos);
  return skip;
}
