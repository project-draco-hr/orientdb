{
  if (iAttribute == null)   throw new IllegalArgumentException("attribute is null");
switch (iAttribute) {
case STATUS:
    return getStatus();
case DEFAULTCLUSTERID:
  return getDefaultClusterId();
case TYPE:
ODatabaseRecord db;
if (getDatabaseOwner() instanceof ODatabaseRecord) db=((ODatabaseRecord)getDatabaseOwner());
 else db=new OGraphDatabase(url);
return db.getMetadata().getSchema().existsClass("OGraphVertex");
}
return null;
}
