{
  Map.Entry<String,ResourceGeneric> found=legacyToGenericMap.floorEntry(resource.toLowerCase());
  if (found == null)   return resource;
  if (resource.length() < found.getKey().length())   return resource;
  if (resource.length() == found.getKey().length())   return null;
  if (resource.substring(0,found.getKey().length()).equalsIgnoreCase(found.getKey()))   return resource.substring(found.getKey().length());
  return resource;
}
