{
  storage.checkConnection();
  final Map<String,String> config=new HashMap<String,String>();
  try {
    final int reqId;
    try {
      reqId=storage.beginRequest(OChannelBinaryProtocol.REQUEST_CONFIG_LIST);
    }
  finally {
      storage.endRequest();
    }
    try {
      storage.beginResponse(reqId);
      final int num=storage.getNetwork().readShort();
      for (int i=0; i < num; ++i)       config.put(storage.getNetwork().readString(),storage.getNetwork().readString());
    }
  finally {
      storage.endResponse();
    }
  }
 catch (  Exception e) {
    OLogManager.instance().exception("Can't retrieve the configuration list",e,OStorageException.class);
    storage.close();
  }
  return config;
}
