{
  assert(!(value instanceof OMVRBTreeRIDSet));
  int pos=OVarIntSerializer.write(bytes,value.size());
  final boolean disabledAutoConvertion=value instanceof ORecordLazyMultiValue && ((ORecordLazyMultiValue)value).isAutoConvertToRecord();
  if (disabledAutoConvertion)   ((ORecordLazyMultiValue)value).setAutoConvertToRecord(false);
  try {
    for (    OIdentifiable itemValue : value) {
      if (itemValue == null)       writeNullLink(bytes);
 else       writeOptimizedLink(bytes,itemValue);
    }
  }
  finally {
    if (disabledAutoConvertion)     ((ORecordLazyMultiValue)value).setAutoConvertToRecord(true);
  }
  return pos;
}
