{
  V value=null;
  int i=0;
  int comp=-1;
  for (; i < size; ++i) {
    comp=((Comparable)getKeyAt(i)).compareTo(iKey);
    if (comp == 0) {
      tree.pageItemFound=true;
      value=getValueAt(tree.pageIndex);
      break;
    }
 else     if (comp > 0)     break;
  }
  tree.pageIndex=i;
  return value;
}
