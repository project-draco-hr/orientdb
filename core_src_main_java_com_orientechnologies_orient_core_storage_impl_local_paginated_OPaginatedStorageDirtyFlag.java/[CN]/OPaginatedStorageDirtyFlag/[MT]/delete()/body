{
  lock.lock();
  try {
    if (dirtyFile == null)     return;
    if (dirtyFile.exists()) {
      dirtyFileData.close();
      boolean deleted=dirtyFile.delete();
      while (!deleted) {
        deleted=!dirtyFile.exists() || dirtyFile.delete();
      }
    }
  }
  finally {
    lock.unlock();
  }
}
