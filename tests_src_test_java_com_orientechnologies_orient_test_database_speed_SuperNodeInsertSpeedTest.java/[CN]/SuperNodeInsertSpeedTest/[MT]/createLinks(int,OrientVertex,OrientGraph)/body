{
  for (int i=0; i < vertexNumber; i++) {
    while (true)     try {
      final OrientVertex v=graph.addVertex("");
      v.addEdge("link",superNode);
      v.save();
      graph.commit();
      break;
    }
 catch (    OConcurrentModificationException e) {
      superNode.getRecord().reload();
      retryCount.incrementAndGet();
    }
  }
}
