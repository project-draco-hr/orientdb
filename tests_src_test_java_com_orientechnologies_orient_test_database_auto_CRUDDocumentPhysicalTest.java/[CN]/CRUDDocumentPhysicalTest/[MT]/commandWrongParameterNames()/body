{
  database=ODatabaseDocumentPool.global().acquire(url,"admin","admin");
  ODocument doc=database.newInstance();
  try {
    doc.field("a:b",10);
    Assert.assertFalse(true);
  }
 catch (  IllegalArgumentException e) {
    Assert.assertTrue(true);
  }
  try {
    doc.field("a,b",10);
    Assert.assertFalse(true);
  }
 catch (  IllegalArgumentException e) {
    Assert.assertTrue(true);
  }
  database.close();
}
