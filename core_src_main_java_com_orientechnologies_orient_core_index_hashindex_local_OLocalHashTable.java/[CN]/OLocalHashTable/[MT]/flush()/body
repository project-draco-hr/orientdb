{
  acquireExclusiveLock();
  try {
    flushSplitBucketsBuffer();
    for (int i=0; i < filesMetadata.length; i++)     if (filesMetadata[i] != null)     buffer.flushFile(name + i,bucketFileExtension);
  }
 catch (  IOException e) {
    throw new OIndexException("Error during hash table flush",e);
  }
 finally {
    releaseExclusiveLock();
  }
}
