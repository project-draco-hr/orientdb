{
  final StringBuilder buffer=new StringBuilder(256);
  buffer.append("id=");
  buffer.append(senderNodeId);
  buffer.append('.');
  buffer.append(id);
  if (task != null) {
    buffer.append(" task=");
    buffer.append(task.toString());
  }
  if (userRID != null) {
    buffer.append(" user=");
    buffer.append(userRID);
  }
  return buffer.toString();
}
