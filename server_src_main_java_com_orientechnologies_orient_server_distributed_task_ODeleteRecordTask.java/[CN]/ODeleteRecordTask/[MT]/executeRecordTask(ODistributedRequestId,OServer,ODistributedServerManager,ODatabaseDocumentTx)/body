{
  ODistributedServerLog.debug(this,iManager.getLocalNodeName(),null,DIRECTION.IN,"delete record %s/%s v.%d",database.getName(),rid.toString(),version);
  final ORecord record=database.load(rid);
  if (record != null) {
    if (delayed)     if (record.getVersion() == version)     ((ODistributedStorage)database.getStorage()).pushDeletedRecord(rid,version);
 else     throw new OConcurrentModificationException(rid,record.getVersion(),version,ORecordOperation.DELETED);
 else     record.delete();
  }
  return true;
}
