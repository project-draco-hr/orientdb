{
  long hashCode=hashCode(fileName,filePosition);
  int index=index(hashCode);
  LRUEntry lruEntry=entries[index];
  LRUEntry prevEntry=null;
  while (lruEntry != null && (lruEntry.hashCode != hashCode || !lruEntry.fileName.equals(fileName) || lruEntry.pageIndex != filePosition)) {
    prevEntry=lruEntry;
    lruEntry=lruEntry.next;
  }
  if (lruEntry == null)   return null;
  LRUEntry before=lruEntry.before;
  LRUEntry after=lruEntry.after;
  if (before != null)   before.after=after;
  if (after != null)   after.before=before;
  if (lruEntry == head)   head=lruEntry.after;
  if (lruEntry == tail)   tail=lruEntry.before;
  if (prevEntry == null)   entries[index]=lruEntry.next;
 else   prevEntry.next=lruEntry.next;
  size--;
  return lruEntry;
}
