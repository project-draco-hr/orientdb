{
  int low=0;
  int high=size - 1;
  while (low <= high) {
    int mid=(low + high) >>> 1;
    ChangesBucket midBucket=changesBuckets[mid];
    if (midBucket.endPosition < startPosition)     low=mid + 1;
 else     if (midBucket.endPosition > startPosition)     high=mid - 1;
 else     return mid;
  }
  return -(low + 1);
}
