{
  StringBuilder result=new StringBuilder();
  if (from != null) {
    result.append(from);
  }
 else {
    result.append(fromSelector.toString());
  }
  if (newRange) {
    result.append("-");
  }
 else {
    result.append("-");
  }
  if (to != null) {
    result.append(to);
  }
 else {
    result.append(toSelector.toString());
  }
  return result.toString();
}
