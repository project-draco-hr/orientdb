{
  if (debug)   OLogManager.instance().debug(this,"Writing strings (4+%d=%d items): %s",iCollection != null ? iCollection.size() : 0,iCollection != null ? iCollection.size() + 4 : 4,iCollection.toString());
  if (iCollection == null)   writeInt(-1);
 else {
    writeInt(iCollection.size());
    for (    String s : iCollection)     writeString(s);
  }
  return this;
}
