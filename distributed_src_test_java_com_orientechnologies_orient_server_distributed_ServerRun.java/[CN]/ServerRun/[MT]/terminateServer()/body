{
  if (server != null) {
    try {
      if (((OHazelcastPlugin)server.getDistributedManager()).getHazelcastInstance().getLifecycleService().isRunning())       ((OHazelcastPlugin)server.getDistributedManager()).getHazelcastInstance().getLifecycleService().terminate();
    }
 catch (    Exception e) {
    }
    server.shutdown();
  }
  closeStorages();
}
