{
  int[] chunksToAdd=idToAdd.chunks;
  int[] result=new int[CHUNKS_SIZE];
  int index=CHUNKS_SIZE;
  long sum=0;
  while (index > 0) {
    index--;
    sum=(chunks[index] & LONG_INT_MASK) + (chunksToAdd[index] & LONG_INT_MASK) + (sum >>> 32);
    result[index]=(int)sum;
  }
  return new ONodeId(result);
}
