{
  database.open("admin","admin");
  long time=System.currentTimeMillis();
  readSubNodes(database.getRoot("LinearGraph"));
  System.out.println("Read of the entire graph with deep=" + nodeReadCounter + " and fixed density="+ DENSITY+ " in "+ ((System.currentTimeMillis() - time) / 1000f)+ " sec.");
