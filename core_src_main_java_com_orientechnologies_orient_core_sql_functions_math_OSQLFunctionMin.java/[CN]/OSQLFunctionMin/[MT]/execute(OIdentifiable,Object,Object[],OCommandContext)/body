{
  Object min=null;
  for (  Object item : iParameters) {
    if (item instanceof Collection<?>) {
      for (      Object subitem : ((Collection<?>)item)) {
        if (min == null || subitem != null && ((Comparable)subitem).compareTo(min) < 0)         min=subitem;
      }
    }
 else {
