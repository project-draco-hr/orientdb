{
  createClusterFromConfig(new OStoragePhysicalClusterConfigurationLocal(configuration,clusters.length,-1,OMetadata.CLUSTER_INTERNAL_NAME));
  configuration.load();
  createClusterFromConfig(new OStoragePhysicalClusterConfigurationLocal(configuration,clusters.length,-1,OMetadata.CLUSTER_INDEX_NAME));
  createClusterFromConfig(new OStoragePhysicalClusterConfigurationLocal(configuration,clusters.length,-1,OMetadata.CLUSTER_MANUAL_INDEX_NAME));
  defaultClusterId=createClusterFromConfig(new OStoragePhysicalClusterConfigurationLocal(configuration,clusters.length,-1,CLUSTER_DEFAULT_NAME));
}
