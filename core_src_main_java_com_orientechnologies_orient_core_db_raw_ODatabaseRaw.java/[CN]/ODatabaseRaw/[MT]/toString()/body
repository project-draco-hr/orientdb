{
  final StringBuilder buffer=new StringBuilder(256);
  buffer.append("OrientDB[");
  buffer.append(url != null ? url : "?");
  buffer.append(']');
  if (getStorage() != null) {
    buffer.append(" (users: ");
    buffer.append(getStorage().getUsers());
    buffer.append(')');
  }
  return buffer.toString();
}
