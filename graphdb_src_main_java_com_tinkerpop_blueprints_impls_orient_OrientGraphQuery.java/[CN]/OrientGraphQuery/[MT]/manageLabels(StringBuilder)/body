{
  if (labels != null && labels.length > 0) {
    text.append(QUERY_LABEL_BEGIN);
    for (int i=0; i < labels.length; ++i) {
      if (i > 0)       text.append(QUERY_SEPARATOR);
      text.append(QUERY_STRING);
      text.append(labels[i]);
      text.append(QUERY_STRING);
    }
    text.append(QUERY_LABEL_END);
  }
}
