{
  acquireSharedLock();
  try {
    OAtomicOperationsManager operationsManager=storage.getAtomicOperationsManager();
    operationsManager.acquireReadLock(this);
    try {
      return clusterPositionMap.getFirstPosition();
    }
  finally {
      operationsManager.releaseReadLock(this);
    }
  }
  finally {
    releaseSharedLock();
  }
}
