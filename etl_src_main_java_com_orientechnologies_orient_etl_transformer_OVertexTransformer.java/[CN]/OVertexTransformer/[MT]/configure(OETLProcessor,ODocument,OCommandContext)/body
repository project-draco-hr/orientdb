{
  super.configure(iProcessor,iConfiguration,iContext);
  clusterName=iConfiguration.field("cluster");
  if (iConfiguration.containsField("class"))   vertexClass=(String)resolve(iConfiguration.field("class"));
  if (iConfiguration.containsField("skipDuplicates"))   skipDuplicates=(Boolean)resolve(iConfiguration.field("skipDuplicates"));
}
