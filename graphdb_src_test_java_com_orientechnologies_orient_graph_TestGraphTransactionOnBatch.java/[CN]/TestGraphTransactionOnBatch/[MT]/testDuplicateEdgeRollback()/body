{
  OClass clazz=db.getMetadata().getSchema().createClass("Test");
  clazz.setSuperClass(E);
  clazz.createProperty("aKey",OType.STRING).createIndex(INDEX_TYPE.UNIQUE);
  try {
    db.command(new OCommandScript("sql","BEGIN \n LET a = create vertex V \n LET b = create vertex V \n LET c =create edge Test from $a to $b SET aKey = \"12345\" \n LET d =create edge Test from $a to $b SET aKey = \"12345\"  \n RETURN $c \n COMMIT")).execute();
    Assert.fail("expected record duplicate exception");
  }
 catch (  ORecordDuplicatedException ex) {
  }
  List<ODocument> res=db.query(new OSQLSynchQuery("select from Test"));
  Assert.assertEquals(0,res.size());
}
