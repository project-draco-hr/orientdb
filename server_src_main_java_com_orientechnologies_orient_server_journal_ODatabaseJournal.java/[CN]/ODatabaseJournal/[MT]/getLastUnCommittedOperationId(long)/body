{
  final int filled=file.getFilledUpTo();
  if (filled == 0 || iOffset <= 0 || iOffset > filled)   return new long[]{-1,-1};
  lock.acquireExclusiveLock();
  try {
    long offset=this.getLongestUncommiteJournal(100,iOffset);
    final long[] ids=new long[2];
    ids[0]=file.readLong(offset - OFFSET_BACK_RUNID);
    ids[1]=file.readLong(offset - OFFSET_BACK_OPERATID);
    return ids;
  }
  finally {
    lock.releaseExclusiveLock();
  }
}
