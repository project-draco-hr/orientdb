{
  checkForRemoteServer();
  if (iText == null)   return;
  long start=System.currentTimeMillis();
  currentResultSet.clear();
  Object result=currentDatabase.command(new OCommandScript("javascript",iText.toString())).execute();
  if (result instanceof Collection<?>) {
    currentResultSet=(List<OIdentifiable>)result;
    dumpResultSet(-1);
    out.printf("Server side script executed in %f sec(s). Returned %d records",(float)(System.currentTimeMillis() - start) / 1000,currentResultSet.size());
  }
 else   out.printf("Server side script executed in %f sec(s). Value returned is: %s",(float)(System.currentTimeMillis() - start) / 1000,result);
}
