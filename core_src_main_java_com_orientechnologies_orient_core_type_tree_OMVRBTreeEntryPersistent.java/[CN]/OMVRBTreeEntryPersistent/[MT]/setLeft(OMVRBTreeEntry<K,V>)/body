{
  if (iLeft != left) {
    left=(OMVRBTreeEntryPersistent<K,V>)iLeft;
    if (left != null && !left.record.getIdentity().equals(leftRid))     markDirty();
    leftRid=iLeft == null ? ORecordId.EMPTY_RECORD_ID : left.record.getIdentity();
    if (left != null && left.parent != this)     left.setParent(this);
    checkEntryStructure();
  }
}
