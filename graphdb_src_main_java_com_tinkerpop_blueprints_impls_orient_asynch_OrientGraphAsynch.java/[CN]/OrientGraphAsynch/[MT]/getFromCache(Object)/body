{
  if (id.equals(lastCachedId)) {
    reusedLastVertex.incrementAndGet();
    return lastCachedVertex.copy();
  }
  if (vertexCache != null) {
    final OrientVertex v=vertexCache.get(id);
    if (v != null) {
      reusedCachedVertex.incrementAndGet();
      return v.copy();
    }
  }
  return null;
}
