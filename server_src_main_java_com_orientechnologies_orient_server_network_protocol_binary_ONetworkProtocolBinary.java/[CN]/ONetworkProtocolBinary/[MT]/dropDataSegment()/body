{
  setDataCommandInfo("Drop data segment");
  if (!isConnectionAlive())   return;
  final String name=channel.readString();
  boolean result=connection.database.dropDataSegment(name);
  beginResponse();
  try {
    sendOk(clientTxId);
    channel.writeByte((byte)(result ? 1 : 0));
  }
  finally {
    endResponse();
  }
}
