{
  int pos=OVarIntSerializer.write(bytes,value.size());
  writeOType(bytes,bytes.alloc(1),OType.ANY);
  for (  Object itemValue : value) {
    if (itemValue == null) {
      writeOType(bytes,bytes.alloc(1),OType.ANY);
      continue;
    }
    OType type;
    if (linkedType == null)     type=getTypeFromValue(itemValue,true);
 else     type=linkedType;
    if (type != null) {
      writeOType(bytes,bytes.alloc(1),type);
      writeSingleValue(bytes,itemValue,type,null);
    }
  }
  return pos;
}
