{
  if (database.isClosed())   database.open("admin","admin");
  List<ORID> result=new ArrayList<ORID>((Set<ORID>)database.command(new OCommandSQL("find references 19:0 [GraphCar]")).execute());
  Assert.assertTrue(result.size() == 1);
  Assert.assertTrue(result.get(0).toString().equals("21:0"));
  result=new ArrayList<ORID>((Set<ORID>)database.command(new OCommandSQL("find references 19:0 [company,cluster:GraphMotocycle]")).execute());
  Assert.assertTrue(result.size() == 1);
  Assert.assertTrue(result.get(0).toString().equals("22:0"));
  result=new ArrayList<ORID>((Set<ORID>)database.command(new OCommandSQL("find references 19:0 [company,account,cluster:OGraphEdge]")).execute());
  Assert.assertTrue(result.size() == 0);
  result.clear();
  result=null;
  database.close();
}
