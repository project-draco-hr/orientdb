{
  try {
    record.setDatabase(ODatabaseRecordThreadLocal.INSTANCE.get());
    record.reload();
  }
 catch (  Exception e) {
    OLogManager.instance().warn(this,"Error on loading index node record %s",e,record.getIdentity());
  }
  record.recycle(this);
  fromStream(record.toStream());
  if (OLogManager.instance().isDebugEnabled())   OLogManager.instance().debug(this,"Loaded tree node %s (%s-%s)",record.getIdentity(),getFirstKey(),getLastKey());
  return this;
}
