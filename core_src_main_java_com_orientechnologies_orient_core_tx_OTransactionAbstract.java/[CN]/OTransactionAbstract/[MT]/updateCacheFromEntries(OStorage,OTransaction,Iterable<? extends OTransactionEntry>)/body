{
  final ODatabaseRecordCache dbCache=iTx.getDatabase().getCache();
  for (  OTransactionEntry txEntry : iEntries) {
    if (txEntry.status == OTransactionEntry.DELETED)     dbCache.deleteRecord(txEntry.getRecord().getIdentity());
 else     if (txEntry.status == OTransactionEntry.UPDATED || txEntry.status == OTransactionEntry.CREATED)     dbCache.updateRecord(txEntry.getRecord());
  }
}
