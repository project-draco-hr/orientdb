{
  if (graph != null)   graph.setCurrentGraphInThreadLocal();
  if (getLabel() == null)   return StringFactory.E + StringFactory.L_BRACKET + getId()+ StringFactory.R_BRACKET+ StringFactory.L_BRACKET+ getVertex(Direction.OUT).getId()+ StringFactory.ARROW+ getVertex(Direction.IN).getId()+ StringFactory.R_BRACKET;
  return StringFactory.edgeString(this);
}
