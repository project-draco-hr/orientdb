{
  if (closed)   return;
  closed=true;
  for (  PoolPartition partition : partitions) {
    if (partition == null)     continue;
    final Queue<DatabaseDocumentTxPolled> queue=partition.queue;
    while (!queue.isEmpty()) {
      DatabaseDocumentTxPolled db=queue.poll();
      OStorage storage=db.getStorage();
      storage.close();
    }
  }
}
