{
synchronized (pendingAlignments) {
    final String key=iNode + "/" + iDatabaseName;
    if (!pendingAlignments.containsKey(key)) {
      ODistributedServerLog.error(this,getLocalNodeName(),iNode,DIRECTION.IN,"received response to postpone an alignment against an unknown node",iDatabaseName);
    }
    pendingAlignments.put(key,Boolean.TRUE);
  }
}
