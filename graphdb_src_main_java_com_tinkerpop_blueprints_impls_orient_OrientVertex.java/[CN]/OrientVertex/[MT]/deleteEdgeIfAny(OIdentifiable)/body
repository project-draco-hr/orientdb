{
  if (iRecord != null) {
    final ODocument doc=iRecord.getRecord();
    if (doc != null && ODocumentInternal.getImmutableSchemaClass(doc) != null && ODocumentInternal.getImmutableSchemaClass(doc).isSubClassOf(OrientEdgeType.CLASS_NAME))     try {
      doc.delete();
    }
 catch (    ORecordNotFoundException e) {
    }
  }
}
