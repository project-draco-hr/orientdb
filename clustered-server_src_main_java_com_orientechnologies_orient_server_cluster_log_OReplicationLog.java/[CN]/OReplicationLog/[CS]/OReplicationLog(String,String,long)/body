{
  super(REPLICATION_DIRECTORY + iDatabase + "/"+ iNodeId.replace('.','_').replace(':','-')+ EXTENSION,OGlobalConfiguration.DISTRIBUTED_LOG_TYPE.getValueAsString());
  synchEnabled=OGlobalConfiguration.DISTRIBUTED_LOG_SYNCH.getValueAsBoolean();
  file.setFailCheck(false);
  if (exists()) {
    open();
  }
 else   create(DEF_START_SIZE);
  limit=iLimit;
}
