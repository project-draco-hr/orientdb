{
  checkOpeness();
  if (iRecordId == null)   return null;
  ODocument record=rid2Records.get(iRecordId);
  if (record == null) {
    record=(ODocument)underlying.load(iRecordId,iFetchPlan,iIgnoreCache);
    if (record == null)     return null;
  }
  Object result=records2Objects.get(record);
  if (result != null)   return (RET)result;
  return OObjectEntityEnhancer.getInstance().getProxiedInstance(record.getClassName(),entityManager,record);
}
