{
  database.open("admin","admin");
  long time=System.currentTimeMillis();
  OGraphVertex rootNode=database.createVertex().set("id",0);
  createSubNodes(rootNode,0);
  database.setRoot("LinearGraph",rootNode);
  System.out.println("Creation of the graph with deep=" + MAX_DEEP + " and fixed density=1"+ ((System.currentTimeMillis() - time) / 1000f)+ " sec.");
  database.close();
}
