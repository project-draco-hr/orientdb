{
  try {
    query=iQuery;
    text=iText.trim();
    textUpperCase=text.toUpperCase();
    if (extractClustersAndClasses()) {
      rootCondition=extractConditions(null);
    }
  }
 catch (  OQueryParsingException e) {
    throw e;
  }
catch (  Throwable t) {
    throw new OQueryParsingException("",text,currentPos);
  }
}
