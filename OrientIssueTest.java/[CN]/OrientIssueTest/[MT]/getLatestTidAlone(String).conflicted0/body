{
  OSQLSynchQuery<ODocument> query=new OSQLSynchQuery<ODocument>("SELECT  " + TID + " FROM "+ cacheClass+ " WHERE id = ? order by version desc LIMIT 1");
  ODatabaseDocumentTx db=getConnection();
  try {
    List<ODocument> elements=db.command(query).execute(id);
    if (!elements.isEmpty()) {
      return elements.get(0).field(TID);
    }
  }
  finally {
    releaseConnection(db);
  }
  return null;
}
