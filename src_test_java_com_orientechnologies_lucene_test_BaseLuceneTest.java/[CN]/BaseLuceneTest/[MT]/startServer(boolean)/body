{
  String javaExec=System.getProperty("java.home") + "/bin/java";
  System.setProperty("ORIENTDB_HOME",buildDirectory);
  ProcessBuilder processBuilder=new ProcessBuilder(javaExec,"-Xmx2048m","-classpath",System.getProperty("java.class.path"),"-DORIENTDB_HOME=" + buildDirectory,RemoteDBRunner.class.getName(),getDatabaseName(),"" + drop);
  processBuilder.inheritIO();
  process=processBuilder.start();
  Thread.sleep(5000);
}
