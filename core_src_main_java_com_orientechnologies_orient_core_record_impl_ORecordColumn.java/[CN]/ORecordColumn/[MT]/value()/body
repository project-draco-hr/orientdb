{
  if (values.size() == 0)   if (_source != null)   extractValues(OBinaryProtocol.bytes2string(_source));
  StringBuilder stream=new StringBuilder();
  int pos=0;
  for (  String f : values) {
    if (pos > 0)     stream.append(separator);
    if (f != null)     stream.append(f);
    pos++;
  }
  return stream.toString();
}
