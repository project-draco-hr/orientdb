{
  if (iName == null)   throw new IllegalArgumentException("Name is null");
  iName=iName.trim();
  if (iName.length() == 0)   throw new IllegalArgumentException("Name is empty");
  for (int i=0; i < iName.length(); ++i) {
    final char c=iName.charAt(i);
    if (c == ':' || c == ',')     return c;
  }
  return null;
}
