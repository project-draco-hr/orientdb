{
  ODistributedDatabaseInfo obj=databases.get(dbName);
  if (obj != null) {
    obj.close();
    databases.remove(dbName);
  }
  obj=new ODistributedDatabaseInfo(id,dbName,replicator.getReplicatorUser().name,replicator.getReplicatorUser().password,iSynchType,STATUS_TYPE.OFFLINE);
  databases.put(dbName,obj);
  return obj;
}
