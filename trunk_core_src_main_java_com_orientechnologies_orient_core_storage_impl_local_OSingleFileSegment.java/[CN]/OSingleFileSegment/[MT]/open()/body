{
  try {
    acquireExclusiveLock();
    boolean softClosed=file.open();
    if (!softClosed) {
      OLogManager.instance().warn(this,"File " + file.getOsFile().getAbsolutePath() + " was not closed correctly last time. Checking segments...");
    }
    return softClosed;
  }
  finally {
    releaseExclusiveLock();
  }
}
