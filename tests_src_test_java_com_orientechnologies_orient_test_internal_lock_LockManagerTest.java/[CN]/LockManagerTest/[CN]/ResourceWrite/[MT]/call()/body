{
  lockMgr.acquireLock(Thread.currentThread(),this,LOCK.EXCLUSIVE);
  try {
    countWrite++;
    if (verbose)     System.out.println("ResourceWrite locked by " + Thread.currentThread());
    if (countWrite != 1)     throw new AssertionError("countWrite:" + countWrite);
  }
  finally {
    countWrite--;
    lockMgr.releaseLock(Thread.currentThread(),this,LOCK.EXCLUSIVE);
  }
  return null;
}
