{
  if (iName == null || iName.length() == 0) {
    throw new IllegalArgumentException("User name null or empty");
  }
  if (configuration.users != null) {
    for (    OServerUserConfiguration user : configuration.users) {
      if (iName.equals(user.name)) {
        return true;
      }
    }
  }
  return false;
}
