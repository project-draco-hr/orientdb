{
  acquireSharedLock();
  try {
    Map.Entry<OClusterPosition,OPhysicalPosition> entry=content.lowerEntry(position.clusterPosition);
    if (entry != null) {
      return new OPhysicalPosition[]{entry.getValue()};
    }
 else {
      return new OPhysicalPosition[0];
    }
  }
  finally {
    releaseSharedLock();
  }
}
