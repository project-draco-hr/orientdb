{
  record.fromStream(toStream());
  if (record.getIdentity().isValid())   record.setVersion(iSt.updateRecord((ORecordId)record.getIdentity(),record.toStream(),-1,record.getRecordType(),(byte)0,null).getResult());
 else {
    if (record.getIdentity().getClusterId() == ORID.CLUSTER_ID_INVALID)     ((ORecordId)record.getIdentity()).clusterId=clusterId;
    final OPhysicalPosition ppos=iSt.createRecord(0,(ORecordId)record.getIdentity(),record.toStream(),0,record.getRecordType(),(byte)0,null).getResult();
    record.setVersion(ppos.recordVersion);
  }
  record.unsetDirty();
}
