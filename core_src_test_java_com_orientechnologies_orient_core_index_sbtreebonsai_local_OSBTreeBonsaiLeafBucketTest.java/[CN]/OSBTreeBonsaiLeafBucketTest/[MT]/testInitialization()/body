{
  long pointer=directMemory.allocate(OGlobalConfiguration.DISK_CACHE_PAGE_SIZE.getValueAsInteger() * 1024);
  OSBTreeBonsaiBucket<Long,OIdentifiable> treeBucket=new OSBTreeBonsaiBucket<Long,OIdentifiable>(pointer,true,OLongSerializer.INSTANCE,OLinkSerializer.INSTANCE,ODurablePage.TrackMode.FULL);
  Assert.assertEquals(treeBucket.size(),0);
  Assert.assertTrue(treeBucket.isLeaf());
  treeBucket=new OSBTreeBonsaiBucket<Long,OIdentifiable>(pointer,OLongSerializer.INSTANCE,OLinkSerializer.INSTANCE,ODurablePage.TrackMode.FULL);
  Assert.assertEquals(treeBucket.size(),0);
  Assert.assertTrue(treeBucket.isLeaf());
  Assert.assertEquals(treeBucket.getLeftSibling(),-1);
  Assert.assertEquals(treeBucket.getRightSibling(),-1);
  directMemory.free(pointer);
}
