{
  final long startTimer=System.currentTimeMillis();
  try {
    send(new OHazelcastDistributedRequest(manager.getLocalNodeName(),databaseName,null,new OResynchTask(),ODistributedRequest.EXECUTION_MODE.RESPONSE));
  }
 catch (  ODistributedException e) {
  }
  Orient.instance().getProfiler().stopChrono("distributed.replication." + databaseName + ".resynch","Synchronization time among all the nodes",startTimer,"distributed.replication.*.resynch");
}
