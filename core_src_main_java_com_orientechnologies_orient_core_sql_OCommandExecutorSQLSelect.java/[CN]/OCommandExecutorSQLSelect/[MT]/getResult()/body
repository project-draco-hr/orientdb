{
  if (groupedResult != null) {
    for (    ORuntimeResult g : groupedResult.values()) {
      final ODocument doc=g.getResult();
      if (doc != null)       request.getResultListener().result(doc);
    }
    if (request instanceof OSQLSynchQuery)     return ((OSQLSynchQuery<ORecordSchemaAware<?>>)request).getResult();
  }
  return super.getResult();
}
