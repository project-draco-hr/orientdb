{
  if (iIndex < size) {
    stream.move(getKeyPositionInStream(iIndex),ORecordId.PERSISTENT_SIZE);
    if (rids != null)     System.arraycopy(rids,iIndex,rids,iIndex + 1,size - iIndex - 1);
  }
  try {
    itemToStream(iKey,iIndex);
  }
 catch (  IOException e) {
    throw new OSerializationException("Cannot serialize entryRID object: " + this,e);
  }
  if (rids != null)   rids[iIndex]=(ORecordId)iKey.getIdentity();
  size++;
  return setDirty();
}
