{
  if (iIndex instanceof OIndexMultiValues)   return new OIndexRemoteMultiValue(iIndex.getName(),iIndex.getType(),iIndex.getIdentity(),iIndex.getDefinition(),getConfiguration(),iIndex.getClusters());
  return new OIndexRemoteOneValue(iIndex.getName(),iIndex.getType(),iIndex.getIdentity(),iIndex.getDefinition(),getConfiguration(),iIndex.getClusters());
}
