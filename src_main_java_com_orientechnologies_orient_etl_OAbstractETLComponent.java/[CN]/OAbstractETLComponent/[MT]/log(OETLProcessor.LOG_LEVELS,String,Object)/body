{
  if (logLevel.ordinal() >= iLevel.ordinal()) {
    Long extractedNum=(Long)context.getVariable("extractedNum");
    if (extractedNum != null)     System.out.println("[" + extractedNum + ":"+ getName()+ "] "+ iLevel+ " "+ String.format(iText,iArgs));
 else     System.out.println("[" + getName() + "] "+ iLevel+ " "+ String.format(iText,iArgs));
  }
}
