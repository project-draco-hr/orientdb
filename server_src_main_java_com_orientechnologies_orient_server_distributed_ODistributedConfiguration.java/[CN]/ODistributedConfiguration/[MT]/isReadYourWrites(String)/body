{
synchronized (configuration) {
    Object value=getClusterConfiguration(iClusterName).field("readYourWrites");
    if (value == null) {
      value=configuration.field("readYourWrites");
      if (value == null) {
        OLogManager.instance().warn(this,"readYourWrites setting not found for cluster=%s in distributed-config.json",iClusterName);
        return true;
      }
    }
    return (Boolean)value;
  }
}
