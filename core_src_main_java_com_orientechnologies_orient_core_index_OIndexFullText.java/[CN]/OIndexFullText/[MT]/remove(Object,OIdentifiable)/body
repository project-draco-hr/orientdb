{
  checkForOptimization();
  acquireExclusiveLock();
  try {
    final Set<OIdentifiable> recs=get(iKey);
    if (recs != null && !recs.isEmpty()) {
      if (recs.remove(value)) {
        map.put(iKey,recs);
        return true;
      }
    }
  }
  finally {
    releaseExclusiveLock();
  }
  return false;
}
