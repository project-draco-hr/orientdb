{
  checkOpeness();
  lock.acquireExclusiveLock();
  try {
    return doAddCluster(clusterName,location,true,parameters);
  }
 catch (  Exception e) {
    OLogManager.instance().exception("Error in creation of new cluster '" + clusterName + "' of type: "+ clusterType,e,OStorageException.class);
  }
 finally {
    lock.releaseExclusiveLock();
  }
  return -1;
}
