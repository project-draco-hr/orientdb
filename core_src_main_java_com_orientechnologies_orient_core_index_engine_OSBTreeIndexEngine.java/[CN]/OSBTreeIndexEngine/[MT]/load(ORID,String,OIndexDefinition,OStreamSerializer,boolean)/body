{
  acquireExclusiveLock();
  try {
    sbTree.load(indexName,determineKeySerializer(indexDefinition),valueSerializer,indexDefinition != null ? indexDefinition.getTypes() : null,determineKeySize(indexDefinition),indexDefinition != null && !indexDefinition.isNullValuesIgnored());
  }
  finally {
    releaseExclusiveLock();
  }
}
