{
  iClusterName=iClusterName.toLowerCase();
  final boolean locked=lock.acquireSharedLock();
  try {
    for (int i=0; i < clusters.size(); ++i)     if (getClusterById(i).getName().equals(iClusterName))     return getClusterById(i).getId();
    return -1;
  }
  finally {
    lock.releaseSharedLock(locked);
  }
}
