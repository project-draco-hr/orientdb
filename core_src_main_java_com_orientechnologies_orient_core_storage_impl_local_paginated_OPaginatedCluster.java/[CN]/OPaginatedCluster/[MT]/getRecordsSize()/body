{
  acquireSharedLock();
  try {
    diskCache.loadPinnedPage(pinnedStateEntry);
    try {
      return new OPaginatedClusterState(pinnedStateEntry,ODurablePage.TrackMode.NONE).getRecordsSize();
    }
  finally {
      diskCache.release(pinnedStateEntry);
    }
  }
  finally {
    releaseSharedLock();
  }
}
