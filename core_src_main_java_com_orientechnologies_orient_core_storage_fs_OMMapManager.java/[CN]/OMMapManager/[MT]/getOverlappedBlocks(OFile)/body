{
  int count=0;
  final List<OMMapBufferEntry> blocks=bufferPoolPerFile.get(iFile);
  long lastPos=-1;
  for (  OMMapBufferEntry block : blocks) {
    if (lastPos > -1 && lastPos >= block.beginOffset)     count++;
    lastPos=block.beginOffset + block.size;
  }
  if (count > 0)   OLogManager.instance().warn(null,"Overlapped blocks for file %s %d",iFile,count);
  return count;
}
