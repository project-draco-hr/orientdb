{
  database.open("admin","admin");
  int i=0;
  for (  ORecordDocument rec : database.browseCluster("Animal")) {
    if (i % 2 == 0)     Assert.assertEquals(rec.field("location"),"Spain");
 else     Assert.assertEquals(rec.field("location"),"Italy");
    Assert.assertTrue((Float)rec.field("price") == i + 100f);
    i++;
  }
  database.close();
}
