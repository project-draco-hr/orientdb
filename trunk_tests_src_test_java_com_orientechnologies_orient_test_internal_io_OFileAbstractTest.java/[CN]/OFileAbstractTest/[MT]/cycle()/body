{
  System.out.println("Writing " + NUMS + " integers...");
  for (int i=0; i < NUMS; ++i)   file.writeInt(file.allocateSpace(OConstants.SIZE_INT),i);
  data.printSnapshot();
  System.out.println("Checking all written data...");
  for (int i=0; i < NUMS; ++i)   Assert.assertTrue(file.readInt(i * OConstants.SIZE_INT) == i);
  data.printSnapshot();
  file.close();
}
