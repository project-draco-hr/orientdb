{
  Iterable<Vertex> vertices;
  vertices=(Iterable<Vertex>)graph.command(new OCommandSQL("select flatten( outE().outV() ) from " + v1.getId())).execute();
  Assert.assertEquals(vertices.iterator().next(),v1);
  vertices=(Iterable<Vertex>)graph.command(new OCommandSQL("select flatten( outE().inV() ) from " + v1.getId())).execute();
  Assert.assertEquals(vertices.iterator().next(),v2);
  vertices=(Iterable<Vertex>)graph.command(new OCommandSQL("select flatten( inE().inV() ) from " + v2.getId())).execute();
  Assert.assertEquals(vertices.iterator().next(),v2);
  vertices=(Iterable<Vertex>)graph.command(new OCommandSQL("select flatten( inE().outV() ) from " + v2.getId())).execute();
  Assert.assertEquals(vertices.iterator().next(),v1);
}
