{
  acquireSharedLock();
  try {
    final Collection<OIndexInternal<?>> rawResult=indexes.values();
    final List<OIndex<?>> result=new ArrayList<OIndex<?>>(rawResult.size());
    for (    final OIndex<?> index : rawResult)     result.add(wrapInTransactional(index));
    return result;
  }
  finally {
    releaseSharedLock();
  }
}
