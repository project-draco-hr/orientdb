{
  database=ODatabaseObjectPool.global().acquire(url,"admin","admin");
  int i=0;
  Account a;
  for (  Object o : database.browseCluster("Account")) {
    a=(Account)o;
    if (i % 2 == 0)     a.getAddresses().set(0,new Address("work",new City(new Country("Spain"),"Madrid"),"Plaza central"));
    a.setSalary(i + 500.10f);
    database.save(a);
    i++;
  }
  database.close();
}
