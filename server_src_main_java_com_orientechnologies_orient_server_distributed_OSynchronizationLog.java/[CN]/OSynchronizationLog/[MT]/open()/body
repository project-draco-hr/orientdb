{
  final boolean result=super.open();
  final int tot=totalEntries();
  for (int i=0; i < tot; ++i) {
    final byte operation=file.readByte(i * RECORD_SIZE);
    if (operation > -1)     pendingLogs++;
  }
  if (pendingLogs > 0)   OLogManager.instance().warn(this,"Synchronization log %s/%s found %d records to align once the node is online again",nodeId,databaseName,pendingLogs);
  return result;
}
