{
  if (aligned == -1) {
    ODistributedServerLog.info(this,iManager.getLocalNodeName(),null,DIRECTION.IN,"alignment postponed for db '%s'",iDatabaseName);
    iManager.postponeAlignment(iManager.getLocalNodeName(),iDatabaseName);
  }
 else {
    ODistributedServerLog.info(this,iManager.getLocalNodeName(),null,DIRECTION.IN,"alignment ended against db '%s': %d operation(s)",iDatabaseName,aligned);
    iManager.endAlignment(iManager.getLocalNodeName(),iDatabaseName,aligned);
  }
  return null;
}
