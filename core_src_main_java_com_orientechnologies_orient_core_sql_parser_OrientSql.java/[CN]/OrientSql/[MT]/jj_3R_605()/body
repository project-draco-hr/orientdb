{
  if (jj_scan_token(REVOKE))   return true;
  if (jj_3R_708())   return true;
  if (jj_scan_token(ON))   return true;
  if (jj_3R_709())   return true;
  Token xsp;
  while (true) {
    xsp=jj_scanpos;
    if (jj_3R_711()) {
      jj_scanpos=xsp;
      break;
    }
  }
  if (jj_scan_token(FROM))   return true;
  if (jj_3R_99())   return true;
  return false;
}
