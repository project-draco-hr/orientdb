{
  Orient.instance().getProfiler().registerHookValue("db." + name + ".data.holes","Number of the holes in local database",METRIC_TYPE.COUNTER,new OProfilerHookValue(){
    public Object getValue(){
      return getHoles();
    }
  }
,"db.*.data.holes");
  Orient.instance().getProfiler().registerHookValue("db." + name + ".data.holeSize","Size of the holes in local database",METRIC_TYPE.SIZE,new OProfilerHookValue(){
    public Object getValue(){
      return getHoleSize();
    }
  }
,"db.*.data.holeSize");
}
