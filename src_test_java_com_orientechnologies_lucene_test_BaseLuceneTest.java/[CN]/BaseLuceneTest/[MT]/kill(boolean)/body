{
  if (!soft) {
    try {
      int pid=getUnixPID(process);
      Runtime.getRuntime().exec("kill -9 " + pid).waitFor();
    }
 catch (    Exception e) {
      process.destroy();
    }
  }
 else {
    process.destroy();
  }
  try {
    Thread.sleep(5000);
  }
 catch (  InterruptedException e) {
    e.printStackTrace();
  }
  System.out.println("Process was destroyed");
}
