{
  database.open("admin","admin");
  String[] fields;
  for (  ORecordFlat rec : database.browseCluster(CLUSTER_NAME)) {
    fields=rec.value().split("-");
    int i=Integer.parseInt(fields[0]);
    if (i % 2 == 0) {
      rec.value(rec.value() + "+");
      rec.save();
    }
  }
  database.close();
}
