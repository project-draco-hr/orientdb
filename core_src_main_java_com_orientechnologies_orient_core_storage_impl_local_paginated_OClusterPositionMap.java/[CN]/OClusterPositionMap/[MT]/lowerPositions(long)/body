{
  acquireSharedLock();
  try {
    atomicOperationsManager.acquireReadLock(this);
    try {
      if (clusterPosition == 0)       return OCommonConst.EMPTY_LONG_ARRAY;
      return floorPositions(clusterPosition - 1);
    }
  finally {
      atomicOperationsManager.releaseReadLock(this);
    }
  }
  finally {
    releaseSharedLock();
  }
}
