{
  if (iAttribute == null)   throw new IllegalArgumentException("attribute is null");
switch (iAttribute) {
case NAME:
    return getName();
case SHORTNAME:
  return getShortName();
case SUPERCLASS:
return getSuperClass();
case OVERSIZE:
return getOverSize();
case STRICTMODE:
return isStrictMode();
case ABSTRACT:
return isAbstract();
}
throw new IllegalArgumentException("Cannot find attribute '" + iAttribute + "'");
}
