{
  List<ODocument> resultset=database.query(new OSQLSynchQuery<ODocument>("select * from Profile where nicl is not null and followers is not null").setFetchPlan("followers:-2 nick:-1"));
  ODocument linked;
  for (  ODocument d : resultset) {
    Assert.assertNotNull(d.field("nick"));
    Assert.assertNull(d.field("followers"));
  }
}
