{
  setDirty();
  if (o instanceof OIdentifiable) {
    int elementIndex=recordList.indexOf(o);
    if (indexLoaded(elementIndex))     list.remove(elementIndex);
    return recordList.remove(o);
  }
 else   if (o instanceof Proxy)   recordList.remove((OIdentifiable)OObjectEntitySerializer.getDocument((Proxy)o));
  return list.remove(o);
}
