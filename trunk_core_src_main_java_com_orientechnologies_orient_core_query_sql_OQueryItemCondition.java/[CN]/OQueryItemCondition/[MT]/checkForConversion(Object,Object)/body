{
  Object[] result=new Object[]{l,r};
  if (r instanceof Integer && !(l instanceof Integer)) {
    if (l instanceof String && ((String)l).indexOf(".") > -1)     result[0]=new Float((String)l).intValue();
 else     result[0]=getInteger(l);
  }
 else   if (l instanceof Integer && !(r instanceof Integer)) {
    if (r instanceof String && ((String)r).indexOf(".") > -1)     result[1]=new Float((String)r).intValue();
 else     result[1]=getInteger(r);
  }
 else   if (r instanceof Float && !(l instanceof Float))   result[0]=getFloat(l);
 else   if (l instanceof Float && !(r instanceof Float))   result[1]=getFloat(r);
 else   return null;
  return result;
}
