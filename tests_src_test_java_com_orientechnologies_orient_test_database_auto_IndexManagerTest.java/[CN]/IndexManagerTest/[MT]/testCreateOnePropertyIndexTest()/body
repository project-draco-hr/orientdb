{
  final OIndexManagerProxy indexManager=database.getMetadata().getIndexManager();
  final OIndex result=indexManager.createIndex("propertyone",OClass.INDEX_TYPE.UNIQUE.toString(),new OPropertyIndexDefinition(CLASS_NAME,"fOne",OType.INTEGER,-1),new int[]{database.getClusterIdByName(CLASS_NAME)},null,null);
  assertEquals(result.getName(),"propertyone");
  indexManager.reload();
  assertEquals(database.getMetadata().getIndexManager().getClassIndex(CLASS_NAME,"propertyone").getName(),result.getName());
}
