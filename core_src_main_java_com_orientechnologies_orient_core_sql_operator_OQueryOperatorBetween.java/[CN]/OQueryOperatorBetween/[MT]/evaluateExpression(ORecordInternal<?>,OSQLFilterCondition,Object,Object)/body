{
  if (!iRight.getClass().isArray())   throw new IllegalArgumentException("Found '" + iRight + "' while was expected: "+ getSyntax());
  final Object[] values=(Object[])iRight;
  if (values.length != 3)   throw new IllegalArgumentException("Found '" + OMultiValue.toString(iRight) + "' while was expected: "+ getSyntax());
  return ((Comparable<Object>)iLeft).compareTo(OType.convert(values[0],iLeft.getClass())) >= 0 && ((Comparable<Object>)iLeft).compareTo(OType.convert(values[2],iLeft.getClass())) <= 0;
}
