{
  database.begin();
  try {
    Assert.assertTrue(database.countClass(Account.class) > 0);
    for (    Account a : database.browseClass(Account.class)) {
      Assert.assertNotNull(a.getId());
      a.setName(a.getName() + " v1");
      database.save(a);
      break;
    }
    database.commit();
    Assert.assertTrue(false);
  }
  finally {
    database.rollback();
  }
}
