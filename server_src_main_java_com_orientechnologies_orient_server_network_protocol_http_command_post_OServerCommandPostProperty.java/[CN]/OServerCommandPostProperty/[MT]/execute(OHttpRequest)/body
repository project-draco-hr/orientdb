{
  ODatabaseDocumentTx db=null;
  try {
    db=getProfiledDatabaseInstance(iRequest);
    if (iRequest.content == null || iRequest.content.length() <= 0)     return addSingleProperty(iRequest,db);
 else {
      return addMultipreProperties(iRequest,db);
    }
  }
  finally {
    if (db != null)     OSharedDocumentDatabase.release(db);
  }
}
