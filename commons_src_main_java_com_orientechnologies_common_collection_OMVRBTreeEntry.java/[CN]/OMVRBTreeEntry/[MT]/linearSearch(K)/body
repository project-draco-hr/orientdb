{
  V value=null;
  int i=0;
  tree.pageItemComparator=-1;
  for (int s=getSize(); i < s; ++i) {
    if (tree.comparator != null)     tree.pageItemComparator=tree.comparator.compare(getKeyAt(i),iKey);
 else     tree.pageItemComparator=((Comparable<? super K>)getKeyAt(i)).compareTo(iKey);
    if (tree.pageItemComparator == 0) {
      tree.pageItemFound=true;
      value=getValueAt(i);
      break;
    }
 else     if (tree.pageItemComparator > 0)     break;
  }
  tree.pageIndex=i;
  return value;
}
