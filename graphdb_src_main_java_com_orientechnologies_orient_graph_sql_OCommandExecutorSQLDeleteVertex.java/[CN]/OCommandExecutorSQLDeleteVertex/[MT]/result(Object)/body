{
  final OIdentifiable id=(OIdentifiable)iRecord;
  if (id.getIdentity().isValid()) {
    final OrientBaseGraph graph=OGraphCommandExecutorSQLFactory.getGraph();
    try {
      final OrientVertex v=graph.getVertex(id);
      if (v != null) {
        v.remove();
        removed++;
        return true;
      }
    }
  finally {
      graph.shutdown();
    }
  }
  return false;
}
