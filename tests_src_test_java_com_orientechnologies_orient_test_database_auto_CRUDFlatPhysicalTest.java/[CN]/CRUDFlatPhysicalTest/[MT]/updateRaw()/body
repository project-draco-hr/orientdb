{
  String[] fields;
  for (  ORecordFlat rec : new ORecordIteratorCluster<ORecordFlat>(database,(ODatabaseRecordTx)database.getUnderlying(),database.getClusterIdByName(CLUSTER_NAME),true)) {
    fields=rec.value().split("-");
    int i=Integer.parseInt(fields[0]);
    if (i % 2 == 0) {
      rec.value(rec.value() + "+");
      rec.save();
    }
  }
}
