{
  record.fromStream(toStream());
  if (record.getIdentity().isValid())   record.setVersion(iSt.updateRecord((ORecordId)record.getIdentity(),record.toStream(),-1,record.getRecordType(),(byte)0,null));
 else {
    if (record.getIdentity().getClusterId() == ORID.CLUSTER_ID_INVALID)     ((ORecordId)record.getIdentity()).clusterId=clusterId;
    iSt.createRecord((ORecordId)record.getIdentity(),record.toStream(),record.getRecordType(),(byte)0,null);
  }
  record.unsetDirty();
}
