{
  try {
    assertEquals("The MD5 checksum of the file '" + fileToBeChecked.getAbsolutePath() + "' does not match the given one.",digest,calculateMD5checksum(new FileInputStream(fileToBeChecked)));
  }
 catch (  NoSuchAlgorithmException e) {
    fail(e.getMessage());
  }
catch (  IOException e) {
    fail(e.getMessage());
  }
}
