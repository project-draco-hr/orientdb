{
  if (iObject == null)   return null;
  if (iObject instanceof OSQLFilterItem)   return ((OSQLFilterItem)iObject).getValue(iRecord,iContext);
 else   if (iObject instanceof String) {
    final String s=((String)iObject).trim();
    if (!s.isEmpty() && !OIOUtils.isStringContent(iObject) && !Character.isDigit(s.charAt(0)))     return ODocumentHelper.getFieldValue(iRecord,s,iContext);
  }
  return iObject;
}
