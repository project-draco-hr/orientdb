{
  acquireExclusiveLock();
  OIndex<?> idx;
  try {
    idx=getIndex(DICTIONARY_NAME);
    if (idx == null)     idx=createIndex(DICTIONARY_NAME,OClass.INDEX_TYPE.DICTIONARY.toString(),new OSimpleKeyIndexDefinition(OType.STRING),null,null);
  }
  finally {
    releaseExclusiveLock();
  }
  return new ODictionary<ORecordInternal<?>>((OIndex<OIdentifiable>)idx);
}
