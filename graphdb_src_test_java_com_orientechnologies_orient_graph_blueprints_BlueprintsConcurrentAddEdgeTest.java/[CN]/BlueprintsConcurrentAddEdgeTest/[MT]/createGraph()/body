{
  OrientGraphNoTx graph=null;
  try {
    graph=new OrientGraphNoTx(DBURL);
    if (graph.getRawGraph().exists())     graph.getRawGraph().drop();
 else     graph.shutdown();
    graph=getGraph();
    graph.createVertexType("CustomVertex");
    graph.createEdgeType("CustomEdge");
  }
 catch (  OSchemaException ex) {
  }
  return graph;
}
