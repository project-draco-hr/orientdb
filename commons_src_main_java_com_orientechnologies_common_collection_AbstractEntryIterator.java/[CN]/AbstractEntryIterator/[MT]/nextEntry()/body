{
  if (next == null)   throw new NoSuchElementException();
  if (tree.pageIndex < next.getSize() - 1) {
    tree.pageIndex++;
  }
 else {
    if (tree.modCount != expectedModCount)     throw new ConcurrentModificationException();
    tree.pageIndex=0;
    next=OMVRBTree.successor(next);
    lastReturned=next;
  }
  return next;
}
