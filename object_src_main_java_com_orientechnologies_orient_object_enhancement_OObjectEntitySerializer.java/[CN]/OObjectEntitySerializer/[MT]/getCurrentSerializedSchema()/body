{
  if (!ODatabaseRecordThreadLocal.INSTANCE.isDefined())   return null;
  OStorage storage=ODatabaseRecordThreadLocal.INSTANCE.get().getStorage();
  OObjectEntitySerializedSchema serializedShchema=storage.getResource(OObjectEntitySerializedSchema.class.getSimpleName(),new Callable<OObjectEntitySerializedSchema>(){
    @Override public OObjectEntitySerializedSchema call() throws Exception {
      return new OObjectEntitySerializedSchema();
    }
  }
);
  return serializedShchema;
}
