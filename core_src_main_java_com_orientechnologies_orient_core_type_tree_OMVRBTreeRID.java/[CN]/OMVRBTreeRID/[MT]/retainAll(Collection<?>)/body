{
  ((OMVRBTreeRIDProvider)dataProvider).lazyUnmarshall();
  if (hasNewItems()) {
    final Collection<ORecord<?>> v=newItems.keySet();
    v.retainAll(c);
    if (newItems.size() == 0)     newItems=null;
  }
  boolean modified=false;
  final Iterator<?> e=iterator();
  while (e.hasNext()) {
    if (!c.contains(e.next())) {
      e.remove();
      modified=true;
    }
  }
  return modified;
}
