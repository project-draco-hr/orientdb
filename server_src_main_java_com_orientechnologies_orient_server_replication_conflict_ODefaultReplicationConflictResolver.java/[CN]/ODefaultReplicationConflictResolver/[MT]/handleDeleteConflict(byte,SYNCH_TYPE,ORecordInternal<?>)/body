{
  logger.log(this,Level.WARNING,TYPE.REPLICATION,DIRECTION.IN,"-> %s (%s mode) CONFLICT %s record %s (cannot be deleted on other node)",ORecordOperation.getName(iOperation),iRecord.getIdentity());
  if (searchForConflict(iRecord) != null) {
    logger.log(this,Level.FINE,TYPE.REPLICATION,DIRECTION.IN,"Conflict already present for record %s, skip it",iRecord.getIdentity());
  }
 else {
    final ODocument doc=createConflictDocument(iOperation,iRecord);
    doc.save();
  }
}
