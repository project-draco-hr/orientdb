{
  if ("memory:test".equals(url))   new ODatabaseDocumentTx(url).create().close();
  db=new ODatabaseDocumentTx(url).open("admin","admin");
  if (db.getMetadata().getSchema().existsClass("Concurrent"))   db.getMetadata().getSchema().dropClass("Concurrent");
  db.getMetadata().getSchema().createClass("Concurrent");
  for (int i=0; i < 1000; ++i) {
    db.newInstance("Concurrent").field("test",i).save();
  }
}
