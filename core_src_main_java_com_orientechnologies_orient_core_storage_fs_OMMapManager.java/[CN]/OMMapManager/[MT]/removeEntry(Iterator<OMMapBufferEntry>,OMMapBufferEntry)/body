{
  if (entry.flush()) {
    it.remove();
    final List<OMMapBufferEntry> file=bufferPoolPerFile.get(entry.file);
    if (file != null) {
      file.remove(entry);
      if (file.isEmpty())       bufferPoolPerFile.remove(entry.file);
    }
    entry.buffer=null;
    totalMemory-=entry.size;
    return true;
  }
  return false;
}
