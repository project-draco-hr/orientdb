{
  V v=values[iIndex];
  if (v == null)   try {
    PROFILER.updateCounter(PROFILER.getProcessMetric("mvrbtree.entry.unserializeValue"),"Deserialize a MVRBTree entry value",1);
    v=(V)valueFromStream(iIndex);
    if (((OMVRBTreeMapProvider<K,V>)treeDataProvider).keepValuesInMemory)     values[iIndex]=v;
  }
 catch (  IOException e) {
    OLogManager.instance().error(this,"Cannot lazy load the value #" + iIndex + " in tree node "+ this,e,OSerializationException.class);
  }
  return v;
}
