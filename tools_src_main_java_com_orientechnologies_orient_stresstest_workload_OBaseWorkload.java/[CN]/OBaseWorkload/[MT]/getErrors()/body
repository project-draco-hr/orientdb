{
  final StringBuilder buffer=new StringBuilder();
  if (!errors.isEmpty()) {
    buffer.append("\nERRORS:");
    for (int i=0; i < errors.size(); ++i) {
      buffer.append("\n");
      buffer.append(i);
      buffer.append(": ");
      buffer.append(errors.get(i));
    }
  }
  return buffer.toString();
}
