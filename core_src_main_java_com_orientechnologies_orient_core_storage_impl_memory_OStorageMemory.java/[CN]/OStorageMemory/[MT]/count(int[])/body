{
  lock.acquireSharedLock();
  try {
    long tot=0;
    for (int i=0; i < iClusterIds.length; ++i) {
      if (iClusterIds[i] > -1) {
        final OCluster cluster=clusters.get(iClusterIds[i]);
        if (cluster != null)         tot+=cluster.getEntries();
      }
    }
    return tot;
  }
  finally {
    lock.releaseSharedLock();
  }
}
