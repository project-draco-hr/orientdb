{
  super.delete();
  if (getLeft() != null)   ((OMVRBTreeEntryPersistent<K,V>)getLeft()).delete();
  leftRid=null;
  if (getRight() != null)   ((OMVRBTreeEntryPersistent<K,V>)getRight()).delete();
  rightRid=null;
  record.setDatabase(ODatabaseRecordThreadLocal.INSTANCE.get());
  record.delete();
  keys=null;
  values=null;
  serializedKeys=null;
  serializedValues=null;
  return this;
}
