{
  if (subBlocks == null || subBlocks.size() == 0) {
    return "";
  }
  if (subBlocks.size() == 1) {
    return subBlocks.get(0).toString();
  }
  StringBuilder result=new StringBuilder();
  boolean first=true;
  for (  OBooleanExpression expr : subBlocks) {
    if (!first) {
      result.append(" AND ");
    }
    result.append(expr.toString());
    first=false;
  }
  return result.toString();
}
