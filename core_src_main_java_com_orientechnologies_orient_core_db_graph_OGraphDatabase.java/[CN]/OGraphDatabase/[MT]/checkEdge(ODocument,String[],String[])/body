{
  boolean good=true;
  if (iClassNames != null) {
    good=false;
    for (    String c : iClassNames) {
      if (c.equals(iEdge.getClassName())) {
        good=true;
        break;
      }
    }
  }
  if (good && iLabels != null) {
    good=false;
    for (    String c : iLabels) {
      if (c.equals(iEdge.field(LABEL))) {
        good=true;
        break;
      }
    }
  }
  return good;
}
