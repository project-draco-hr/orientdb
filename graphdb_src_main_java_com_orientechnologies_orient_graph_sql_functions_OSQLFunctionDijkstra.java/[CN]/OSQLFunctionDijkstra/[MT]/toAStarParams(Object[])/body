{
  Object[] result=new Object[4];
  result[0]=iParams[0];
  result[1]=iParams[1];
  result[2]=iParams[2];
  Map<String,Object> options=new HashMap<String,Object>();
  options.put("emptyIfMaxDepth",true);
  if (iParams.length > 3) {
    options.put("direction",iParams[3]);
  }
  result[3]=options;
  return result;
}
