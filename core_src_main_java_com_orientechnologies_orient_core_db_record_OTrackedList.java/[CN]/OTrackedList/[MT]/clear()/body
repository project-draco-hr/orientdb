{
  final List<T> origValues;
  if (changeListeners.isEmpty())   origValues=null;
 else   origValues=new ArrayList<T>(this);
  super.clear();
  if (origValues != null)   for (int i=origValues.size() - 1; i >= 0; i--) {
    fireCollectionChangedEvent(new OMultiValueChangeEvent<Integer,T>(OMultiValueChangeEvent.OChangeType.REMOVE,i,null,origValues.get(i)));
  }
 else   setDirty();
}
