{
  if (maxValuesToFetch < 0)   return getValuesMinor(toKey,isInclusive,true);
  final OCommandRequest cmd;
  if (isInclusive)   cmd=formatCommand(QUERY_GET_VALUE_MINOR_EQUALS + QUERY_GET_VALUES_LIMIT + maxValuesToFetch,name);
 else   cmd=formatCommand(QUERY_GET_VALUE_MINOR + QUERY_GET_VALUES_LIMIT + maxValuesToFetch,name);
  return (Collection<OIdentifiable>)getDatabase().command(cmd).execute(toKey);
}
