{
  record.fromStream(toStream());
  if (record.getIdentity().isValid())   record.setVersion(iSt.updateRecord((ORecordId)record.getIdentity(),record.toStream(),-1,record.getRecordType(),(byte)0,null));
 else {
    if (record.getIdentity().getClusterId() == ORID.CLUSTER_ID_INVALID)     ((ORecordId)record.getIdentity()).clusterId=treeDataProvider.clusterId;
    record.setIdentity(record.getIdentity().getClusterId(),iSt.createRecord(0,(ORecordId)record.getIdentity(),record.toStream(),record.getRecordType(),(byte)0,null));
  }
  record.unsetDirty();
  if (!record.getIdentity().isTemporary())   record.removeListener(this);
}
