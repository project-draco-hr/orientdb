{
  super(iStorage,iConfig,DEF_EXTENSION,RECORD_SIZE);
  id=iConfig.getId();
  iConfig.holeFile=new OStorageClusterHoleConfiguration(iConfig,OStorageVariableParser.DB_PATH_VARIABLE + "/" + iConfig.name,iConfig.fileType,iConfig.fileMaxSize);
  holeSegment=new OClusterLocalHole(this,iStorage,iConfig.holeFile);
}
