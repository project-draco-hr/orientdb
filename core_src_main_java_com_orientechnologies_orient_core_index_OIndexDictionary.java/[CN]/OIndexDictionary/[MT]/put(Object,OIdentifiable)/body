{
  modificationLock.requestModificationLock();
  key=getCollatingValue(key);
  try {
    checkForKeyType(key);
    acquireSharedLock();
    try {
      markStorageDirty();
      indexEngine.put(key,value);
      return this;
    }
  finally {
      releaseSharedLock();
    }
  }
  finally {
    modificationLock.releaseModificationLock();
  }
}
