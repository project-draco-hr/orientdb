{
  if (tempResult != null) {
    int fetched=0;
    if (tempResult instanceof OAutoConvertToRecord)     ((OAutoConvertToRecord)tempResult).setAutoConvertToRecord(true);
    for (    Object d : tempResult)     if (d != null) {
      if (!(d instanceof OIdentifiable))       d=new ODocument().field("value",d);
 else       if (!(d instanceof ORID || d instanceof ORecord))       d=((OIdentifiable)d).getRecord();
      if (limit > -1 && fetched >= limit)       break;
      if (!request.getResultListener().result(d))       break;
      ++fetched;
    }
  }
  if (request instanceof OSQLSynchQuery)   return ((OSQLSynchQuery<ODocument>)request).getResult();
  return null;
}
