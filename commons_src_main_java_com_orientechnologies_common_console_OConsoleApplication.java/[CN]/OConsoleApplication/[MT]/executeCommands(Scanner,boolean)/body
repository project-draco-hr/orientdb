{
  iScanner.useDelimiter(";");
  try {
    while (iScanner.hasNext()) {
      String commandLine=iScanner.next();
      final RESULT status=execute(commandLine);
      if (status == RESULT.EXIT || status == RESULT.ERROR && iExitOnException)       return false;
    }
  }
  finally {
    iScanner.close();
  }
  return true;
}
