{
  StringBuilder builder=new StringBuilder();
switch (type) {
case INDEX:
    builder.append("INDEX");
  break;
case VALUES:
builder.append("INDEXVALUES");
break;
case VALUESASC:
builder.append("INDEXVALUESASC");
break;
case VALUESDESC:
builder.append("INDEXVALUESDESC");
break;
}
builder.append(":");
builder.append(indexName);
return builder.toString();
}
