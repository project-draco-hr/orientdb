{
  final OIndex<?> index=indexes.get(iName.toLowerCase());
  if (index == null)   return null;
  if (index instanceof OIndexMultiValues)   return new OIndexTxAwareMultiValue(getDatabase(),(OIndex<Collection<OIdentifiable>>)getIndexInstance(index));
 else   return new OIndexTxAwareOneValue(getDatabase(),(OIndex<OIdentifiable>)getIndexInstance(index));
}
