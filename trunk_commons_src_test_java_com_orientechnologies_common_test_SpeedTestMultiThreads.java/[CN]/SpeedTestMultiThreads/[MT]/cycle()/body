{
  SpeedTestThread t;
  for (int i=0; i < threads; ++i)   try {
    t=threadClass.newInstance();
    t.setOwner(this);
    t.setCycles(threadCycles / threads);
    t.start();
  }
 catch (  Exception e) {
    e.printStackTrace();
    return;
  }
  while (activeThreads > 0) {
    try {
      Thread.sleep(50);
    }
 catch (    InterruptedException e) {
    }
  }
}
