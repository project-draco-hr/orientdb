{
  Class<?> clz=m.getDeclaringClass();
  if (!fieldName.startsWith("set") || !checkIfFirstCharAfterPrefixIsUpperCase(fieldName,"set") || (isScalaClass(clz) && !fieldName.endsWith("_$eq")))   return false;
  if (m.getParameterTypes() != null && m.getParameterTypes().length != 1)   return false;
  return !OObjectEntitySerializer.isTransientField(m.getDeclaringClass(),getFieldName(m));
}
