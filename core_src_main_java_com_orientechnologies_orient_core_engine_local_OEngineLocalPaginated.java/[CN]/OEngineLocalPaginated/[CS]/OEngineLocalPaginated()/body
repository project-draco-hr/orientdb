{
  readCache=new O2QCache((long)(OGlobalConfiguration.DISK_CACHE_SIZE.getValueAsLong() * 1024 * 1024* ((100 - OGlobalConfiguration.DISK_WRITE_CACHE_PART.getValueAsInteger()) / 100.0)),OGlobalConfiguration.DISK_CACHE_PAGE_SIZE.getValueAsInteger() * 1024,true);
  try {
    readCache.registerMBean();
  }
 catch (  Exception e) {
    OLogManager.instance().error(this,"MBean for read cache can not be registered",e);
  }
}
