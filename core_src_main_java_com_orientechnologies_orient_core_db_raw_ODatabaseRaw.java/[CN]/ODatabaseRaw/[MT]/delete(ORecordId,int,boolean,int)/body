{
  try {
    if (!storage.deleteRecord(iRid,iVersion,iMode,null) && iRequired)     throw new ORecordNotFoundException("The record with id " + iRid + " was not found");
  }
 catch (  OException e) {
    throw e;
  }
catch (  Exception e) {
    OLogManager.instance().exception("Error on deleting record " + iRid,e,ODatabaseException.class);
  }
}
