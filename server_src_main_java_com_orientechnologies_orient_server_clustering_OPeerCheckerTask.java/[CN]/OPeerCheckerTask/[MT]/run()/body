{
  final List<ORemotePeer> nodeList=leader.getPeerNodeList();
  if (nodeList == null || nodeList.size() == 0)   return;
  try {
    for (    ORemotePeer node : nodeList) {
      if (node.getStatus() == STATUS.CONNECTED)       if (!node.sendHeartBeat(leader.getManager().getConfig().networkTimeoutLeader)) {
        leader.handlePeerNodeFailure(node);
      }
    }
    nodeList.clear();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
