{
  ODistributedTxContextImpl ctx=new ODistributedTxContextImpl(this,reqId);
  final ODistributedTxContextImpl prevCtx=activeTxContexts.putIfAbsent(reqId,ctx);
  if (prevCtx != null) {
    ODistributedServerLog.debug(this,getLocalNodeName(),null,DIRECTION.NONE,"Distributed transaction: repeating request %s in database '%s'",reqId,databaseName);
    ctx=prevCtx;
  }
 else   ODistributedServerLog.debug(this,getLocalNodeName(),null,DIRECTION.NONE,"Distributed transaction: registered request %s in database '%s'",reqId,databaseName);
  return ctx;
}
