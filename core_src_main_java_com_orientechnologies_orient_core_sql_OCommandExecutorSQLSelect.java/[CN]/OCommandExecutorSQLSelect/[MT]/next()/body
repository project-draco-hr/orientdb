{
  if (lastRecord != null) {
    final OIdentifiable result=lastRecord;
    lastRecord=null;
    return result;
  }
  if (target == null)   assignTarget(null);
  if (subiterator == null)   subiterator=(Iterator<OIdentifiable>)target.iterator();
  while (subiterator.hasNext()) {
    if (!executeSearchRecord(subiterator.next()))     break;
    if (lastRecord != null)     return lastRecord;
  }
  return lastRecord;
}
