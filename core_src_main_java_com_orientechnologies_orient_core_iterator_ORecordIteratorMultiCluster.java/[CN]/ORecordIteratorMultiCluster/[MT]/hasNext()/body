{
  if (limit > -1 && browsedRecords >= limit)   return false;
  if (browsedRecords >= totalAvailableRecords)   return false;
  if (currentClusterIdx < clusterIds.length - 1)   return true;
  if (liveUpdated)   return currentClusterPosition < database.getStorage().getClusterLastEntryPosition(lastClusterId) - 1;
  return currentClusterPosition < lastClusterPosition - 1;
}
