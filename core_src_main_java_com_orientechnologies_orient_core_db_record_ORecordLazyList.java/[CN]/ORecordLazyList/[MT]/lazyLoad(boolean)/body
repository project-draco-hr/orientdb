{
  if (loaded || stream == null)   return false;
  final List<String> items=OStringSerializerHelper.smartSplit(stream,OStringSerializerHelper.RECORD_SEPARATOR);
  super.clear();
  for (  String item : items) {
    if (item != null && item.length() > 0)     item=item.substring(1);
    if (item.length() == 0)     continue;
    super.add(new ORecordId(item));
  }
  if (iInvalidateStream)   stream=null;
  loaded=true;
  return true;
}
