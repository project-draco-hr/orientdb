{
  int currentIndex=-1;
  int maxDepth=0;
  for (int i=start; i < end; i++) {
    final long nodePosition=node[i];
    if (nodePosition >= 0)     continue;
    final int index=(int)((nodePosition & Long.MAX_VALUE) >>> 8);
    if (index == currentIndex)     continue;
    currentIndex=index;
    if (maxDepth < nodeLocalDepths[index])     maxDepth=nodeLocalDepths[index];
  }
  return maxDepth;
}
