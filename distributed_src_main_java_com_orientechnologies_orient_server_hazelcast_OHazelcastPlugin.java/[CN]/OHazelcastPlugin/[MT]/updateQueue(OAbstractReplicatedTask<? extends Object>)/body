{
  ODistributedThreadLocal.INSTANCE.set(null);
synchronized (lockQueue) {
    ODistributedServerLog.debug(this,iTask.getNodeSource(),iTask.getNodeDestination(),DIRECTION.IN,"completed operation=%d:%d",iTask.getRunId(),iTask.getOperationSerial());
    executionQueue.put(iTask.getRunId(),iTask.getOperationSerial());
    lockQueue.notifyAll();
  }
}
