{
  if (isEnabled()) {
synchronized (_AuthenticatorsList) {
      for (      OSecurityAuthenticator am : _AuthenticatorsList) {
        if (authMethod == null || authMethod.isEmpty())         return am;
        if (am.getName() != null && am.getName().equalsIgnoreCase(authMethod))         return am;
      }
    }
  }
  return null;
}
