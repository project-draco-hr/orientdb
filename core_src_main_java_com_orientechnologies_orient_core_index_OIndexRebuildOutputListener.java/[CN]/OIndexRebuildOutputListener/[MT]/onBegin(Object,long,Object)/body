{
  startTime=System.currentTimeMillis();
  lastDump=startTime;
  rebuild=(Boolean)iRebuild;
  if (rebuild)   OLogManager.instance().info(this,"- Rebuilding index %s.%s...",idx.getDatabaseName(),idx.getName());
 else   OLogManager.instance().debug(this,"- Building index %s.%s...",idx.getDatabaseName(),idx.getName());
}
