{
  interactiveMode=isInteractiveMode(args);
  onBefore();
  if (interactiveMode) {
    String consoleInput;
    while (true) {
      out.println();
      out.print("> ");
      consoleInput=reader.readLine();
      if (consoleInput == null || consoleInput.length() == 0)       continue;
      if (!executeCommands(consoleInput))       break;
    }
  }
 else {
    executeBatch(getCommandLine(args));
  }
  onAfter();
}
