{
  int newPos=OSQLHelper.nextWord(text,textUpperCase,currentPos,tempParseWord,true);
  if (newPos == -1)   throw new OCommandSQLParsingException("Cannot find expected keyword '" + Arrays.toString(iWords) + "'. Use "+ getSyntax(),text,currentPos);
  boolean found=false;
  for (  String w : iWords) {
    if (tempParseWord.toString().equals(w)) {
      found=true;
      break;
    }
  }
  if (!found)   throw new OCommandSQLParsingException("Found unexpected keyword '" + tempParseWord + "' while it was expected '"+ Arrays.toString(iWords)+ "'. Use "+ getSyntax(),text,currentPos);
  currentPos=newPos;
}
