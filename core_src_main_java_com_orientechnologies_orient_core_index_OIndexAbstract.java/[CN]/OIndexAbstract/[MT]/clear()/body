{
  checkForRebuild();
  modificationLock.requestModificationLock();
  try {
    acquireSharedLock();
    try {
      markStorageDirty();
      indexEngine.clear();
      return this;
    }
  finally {
      releaseSharedLock();
    }
  }
  finally {
    modificationLock.releaseModificationLock();
  }
}
