{
  final OPhysicalPosition result=getStorage().createRecord(0,rid,content,version,recordType,0,null);
  if (rid.getClusterPosition() != result.clusterPosition)   OLogManager.instance().warn(this,"DISTRIBUTED <- detected conflict against operation %s RID local %s != remote #%d:%d",getName(),rid,rid.getClusterId(),result.clusterPosition);
  if (isRedistribute())   dbSynchronizer.updateOperationRid(rid,result.recordVersion,getNodeSource());
  return result;
}
