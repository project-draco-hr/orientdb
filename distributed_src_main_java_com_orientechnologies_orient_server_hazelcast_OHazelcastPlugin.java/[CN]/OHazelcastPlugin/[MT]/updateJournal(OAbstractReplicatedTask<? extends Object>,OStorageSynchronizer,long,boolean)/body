{
  try {
    if (iSuccess)     iTask.setAsCommitted(dbSynchronizer,operationLogOffset);
 else     iTask.setAsCanceled(dbSynchronizer,operationLogOffset);
  }
 catch (  IOException e) {
    ODistributedServerLog.error(this,getLocalNodeName(),iTask.getNodeSource(),DIRECTION.IN,"error on changing the log status for %s db=%s %s",e,getName(),iTask.getDatabaseName(),iTask.getPayload());
    throw new ODistributedException("Error on changing the log status",e);
  }
}
