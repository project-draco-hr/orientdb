{
  if (streamKeySerializer instanceof OBinarySerializer)   return (OBinarySerializer<K>)streamKeySerializer;
  if (streamKeySerializer instanceof OStreamSerializerLiteral)   return (OBinarySerializer<K>)new OSimpleKeySerializer();
  if (streamKeySerializer instanceof OStreamSerializerLong)   return (OBinarySerializer<K>)OLongSerializer.INSTANCE;
  throw new OSerializationException("Given serializer " + streamKeySerializer.getClass().getName() + " can not be converted into "+ OBinarySerializer.class.getName()+ ".");
}
