{
  if (iIndex < size)   stream.move(getKeyPositionInStream(iIndex),ORecordId.PERSISTENT_SIZE);
  try {
    iKey.getIdentity().toStream(moveToIndex(iIndex));
  }
 catch (  IOException e) {
    throw new OSerializationException("Cannot serialize entryRID object: " + this,e);
  }
  size++;
  return setDirty();
}
