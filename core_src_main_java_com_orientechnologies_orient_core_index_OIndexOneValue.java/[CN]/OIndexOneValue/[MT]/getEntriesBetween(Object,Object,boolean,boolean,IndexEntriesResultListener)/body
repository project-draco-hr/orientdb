{
  checkForRebuild();
  if (iRangeFrom.getClass() != iRangeTo.getClass())   throw new IllegalArgumentException("Range from-to parameters are of different types");
  iRangeFrom=getCollatingValue(iRangeFrom);
  iRangeTo=getCollatingValue(iRangeTo);
  acquireSharedLock();
  try {
    indexEngine.getEntriesBetween(iRangeFrom,iRangeTo,inclusive,ascOrder,null,new OIndexEngine.EntriesResultListener(){
      @Override public boolean addResult(      ODocument entry){
        return entriesResultListener.addResult(entry);
      }
    }
);
  }
  finally {
    releaseSharedLock();
  }
}
