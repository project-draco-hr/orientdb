{
  final OIndexDefinition indexDefinition=index.getDefinition();
  if (indexDefinition.getParamCount() > 1)   return null;
  final OIndex<?> internalIndex=index.getInternal();
  final Object result;
  if (internalIndex instanceof OIndexFullText) {
    final Object indexResult=index.get(indexDefinition.createValue(keyParams));
    if (indexResult instanceof Collection)     result=indexResult;
 else     if (indexResult == null)     result=Collections.emptyList();
 else     result=Collections.singletonList((OIdentifiable)indexResult);
  }
 else   return null;
  updateProfiler(iContext,internalIndex,keyParams,indexDefinition);
  return result;
}
