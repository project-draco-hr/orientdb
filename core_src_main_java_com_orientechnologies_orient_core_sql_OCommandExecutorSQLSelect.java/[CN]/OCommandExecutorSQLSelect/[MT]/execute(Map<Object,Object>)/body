{
  if (iArgs != null)   for (  Entry<Object,Object> arg : iArgs.entrySet())   context.setVariable(arg.getKey().toString(),arg.getValue());
  if (timeoutMs > 0)   getContext().beginExecution(timeoutMs,timeoutStrategy);
  if (!optimizeExecution()) {
    fetchLimit=getQueryFetchLimit();
    executeSearch(iArgs);
    applyFlatten();
    handleNoTarget();
    handleGroupBy();
    applyOrderBy();
    applyLimitAndSkip();
  }
  return getResult();
}
