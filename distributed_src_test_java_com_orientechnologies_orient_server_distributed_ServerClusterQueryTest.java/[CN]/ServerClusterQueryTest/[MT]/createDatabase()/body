{
  OrientGraphFactory factory=new OrientGraphFactory("plocal:target/server0/databases/" + getDatabaseName());
  OrientGraphNoTx g=factory.getNoTx();
  try {
    g.createVertexType("V1");
    g.createEdgeType("E1");
    v1=g.addVertex("class:V1");
    v2=g.addVertex("class:V2");
    v1.addEdge("E1",v2);
  }
  finally {
    g.shutdown();
  }
}
