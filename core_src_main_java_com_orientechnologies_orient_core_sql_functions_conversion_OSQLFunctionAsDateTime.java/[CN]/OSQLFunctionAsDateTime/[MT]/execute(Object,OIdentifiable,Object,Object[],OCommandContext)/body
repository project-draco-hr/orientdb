{
  final Object value=iFuncParams[0];
  if (value != null) {
    if (value instanceof Date)     return value;
 else     if (value instanceof Number)     return new Date(((Number)value).longValue());
 else     try {
      return ODatabaseRecordThreadLocal.INSTANCE.get().getStorage().getConfiguration().getDateTimeFormatInstance().parse(value.toString());
    }
 catch (    ParseException e) {
    }
  }
  return null;
}
