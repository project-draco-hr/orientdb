{
  lock.acquireSharedLock();
  try {
    if (iDataId < 0 || iDataId > dataSegments.size() - 1)     throw new IllegalArgumentException("Invalid data segment id " + iDataId + ". Range is 0-"+ (dataSegments.size() - 1));
    return dataSegments.get(iDataId);
  }
  finally {
    lock.releaseSharedLock();
  }
}
