{
  checkConnection();
  do {
    boolean locked=acquireExclusiveLock();
    try {
      writeCommand(OChannelBinaryProtocol.REQUEST_DICTIONARY_SIZE);
      readStatus();
      return network.readInt();
    }
 catch (    Exception e) {
      if (handleException("Error on getting size of database's dictionary",e))       break;
    }
 finally {
      releaseExclusiveLock(locked);
    }
  }
 while (true);
  return -1;
}
