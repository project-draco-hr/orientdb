{
  List<OGraphEdge> tempList=outEdges != null ? outEdges.get() : null;
  if (tempList == null) {
    tempList=new ArrayList<OGraphEdge>();
    outEdges=new SoftReference<List<OGraphEdge>>(tempList);
    final List<Object> docs=document.field(FIELD_OUT_EDGES);
    if (docs != null) {
      ODocument doc;
      for (      Object o : docs) {
        if (o instanceof ODocument)         doc=(ODocument)o;
 else         doc=database.getRecordById((ORID)o);
        tempList.add((OGraphEdge)database.getUserObjectByRecord(doc,null));
      }
    }
  }
  return tempList;
}
