{
  message("\nPROPERTIES:");
  message("\n+-------------------------------+--------------------------------+");
  message("\n| %-30s| %-30s |","NAME","VALUE");
  message("\n+-------------------------------+--------------------------------+");
  for (  Entry<String,String> p : properties.entrySet()) {
    message("\n| %-30s| %-30s |",p.getKey(),p.getValue());
  }
  message("\n+-------------------------------+--------------------------------+");
}
