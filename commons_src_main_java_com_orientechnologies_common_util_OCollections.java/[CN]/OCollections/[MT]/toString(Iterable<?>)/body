{
  final StringBuilder builder=new StringBuilder();
  builder.append('[');
  int cnt=0;
  final Iterator<?> ite=iterable.iterator();
  while (ite.hasNext()) {
    if (cnt != 0) {
      builder.append(',');
    }
    cnt++;
    final Object obj=ite.next();
    builder.append(obj);
  }
  builder.append(']');
  return builder.toString();
}
