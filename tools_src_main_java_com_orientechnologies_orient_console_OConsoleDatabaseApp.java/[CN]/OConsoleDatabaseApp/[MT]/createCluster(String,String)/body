{
  checkCurrentDatabase();
  out.println("Creating cluster [" + iClusterName + "] of type '"+ iClusterType+ "' in database "+ currentDatabaseName+ "...");
  int clusterId=iClusterType.equalsIgnoreCase("physical") ? currentDatabase.addPhysicalCluster(iClusterName,iClusterName,-1) : currentDatabase.addLogicalCluster(iClusterName,currentDatabase.getClusterIdByName(OStorage.CLUSTER_INTERNAL_NAME));
  out.println((iClusterType.equalsIgnoreCase("physical") ? "Physical" : "Logical") + " cluster created correctly with id #" + clusterId);
}
