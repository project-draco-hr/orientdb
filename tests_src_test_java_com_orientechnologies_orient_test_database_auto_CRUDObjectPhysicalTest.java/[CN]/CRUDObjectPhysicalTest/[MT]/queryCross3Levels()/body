{
  database=ODatabaseObjectPool.global().acquire(url,"admin","admin");
  final List<Profile> result=database.query(new OSQLSynchQuery<Profile>("select from Profile where location.city.country.name = 'Italy'"));
  Assert.assertTrue(result.size() > 0);
  Profile profile;
  for (int i=0; i < result.size(); ++i) {
    profile=result.get(i);
    Assert.assertEquals(profile.getLocation().getCity().getCountry().getName(),"Italy");
  }
  database.close();
}
