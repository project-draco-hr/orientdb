{
  OLogManager.instance().debug(this,"Discovering distributed server nodes using IP Multicast with address %s, port %d...",dgram.getAddress(),dgram.getPort());
  try {
    socket.send(dgram);
  }
 catch (  Throwable t) {
    shutdown();
    OLogManager.instance().error(this,"Error on sending signal for distributed server presence, probably the IP MULTICAST is disabled in current network configuration: %s",t.getMessage());
  }
 finally {
  }
}
