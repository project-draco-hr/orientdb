{
  try {
    compareClusters();
    compareRecords();
    if (differences == 0) {
      listener.onMessage("\n\nDatabases match.");
      return true;
    }
 else {
      listener.onMessage("\n\nDatabases not match. Found " + differences + " difference(s).");
      return false;
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
    throw new ODatabaseExportException("Error on compare of databases '" + storage1.getName() + "' against '"+ storage2.getName()+ "'",e);
  }
 finally {
    storage1.close();
    storage2.close();
  }
}
