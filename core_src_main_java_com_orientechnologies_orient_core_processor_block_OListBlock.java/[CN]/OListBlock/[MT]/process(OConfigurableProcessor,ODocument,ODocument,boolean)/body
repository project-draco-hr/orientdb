{
  final Object value=((ODocument)iConfig).field("value");
  if (!OMultiValue.isIterable(value))   throw new OProcessException("Content in not multi-value (collection, array, map)");
  final List<Object> list=new ArrayList<Object>();
  for (  Object item : OMultiValue.getMultiValueIterable(value)) {
    final Object result;
    if (isBlock(item))     result=iManager.process(null,(ODocument)item,iContext,iReadOnly);
 else     result=resolveInContext(item,iContext);
    list.add(result);
  }
  return list;
}
