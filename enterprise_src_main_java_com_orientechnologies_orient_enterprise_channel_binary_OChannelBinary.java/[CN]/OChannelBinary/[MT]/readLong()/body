{
  if (debug) {
    OLogManager.instance().debug(this,"%s - Reading long (8 bytes)...",socket.getRemoteSocketAddress());
    final long value=in.readLong();
    OLogManager.instance().debug(this,"%s - Read long: %d",socket.getRemoteSocketAddress(),value);
    return value;
  }
  return in.readLong();
}
