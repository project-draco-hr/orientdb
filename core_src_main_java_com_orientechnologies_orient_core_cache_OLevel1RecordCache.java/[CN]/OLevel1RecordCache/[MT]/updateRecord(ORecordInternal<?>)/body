{
  if (!isEnabled() || record.getIdentity().getClusterId() == excludedCluster || !record.getIdentity().isValid())   return;
  underlying.lock(record.getIdentity());
  try {
    if (underlying.get(record.getIdentity()) != record)     underlying.put(record);
  }
  finally {
    underlying.unlock(record.getIdentity());
  }
  secondary.get().updateRecord(record);
}
