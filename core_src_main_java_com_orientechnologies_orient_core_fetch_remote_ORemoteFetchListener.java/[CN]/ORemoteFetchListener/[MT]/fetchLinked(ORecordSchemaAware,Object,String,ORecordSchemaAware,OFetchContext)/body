{
  if (iLinked instanceof ORecord) {
    sendRecord(iLinked);
    return true;
  }
 else   if (iLinked instanceof Collection<?>) {
    for (    ORecord rec : (Collection<ORecord>)iLinked)     sendRecord(rec);
    return true;
  }
 else   if (iLinked instanceof Map<?,?>) {
    for (    ORecord rec : ((Map<String,? extends ORecord>)iLinked).values())     sendRecord(rec);
    return true;
  }
 else   throw new OFetchException("Unrecognized type while fetching records: " + iLinked);
}
