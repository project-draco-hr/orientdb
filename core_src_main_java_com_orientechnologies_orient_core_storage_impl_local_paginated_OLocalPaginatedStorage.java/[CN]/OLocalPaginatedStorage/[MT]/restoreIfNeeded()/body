{
  boolean wasSoftlyClosed=true;
  for (  OCluster cluster : clusters)   if (cluster != null && !cluster.wasSoftlyClosed())   wasSoftlyClosed=false;
  if (!wasSoftlyClosed) {
    OLogManager.instance().warn(this,"Storage " + name + " was not closed properly. Will try to restore from write ahead logging.");
    restoreFromWAL();
  }
}
