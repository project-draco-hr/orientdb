{
  if (serverAdmin != null) {
    serverAdmin.close();
    serverAdmin=null;
  }
  checkCurrentDatabase();
  out.print("Disconnecting from the database [" + currentDatabaseName + "]...");
  currentDatabase.close();
  currentDatabase=null;
  currentDatabaseName=null;
  currentRecord=null;
  out.println("OK");
}
