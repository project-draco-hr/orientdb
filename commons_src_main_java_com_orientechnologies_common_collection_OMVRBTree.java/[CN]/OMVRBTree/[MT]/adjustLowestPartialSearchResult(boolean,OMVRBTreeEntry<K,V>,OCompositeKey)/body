{
  final int oldPageIndex=pageIndex;
  final OMVRBTreeEntry<K,V> oldNode=lastNode;
  if (pageIndex >= lastNode.getSize()) {
    lastNode=next(lastNode);
    if (lastNode == null) {
      lastNode=oldNode;
      pageIndex=oldPageIndex;
      pageItemFound=false;
      if (iGetContainer)       return lastNode;
      return null;
    }
  }
  pageItemComparator=((OCompositeKey)lastNode.getKey()).compareTo(compositeKey);
  if (pageItemComparator == 0) {
    pageItemFound=true;
    return lastNode;
  }
 else {
    pageItemFound=false;
    if (iGetContainer)     return lastNode;
    return null;
  }
}
