{
  if (ioResult != null) {
    if (ioResult instanceof Number) {
      ioResult=new Date(((Number)ioResult).longValue());
    }
 else     if (!(ioResult instanceof Date)) {
      ioResult=ODatabaseRecordThreadLocal.INSTANCE.get().getStorage().getConfiguration().getDateTimeFormatInstance().parse(ioResult.toString());
    }
  }
  return ioResult;
}
