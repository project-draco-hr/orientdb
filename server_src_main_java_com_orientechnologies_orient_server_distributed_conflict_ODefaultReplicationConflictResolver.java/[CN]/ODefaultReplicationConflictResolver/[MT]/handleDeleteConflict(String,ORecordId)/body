{
  ODistributedServerLog.warn(this,cluster.getLocalNodeName(),iRemoteNode,DIRECTION.IN,"Conflict on DELETE record %s/%s (cannot be deleted on other node)",database.getName(),iCurrentRID);
  if (!existConflictsForRecord(iCurrentRID)) {
    final ODocument doc=createConflictDocument(ORecordOperation.DELETED,iCurrentRID,iRemoteNode);
    try {
      doc.save();
    }
 catch (    Exception e) {
      errorOnWriteConflict(iRemoteNode,doc);
    }
  }
}
