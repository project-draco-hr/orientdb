{
  K k=keys[iIndex];
  if (k == null)   try {
    PROFILER.updateCounter(PROFILER.getProcessMetric("mvrbtree.entry.unserializeKey"),1);
    k=(K)keyFromStream(iIndex);
    if (iIndex == 0 || iIndex == size || ((OMVRBTreeMapProvider<K,V>)treeDataProvider).keepKeysInMemory)     keys[iIndex]=k;
  }
 catch (  IOException e) {
    OLogManager.instance().error(this,"Cannot lazy load the key #" + iIndex + " in tree node "+ this,e,OSerializationException.class);
  }
  return k;
}
