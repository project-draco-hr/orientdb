{
  if (iRecordType == ODocument.RECORD_TYPE)   return "document";
 else   if (iRecordType == ORecordFlat.RECORD_TYPE)   return "flat";
 else   if (iRecordType == ORecordBytes.RECORD_TYPE)   return "bytes";
 else   if (iRecordType == ORecordColumn.RECORD_TYPE)   return "column";
  throw new IllegalArgumentException("Unsupported record type" + iRecordType);
}
