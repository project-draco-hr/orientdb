{
  if ("depth".equalsIgnoreCase(iName))   return depth;
 else   if ("path".equalsIgnoreCase(iName))   return getPath();
 else   if ("stack".equalsIgnoreCase(iName))   return stack;
 else   if ("history".equalsIgnoreCase(iName))   return history;
 else   if (nestedStack != null)   nestedStack.getVariable(iName);
  return null;
}
