{
  checkForRebuild();
  iRangeFrom=getCollatingValue(iRangeFrom);
  acquireSharedLock();
  try {
    indexEngine.getEntriesMajor(iRangeFrom,isInclusive,MultiValuesTransformer.INSTANCE,new OIndexEngine.EntriesResultListener(){
      @Override public boolean addResult(      ODocument entry){
        return entriesResultListener.addResult(entry);
      }
    }
);
  }
  finally {
    releaseSharedLock();
  }
}
