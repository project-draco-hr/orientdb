{
  try {
    dgram=new DatagramPacket(discoveryPacket,discoveryPacket.length,manager.getConfig().networkMulticastAddress,manager.getConfig().networkMulticastPort);
    socket=new DatagramSocket();
  }
 catch (  Exception e) {
    OLogManager.instance().error(this,"Cannot startup distributed server discovery signaler",e);
  }
  super.startup();
}
