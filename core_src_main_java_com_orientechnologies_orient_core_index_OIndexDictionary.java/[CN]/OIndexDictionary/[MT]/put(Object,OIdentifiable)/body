{
  checkForOptimization();
  acquireExclusiveLock();
  try {
    checkForKeyType(iKey);
    OIdentifiable value=map.get(iKey);
    checkForOptimization();
    if (value == null || !value.equals(iSingleValue))     map.put(iKey,iSingleValue);
    return this;
  }
  finally {
    releaseExclusiveLock();
  }
}
