{
  if (!classes.contains(iClass)) {
    registerClass(iClass);
  }
  Field f=fieldIds.get(iClass);
  if (f.getType().equals(String.class))   setFieldValue(f,iObject,iValue.toString());
 else   if (f.getType().equals(Long.class))   setFieldValue(f,iObject,Long.valueOf(iValue.getClusterPosition()));
 else   if (f.getType().equals(Object.class))   setFieldValue(f,iObject,iValue);
}
