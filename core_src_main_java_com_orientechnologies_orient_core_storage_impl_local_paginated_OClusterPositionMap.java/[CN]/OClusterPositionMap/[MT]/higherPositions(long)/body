{
  acquireSharedLock();
  try {
    atomicOperationsManager.acquireReadLock(this);
    try {
      if (clusterPosition == Long.MAX_VALUE)       return OCommonConst.EMPTY_LONG_ARRAY;
      return ceilingPositions(clusterPosition + 1);
    }
  finally {
      atomicOperationsManager.releaseReadLock(this);
    }
  }
  finally {
    releaseSharedLock();
  }
}
