{
  final ORef<Long> journalOffset=new ORef<Long>();
  final T result;
  try {
    result=(T)getDistributedServerManager().enqueueLocalExecution(this,journalOffset);
    if (journalOffset.value != null)     getDistributedServerManager().updateJournal(this,getDatabaseSynchronizer(),journalOffset.value,true);
    return result;
  }
 catch (  Exception e) {
    if (journalOffset.value != null)     getDistributedServerManager().updateJournal(this,getDatabaseSynchronizer(),journalOffset.value,false);
    throw e;
  }
}
