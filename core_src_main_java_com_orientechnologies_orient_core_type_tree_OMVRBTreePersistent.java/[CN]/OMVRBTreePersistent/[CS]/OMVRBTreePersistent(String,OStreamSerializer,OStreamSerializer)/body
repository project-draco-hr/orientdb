{
  super(1024,0.7f);
  Orient.instance().getMemoryWatchDog().addListener(new Listener(){
    public void memoryUsageLow(    final TYPE iType,    final long usedMemory,    final long maxMemory){
      if (iType == TYPE.JVM)       optimize();
    }
  }
);
  config();
  clusterName=iClusterName;
  record=new ORecordBytesLazy(this);
  keySerializer=iKeySerializer;
  valueSerializer=iValueSerializer;
  entryRecordBuffer=new OMemoryOutputStream(getPageSize() * 15);
  setListener(this);
}
