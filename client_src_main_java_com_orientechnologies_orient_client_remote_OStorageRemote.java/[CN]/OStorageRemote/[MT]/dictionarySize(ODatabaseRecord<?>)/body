{
  checkConnection();
  do {
    boolean locked=lock.acquireExclusiveLock();
    try {
      try {
        beginRequest(OChannelBinaryProtocol.REQUEST_DICTIONARY_SIZE);
      }
  finally {
        endRequest();
      }
      try {
        beginResponse();
        return network.readInt();
      }
  finally {
        endResponse();
      }
    }
 catch (    Exception e) {
      handleException("Error on getting size of database's dictionary",e);
    }
 finally {
      lock.releaseExclusiveLock(locked);
    }
  }
 while (true);
}
