{
  for (  OSynchronizationLog localLog : logs.values()) {
    if (!localLog.getNodeId().equals(iNodeSource))     try {
      localLog.updateLog(iRid,iVersion);
    }
 catch (    IOException e) {
      OLogManager.instance().error(this,"DISTRIBUTED -> Error on updating log in file: %s. The coherence of the cluster is not more guaranteed",e,localLog);
    }
  }
}
