{
  database.open("admin","admin");
  OIndex<?> index=getManualIndex();
  if (index == null) {
    index=database.getMetadata().getIndexManager().createIndex("byte-array-manualIndex","UNIQUE",new OSimpleKeyIndexDefinition(OType.BINARY),null,null);
    this.manualIndex=index;
  }
 else {
    index=database.getMetadata().getIndexManager().getIndex("byte-array-manualIndex");
    this.manualIndex=index;
  }
}
