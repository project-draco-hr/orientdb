{
  if (targetClasses != null) {
    final OClass cls=targetClasses.keySet().iterator().next();
    if (iRecord.getSchemaClass() == null || !iRecord.getSchemaClass().isSubClassOf(cls))     return false;
  }
  if (rootCondition == null)   return true;
  return (Boolean)rootCondition.evaluate(iRecord);
}
