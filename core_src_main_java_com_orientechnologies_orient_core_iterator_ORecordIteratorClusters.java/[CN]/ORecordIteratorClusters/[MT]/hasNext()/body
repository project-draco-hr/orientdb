{
  checkDirection(true);
  if (limit > -1 && browsedRecords >= limit)   return false;
  if (browsedRecords >= totalAvailableRecords)   return false;
  if (liveUpdated)   lastClusterPosition=database.getStorage().getClusterDataRange(current.clusterId)[1];
  ORecordInternal<?> record=getRecord();
  while (currentClusterIdx < clusterIds.length) {
    long recordsToBrowse=current.clusterPosition > -2 && lastClusterPosition > -1 ? lastClusterPosition - current.clusterPosition : 0;
    while (recordsToBrowse > 0) {
      currentRecord=readCurrentRecord(record,+1);
      if (currentRecord != null)       if (include(currentRecord))       return true;
      recordsToBrowse--;
    }
    currentClusterIdx++;
    if (currentClusterIdx >= clusterIds.length)     break;
    updateClusterRange();
    current.clusterPosition=firstClusterPosition - 1;
  }
  if (txEntries != null && txEntries.size() - (currentTxEntryPosition + 1) > 0)   return true;
  currentRecord=null;
  return false;
}
