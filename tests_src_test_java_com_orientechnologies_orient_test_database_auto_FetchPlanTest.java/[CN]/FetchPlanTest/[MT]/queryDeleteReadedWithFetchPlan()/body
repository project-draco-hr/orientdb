{
  List<ODocument> resultset=database.query(new OSQLSynchQuery<ODocument>("select * from SecondFetchClass where name = 'fifth'").setFetchPlan("nema:-1 surname:-2"));
  for (  ODocument d : resultset) {
    Assert.assertNotNull(d.field("name"));
    Assert.assertNull(d.field("surname"));
    database.delete(d);
  }
  database.getLocalCache().clear();
  resultset=database.query(new OSQLSynchQuery<ODocument>("select * from SecondFetchClass where name = 'fifth'").setFetchPlan("ref:-1 "));
  Assert.assertEquals(resultset.size(),0);
}
