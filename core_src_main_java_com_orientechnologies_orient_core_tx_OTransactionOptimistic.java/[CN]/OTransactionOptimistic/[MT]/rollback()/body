{
  checkTransaction();
  status=TXSTATUS.ROLLBACKING;
  database.getStorage().rollback(this);
  database.getLevel1Cache().invalidate();
  for (  OTransactionRecordEntry v : recordEntries.values()) {
    v.getRecord().unload();
  }
  recordEntries.clear();
  indexEntries.clear();
  newObjectCounter=-2;
  status=TXSTATUS.INVALID;
}
