{
  if (iClusterId == -1)   return new long[]{-1,-1};
  checkOpeness();
  lock.acquireSharedLock();
  try {
    return clusters[iClusterId] != null ? new long[]{clusters[iClusterId].getFirstEntryPosition(),clusters[iClusterId].getLastEntryPosition()} : new long[0];
  }
  finally {
    lock.releaseSharedLock();
  }
}
