{
  try {
    Assert.assertEquals("The MD5 checksum of the file '" + fileToBeChecked.getAbsolutePath() + "' does not match the given one.",digest,this.calculateMD5checksum(new FileInputStream(fileToBeChecked)));
  }
 catch (  NoSuchAlgorithmException e) {
    e.printStackTrace();
    Assert.fail(e.getMessage());
  }
catch (  IOException e) {
    e.printStackTrace();
    Assert.fail(e.getMessage());
  }
}
