{
  if (names.length == 1)   return transformValue(iRecord.getDatabase(),((ORecordSchemaAware<?>)iRecord).field(names[0]));
  Object[] values=((ORecordSchemaAware<?>)iRecord).fieldValues();
  if (hasChainOperators()) {
    for (int i=0; i < values.length; ++i)     values[i]=transformValue(iRecord.getDatabase(),values[i]);
  }
  return new OQueryRuntimeValueMulti(this,values);
}
