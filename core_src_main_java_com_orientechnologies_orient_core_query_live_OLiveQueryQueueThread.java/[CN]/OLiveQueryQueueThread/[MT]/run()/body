{
  while (!stopped) {
    ORecordOperation next=null;
    try {
      next=queue.take();
    }
 catch (    InterruptedException e) {
      break;
    }
    if (next == null) {
      continue;
    }
    for (    OLiveQueryListener listener : subscribers.values()) {
      listener.onLiveResult(next);
    }
  }
}
