{
  List<ODocument> result=database.command(new OSQLSynchQuery<ODocument>("SELECT FLATTEN( outE() ) FROM V WHERE outE() TRAVERSE(1,1) (@class = 'E')")).execute();
  Assert.assertTrue(result.size() != 0);
  for (  ODocument d : result) {
    Assert.assertTrue(d.getSchemaClass().isSubClassOf("E"));
    Assert.assertEquals(d.getRecordType(),ODocument.RECORD_TYPE);
  }
}
