{
  for (  Map.Entry<Object,Object> snapshotEntry : snapshot.entrySet()) {
    Object key=snapshotEntry.getKey();
    Object value=snapshotEntry.getValue();
    checkForKeyType(key);
    if (value.equals(RemovedValue.INSTANCE))     indexEngine.remove(key);
 else     indexEngine.put(key,(Set<OIdentifiable>)value);
  }
}
