{
  if (p != null) {
    OTreeMapEntry<K,V> r=p.getRight();
    p.setRight(r.getLeft());
    if (r.getLeft() != null)     r.getLeft().setParent(p);
    r.setParent(p.getParent());
    if (p.getParent() == null)     root=r;
 else     if (p.getParent().getLeft() == p)     p.getParent().setLeft(r);
 else     p.getParent().setRight(r);
    r.setLeft(p);
    p.setParent(r);
  }
}
