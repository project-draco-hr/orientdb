{
  database.open("admin","admin");
  OIndex<?> index=getIndex();
  if (index == null) {
    index=database.getMetadata().getIndexManager().createIndex("byte-array-index","UNIQUE",new OSimpleKeyIndexDefinition(OType.BINARY),null,null);
    this.index=index;
  }
 else {
    index=database.getMetadata().getIndexManager().getIndex("byte-array-index");
    this.index=index;
  }
}
