{
synchronized (configuration) {
    final Object value=getClusterConfiguration(iClusterName).field("readYourWrites");
    if (value != null)     return (Boolean)value;
 else {
      OLogManager.instance().warn(this,"readYourWrites setting not found for cluster=%s in distributed-config.json",iClusterName);
      return true;
    }
  }
}
