{
  final OSQLSynchQuery<ODocument> query=new OSQLSynchQuery<ODocument>("select @rid.trim() as oid, name from Profile where (@rid in [#11:5] or @rid in [#11:25]) AND @rid > ? LIMIT 10000");
  List<ODocument> resultset=database.query(query,new ORecordId(11,7));
  Assert.assertEquals(resultset.size(),1);
  Assert.assertEquals(resultset.get(0).field("oid"),new ORecordId(11,25).toString());
}
