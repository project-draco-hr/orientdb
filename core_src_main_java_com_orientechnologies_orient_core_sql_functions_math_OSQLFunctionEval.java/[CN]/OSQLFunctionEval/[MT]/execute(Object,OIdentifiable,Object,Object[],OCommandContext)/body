{
  if (predicate == null)   predicate=new OSQLPredicate((String)iParams[0].toString());
  try {
    return predicate.evaluate(iRecord != null ? iRecord.getRecord() : null,(ODocument)iCurrentResult,iContext);
  }
 catch (  ArithmeticException e) {
    return 0;
  }
catch (  Exception e) {
    return null;
  }
}
