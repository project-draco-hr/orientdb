{
  long pointer=directMemory.allocate(OSBTreeBucket.MAX_PAGE_SIZE_BYTES);
  OSBTreeBucket<Long,OIdentifiable> treeBucket=new OSBTreeBucket<Long,OIdentifiable>(pointer,true,OLongSerializer.INSTANCE,OLinkSerializer.INSTANCE,ODurablePage.TrackMode.FULL);
  Assert.assertEquals(treeBucket.size(),0);
  Assert.assertTrue(treeBucket.isLeaf());
  treeBucket=new OSBTreeBucket<Long,OIdentifiable>(pointer,OLongSerializer.INSTANCE,OLinkSerializer.INSTANCE,ODurablePage.TrackMode.FULL);
  Assert.assertEquals(treeBucket.size(),0);
  Assert.assertTrue(treeBucket.isLeaf());
  Assert.assertEquals(treeBucket.getLeftSibling(),-1);
  Assert.assertEquals(treeBucket.getRightSibling(),-1);
  directMemory.free(pointer);
}
