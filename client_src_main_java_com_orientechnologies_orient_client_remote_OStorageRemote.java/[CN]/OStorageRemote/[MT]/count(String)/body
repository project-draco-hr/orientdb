{
  checkConnection();
  do {
    boolean locked=acquireExclusiveLock();
    try {
      writeCommand(OChannelBinaryProtocol.REQUEST_COUNT);
      network.writeString(iClassName);
      network.readStatus();
      return network.readLong();
    }
 catch (    Exception e) {
      if (handleException("Error on executing count on class: " + iClassName,e))       break;
    }
 finally {
      releaseExclusiveLock(locked);
    }
  }
 while (true);
  return -1;
}
