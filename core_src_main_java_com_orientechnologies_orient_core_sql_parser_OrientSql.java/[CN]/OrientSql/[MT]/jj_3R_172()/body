{
  if (jj_scan_token(ORDER))   return true;
  if (jj_scan_token(BY))   return true;
  Token xsp;
  xsp=jj_scanpos;
  if (jj_3R_228()) {
    jj_scanpos=xsp;
    if (jj_3R_229()) {
      jj_scanpos=xsp;
      if (jj_scan_token(53))       return true;
    }
  }
  xsp=jj_scanpos;
  if (jj_3R_230())   jj_scanpos=xsp;
  while (true) {
    xsp=jj_scanpos;
    if (jj_3R_231()) {
      jj_scanpos=xsp;
      break;
    }
  }
  return false;
}
