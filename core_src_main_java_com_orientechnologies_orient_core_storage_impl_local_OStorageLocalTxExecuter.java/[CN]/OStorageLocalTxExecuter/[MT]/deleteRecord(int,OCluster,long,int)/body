{
  try {
    final ORecordId rid=new ORecordId(iClusterSegment.getId(),iPosition);
    final ORawBuffer buffer=storage.readRecord(iClusterSegment,rid,false);
    txSegment.addLog(OTxSegment.OPERATION_DELETE,iTxId,iClusterSegment.getId(),iPosition,buffer.recordType,buffer.version,buffer.buffer);
    return storage.deleteRecord(iClusterSegment,rid,iVersion);
  }
 catch (  IOException e) {
    OLogManager.instance().error(this,"Error on deleting entry #" + iPosition + " in log segment: "+ iClusterSegment,e,OTransactionException.class);
  }
  return false;
}
