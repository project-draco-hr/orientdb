{
  if (iRangeFrom.getClass() != iRangeTo.getClass())   throw new IllegalArgumentException("Range from-to parameters are of different types");
  acquireExclusiveLock();
  try {
    final ONavigableMap<Object,Set<ORecord<?>>> subSet=map.subMap(iRangeFrom,iInclusive,iRangeTo,iInclusive);
    if (subSet == null)     return Collections.EMPTY_SET;
    final Set<ORecord<?>> result=new HashSet<ORecord<?>>();
    for (    Set<ORecord<?>> v : subSet.values()) {
      result.addAll(v);
    }
    return result;
  }
  finally {
    releaseExclusiveLock();
  }
}
