{
  final HashSet batch;
  if (approximateRemainingSize > -1) {
    if (approximateRemainingSize > 10000)     batch=new HashSet(13400);
 else     batch=new HashSet((int)(approximateRemainingSize / 0.75));
  }
 else {
    batch=new HashSet();
  }
  int count=0;
  while (count < 10000 && it.hasNext()) {
    batch.add(it.next());
    count++;
  }
  return batch;
}
