{
  Lock fileLock;
  cacheLock.acquireReadLock();
  try {
    fileLock=fileLockManager.acquireExclusiveLock(fileId);
    try {
      writeCache.truncateFile(fileId);
      clearFile(fileId);
    }
  finally {
      fileLockManager.releaseLock(fileLock);
    }
  }
  finally {
    cacheLock.releaseReadLock();
  }
}
