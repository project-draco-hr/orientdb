{
  checkForRebuild();
  if (iRangeFrom.getClass() != iRangeTo.getClass())   throw new IllegalArgumentException("Range from-to parameters are of different types");
  iRangeFrom=getCollatingValue(iRangeFrom);
  iRangeTo=getCollatingValue(iRangeTo);
  acquireSharedLock();
  try {
    indexEngine.getValuesBetween(iRangeFrom,iFromInclusive,iRangeTo,iToInclusive,null,new OIndexEngine.ValuesResultListener(){
      @Override public boolean addResult(      OIdentifiable identifiable){
        return resultListener.addResult(identifiable);
      }
    }
);
  }
  finally {
    releaseSharedLock();
  }
}
