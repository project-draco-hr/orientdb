{
  lock.acquireExclusiveLock();
  try {
    txManager.commitAllPendingRecords((OTransaction)iTx);
    incrementVersion();
    if (OGlobalConfiguration.TX_COMMIT_SYNCH.getValueAsBoolean())     synch();
  }
 catch (  IOException e) {
    rollback(iTx);
  }
 finally {
    lock.releaseExclusiveLock();
  }
}
