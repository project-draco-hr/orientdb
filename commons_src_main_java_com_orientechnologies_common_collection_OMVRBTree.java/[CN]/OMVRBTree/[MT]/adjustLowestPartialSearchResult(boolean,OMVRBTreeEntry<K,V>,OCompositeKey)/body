{
  if (pageIndex >= lastNode.getSize()) {
    lastNode=next(lastNode);
  }
  pageItemComparator=((OCompositeKey)lastNode.getKey()).compareTo(compositeKey);
  if (pageItemComparator == 0) {
    pageItemFound=true;
    return lastNode;
  }
 else {
    pageItemFound=false;
    if (iGetContainer)     return lastNode;
    return null;
  }
}
