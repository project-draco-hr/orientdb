{
  ODistributedThreadLocal.INSTANCE.set(null);
  final boolean updated;
  if (iOperationSerial > -1)   updated=getDatabaseSynchronizer(iDatabaseName).getLog().updateLastOperation(iRunId,iOperationSerial,iForce);
 else   updated=false;
synchronized (lockQueue) {
    lockQueue.notifyAll();
  }
  return updated;
}
