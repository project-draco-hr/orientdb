{
  checkForRebuild();
  fromKey=getCollatingValue(fromKey);
  toKey=getCollatingValue(toKey);
  acquireSharedLock();
  try {
    return storage.iterateIndexEntriesBetween(indexId,fromKey,fromInclusive,toKey,toInclusive,ascOrder,MultiValuesTransformer.INSTANCE);
  }
  finally {
    releaseSharedLock();
  }
}
