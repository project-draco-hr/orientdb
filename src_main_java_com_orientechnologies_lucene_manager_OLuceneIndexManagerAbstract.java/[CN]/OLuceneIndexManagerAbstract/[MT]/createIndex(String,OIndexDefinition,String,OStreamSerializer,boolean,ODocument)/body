{
  this.index=indexDefinition;
  this.indexName=indexName;
  this.serializer=valueSerializer;
  this.automatic=isAutomatic;
  this.clusterIndexName=clusterIndexName;
  this.metadata=metadata;
  try {
    this.index=indexDefinition;
    reOpen(metadata);
  }
 catch (  IOException e) {
