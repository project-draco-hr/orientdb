{
  final Object[] result;
  int executedTasks=0;
  try {
    result=new Object[tasks.size()];
    ODistributedServerLog.warn(this,iManager.getLocalNodeName(),null,DIRECTION.IN,"****** BEGIN EXECUTING ALIGNMENT BLOCK db=%s tasks=%d ******",iDatabaseName,tasks.size());
    for (; executedTasks < tasks.size(); ++executedTasks) {
      final OAbstractRemoteTask task=tasks.get(executedTasks);
      try {
        result[executedTasks]=task.execute(iServer,iManager,iDatabaseName);
      }
 catch (      OSkippedOperationException e) {
      }
    }
  }
  finally {
    ODistributedServerLog.warn(this,iManager.getLocalNodeName(),null,DIRECTION.IN,"****** END EXECUTING ALIGNMENT BLOCK db=%s tasks=%d/%d ******",iDatabaseName,executedTasks,tasks.size());
  }
  return result;
}
