{
  iRequest.sessionId=SESSIONID_UNAUTHORIZED;
  if (isJsonResponse(iResponse)) {
    sendJsonError(iResponse,OHttpUtils.STATUS_AUTH_CODE,OHttpUtils.STATUS_AUTH_DESCRIPTION,OHttpUtils.CONTENT_TEXT_PLAIN,"401 Unauthorized.","WWW-Authenticate: Basic realm=\"OrientDB Server\"");
  }
 else {
    iResponse.send(OHttpUtils.STATUS_AUTH_CODE,OHttpUtils.STATUS_AUTH_DESCRIPTION,OHttpUtils.CONTENT_TEXT_PLAIN,"401 Unauthorized.","WWW-Authenticate: Basic realm=\"OrientDB Server\"");
  }
}
