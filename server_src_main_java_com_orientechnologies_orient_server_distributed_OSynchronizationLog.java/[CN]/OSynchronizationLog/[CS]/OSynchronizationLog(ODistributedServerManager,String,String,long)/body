{
  super(SYNCHRONIZATION_DIRECTORY + iDatabase + "/"+ iNodeId.replace('.','_').replace(':','-')+ EXTENSION,OGlobalConfiguration.DISTRIBUTED_LOG_TYPE.getValueAsString());
  databaseName=iDatabase;
  nodeId=iNodeId;
  synchEnabled=OGlobalConfiguration.DISTRIBUTED_LOG_SYNCH.getValueAsBoolean();
  manager=iManager;
  file.setFailCheck(false);
  if (exists()) {
    open();
  }
 else   create(DEF_START_SIZE);
  limit=iLimit;
}
