{
switch (iOperation) {
case ORecordOperation.CREATED:
    if (iResponse != iRecord.getIdentity().getClusterPosition())     conflictResolver.handleCreateConflict(ORecordOperation.CREATED,iRequestType,iRecord,iResponse);
  break;
case ORecordOperation.UPDATED:
if ((int)iResponse != iRecord.getVersion()) conflictResolver.handleUpdateConflict(ORecordOperation.UPDATED,iRequestType,iRecord,iRecord.getVersion(),(int)iResponse);
break;
case ORecordOperation.DELETED:
if ((int)iResponse == 0) conflictResolver.handleDeleteConflict(ORecordOperation.DELETED,iRequestType,iRecord);
break;
}
}
