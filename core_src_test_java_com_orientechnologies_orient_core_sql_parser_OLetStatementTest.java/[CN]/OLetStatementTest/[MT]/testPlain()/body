{
  checkRightSyntax("let foo = select from foo where name = 'bar'");
  checkRightSyntax("LET foo = select from foo where name = 'bar'");
  checkRightSyntax("LET foo = select from foo where name = 'bar';");
  checkRightSyntax("LET $foo = select from foo where name = 'bar'");
  checkRightSyntax("LET $foo = (select from foo where name = 'bar')");
  checkRightSyntax("LET $foo = bar");
  checkRightSyntax("LET $foo = 1 + 2 + 3");
  checkRightSyntax("LET $foo = [1, 3, 5]");
  checkWrongSyntax("LET ");
  checkWrongSyntax("LET foo");
}
