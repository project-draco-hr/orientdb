{
  acquireExclusiveLock();
  try {
    for (int retry=0; retry < 10; retry++)     try {
      super.save();
      getDatabase().getStorage().synch();
      return (RET)this;
    }
 catch (    OConcurrentModificationException e) {
      reload(null,true);
    }
    super.save();
    getDatabase().getStorage().synch();
    return (RET)this;
  }
  finally {
    releaseExclusiveLock();
  }
}
