{
  current.clusterId=clusterIds[currentClusterIdx];
  final long[] range=database.getStorage().getClusterDataRange(current.clusterId);
  firstClusterPosition=beginRange != null && beginRange.getClusterId() == current.clusterId ? beginRange.getClusterPosition() : range[0];
  lastClusterPosition=endRange != null && endRange.getClusterId() == current.clusterId ? endRange.getClusterPosition() : range[1];
}
