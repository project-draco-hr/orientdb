{
  final List<Integer> list=new ArrayList<Integer>();
  list.add(0);
  final ODocument document=new ODocument();
  document.field("list",list);
  Assert.assertEquals(document.field("list"),list);
  document.save();
  Assert.assertEquals(document.field("list"),list);
  db.begin();
  list.add(1);
  Assert.assertEquals(document.field("list"),list);
  document.save();
  Assert.assertEquals(document.field("list"),list);
  db.commit();
  Assert.assertEquals(document.field("list"),list);
}
