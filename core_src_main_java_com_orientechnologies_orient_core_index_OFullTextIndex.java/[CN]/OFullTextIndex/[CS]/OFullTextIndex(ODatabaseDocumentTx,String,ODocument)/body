{
  config=iIndexConfig;
  map=new OTreeMapDatabaseLazySave<String,List<ORecordId>>((ODatabaseRecord<?>)iDatabase.getUnderlying(),(String)iIndexConfig.field(FIELD_CLUSTER_NAME),new ORecordId((String)iIndexConfig.field(FIELD_MAP_RID)));
  try {
    map.load();
  }
 catch (  IOException e) {
    throw new OIndexException("Can't load tree map for Full Text index '" + iIndexName + "'");
  }
  init();
}
