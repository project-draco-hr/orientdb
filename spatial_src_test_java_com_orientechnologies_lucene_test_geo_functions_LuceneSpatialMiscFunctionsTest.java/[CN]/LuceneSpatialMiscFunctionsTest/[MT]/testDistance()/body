{
  OrientGraphNoTx graph=new OrientGraphNoTx("memory:functionsTest");
  try {
    ODatabaseDocumentTx db=graph.getRawGraph();
    List<ODocument> execute=db.command(new OCommandSQL("SELECT ST_Distance(ST_GeomFromText('POINT(-72.1235 42.3521)'),ST_GeomFromText('LINESTRING(-72.1260 42.45, -72.123 42.1546)'))")).execute();
    ODocument next=execute.iterator().next();
    Assert.assertEquals(next.field("ST_Distance"),0.0015056772638228177);
    execute=db.command(new OCommandSQL("SELECT  ST_Distance( ST_GeomFromText('LINESTRING(13.45 52.47,13.46 52.48)'), ST_GeomFromText('LINESTRING(13.00 52.00,13.1 52.2)'))")).execute();
    next=execute.iterator().next();
    Assert.assertEquals(next.field("ST_Distance"),0.44204072210600415);
  }
  finally {
    graph.drop();
  }
}
