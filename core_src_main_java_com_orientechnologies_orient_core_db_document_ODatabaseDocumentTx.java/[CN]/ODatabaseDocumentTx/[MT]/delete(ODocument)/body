{
  if (iContent.getClassName() != null)   checkSecurity(ODatabaseSecurityResources.CLASS + "." + iContent.getClassName(),ORole.OPERATIONS.DELETE);
  try {
    underlying.delete(iContent);
  }
 catch (  Exception e) {
    OLogManager.instance().exception("Error on deleting record #%s of class '%s'",e,ODatabaseException.class,iContent.getIdentity(),iContent.getClassName());
  }
  return this;
}
