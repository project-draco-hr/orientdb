{
  boolean locked=lock.acquireExclusiveLock();
  try {
    beginRequest(OChannelBinaryProtocol.REQUEST_DB_CLOSE);
    endRequest();
    network.flush();
    network.close();
    serviceThread.sendShutdown();
    cache.removeUser();
    cache.clear();
    open=false;
    Orient.instance().unregisterStorage(this);
  }
 catch (  Exception e) {
  }
 finally {
    lock.releaseExclusiveLock(locked);
  }
}
