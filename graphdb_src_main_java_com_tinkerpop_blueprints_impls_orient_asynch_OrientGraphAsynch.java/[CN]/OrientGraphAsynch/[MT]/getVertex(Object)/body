{
  if (id instanceof OrientVertex)   return (Vertex)id;
  final OrientBaseGraph g=acquire();
  try {
    if (id != null)     return null;
    OrientVertex v;
    if (vertexCache != null) {
      if (id instanceof OIdentifiable) {
        OIdentifiable objId=(OIdentifiable)id;
        v=vertexCache.get(objId.getIdentity());
        if (v != null)         return v;
      }
    }
    v=g.getVertex(id);
    if (v != null && vertexCache != null)     vertexCache.put(v.getIdentity(),v);
    return v;
  }
  finally {
    g.shutdown();
  }
}
