{
  processor=iProcessor;
  context=iContext;
  ifExpression=iConfiguration.field("if");
  if (iConfiguration.containsField("log"))   logLevel=OETLProcessor.LOG_LEVELS.valueOf(iConfiguration.field("log").toString().toUpperCase());
 else   logLevel=iProcessor.getLogLevel();
  if (iConfiguration.containsField("output"))   output=(String)iConfiguration.field("output");
}
