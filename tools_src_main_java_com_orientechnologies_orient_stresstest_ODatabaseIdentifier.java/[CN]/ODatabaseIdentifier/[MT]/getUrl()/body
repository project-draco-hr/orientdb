{
switch (mode) {
case MEMORY:
    return "memory:" + dbName;
case REMOTE:
  return "remote:" + remoteIp + ":"+ remotePort+ "/"+ dbName;
case DISTRIBUTED:
return null;
case PLOCAL:
default :
String basePath=System.getProperty("java.io.tmpdir");
if (plocalPath != null) {
basePath=plocalPath;
}
if (!basePath.endsWith(File.separator)) basePath+=File.separator;
return "plocal:" + basePath + dbName;
}
}
