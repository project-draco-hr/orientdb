{
synchronized (syncObject) {
    close();
    for (    LogSegment logSegment : logSegments)     if (!logSegment.delete())     OLogManager.instance().error(this,"Can not delete WAL segment %s for storage %s",logSegment.getPath(),paginatedStorage.getName());
    if (!masterRecordFile.delete())     OLogManager.instance().error(this,"Can not delete WAL state file for %s storage",paginatedStorage.getName());
  }
}
