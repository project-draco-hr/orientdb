{
  final OLevel1RecordCache dbCache=(OLevel1RecordCache)iTx.getDatabase().getLevel1Cache();
  for (  OTransactionRecordEntry txEntry : iEntries) {
    if (txEntry.status == OTransactionRecordEntry.DELETED)     dbCache.deleteRecord(txEntry.getRecord().getIdentity());
 else     if (txEntry.status == OTransactionRecordEntry.UPDATED || txEntry.status == OTransactionRecordEntry.CREATED)     dbCache.updateRecord(txEntry.getRecord());
  }
}
