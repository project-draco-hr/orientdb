{
  if (record.isDirty())   return;
  if (left != null && !((OTreeMapEntryPersistent<K,V>)left).record.isDirty())   ((OTreeMapEntryPersistent<K,V>)left).clear();
  if (right != null && !((OTreeMapEntryPersistent<K,V>)right).record.isDirty())   ((OTreeMapEntryPersistent<K,V>)right).clear();
