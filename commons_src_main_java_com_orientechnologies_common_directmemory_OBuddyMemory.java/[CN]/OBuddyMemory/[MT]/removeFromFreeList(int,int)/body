{
  final int next=next(pointer);
  final int previous=previous(pointer);
  if (freeListHeader[level] == pointer) {
    freeListHeader[level]=next;
  }
 else {
    next(previous,next);
  }
  if (freeListTail[level] == pointer) {
    freeListTail[level]=previous;
  }
 else {
    previous(next,previous);
  }
}
