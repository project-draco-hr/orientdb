{
  if (database == null || iClassName == null) {
    clazz=null;
    return;
  }
  setClass(database.getMetadata().getSchema().getClass(iClassName));
  if (clazz == null) {
    setClass(database.getMetadata().getSchema().createClass(iClassName));
    database.getMetadata().getSchema().save();
  }
}
