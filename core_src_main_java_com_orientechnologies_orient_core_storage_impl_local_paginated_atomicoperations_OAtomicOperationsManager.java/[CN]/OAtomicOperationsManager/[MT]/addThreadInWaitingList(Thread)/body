{
  final WaitingListNode node=new WaitingListNode(thread);
  while (true) {
    final WaitingListNode last=waitingTail.get();
    if (waitingTail.compareAndSet(last,node)) {
      if (last == null) {
        waitingHead.set(node);
      }
 else {
        last.next=node;
      }
      node.linkLatch.countDown();
      break;
    }
  }
}
