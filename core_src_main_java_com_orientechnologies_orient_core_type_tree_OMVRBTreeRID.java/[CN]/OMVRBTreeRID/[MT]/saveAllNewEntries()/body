{
  if (hasNewItems()) {
    final Set<ORecord<?>> temp=new HashSet<ORecord<?>>(newEntries.keySet());
    for (    ORecord<?> record : temp) {
      if (record.getIdentity().isNew())       record.save();
      if (!record.getIdentity().isNew()) {
        newEntries.remove(record);
        if (newEntries.size() == 0)         newEntries=null;
        internalPut(record,null);
      }
    }
    commitChanges();
    if (newEntries != null)     return false;
  }
  return true;
}
