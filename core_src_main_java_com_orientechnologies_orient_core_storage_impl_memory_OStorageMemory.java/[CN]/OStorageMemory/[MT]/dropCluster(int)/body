{
  final boolean locked=lock.acquireExclusiveLock();
  try {
    OCluster c=clusters.get(iClusterId);
    c.delete();
    clusters.set(iClusterId,null);
    getLevel2Cache().freeCluster(iClusterId);
  }
 catch (  IOException e) {
  }
 finally {
    lock.releaseExclusiveLock(locked);
  }
  return false;
}
