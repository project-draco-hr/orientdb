{
  database.open("admin","admin");
  List<ODocument> result=database.command(new OSQLSynchQuery<ODocument>("select MIN(id) as min, max(id) as max, AVG(id) as average, count(id) as total from Account")).execute();
  Assert.assertTrue(result.size() == 1);
  for (  ODocument d : result) {
    Assert.assertNotNull(d.field("min"));
    Assert.assertNotNull(d.field("max"));
    Assert.assertNotNull(d.field("average"));
    Assert.assertNotNull(d.field("total"));
    Assert.assertTrue(((Number)d.field("max")).longValue() > ((Number)d.field("average")).longValue());
    Assert.assertTrue(((Number)d.field("average")).longValue() >= ((Number)d.field("min")).longValue());
    if (!database.getStorage().isLHClustersAreUsed())     Assert.assertTrue(((Number)d.field("total")).longValue() >= ((Number)d.field("max")).longValue(),"Total " + d.field("total") + " max "+ d.field("max"));
  }
  database.close();
}
