{
  checkForRebuild();
  if (iRangeFrom.getClass() != iRangeTo.getClass())   throw new IllegalArgumentException("Range from-to parameters are of different types");
  acquireSharedLock();
  try {
    return indexEngine.getValuesBetween(iRangeFrom,iFromInclusive,iRangeTo,iToInclusive,maxValuesToFetch,null);
  }
  finally {
    releaseSharedLock();
  }
}
