{
  if (iRecord == null)   throw new ODatabaseException("Cannot delete null document");
  if (iRecord.getClassName() != null)   checkSecurity(ODatabaseSecurityResources.CLASS,ORole.PERMISSION_DELETE,iRecord.getClassName());
  try {
    underlying.delete(iRecord);
  }
 catch (  Exception e) {
    OLogManager.instance().exception("Error on deleting record %s of class '%s'",e,ODatabaseException.class,iRecord.getIdentity(),iRecord.getClassName());
  }
  return this;
}
