{
  if (hasNewItems()) {
    for (    ORecord<?> record : newItems.keySet()) {
      if (record.getIdentity().isNew() || getStreamedContent() == null || getStreamedContent().indexOf(record.getIdentity().toString()) == -1)       add(record.getIdentity());
    }
    newItems.clear();
    newItems=null;
  }
}
