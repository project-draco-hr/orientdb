{
  if (iClusterName.endsWith("_" + nodeName)) {
    final String bestCluster=cfg.getLeaderServer(iClusterName);
    if (bestCluster == null) {
      ODistributedServerLog.info(this,nodeName,null,DIRECTION.NONE,"change mastership of cluster '%s' (id=%d) to local node '%s'",iClusterName,iDatabase.getClusterIdByName(iClusterName),nodeName);
      cfg.setMasterServer(iClusterName,nodeName);
      return true;
    }
  }
  return false;
}
