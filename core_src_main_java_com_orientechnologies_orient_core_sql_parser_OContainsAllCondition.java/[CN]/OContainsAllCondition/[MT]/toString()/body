{
  StringBuilder result=new StringBuilder();
  result.append(left.toString());
  result.append(" CONTAINSALL ");
  if (right != null) {
    result.append(right.toString());
  }
 else   if (rightBlock != null) {
    result.append("(");
    result.append(rightBlock.toString());
    result.append(")");
  }
  return result.toString();
}
