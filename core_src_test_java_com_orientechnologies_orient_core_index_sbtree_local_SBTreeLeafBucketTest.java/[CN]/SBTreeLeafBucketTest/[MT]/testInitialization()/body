{
  long pointer=directMemory.allocate(OSBTreeBucket.MAX_BUCKET_SIZE_BYTES);
  OSBTreeBucket<Long> treeBucket=new OSBTreeBucket<Long>(pointer,true,OLongSerializer.INSTANCE,com.orientechnologies.orient.core.storage.impl.local.paginated.OAbstractPLocalPage.TrackMode.BOTH);
  Assert.assertEquals(treeBucket.size(),0);
  Assert.assertTrue(treeBucket.isLeaf());
  treeBucket=new OSBTreeBucket<Long>(pointer,OLongSerializer.INSTANCE,com.orientechnologies.orient.core.storage.impl.local.paginated.OAbstractPLocalPage.TrackMode.BOTH);
  Assert.assertEquals(treeBucket.size(),0);
  Assert.assertTrue(treeBucket.isLeaf());
  Assert.assertEquals(treeBucket.getLeftSibling(),-1);
  Assert.assertEquals(treeBucket.getRightSibling(),-1);
  directMemory.free(pointer);
}
