{
  if (recording < 0)   return -1;
  OProfilerEntry c=iValues.get(iName);
  if (c == null) {
    c=new OProfilerEntry();
    iValues.put(iName,c);
  }
  c.name=iName;
  c.items++;
  c.last=iValue;
  c.total+=c.last;
  c.average=c.total / c.items;
  if (c.last < c.min)   c.min=c.last;
  if (c.last > c.max)   c.max=c.last;
  return c.last;
}
