{
  if (transaction.get() != null)   return doGetIndexValue(indexId,key);
  checkOpeness();
  stateLock.acquireReadLock();
  try {
    checkOpeness();
    dataLock.acquireSharedLock();
    try {
      return doGetIndexValue(indexId,key);
    }
  finally {
      dataLock.releaseSharedLock();
    }
  }
  finally {
    stateLock.releaseReadLock();
  }
}
