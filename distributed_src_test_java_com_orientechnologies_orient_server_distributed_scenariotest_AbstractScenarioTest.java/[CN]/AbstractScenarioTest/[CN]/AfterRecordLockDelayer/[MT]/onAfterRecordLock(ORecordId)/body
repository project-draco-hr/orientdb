{
  try {
    OLogManager.instance().info(this,"Waiting %s for %dms with locked record [%s]",Thread.currentThread().getId(),delay,rid.toString());
    Thread.sleep(delay);
    OLogManager.instance().info(this,"Finished %s waiting for %dms with locked record [%s]",Thread.currentThread().getId(),delay,rid.toString());
  }
 catch (  InterruptedException e) {
  }
}
