{
  if (jj_scan_token(ORDER))   return true;
  if (jj_scan_token(BY))   return true;
  if (jj_3R_37())   return true;
  Token xsp;
  while (true) {
    xsp=jj_scanpos;
    if (jj_3R_118()) {
      jj_scanpos=xsp;
      break;
    }
  }
  xsp=jj_scanpos;
  if (jj_3R_119())   jj_scanpos=xsp;
  return false;
}
