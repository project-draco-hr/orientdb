{
  if (buffer != null) {
    buffer.close();
    buffer=null;
  }
  if (writeAheadLog != null) {
    writeAheadLog.delete();
    writeAheadLog=null;
  }
  File file=new File(storageLocal.getConfiguration().getDirectory() + "/o2QCacheTest.tst");
  if (file.exists()) {
    boolean delete=file.delete();
    Assert.assertTrue(delete);
  }
}
