{
  final Bindings binding=iEngine.createBindings();
  for (  OScriptInjection i : injections)   i.bind(binding);
  if (db != null) {
    binding.put("db",new OScriptDocumentDatabaseWrapper(db));
    binding.put("gdb",new OScriptGraphDatabaseWrapper(db));
  }
  if (iContext != null) {
    for (    Entry<String,Object> a : iContext.entrySet())     binding.put(a.getKey(),a.getValue());
  }
  if (iArgs != null) {
    for (    Entry<Object,Object> a : iArgs.entrySet())     binding.put(a.getKey().toString(),a.getValue());
    binding.put("params",iArgs.values());
  }
 else   binding.put("params",Collections.emptyList());
  return binding;
}
