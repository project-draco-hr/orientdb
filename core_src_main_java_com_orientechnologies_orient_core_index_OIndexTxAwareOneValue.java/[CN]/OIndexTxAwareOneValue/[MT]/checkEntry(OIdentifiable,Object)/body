{
  final OIdentifiable previousRecord=get(iKey);
  if (previousRecord != null && !previousRecord.equals(iRecord))   throw new ORecordDuplicatedException(String.format("Cannot index record %s: found duplicated key '%s' in index '%s' previously assigned to the record %s",iRecord,iKey,getName(),previousRecord),previousRecord.getIdentity());
  super.checkEntry(iRecord,iKey);
}
