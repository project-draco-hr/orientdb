{
  OStorageRemoteNodeSession nodeSession=session.get(getServerURL());
  writeByte(iCommand);
  writeInt(nodeSession.getSessionId());
  if (nodeSession.getToken() != null) {
    if (!session.has(this)) {
      writeBytes(nodeSession.getToken());
      session.add(this);
    }
 else     writeBytes(new byte[]{});
  }
}
