{
  final int position=getHoles() * RECORD_SIZE;
  file.allocateSpace(RECORD_SIZE);
  file.writeLong(position,iPosition);
  if (OLogManager.instance().isDebugEnabled())   OLogManager.instance().debug(this,"Pushed new hole at #%d containing the position #%d:%d",position / RECORD_SIZE,owner.getId(),iPosition);
  return position;
}
