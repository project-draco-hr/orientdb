{
  ODatabaseDocumentTx db=new ODatabaseDocumentTx("memory:test");
  db.create();
  try {
    Shape parse=JtsSpatialContext.GEO.makeRectangle(-83.7662120858887,-83.71986351411135,42.26531323615103,42.29239784478525);
    Point point=JtsSpatialContext.GEO.makePoint(-83.7605452,42.2814837);
    point.relate(parse);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
 finally {
    db.drop();
  }
}
