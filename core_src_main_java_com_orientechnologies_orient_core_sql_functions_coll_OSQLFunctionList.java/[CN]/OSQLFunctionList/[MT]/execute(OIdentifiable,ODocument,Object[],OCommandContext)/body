{
  if (iParameters.length > 1)   context=new ArrayList<Object>();
  for (  Object value : iParameters) {
    if (value != null) {
      if (iParameters.length == 1 && context == null)       context=new ArrayList<Object>();
      if (value instanceof Collection<?>)       context.addAll((Collection<?>)value);
 else       context.add(value);
    }
  }
  return prepareResult(context);
}
