{
  if ("add".equals(operation)) {
    iRequest.data.commandInfo="Studio add cluster";
    int clusterId=db.addCluster(fields.get("name"),CLUSTER_TYPE.valueOf(fields.get("type")));
    sendTextContent(iRequest,OHttpUtils.STATUS_OK_CODE,"OK",null,OHttpUtils.CONTENT_TEXT_PLAIN,"Cluster " + fields.get("name") + "' created successfully with id="+ clusterId);
  }
 else   if ("del".equals(operation)) {
    iRequest.data.commandInfo="Studio delete cluster";
    db.dropCluster(fields.get("name"));
    sendTextContent(iRequest,OHttpUtils.STATUS_OK_CODE,"OK",null,OHttpUtils.CONTENT_TEXT_PLAIN,"Cluster " + fields.get("name") + "' deleted successfully");
  }
}
