{
  OCluster cluster=clusterMap.get(iConfig.getName());
  if (cluster != null) {
    if (cluster instanceof OClusterLocal)     ((OClusterLocal)cluster).config=(OStorageSegmentConfiguration)iConfig;
    return -1;
  }
  if (iConfig instanceof OStoragePhysicalClusterConfiguration)   cluster=new OClusterLocal(this,(OStoragePhysicalClusterConfiguration)iConfig);
 else   cluster=new OClusterLogical(this,(OStorageLogicalClusterConfiguration)iConfig);
  return registerCluster(cluster);
}
