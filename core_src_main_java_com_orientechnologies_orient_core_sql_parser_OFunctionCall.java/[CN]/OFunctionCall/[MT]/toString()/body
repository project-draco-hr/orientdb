{
  StringBuilder result=new StringBuilder();
  result.append(name.toString());
  result.append("(");
  if (star) {
    result.append("*");
  }
 else {
    boolean first=true;
    for (    OExpression expr : params) {
      if (!first) {
        result.append(", ");
      }
      result.append(expr.toString());
      first=false;
    }
  }
  result.append(")");
  return result.toString();
}
