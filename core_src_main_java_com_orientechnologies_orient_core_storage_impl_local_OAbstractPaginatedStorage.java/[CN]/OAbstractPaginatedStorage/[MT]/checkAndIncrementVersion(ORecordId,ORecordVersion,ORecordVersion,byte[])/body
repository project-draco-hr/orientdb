{
switch (version.getCounter()) {
case -1:
    iDatabaseVersion.increment();
  break;
case -2:
iDatabaseVersion.setCounter(-2);
break;
default :
if (!version.equals(iDatabaseVersion)) return conflictResolver.onUpdate(rid,version,iRecordContent,iDatabaseVersion);
iDatabaseVersion.increment();
}
return null;
}
