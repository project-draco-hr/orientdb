{
  if (iSource.length == 0)   return iRecord;
  if (iRecord == null)   iRecord=new ODocument();
 else   checkTypeODocument(iRecord);
  BytesContainer container=new BytesContainer(iSource);
  container.skip(1);
  serializerByVersion[iSource[0]].deserialize((ODocument)iRecord,container,iFields);
  return iRecord;
}
