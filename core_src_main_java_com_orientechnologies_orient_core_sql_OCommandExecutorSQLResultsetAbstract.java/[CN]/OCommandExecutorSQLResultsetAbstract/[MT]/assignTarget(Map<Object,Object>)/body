{
  parameters=iArgs;
  compiledFilter.bindParameters(iArgs);
  if (target == null)   if (compiledFilter.getTargetClasses() != null)   searchInClasses();
 else   if (compiledFilter.getTargetClusters() != null)   searchInClusters();
 else   if (compiledFilter.getTargetRecords() != null)   target=compiledFilter.getTargetRecords();
 else   return false;
  return true;
}
