{
  if (isClosed())   return;
  objects2Records.clear();
  records2Objects.clear();
  rid2Records.clear();
  checkOpeness();
  rollback();
  ((ODatabaseRaw)((ODatabaseRecord)underlying.getUnderlying()).getUnderlying()).callOnCloseListeners();
  getLevel1Cache().clear();
  ownerPool.release(this);
  ownerPool=null;
}
