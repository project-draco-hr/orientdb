{
  String key=iClassName.toLowerCase();
  if (classesMap.containsKey(key))   throw new OSchemaException("Class " + iClassName + " already exists in current database");
  OClass cls=new OClass(this,classesMap.size(),iClassName,iClusterIds,iDefaultClusterId);
  classesMap.put(key,cls);
  classes.add(cls);
  setDirty();
  return cls;
}
