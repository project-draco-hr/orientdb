{
  if (words[0].startsWith("'") || words[0].startsWith("\""))   return stringContent(words[1]);
 else   if (Character.isDigit(words[0].charAt(0)) || words[0].charAt(0) == '+' || words[0].charAt(0) == '-') {
    if (words[0].contains("."))     return new Float(words[0]);
 else     return new Integer(words[0]);
  }
  return new OSQLFilterItemField(this,words[1]);
}
