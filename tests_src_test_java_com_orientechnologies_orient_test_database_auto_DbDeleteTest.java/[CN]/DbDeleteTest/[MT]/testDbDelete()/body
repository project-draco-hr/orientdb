{
  ODatabaseDocument db;
  if (url.startsWith("plocal:"))   db=new ODatabaseDocumentTx("plocal:" + testPath + "/"+ DbImportExportTest.NEW_DB_URL);
 else   db=new ODatabaseDocumentTx("local:" + testPath + "/"+ DbImportExportTest.NEW_DB_URL);
  if (!db.exists())   db.create();
  ODatabaseHelper.dropDatabase(db);
  Assert.assertFalse(new File(testPath + "/" + DbImportExportTest.NEW_DB_PATH).exists());
}
