{
  Lock fileLock;
  cacheLock.readLock().lock();
  try {
    fileLock=fileLockManager.acquireExclusiveLock(fileId);
    try {
      writeCache.renameFile(fileId,oldFileName,newFileName);
    }
  finally {
      fileLockManager.releaseLock(fileLock);
    }
  }
  finally {
    cacheLock.readLock().unlock();
  }
}
