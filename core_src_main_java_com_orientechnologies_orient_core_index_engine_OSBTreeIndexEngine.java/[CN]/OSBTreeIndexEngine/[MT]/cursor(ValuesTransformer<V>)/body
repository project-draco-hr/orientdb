{
  acquireSharedLock();
  try {
    final Object firstKey=sbTree.firstKey();
    if (firstKey == null)     return new OIndexCursor(){
      @Override public Map.Entry<Object,OIdentifiable> next(      int prefetchSize){
        return null;
      }
    }
;
    return new OSBTreeIndexCursor<V>(sbTree.iterateEntriesMajor(firstKey,true,true),valuesTransformer);
  }
  finally {
    releaseSharedLock();
  }
}
