{
  if (values[iIndex] == null)   try {
    OProfiler.getInstance().updateCounter("OMVRBTreeEntryP.unserializeValue",1);
    values[iIndex]=(V)valueFromStream(iIndex);
  }
 catch (  IOException e) {
    OLogManager.instance().error(this,"Can't lazy load the value #" + iIndex + " in tree node "+ this,e,OSerializationException.class);
  }
  return values[iIndex];
}
