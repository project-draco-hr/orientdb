{
  OTreeMapEntry<K,V> t=this;
  OTreeMapEntry<K,V> p=null;
  if (t.getLeftInMemory() != null) {
    p=t.getLeftInMemory();
    while (p.getRightInMemory() != null)     p=p.getRightInMemory();
  }
 else {
    p=t.getParentInMemory();
    while (p != null && t == p.getLeftInMemory()) {
      t=p;
      p=p.getParentInMemory();
    }
  }
  return p;
}
