{
  if (alias != null) {
    builder.append(alias);
    if (modifier != null) {
      modifier.toString(params,builder);
    }
  }
 else   if (recordAttr != null) {
    builder.append(recordAttr);
  }
 else   if (rid != null) {
    rid.toString(params,builder);
  }
  if (type != null) {
    builder.append(" " + type);
  }
}
