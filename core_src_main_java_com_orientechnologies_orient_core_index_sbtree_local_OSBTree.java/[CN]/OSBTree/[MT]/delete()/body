{
  acquireExclusiveLock();
  try {
    diskCache.deleteFile(fileId);
    if (nullPointerSupport)     diskCache.deleteFile(nullBucketFileId);
  }
 catch (  IOException e) {
    throw new OSBTreeException("Error during delete of sbtree with name " + name,e);
  }
 finally {
    releaseExclusiveLock();
  }
}
