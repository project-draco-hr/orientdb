{
  if (graph != null)   graph.setCurrentGraphInThreadLocal();
  final String clsName=getRecord().getClassName();
  if (clsName.equals(OrientVertexType.CLASS_NAME))   return StringFactory.vertexString(this);
  return StringFactory.V + "(" + clsName+ ")"+ StringFactory.L_BRACKET+ getId()+ StringFactory.R_BRACKET;
}
