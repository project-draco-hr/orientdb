{
  this(OGlobalConfiguration.WAL_CACHE_SIZE.getValueAsInteger(),OGlobalConfiguration.WAL_COMMIT_TIMEOUT.getValueAsInteger(),OGlobalConfiguration.WAL_MAX_SEGMENT_SIZE.getValueAsInteger() * ONE_KB * ONE_KB,OGlobalConfiguration.WAL_LOCATION.getValueAsString(),storage,OGlobalConfiguration.WAL_FILE_AUTOCLOSE_INTERVAL.getValueAsInteger());
