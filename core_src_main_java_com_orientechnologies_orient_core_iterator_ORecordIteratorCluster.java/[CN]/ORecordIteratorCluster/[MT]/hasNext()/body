{
  checkDirection(true);
  if (limit > -1 && browsedRecords >= limit)   return false;
  if (browsedRecords >= totalAvailableRecords)   return false;
  if (liveUpdated)   lastClusterPosition=getRangeTo();
  long recordsToBrowse=current.clusterPosition > -2 && lastClusterPosition > -1 ? lastClusterPosition - current.clusterPosition : 0;
  if (recordsToBrowse > 0)   return true;
  if (txEntries != null)   recordsToBrowse+=txEntries.size() - (currentTxEntryPosition + 1);
  return recordsToBrowse > 0;
}
