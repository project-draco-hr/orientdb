{
  String name;
  endBlocks=new ArrayList<OBlock>();
  if (iEndBlocks != null) {
    for (    ODocument block : iEndBlocks) {
      name=block.fieldNames()[0];
      final OBlock b=factory.getBlock(name);
      endBlocks.add(b);
      configureComponent(b,(ODocument)block.field(name),iContext);
    }
  }
}
