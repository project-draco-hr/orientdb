  OETLProcessor proc=getProcessor("{source: { content: { value: 'name,\nGregor\nGregor\nHans' } }, extractor : { row: {} }," + " transformers: [{csv: {}}, {vertex: {class:'Person', skipDuplicates:false}}," + "], loader: { orientdb: { dbURL: 'memory:EdgeTransformerTest', dbType:'graph', useLightweightEdges:false } } }").execute();
