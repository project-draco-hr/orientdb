{
  final OSQLCondition condition;
  final Object target;
  if (iCondition.getLeft() instanceof OSQLCondition) {
    condition=(OSQLCondition)iCondition.getLeft();
    target=iRight;
  }
 else {
    condition=(OSQLCondition)iCondition.getRight();
    target=iRight;
  }
  return traverse(condition,target,0);
}
