{
  OLogManager.instance().debug(this,"Discovering cluster nodes using IP Multicast with address %s, port %d...",dgram.getAddress(),dgram.getPort());
  try {
    socket.send(dgram);
  }
 catch (  Throwable t) {
    OLogManager.instance().error(this,"Error on sending signal for cluster presence",t);
  }
 finally {
  }
}
