{
  try {
    return OIOUtils.readStreamAsString(in);
  }
 catch (  IOException e) {
  }
  String script="";
  try {
    BufferedReader reader=new BufferedReader(new InputStreamReader(in));
    StringBuilder out=new StringBuilder();
    String line;
    while ((line=reader.readLine()) != null) {
      out.append(line + "\n");
    }
    script=out.toString();
    reader.close();
  }
 catch (  Exception e) {
  }
  return script;
}
