{
  final int queueSize=iQueue.size();
  if (queueSize > 0) {
    if (!iUnqueuePendingMessages) {
      ODistributedServerLog.warn(this,manager.getLocalNodeName(),null,DIRECTION.NONE,"found %d messages in queue %s, clearing them...",queueSize,iQueueName);
      iQueue.clear();
    }
 else {
      ODistributedServerLog.warn(this,manager.getLocalNodeName(),null,DIRECTION.NONE,"found %d messages in queue %s, aligning the database...",queueSize,iQueueName);
      return true;
    }
  }
 else   ODistributedServerLog.info(this,manager.getLocalNodeName(),null,DIRECTION.NONE,"found no previous messages in queue %s",iQueueName);
  return false;
}
