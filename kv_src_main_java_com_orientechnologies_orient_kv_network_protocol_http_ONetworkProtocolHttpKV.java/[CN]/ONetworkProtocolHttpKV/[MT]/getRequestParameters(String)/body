{
  if (iParameters == null || iParameters.length() <= 1)   throw new ONetworkProtocolException("Requested URI is invalid: " + iParameters);
  iParameters=iParameters.substring(1);
  String[] pars=iParameters.split("/");
  if (pars == null || pars.length < 3)   throw new ONetworkProtocolException("Requested URI is invalid: " + iParameters);
  return pars;
}
