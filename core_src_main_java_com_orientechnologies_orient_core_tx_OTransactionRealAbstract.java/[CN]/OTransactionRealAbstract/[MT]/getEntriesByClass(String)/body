{
  final List<OTransactionEntry<?>> result=new ArrayList<OTransactionEntry<?>>();
  if (iClassName == null || iClassName.length() == 0)   for (  OTransactionEntry<REC> entry : entries.values()) {
    result.add(entry);
  }
 else   for (  OTransactionEntry<REC> entry : entries.values()) {
    if (entry.record != null && entry.record instanceof ODocument && iClassName.equals(((ODocument)entry.record).getClassName()))     result.add(entry);
  }
  return result;
}
