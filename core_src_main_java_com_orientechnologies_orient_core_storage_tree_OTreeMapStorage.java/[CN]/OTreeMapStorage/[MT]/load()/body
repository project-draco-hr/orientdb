{
  lock.acquireExclusiveLock();
  try {
    ORawBuffer raw=storage.readRecord(-1,clusterId,record.getIdentity().getClusterPosition());
    if (raw == null)     throw new OConfigurationException("Can't load map with id " + clusterId + ":"+ record.getIdentity().getClusterPosition());
    record.setVersion(raw.version);
    fromStream(raw.buffer);
    return this;
  }
  finally {
    lock.releaseExclusiveLock();
  }
}
