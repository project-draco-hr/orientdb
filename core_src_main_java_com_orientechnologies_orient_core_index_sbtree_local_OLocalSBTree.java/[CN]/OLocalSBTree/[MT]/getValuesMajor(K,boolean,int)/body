{
  final List<ORID> result=new ArrayList<ORID>();
  loadValuesMajor(key,inclusive,new RangeResultListener<K>(){
    @Override public boolean addResult(    OSBTreeBucket.SBTreeEntry<K> entry){
      result.add(entry.value);
      if (maxValuesToFetch > -1 && result.size() >= maxValuesToFetch)       return false;
      return true;
    }
  }
);
  return result;
}
