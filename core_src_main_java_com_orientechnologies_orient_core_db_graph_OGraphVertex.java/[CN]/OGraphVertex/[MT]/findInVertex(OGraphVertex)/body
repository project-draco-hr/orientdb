{
  final List<ODocument> docs=document.field(OGraphDatabase.VERTEX_FIELD_IN_EDGES);
  if (docs == null || docs.size() == 0)   return -1;
  for (int i=0; i < docs.size(); ++i) {
    if (docs.get(i).field(OGraphDatabase.EDGE_FIELD_IN).equals(iVertexDocument.getDocument()))     return i;
  }
  return -1;
}
