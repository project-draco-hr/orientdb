{
  acquireSharedLock();
  try {
    OExtendibleHashingTable.Entry[] entries=content.higherEntries(OClusterPositionFactory.INSTANCE.valueOf(-1));
    if (entries.length == 0)     return OClusterPosition.INVALID_POSITION;
 else     return entries[0].key;
  }
  finally {
    releaseSharedLock();
  }
}
