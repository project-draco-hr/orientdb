{
  @SuppressWarnings("unused") final Object[] result=(Object[])dManager.sendOperation2Node(nodeSource,tasks);
  for (int i=0; i < positions.size(); ++i)   tasks.getTask(i).setAsCompleted(synchronizer,positions.get(i));
  final int aligned=tasks.getTasks();
  tasks.clearTasks();
  positions.clear();
  return aligned;
}
