{
  final OClassImpl cls;
  if (iSuperClass instanceof OClassAbstractDelegate)   cls=(OClassImpl)((OClassAbstractDelegate)iSuperClass).delegate;
 else   cls=(OClassImpl)iSuperClass;
  if (cls != null)   cls.addBaseClasses(this);
 else   if (superClass != null)   superClass.removeBaseClassInternal(this);
  this.superClass=cls;
}
