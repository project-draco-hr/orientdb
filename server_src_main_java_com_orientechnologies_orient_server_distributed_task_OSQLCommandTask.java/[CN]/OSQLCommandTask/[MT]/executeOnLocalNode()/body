{
  ODistributedServerLog.debug(this,getDistributedServerManager().getLocalNodeName(),getNodeSource(),DIRECTION.IN,"execute command=%s db=%s",text.toString(),databaseName);
  final ODatabaseDocumentTx db=openDatabase();
  try {
    return openDatabase().command(new OCommandSQL(text)).execute();
  }
  finally {
    closeDatabase(db);
  }
}
