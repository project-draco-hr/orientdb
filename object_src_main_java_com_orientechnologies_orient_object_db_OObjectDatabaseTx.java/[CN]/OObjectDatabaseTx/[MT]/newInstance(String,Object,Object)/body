{
  checkSecurity(ODatabaseSecurityResources.CLASS,ORole.PERMISSION_CREATE,iClassName);
  try {
    RET enhanced=(RET)OObjectEntityEnhancer.getInstance().getProxiedInstance(entityManager.getEntityClass(iClassName),iEnclosingClass,underlying.newInstance(iClassName),iArgs);
    return (RET)enhanced;
  }
 catch (  Exception e) {
    OLogManager.instance().error(this,"Error on creating object of class " + iClassName,e,ODatabaseException.class);
  }
  return null;
}
