{
  Set<OIdentifiable> results=new HashSet<OIdentifiable>();
  Query q=null;
  try {
    q=OLuceneIndexType.createFullQuery(index,key,indexWriter.getAnalyzer(),getVersion(metadata));
    return getResults(q);
  }
 catch (  ParseException e) {
