{
  final int size=iText.length();
  final int jumpCharSize=iJumpChars.length();
  for (; iCurrentPosition < size; ++iCurrentPosition)   for (int jumpIndex=0; jumpIndex < jumpCharSize; ++jumpIndex)   if (iJumpChars.charAt(jumpIndex) != iText.charAt(iCurrentPosition))   return iCurrentPosition;
  return iCurrentPosition;
}
