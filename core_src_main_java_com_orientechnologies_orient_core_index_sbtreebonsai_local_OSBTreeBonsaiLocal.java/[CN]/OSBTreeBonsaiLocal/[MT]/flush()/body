{
  atomicOperationsManager.acquireReadLock(this);
  try {
    acquireSharedLock();
    try {
      try {
        diskCache.flushBuffer();
      }
 catch (      IOException e) {
        throw new OSBTreeException("Error during flush of sbtree [" + name + "] data");
      }
    }
  finally {
      releaseSharedLock();
    }
  }
  finally {
    atomicOperationsManager.releaseReadLock(this);
  }
}
