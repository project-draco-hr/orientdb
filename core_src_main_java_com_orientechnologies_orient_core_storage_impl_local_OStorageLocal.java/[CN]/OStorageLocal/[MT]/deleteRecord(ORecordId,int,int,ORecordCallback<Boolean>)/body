{
  checkOpeness();
  final OCluster cluster=getClusterById(iRid.clusterId);
  if (txManager.isCommitting())   return txManager.deleteRecord(txManager.getCurrentTransaction().getId(),cluster,iRid.clusterPosition,iVersion);
 else {
    final boolean deleted=deleteRecord(cluster,iRid,iVersion);
    if (deleted && OGlobalConfiguration.NON_TX_RECORD_UPDATE_SYNCH.getValueAsBoolean())     synch();
    return deleted;
  }
}
