{
  oServer.setVariable("ODistributedAbstractPlugin",this);
  for (  OServerParameterConfiguration param : iParams) {
    if (param.name.equalsIgnoreCase("enabled")) {
      if (!Boolean.parseBoolean(param.value)) {
        enabled=false;
        return;
      }
    }
 else     if (param.name.equalsIgnoreCase("alias"))     alias=param.value;
 else     if (param.name.startsWith("db."))     databaseConfiguration.put(param.name.substring("db.".length()),(ODocument)new ODocument().fromJSON(param.value.trim(),"noMap"));
  }
  if (!databaseConfiguration.containsKey("*"))   throw new OConfigurationException("Invalid cluster configuration: cannot find settings for the default synchronization as 'db.*'");
}
