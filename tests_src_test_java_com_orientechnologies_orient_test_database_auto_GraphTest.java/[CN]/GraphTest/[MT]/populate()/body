{
  database.open("admin","admin");
  ONode newNode;
  ONode currentNode=database.createNode().set("id",0);
  for (int i=1; i <= TOT_RECORDS; ++i) {
    newNode=database.createNode().set("id",i);
    currentNode.link(newNode).set("createdOn",new Date());
    currentNode=newNode;
  }
  database.close();
}
