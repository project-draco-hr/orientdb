{
  record.reset();
  record.setClassName("Account");
  record.fromString(new String("Account@id:" + data.getCyclesDone() + ",name:'Luca',surname:'Garulli',birthDate:"+ date.getTime()+ ",salary:"+ 3000f+ data.getCyclesDone()));
