{
  try {
    tree=new OTreeMapDatabaseLazySave<String,T>((ODatabaseRecord<?>)database,new ORecordId(database.getStorage().getConfiguration().dictionaryRecordId));
    tree.load();
  }
 catch (  IOException e) {
    OLogManager.instance().error(this,"Can't load tree from the database",e,ODatabaseException.class);
  }
}
