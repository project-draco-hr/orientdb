{
  List<ODocument> results=databaseDocumentTx.command(new OCommandSQL("select *, ST_AsText(geometry) as text from Location where name = ? ")).execute(name);
  Assert.assertEquals(1,results.size());
  ODocument doc=results.iterator().next();
  String asText=doc.field("text");
  Assert.assertNotNull(asText);
  Assert.assertEquals(asText,wkt);
}
