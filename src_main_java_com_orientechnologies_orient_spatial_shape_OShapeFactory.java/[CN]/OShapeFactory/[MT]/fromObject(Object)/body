{
  if (obj instanceof String) {
    return fromText((String)obj);
  }
  if (obj instanceof ODocument) {
    return fromDoc((ODocument)obj);
  }
  if (obj instanceof Map) {
    Map map=(Map)((Map)obj).get("shape");
    if (map == null) {
      map=(Map)obj;
    }
    return fromMapGeoJson(map);
  }
  return null;
}
