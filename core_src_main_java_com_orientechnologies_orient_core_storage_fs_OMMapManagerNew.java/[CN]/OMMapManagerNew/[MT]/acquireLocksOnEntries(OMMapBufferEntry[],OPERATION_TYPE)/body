{
  for (  OMMapBufferEntry entry : entries) {
    if (OPERATION_TYPE.WRITE.equals(operationType))     entry.acquireWriteLock();
 else     entry.acquireReadLock();
  }
  if (operationType == OPERATION_TYPE.WRITE) {
    for (    OMMapBufferEntry entry : entries) {
      entry.setDirty();
    }
  }
}
