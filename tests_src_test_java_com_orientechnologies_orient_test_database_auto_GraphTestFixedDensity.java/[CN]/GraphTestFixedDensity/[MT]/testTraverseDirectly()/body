{
  long time=System.currentTimeMillis();
  nodeReadCounter=-1;
  readSubNodesDirectly(database.getRoot("LinearGraph"));
  System.out.println("Direct traverse with hot cache of the entire graph with deep=" + nodeReadCounter + " and fixed density="+ DENSITY+ " in "+ ((System.currentTimeMillis() - time) / 1000f)+ " sec.");
  Assert.assertEquals(nodeReadCounter,nodeWrittenCounter);
  database.close();
}
