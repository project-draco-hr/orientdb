{
  if (!ascSortOrder)   throw new IllegalStateException("Descending sort order is not supported");
  final OCommandRequest cmd;
  if (isInclusive)   cmd=formatCommand(QUERY_GET_VALUE_MAJOR_EQUALS,name);
 else   cmd=formatCommand(QUERY_GET_VALUE_MAJOR,name);
  return getDatabase().command(cmd).execute(fromKey);
}
