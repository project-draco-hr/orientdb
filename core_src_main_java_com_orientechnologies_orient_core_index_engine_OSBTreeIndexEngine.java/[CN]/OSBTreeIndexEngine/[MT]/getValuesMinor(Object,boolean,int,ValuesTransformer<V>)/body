{
  acquireSharedLock();
  try {
    final Set<OIdentifiable> result=new HashSet<OIdentifiable>();
    sbTree.loadEntriesMinor(toKey,isInclusive,new OSBTree.RangeResultListener<Object,V>(){
      @Override public boolean addResult(      OTree.BucketEntry<Object,V> entry){
        addToResult(transformer,result,entry.getValue(),maxValuesToFetch);
        if (maxValuesToFetch > -1 && result.size() == maxValuesToFetch)         return false;
        return true;
      }
    }
);
    return result;
  }
  finally {
    releaseSharedLock();
  }
}
