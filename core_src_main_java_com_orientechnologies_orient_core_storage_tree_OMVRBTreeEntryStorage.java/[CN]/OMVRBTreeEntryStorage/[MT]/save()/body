{
  record.fromStream(toStream());
  if (record.getIdentity().isValid())   record.setVersion(((OMVRBTreeStorage<K,V>)tree).storage.updateRecord(0,record.getIdentity().getClusterId(),record.getIdentity().getClusterPosition(),record.toStream(),-1,record.getRecordType()));
 else {
    record.setIdentity(record.getIdentity().getClusterId(),((OMVRBTreeStorage<K,V>)tree).storage.createRecord(record.getIdentity().getClusterId(),record.toStream(),record.getRecordType()));
  }
  record.unsetDirty();
  flush2Record();
  return this;
}
