{
  database.open("admin","admin");
  long time=System.currentTimeMillis();
  readSubNodesThroughEdges((OGraphVertex)database.load(rootId));
  System.out.println("Read of the entire graph with depth=" + nodeReadCounter + " and fixed density="+ DENSITY+ " in "+ ((System.currentTimeMillis() - time) / 1000f)+ " sec.");
  Assert.assertEquals(nodeReadCounter,nodeWrittenCounter);
}
