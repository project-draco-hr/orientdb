{
  PoolTester pt=new PoolTester();
  int runs=20;
  logger.info("start");
  for (int i=0; i < runs; i++) {
    pt.doStuff(i);
  }
  logger.info("***** sleeping for 30 sec, stop and start Orientdb now! *****");
  Thread.sleep(30 * 1000);
  for (int i=0; i < runs; i++) {
    pt.doStuff(i);
  }
  logger.info("done");
}
