{
  List<ORecordId> indexedRIDs=get(iKey);
  if (indexedRIDs != null && indexedRIDs.size() > 0) {
    Object obj=indexedRIDs.get(0);
    ORID rid;
    if (obj instanceof ORecordId)     rid=(ORID)obj;
 else     rid=((ODocument)obj).getIdentity();
    if (!rid.equals(iRecord.getIdentity()))     OLogManager.instance().exception("Found duplicated key '%s' previously assigned to the record %s",null,OIndexException.class,iKey,rid);
  }
}
