{
  checkDirection(false);
  ORecordInternal<?> record=getRecord();
  if (currentRecord != null) {
    try {
      return (REC)currentRecord;
    }
  finally {
      currentRecord=null;
    }
  }
  while (hasPrevious()) {
    try {
      return (REC)currentRecord;
    }
  finally {
      currentRecord=null;
    }
  }
  return null;
}
