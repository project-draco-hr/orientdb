{
  lockValue(id);
  try {
    final OrientBaseGraph g=acquire();
    try {
      OrientVertex v=getFromCache(id);
      if (v != null)       return v;
      v=g.addVertex(id);
      verticesCreated.incrementAndGet();
      addInCache(id,v);
      return v;
    }
  finally {
      g.shutdown();
    }
  }
  finally {
    unlockValue(id);
  }
}
