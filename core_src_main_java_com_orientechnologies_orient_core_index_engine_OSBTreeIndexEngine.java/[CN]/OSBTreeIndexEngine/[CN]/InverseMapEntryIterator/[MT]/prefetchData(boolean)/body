{
  sbTree.loadEntriesMinor(lastKey,firstTime,new OSBTree.RangeResultListener<Object,V>(){
    @Override public boolean addResult(    final Map.Entry<Object,V> entry){
      preFetchedValues.add(entry);
      return preFetchedValues.size() <= 8000;
    }
  }
);
  if (preFetchedValues.isEmpty())   preFetchedValues=null;
 else   lastKey=preFetchedValues.getLast().getKey();
}
