{
  if (iClusterId == -1)   throw new OStorageException("Cluster Id is invalid: " + iClusterId);
  checkOpeness();
  final boolean locked=lock.acquireSharedLock();
  try {
    return clusters[iClusterId] != null ? new long[]{clusters[iClusterId].getFirstEntryPosition(),clusters[iClusterId].getLastEntryPosition()} : new long[0];
  }
 catch (  IOException e) {
    OLogManager.instance().error(this,"Error on getting last entry position",e);
    return null;
  }
 finally {
    lock.releaseSharedLock(locked);
  }
}
