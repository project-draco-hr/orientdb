{
  int paramCounter=0;
  for (int i=0; i < iFieldNames.size(); ++i) {
    if (iFieldValues[i] instanceof OSQLFilterItemField) {
      OSQLFilterItemField f=(OSQLFilterItemField)iFieldValues[i];
      if (f.getName().equals("?"))       iDocument.field(iFieldNames.get(i),iArgs.get(paramCounter++));
 else       if (f.getName().startsWith(":"))       iDocument.field(iFieldNames.get(i),iArgs.get(f.getName().substring(1)));
    }
 else     iDocument.field(iFieldNames.get(i),OSQLHelper.getValue(iFieldValues[i],iDocument));
  }
}
