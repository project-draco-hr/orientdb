{
  if (iFieldValue instanceof ORecordLazyList)   OObjectSerializerHelper.setFieldValue(iUserObject,iFieldName,new OLazyObjectList(iRecord,(ORecordLazyList)iFieldValue));
 else   if (iFieldValue instanceof ORecordLazySet)   OObjectSerializerHelper.setFieldValue(iUserObject,iFieldName,new OLazyObjectSet(iRecord,(ORecordLazyList)iFieldValue));
 else   if (iFieldValue instanceof ORecordLazyMap)   OObjectSerializerHelper.setFieldValue(iUserObject,iFieldName,new OLazyObjectMap(iRecord,(ORecordLazyMap)iFieldValue));
 else   OObjectSerializerHelper.setFieldValue(iUserObject,iFieldName,iFieldValue);
}
