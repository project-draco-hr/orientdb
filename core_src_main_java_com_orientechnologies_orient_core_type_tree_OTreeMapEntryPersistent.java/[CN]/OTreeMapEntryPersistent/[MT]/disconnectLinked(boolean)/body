{
  int disconnected=0;
  if (parent != null) {
    if (parent.left == this) {
      parent.left=null;
    }
 else {
      parent.right=null;
    }
    parent=null;
  }
  if (left != null) {
    left.parent=null;
    disconnected+=left.disconnectLinked(iForceDirty);
    left=null;
  }
  if (right != null) {
    right.parent=null;
    disconnected+=right.disconnectLinked(iForceDirty);
    right=null;
  }
  return disconnected;
}
