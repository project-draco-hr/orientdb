{
  this._lazyLoad=iLazyLoad;
  checkForFields();
  if (_fieldValues != null) {
    for (    Entry<String,Object> field : _fieldValues.entrySet()) {
      if (field.getValue() instanceof ORecordLazyMultiValue)       ((ORecordLazyMultiValue)field.getValue()).setAutoConvertToRecord(false);
    }
  }
}
