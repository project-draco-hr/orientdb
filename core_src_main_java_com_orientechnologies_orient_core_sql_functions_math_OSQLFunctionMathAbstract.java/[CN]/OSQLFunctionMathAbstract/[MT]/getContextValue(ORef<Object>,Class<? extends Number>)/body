{
  if (iClass != iContext.value.getClass()) {
    if (iClass == Long.class)     iContext.value=new Long(((Number)iContext.value).longValue());
 else     if (iClass == Float.class)     iContext.value=new Float(((Number)iContext.value).floatValue());
 else     if (iClass == Double.class)     iContext.value=new Double(((Number)iContext.value).doubleValue());
  }
  return (Number)iContext.value;
}
