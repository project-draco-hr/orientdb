{
  sendStatus(iCode,iReason);
  sendResponseHeaders(iContentType);
  if (iHeaders != null)   writeLine(iHeaders);
  if (iContent != null)   writeLine(OHttpUtils.CONTENT_LENGTH + iContent.length());
  writeLine(null);
  if (iContent != null && iContent.length() > 0) {
    writeLine(iContent);
  }
  channel.flush();
}
