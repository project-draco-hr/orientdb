{
  if (iFieldValue instanceof ORecordLazyList)   OObjectSerializerHelper.setFieldValue(iUserObject,iFieldName,new OObjectLazyList(iRecord,(ORecordLazyList)iFieldValue,OObjectEntitySerializer.isCascadeDeleteField(iUserObject.getClass(),iFieldName)));
 else   if (iFieldValue instanceof ORecordLazySet)   OObjectSerializerHelper.setFieldValue(iUserObject,iFieldName,new OObjectLazySet(iRecord,(ORecordLazySet)iFieldValue,OObjectEntitySerializer.isCascadeDeleteField(iUserObject.getClass(),iFieldName)));
 else   if (iFieldValue instanceof ORecordLazyMap)   OObjectSerializerHelper.setFieldValue(iUserObject,iFieldName,new OObjectLazyMap(iRecord,(ORecordLazyMap)iFieldValue,OObjectEntitySerializer.isCascadeDeleteField(iUserObject.getClass(),iFieldName)));
 else   OObjectSerializerHelper.setFieldValue(iUserObject,iFieldName,iFieldValue);
}
