{
  try {
    exportInfo();
    exportDictionary();
    exportSchema();
    exportAllClusters();
    listener.onMessage("\nExport of database completed.");
    writer.flush();
  }
 catch (  Exception e) {
    throw new ODatabaseExportException("Error on exporting database '" + database.getName() + " to: "+ fileName,e);
  }
 finally {
    close();
  }
  return this;
}
