{
  checkForRebuild();
  acquireSharedLock();
  try {
    indexEngine.getEntriesMinor(toKey,isInclusive,null,new OIndexEngine.EntriesResultListener(){
      @Override public boolean addResult(      ODocument entry){
        return entriesResultListener.addResult(entry);
      }
    }
);
  }
  finally {
    releaseSharedLock();
  }
}
