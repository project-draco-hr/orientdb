{
  setDataCommandInfo("Get the begin/end range of data in cluster");
  checkDatabase();
  long[] pos=connection.database.getStorage().getClusterDataRange(channel.readShort());
  beginResponse();
  try {
    sendOk(clientTxId);
    channel.writeLong(pos[0]);
    channel.writeLong(pos[1]);
  }
  finally {
    endResponse();
  }
}
