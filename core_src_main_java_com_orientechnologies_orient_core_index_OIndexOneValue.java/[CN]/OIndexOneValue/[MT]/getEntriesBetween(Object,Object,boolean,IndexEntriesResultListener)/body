{
  checkForRebuild();
  if (rangeFrom.getClass() != rangeTo.getClass())   throw new IllegalArgumentException("Range from-to parameters are of different types");
  acquireSharedLock();
  try {
    indexEngine.getEntriesBetween(rangeFrom,rangeTo,inclusive,null,new OIndexEngine.EntriesResultListener(){
      @Override public boolean addResult(      ODocument entry){
        return entriesResultListener.addResult(entry);
      }
    }
);
  }
  finally {
    releaseSharedLock();
  }
}
