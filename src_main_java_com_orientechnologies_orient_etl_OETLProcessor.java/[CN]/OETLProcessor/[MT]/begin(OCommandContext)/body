{
  OLogManager.instance().info(this,"BEGIN");
  if (dumpEveryMs > 0) {
    dumpTask=new TimerTask(){
      @Override public void run(){
        dumpProgress();
      }
    }
;
    Orient.instance().getTimer().schedule(dumpTask,dumpEveryMs,dumpEveryMs);
    startTime=System.currentTimeMillis();
  }
}
