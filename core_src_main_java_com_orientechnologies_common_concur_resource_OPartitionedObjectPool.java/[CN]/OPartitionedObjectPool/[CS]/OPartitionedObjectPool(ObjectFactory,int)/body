{
  this.factory=factory;
  this.maxSize=maxSize;
  final PoolPartition[] pts=new PoolPartition[2];
  for (int i=0; i < pts.length; i++) {
    final PoolPartition partition=new PoolPartition();
    pts[i]=partition;
    initQueue(partition);
  }
  partitions=pts;
  Orient.instance().registerListener(this);
}
