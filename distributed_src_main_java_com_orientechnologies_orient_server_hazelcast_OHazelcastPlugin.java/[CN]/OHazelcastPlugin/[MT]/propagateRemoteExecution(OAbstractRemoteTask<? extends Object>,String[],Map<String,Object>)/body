{
  if (iNodes == null || iNodes.length == 0)   return;
  final EXECUTION_MODE mode=iTask.getMode();
  ODistributedServerLog.debug(this,iTask.getNodeSource(),iNodes.toString(),DIRECTION.OUT,"propagate to %s nodes %s oper=%d.%d",mode,iTask.getName().toUpperCase(),iTask.getRunId(),iTask.getOperationSerial());
  for (  String nodeId : iNodes) {
    final Member m=remoteClusterNodes.get(nodeId);
    if (m == null)     ODistributedServerLog.warn(this,getLocalNodeId(),nodeId,DIRECTION.OUT,"cannot propagate %s operation on remote member because is disconnected",mode);
 else     result.put(nodeId,sendOperation2Node(nodeId,iTask,mode));
  }
}
