{
  if (!enabled)   return null;
  acquireExclusiveLock();
  try {
    final ORecordInternal<?> record=entries.remove(iRID);
    if (record == null || record.isDirty())     return null;
    if (strategy == STRATEGY.COPY_RECORD)     entries.put(iRID,(ORecordInternal<?>)record.flatCopy());
    return record;
  }
  finally {
    releaseExclusiveLock();
  }
}
