{
  ODocument doc=new ODocument("Account");
  doc.field("version",0);
  doc.save();
  doc.setDirty();
  doc.field("testmvcc",true);
  doc.getRecordVersion().increment();
  try {
    doc.save();
    Assert.assertTrue(false);
  }
 catch (  OConcurrentModificationException e) {
    Assert.assertTrue(true);
  }
}
