{
  ODocument cfg=databaseConfiguration.get(iDatabaseName);
  if (cfg == null) {
    cfg=loadDatabaseConfiguration(iDatabaseName,serverInstance.getDatabaseDirectory() + iDatabaseName + "/"+ FILE_DISTRIBUTED_DB_CONFIG);
    if (cfg == null) {
      cfg=databaseConfiguration.get("*");
      saveDatabaseConfiguration(iDatabaseName,cfg);
    }
  }
  return cfg;
}
