{
  List<ORecord<?>> indexedRIDs=get(iKey);
  if (indexedRIDs != null && indexedRIDs.size() > 0) {
    final ORecord<?> rec=indexedRIDs.get(0);
    if (!rec.equals(iRecord))     OLogManager.instance().exception("Found duplicated key '%s' previously assigned to the record %s",null,OIndexException.class,iKey,rec);
  }
}
