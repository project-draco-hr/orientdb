{
  this.pageSize=pageSize;
  updateTimer=Executors.newSingleThreadScheduledExecutor(new SnapshotTaskFactory(storageName));
  mbeanName="com.orientechnologies.orient.core.storage.impl.local.statistic:type=OStoragePerformanceStatisticMXBean,name=" + storageName + ",id="+ storageId;
  this.nanoTimer=nanoTimer;
