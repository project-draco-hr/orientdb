{
  if (obj instanceof String)   return ((String)obj).toLowerCase();
 else   if (obj instanceof OCompositeKey) {
    final OCompositeKey result=new OCompositeKey();
    for (    Object k : ((OCompositeKey)obj).getKeys()) {
      if (k instanceof String)       result.addKey(((String)k).toLowerCase());
 else       result.addKey(k);
    }
    return result;
  }
  return obj;
}
