{
  final Member member;
  if (iKey == null)   member=((OHazelcastPlugin)iManager).getHazelcastInstance().getCluster().getLocalMember();
 else   member=((OHazelcastPlugin)iManager).getHazelcastInstance().getPartitionService().getPartition(iKey).getOwner();
  return ((OHazelcastPlugin)iManager).getNodeId(member);
}
