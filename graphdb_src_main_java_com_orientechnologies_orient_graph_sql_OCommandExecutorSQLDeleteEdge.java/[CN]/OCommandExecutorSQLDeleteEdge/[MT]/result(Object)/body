{
  final OIdentifiable id=(OIdentifiable)iRecord;
  if (compiledFilter != null) {
    if (!(Boolean)compiledFilter.evaluate((ODocument)id.getRecord(),null,context))     return true;
  }
  if (id.getIdentity().isValid()) {
    final OrientEdge e=graph.getEdge(id);
    if (e != null) {
      e.remove();
      removed++;
      if (!txAlreadyBegun && batch > 0 && removed % batch == 0) {
        graph.commit();
        graph.begin();
      }
    }
  }
  return true;
}
