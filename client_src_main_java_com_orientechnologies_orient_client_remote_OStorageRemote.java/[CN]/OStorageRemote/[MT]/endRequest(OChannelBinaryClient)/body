{
  if (iNetwork == null)   return;
  try {
    iNetwork.flush();
  }
 catch (  IOException e) {
    try {
      iNetwork.close();
    }
 catch (    Exception e2) {
    }
 finally {
      networkPoolLock.lock();
      try {
        networkPool.remove(iNetwork);
      }
  finally {
        networkPoolLock.unlock();
      }
    }
    throw e;
  }
 finally {
    iNetwork.releaseWriteLock();
    networkPoolLock.lock();
    try {
synchronized (networkPool) {
        networkPool.notifyAll();
      }
    }
  finally {
      networkPoolLock.unlock();
    }
  }
}
