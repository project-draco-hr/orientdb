{
  OGlobalConfiguration.CACHE_LOCAL_ENABLED.setValue(false);
  OGlobalConfiguration.CACHE_LOCAL_SIZE.setValue(0);
  OServer server=OServerMain.create();
  server.startup(RemoteDBRunner.class.getResourceAsStream("/com/orientechnologies/orient/core/storage/impl/local/paginated/db-mix-config.xml"));
  server.activate();
  while (true)   ;
}
