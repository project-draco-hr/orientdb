{
  int id=(int)(startNum + data.getCyclesDone());
  record.reset();
  record=record.value("{ 'id' : " + id + " , 'name' : 'Gipsy' , 'type' : 'Cat' , 'race' : 'European' , 'country' : 'Italy' , 'price' :"+ (data.getCyclesDone() + 300)+ ".00");
  record.save("Animal");
  database.getDictionary().put("doc-" + id,record);
}
