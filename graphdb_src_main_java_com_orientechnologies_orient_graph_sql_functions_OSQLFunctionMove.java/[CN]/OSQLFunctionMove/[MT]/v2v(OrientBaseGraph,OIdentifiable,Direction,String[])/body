{
  final ODocument rec=iRecord.getRecord();
  if (ODocumentInternal.getImmutableSchemaClass(rec) != null)   if (ODocumentInternal.getImmutableSchemaClass(rec).isSubClassOf(OrientVertexType.CLASS_NAME)) {
    final OrientVertex vertex=graph.getVertex(rec);
    if (vertex != null)     return vertex.getVertices(iDirection,iLabels);
  }
  return null;
}
