{
  ODistributedRequest req=nextMessage();
  if (manager.isOffline())   waitNodeIsOnline();
  while (distributed.waitForMessageId.get() > -1) {
    if (req != null) {
      if (req.getId() > distributed.waitForMessageId.get()) {
        ODistributedServerLog.info(this,manager.getLocalNodeName(),req.getSenderNodeName(),DIRECTION.IN,"Reached waited request %d on request=%s sourceNode=%s",distributed.waitForMessageId.get(),req,req.getSenderNodeName());
        distributed.waitForMessageId.set(-1);
        break;
      }
 else {
        ODistributedServerLog.info(this,manager.getLocalNodeName(),req.getSenderNodeName(),DIRECTION.IN,"Discarded request %d because waiting for %d request=%s sourceNode=%s",req.getId(),distributed.waitForMessageId.get(),req,req.getSenderNodeName());
        sendResponseBack(req,req.getTask(),new ODiscardedResponse());
        req=nextMessage();
      }
    }
  }
  if (ODistributedServerLog.isDebugEnabled())   ODistributedServerLog.debug(this,manager.getLocalNodeName(),req.getSenderNodeName(),DIRECTION.IN,"processing request=%s sourceNode=%s",req,req.getSenderNodeName());
  return req;
}
