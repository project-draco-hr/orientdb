{
  List<ORecordId> values=map.get(iKey);
  if (values == null)   values=new ArrayList<ORecordId>();
  if (values.size() > 0)   throw new OIndexException("Found duplicated key '" + iKey + "' on unique index defined in property: "+ owner);
  values.add(iSingleValue);
  map.put(iKey.toString(),values);
}
