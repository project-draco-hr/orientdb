{
  if (limit > -1 && browsedRecords >= limit)   return null;
  if (!nextPosition(iMovement))   return null;
  if (iRecord != null) {
    iRecord.setIdentity(new ORecordId(current.clusterId,current.clusterPosition));
    iRecord=lowLevelDatabase.load(iRecord,fetchPlan);
  }
 else   iRecord=lowLevelDatabase.load(current,fetchPlan);
  if (iRecord != null)   browsedRecords++;
  return iRecord;
}
