{
  OServerUserConfiguration userCfg=null;
synchronized (_UsersMap) {
    if (username != null) {
      String checkName=username;
      if (!isCaseSensitive())       checkName=username.toLowerCase();
      if (_UsersMap.containsKey(checkName)) {
        userCfg=_UsersMap.get(checkName);
      }
    }
  }
  return userCfg;
}
