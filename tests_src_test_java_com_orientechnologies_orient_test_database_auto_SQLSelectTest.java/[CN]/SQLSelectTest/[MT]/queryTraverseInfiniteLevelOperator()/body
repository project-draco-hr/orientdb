{
  List<ODocument> result=database.command(new OSQLSynchQuery<ODocument>("select from Profile where any() traverse(0,-1) ( any().indexOf('Navona') > -1 )")).execute();
  Assert.assertTrue(result.size() > 0);
  for (int i=0; i < result.size(); ++i) {
    record=result.get(i);
    Assert.assertTrue(record.getClassName().equalsIgnoreCase("Profile"));
  }
}
