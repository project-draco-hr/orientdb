{
  if (iThis == null || iParams[0] == null) {
    return null;
  }
  if (iParams.length > 1) {
    return iThis.toString().substring(Integer.parseInt(iParams[0].toString()),Integer.parseInt(iParams[1].toString()));
  }
 else {
    return iThis.toString().substring(Integer.parseInt(iParams[0].toString()));
  }
}
