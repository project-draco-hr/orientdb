{
  int records=0;
  int pos=textUpperCase.indexOf(KEYWORD_INTO);
  if (pos == -1)   throw new OCommandSQLParsingException("Keyword " + KEYWORD_INTO + " not found",text,0);
  pos+=KEYWORD_INTO.length() + 1;
  StringBuilder word=new StringBuilder();
  pos=OSQLHelper.nextWord(text,textUpperCase,pos,word,true);
  if (pos == -1)   throw new OCommandSQLParsingException("Invalid cluster/class name",text,pos);
  return records;
}
