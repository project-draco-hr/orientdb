{
  if (iFuncParams[0] == null || iFuncParams[1] == null)   return null;
  final Object value=iFuncParams[0];
  if (iFuncParams.length > 2)   return value.toString().substring(Integer.parseInt(iFuncParams[1].toString()),Integer.parseInt(iFuncParams[2].toString()));
 else   return value.toString().substring(Integer.parseInt(iFuncParams[1].toString()));
}
