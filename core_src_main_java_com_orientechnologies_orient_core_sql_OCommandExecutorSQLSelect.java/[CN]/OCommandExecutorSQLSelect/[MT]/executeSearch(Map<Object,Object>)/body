{
  assignTarget(iArgs);
  if (target == null) {
    if (let != null)     assignLetClauses(lastRecord != null ? lastRecord.getRecord() : null);
    return;
  }
  final long startFetching=System.currentTimeMillis();
  try {
    while (target.hasNext())     if (!executeSearchRecord(target.next()))     break;
  }
  finally {
    context.setVariable("fetchingFromTargetElapsed",(System.currentTimeMillis() - startFetching));
  }
}
