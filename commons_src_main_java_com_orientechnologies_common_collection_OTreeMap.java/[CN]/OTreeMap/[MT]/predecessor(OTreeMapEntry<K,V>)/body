{
  if (t == null)   return null;
 else   if (t.getLeft() != null) {
    OTreeMapEntry<K,V> p=t.getLeft();
    while (p.getRight() != null)     p=p.getRight();
    return p;
  }
 else {
    OTreeMapEntry<K,V> p=t.getParent();
    Entry<K,V> ch=t;
    while (p != null && ch == p.getLeft()) {
      ch=p;
      p=p.getParent();
    }
    return p;
  }
}
