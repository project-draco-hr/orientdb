{
  final int num=files.length - 1;
  final OFile file=OFileFactory.instance().create(type,config.getLocation() + "/" + name+ "."+ num+ fileExtension,storage.getMode());
  file.setMaxSize(fileMaxSize);
  file.create(fileStartSize);
  files[num]=file;
  addInfoFileConfigEntry(file);
  return file;
}
