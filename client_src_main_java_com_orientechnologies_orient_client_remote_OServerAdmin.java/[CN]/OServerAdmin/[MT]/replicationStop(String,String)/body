{
  sendRequest(OChannelBinaryProtocol.REQUEST_REPLICATION,new ODocument().field("operation","stop").field("node",iRemoteServer).field("db",iDatabaseName),"Stop replication");
  OLogManager.instance().debug(this,"Stopped replication of database '%s' from server '%s' to '%s'",iDatabaseName,storage.getURL(),iRemoteServer);
  return this;
}
