{
  ODatabaseDocumentTx db=new ODatabaseDocumentTx(url);
  db.open("admin","admin");
  ODocument doc=new ODocument(db,"Account");
  doc.field("version",0);
  doc.save();
  doc.setDirty();
  doc.setVersion(1);
  try {
    doc.save();
    Assert.assertTrue(false);
  }
 catch (  OConcurrentModificationException e) {
    Assert.assertTrue(true);
  }
  db.close();
}
