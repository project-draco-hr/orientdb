{
  if (txEntries != null) {
    OTransactionEntry<?> entry;
    for (int i=currentTxEntryPosition + 1; i < txEntries.size(); ++i) {
      entry=txEntries.get(i);
      if (entry.status == OTransactionEntry.CREATED) {
        currentTxEntryPosition=i;
        return true;
      }
    }
  }
  return false;
}
