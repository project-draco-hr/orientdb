{
  if (!(groupedResult != null && projections.entrySet().size() == 1))   return false;
  final Object projection=projections.values().iterator().next();
  if (!(projection instanceof OSQLFunctionRuntime))   return false;
  final OSQLFunctionRuntime f=(OSQLFunctionRuntime)projection;
  if (!f.getRoot().equals(OSQLFunctionCount.NAME))   return false;
  return (f.configuredParameters == null || f.configuredParameters.length == 0) || (f.configuredParameters.length == 1 && f.configuredParameters[0].equals("*"));
}
