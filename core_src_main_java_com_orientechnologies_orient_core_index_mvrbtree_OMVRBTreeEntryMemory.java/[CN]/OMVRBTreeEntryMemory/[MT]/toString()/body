{
  if (keys == null)   return "?";
  final StringBuilder buffer=new StringBuilder(256);
  final Object k=tree.pageIndex >= size ? '?' : getKey();
  buffer.append(k);
  buffer.append(" (size=");
  buffer.append(size);
  if (size > 0) {
    buffer.append(" [");
    buffer.append(keys[0] != null ? keys[0] : "{lazy}");
    buffer.append('-');
    buffer.append(keys[size - 1] != null ? keys[size - 1] : "{lazy}");
    buffer.append(']');
  }
  buffer.append(')');
  return buffer.toString();
}
