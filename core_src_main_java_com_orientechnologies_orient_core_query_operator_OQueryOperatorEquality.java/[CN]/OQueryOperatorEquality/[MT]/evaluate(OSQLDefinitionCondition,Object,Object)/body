{
  if (iLeft instanceof OSQLRuntimeValueMulti) {
    OSQLRuntimeValueMulti left=(OSQLRuntimeValueMulti)iLeft;
    if (left.values.length == 0)     return false;
    if (left.getDefinition().getName().equals(OSQLDefinitionItemFieldAll.NAME)) {
      for (      Object v : left.values)       if (v == null || !evaluateExpression(iCondition,v,iRight))       return false;
      return true;
    }
 else {
      for (      Object v : left.values)       if (v != null && evaluateExpression(iCondition,v,iRight))       return true;
      return false;
    }
  }
 else   if (iRight instanceof OSQLRuntimeValueMulti) {
    OSQLRuntimeValueMulti right=(OSQLRuntimeValueMulti)iRight;
    if (right.values.length == 0)     return false;
    if (right.getDefinition().getName().equals(OSQLDefinitionItemFieldAll.NAME)) {
      for (      Object v : right.values)       if (v == null || !evaluateExpression(iCondition,iLeft,v))       return false;
      return true;
    }
 else {
      for (      Object v : right.values)       if (v != null && evaluateExpression(iCondition,iLeft,v))       return true;
      return false;
    }
  }
 else   return evaluateExpression(iCondition,iLeft,iRight);
}
