{
  this(OGlobalConfiguration.WAL_CACHE_SIZE.getValueAsInteger(),OGlobalConfiguration.WAL_COMMIT_TIMEOUT.getValueAsInteger(),OGlobalConfiguration.WAL_MAX_SEGMENT_SIZE.getValueAsInteger() * ONE_KB * ONE_KB,OGlobalConfiguration.WAL_MAX_SIZE.getValueAsInteger() * ONE_KB * ONE_KB,storage.getName(),calculateWalPath(storage));
}
