{
  super.extract(iReader);
  jsonReader=new OJSONReader(reader);
  try {
    char first=jsonReader.nextChar();
    if (first == '[') {
      collection=true;
    }
 else     if (first == '{') {
      collection=false;
    }
 else     throw new OExtractorException("json: found unexpected character '" + first + "' at the beginning of input");
  }
 catch (  Exception e) {
    throw new OExtractorException(e);
  }
}
