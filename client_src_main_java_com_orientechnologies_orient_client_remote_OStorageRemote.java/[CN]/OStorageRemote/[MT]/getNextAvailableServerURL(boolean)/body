{
  String url=null;
switch (connectionStrategy) {
case STICKY:
    url=getServerURL();
  if (url == null)   url=getServerURFromList(false);
break;
case ROUND_ROBIN_CONNECT:
if (!iIsConnectOperation) url=getServerURL();
if (url == null) url=getServerURFromList(iIsConnectOperation);
break;
case ROUND_ROBIN_REQUEST:
url=getServerURFromList(true);
break;
default :
throw new OConfigurationException("Connection mode " + connectionStrategy + " is not supported");
}
return url;
}
