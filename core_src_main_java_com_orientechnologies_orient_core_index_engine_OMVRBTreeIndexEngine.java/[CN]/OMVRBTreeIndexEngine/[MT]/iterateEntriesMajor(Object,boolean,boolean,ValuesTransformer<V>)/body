{
  acquireExclusiveLock();
  try {
    if (ascSortOrder)     return new OMBRBTreeIndexCursor(map.tailMap(fromKey,isInclusive).entrySet().iterator(),transformer);
    return new OMBRBTreeIndexCursor(map.tailMap(fromKey,isInclusive).descendingMap().entrySet().iterator(),transformer);
  }
  finally {
    releaseExclusiveLock();
  }
}
