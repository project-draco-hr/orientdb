{
  if (iRight == right)   return this;
  right=(OTreeMapEntryPersistent<K,V>)iRight;
  markDirty();
  rightRid=iRight == null ? ORecordId.EMPTY_RECORD_ID : right.record.getIdentity();
  if (iRight != null && iRight.getParent() != this)   iRight.setParent(this);
  checkEntryStructure();
  return right;
}
