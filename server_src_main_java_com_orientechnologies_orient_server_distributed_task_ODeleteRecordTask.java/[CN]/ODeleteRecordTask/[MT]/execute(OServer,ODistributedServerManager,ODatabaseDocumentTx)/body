{
  ODistributedServerLog.debug(this,iManager.getLocalNodeName(),null,DIRECTION.IN,"delete record %s/%s v.%s",database.getName(),rid.toString(),version.toString());
  final ORecordInternal<?> record=database.load(rid);
  if (record != null) {
    record.getRecordVersion().copyFrom(version);
    if (!version.isUntracked()) {
      record.getRecordVersion().setRollbackMode();
      record.setDirty();
      record.save();
    }
    record.delete();
    return Boolean.TRUE;
  }
  return Boolean.FALSE;
}
