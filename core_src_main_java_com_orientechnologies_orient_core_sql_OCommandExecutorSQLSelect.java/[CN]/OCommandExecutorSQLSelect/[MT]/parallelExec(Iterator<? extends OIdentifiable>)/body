{
  final OResultSet result=(OResultSet)getResultInstance();
  final ODatabaseDocumentInternal db=getDatabase();
  if (limit > -1) {
    if (result != null) {
      result.setLimit(limit);
    }
  }
  boolean res;
  if (iTarget instanceof ORecordIteratorClusters && ((ORecordIteratorClusters)iTarget).getClusterIds().length > 1)   res=execParallelWithMultipleThreads((ORecordIteratorClusters)iTarget,(ODatabaseDocumentTx)db);
 else   res=execParallelWithPool(iTarget,db);
  if (OLogManager.instance().isDebugEnabled())   OLogManager.instance().debug(this,"Parallel query '%s' completed",parserText);
  return res;
}
