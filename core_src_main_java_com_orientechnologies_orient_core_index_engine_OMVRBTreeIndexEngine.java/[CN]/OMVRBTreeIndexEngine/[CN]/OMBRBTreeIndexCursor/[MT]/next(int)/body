{
  if (!treeIterator.hasNext())   return null;
  if (valuesTransformer == null)   return (Map.Entry<Object,OIdentifiable>)treeIterator.next();
  while (!currentIterator.hasNext() && treeIterator.hasNext()) {
    final Map.Entry<Object,V> entry=treeIterator.next();
    currentKey=entry.getKey();
    currentIterator=valuesTransformer.transformFromValue(entry.getValue()).iterator();
  }
  if (!currentIterator.hasNext())   return null;
  final OIdentifiable value=currentIterator.next();
  return new Map.Entry<Object,OIdentifiable>(){
    @Override public Object getKey(){
      return currentKey;
    }
    @Override public OIdentifiable getValue(){
      return value;
    }
    @Override public OIdentifiable setValue(    OIdentifiable value){
      throw new UnsupportedOperationException("setValue");
    }
  }
;
}
