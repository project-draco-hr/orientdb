{
  ODocument polygon=new ODocument("Rectangle");
  polygon.field("coordinates",new ArrayList<Double>(){
{
      add(-45d);
      add(-30d);
      add(45d);
      add(30d);
    }
  }
);
  checkFromText(polygon,"select ST_GeomFromText('POLYGON ((-45 30, 45 30, 45 -30, -45 -30,-45 30))') as geom");
}
