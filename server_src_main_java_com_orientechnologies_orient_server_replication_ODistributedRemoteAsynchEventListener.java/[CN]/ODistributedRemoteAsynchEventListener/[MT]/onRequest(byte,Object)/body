{
  if (iRequestCode == OClusterProtocol.PUSH_LEADER_AVAILABLE_DBS) {
    try {
      ODocument updateCfg=manager.getLeader().updatePeerDatabases(nodeId,(ODocument)obj);
      manager.getReplicator().updateConfiguration(updateCfg);
    }
 catch (    Exception e) {
      OLogManager.instance().error(this,"CLUSTER <%s>: error on updating leader's database list for peer %s",e,manager.getConfig().name,nodeId);
    }
  }
 else   if (wrapped != null)   wrapped.onRequest(iRequestCode,obj);
}
