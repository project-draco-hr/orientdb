{
  int entryPosition=OIntegerSerializer.INSTANCE.deserializeFromDirectMemory(directMemory,cachePointer + index * OIntegerSerializer.INT_SIZE + POSITIONS_ARRAY_OFFSET);
  if (!isLeaf)   entryPosition+=2 * OLongSerializer.LONG_SIZE;
  return keySerializer.deserializeFromDirectMemory(directMemory,cachePointer + entryPosition);
}
