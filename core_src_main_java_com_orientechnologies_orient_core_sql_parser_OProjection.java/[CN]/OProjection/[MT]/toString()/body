{
  if (items == null) {
    return "";
  }
  boolean first=true;
  StringBuilder builder=new StringBuilder();
  for (  OProjectionItem item : items) {
    if (item.isAll()) {
      if (!first) {
        builder.append(", ");
      }
      builder.append(item.toString());
      first=false;
    }
  }
  for (  OProjectionItem item : items) {
    if (!item.isAll()) {
      if (!first) {
        builder.append(", ");
      }
      builder.append(item.toString());
      first=false;
    }
  }
  return builder.toString();
}
