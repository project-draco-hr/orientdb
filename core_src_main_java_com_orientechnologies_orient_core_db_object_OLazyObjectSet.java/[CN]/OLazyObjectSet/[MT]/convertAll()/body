{
  if (converted || !convertToRecord)   return;
  final Set<Object> copy=new HashSet<Object>(this);
  underlying.clear();
  final ODatabasePojoAbstract<TYPE> database=getDatabase();
  for (  Object e : copy) {
    if (e != null) {
      if (e instanceof ORID)       add(database.getUserObjectByRecord((ORecordInternal<?>)((ODatabaseRecord)getDatabase().getUnderlying()).load((ORID)e,fetchPlan),fetchPlan));
 else       if (e instanceof ODocument)       add(database.getUserObjectByRecord((ORecordInternal<?>)e,fetchPlan));
    }
  }
  converted=true;
}
