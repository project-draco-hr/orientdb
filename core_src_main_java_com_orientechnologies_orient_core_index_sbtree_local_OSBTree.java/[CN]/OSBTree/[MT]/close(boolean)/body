{
  acquireExclusiveLock();
  try {
    diskCache.closeFile(fileId,flush);
    if (nullPointerSupport)     diskCache.closeFile(nullBucketFileId,flush);
  }
 catch (  IOException e) {
    throw new OSBTreeException("Error during close of index " + name,e);
  }
 finally {
    releaseExclusiveLock();
  }
}
