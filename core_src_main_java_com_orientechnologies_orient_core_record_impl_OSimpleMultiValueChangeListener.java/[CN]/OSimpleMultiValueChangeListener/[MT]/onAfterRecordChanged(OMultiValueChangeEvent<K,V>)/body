{
  if (this.oDocument.getInternalStatus() != STATUS.UNMARSHALLING) {
    if (event.isChangesOwnerContent())     this.oDocument.setDirty();
 else     this.oDocument.setDirtyNoChanged();
  }
  if (!(this.oDocument._trackingChanges && this.oDocument.getIdentity().isValid()) || this.oDocument.getInternalStatus() == STATUS.UNMARSHALLING)   return;
  if (this.oDocument._fields == null)   return;
  ODocumentEntry entry=this.oDocument._fields.get(fieldName);
  if (entry == null || entry.changed)   return;
  if (entry.timeLine == null) {
    entry.timeLine=new OMultiValueChangeTimeLine<Object,Object>();
  }
  entry.timeLine.addCollectionChangeEvent((OMultiValueChangeEvent<Object,Object>)event);
}
