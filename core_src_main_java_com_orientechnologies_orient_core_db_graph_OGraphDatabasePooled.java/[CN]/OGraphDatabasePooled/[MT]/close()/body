{
  if (isClosed())   return;
  vertexBaseClass=null;
  edgeBaseClass=null;
  checkOpeness();
  try {
    rollback();
  }
 catch (  Exception e) {
    OLogManager.instance().error(this,"Error on releasing database '%s' in pool",e,getName());
  }
  try {
    ((ODatabaseRaw)underlying.getUnderlying()).callOnCloseListeners();
  }
 catch (  Exception e) {
    OLogManager.instance().error(this,"Error on releasing database '%s' in pool",e,getName());
  }
  getLevel1Cache().clear();
  final OGraphDatabasePool pool=ownerPool;
  ownerPool=null;
  pool.release(this);
}
