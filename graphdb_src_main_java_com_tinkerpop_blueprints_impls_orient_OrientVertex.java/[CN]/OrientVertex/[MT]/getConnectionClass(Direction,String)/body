{
  if (iDirection == Direction.OUT) {
    if (iFieldName.length() > CONNECTION_OUT_PREFIX.length())     return iFieldName.substring(CONNECTION_OUT_PREFIX.length());
  }
 else   if (iDirection == Direction.IN) {
    if (iFieldName.length() > CONNECTION_IN_PREFIX.length())     return iFieldName.substring(CONNECTION_IN_PREFIX.length());
  }
  return OrientEdgeType.CLASS_NAME;
}
