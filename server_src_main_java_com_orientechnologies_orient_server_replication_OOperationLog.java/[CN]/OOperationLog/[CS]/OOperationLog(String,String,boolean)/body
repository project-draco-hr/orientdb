{
  super(OReplicator.DIRECTORY_NAME + "/" + iDatabase+ "/"+ iNodeId.replace('.','_').replace(':','-')+ EXTENSION,OGlobalConfiguration.DISTRIBUTED_LOG_TYPE.getValueAsString());
  nodeId=iNodeId;
  synchEnabled=OGlobalConfiguration.DISTRIBUTED_LOG_SYNCH.getValueAsBoolean();
  logger.setNode(nodeId);
  logger.setDatabase(iDatabase);
  file.setFailCheck(false);
  if (exists()) {
    if (iReset) {
      delete();
      create(DEF_START_SIZE);
    }
 else     open();
  }
 else   create(DEF_START_SIZE);
  serial=getLastOperationId() + 1;
}
