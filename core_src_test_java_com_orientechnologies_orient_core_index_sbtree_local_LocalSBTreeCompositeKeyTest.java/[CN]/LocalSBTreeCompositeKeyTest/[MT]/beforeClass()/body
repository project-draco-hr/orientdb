{
  buildDirectory=System.getProperty("buildDirectory");
  if (buildDirectory == null)   buildDirectory=".";
  databaseDocumentTx=new ODatabaseDocumentTx("local:" + buildDirectory + "/localSBTreeCompositeKeyTest");
  if (databaseDocumentTx.exists()) {
    databaseDocumentTx.open("admin","admin");
    databaseDocumentTx.drop();
  }
  databaseDocumentTx.create();
  localSBTree=new OLocalSBTree<OCompositeKey>(".sbt",2);
  localSBTree.create("localSBTreeCompositeKeyTest",OCompositeKeySerializer.INSTANCE,(OStorageLocal)databaseDocumentTx.getStorage());
}
