{
  checkServerAccess("server.dblist");
  final ODocument result=new ODocument();
  result.field("databases",server.getAvailableStorageNames());
  setDataCommandInfo("List databases");
  beginResponse();
  try {
    sendOk(clientTxId);
    channel.writeBytes(result.toStream());
  }
  finally {
    endResponse();
  }
}
