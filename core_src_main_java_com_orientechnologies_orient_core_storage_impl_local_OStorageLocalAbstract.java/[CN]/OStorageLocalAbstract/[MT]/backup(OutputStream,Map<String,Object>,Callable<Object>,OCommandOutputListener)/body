{
  freeze(false);
  try {
    if (callable != null)     try {
      callable.call();
    }
 catch (    Exception e) {
      OLogManager.instance().error(this,"Error on callback invocation during backup",e);
    }
    OZIPCompressionUtil.compressDirectory(getStoragePath(),out,new String[]{".wal"},iOutput);
  }
  finally {
    release();
  }
}
