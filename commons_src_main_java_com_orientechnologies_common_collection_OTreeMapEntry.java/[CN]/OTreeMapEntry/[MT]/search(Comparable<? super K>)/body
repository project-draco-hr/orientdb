{
  tree.pageItemFound=false;
  if (size == 0)   return null;
  int comp=iKey.compareTo(getKeyAt(0));
  if (comp == 0) {
    tree.pageItemFound=true;
    tree.pageIndex=0;
    return getValueAt(tree.pageIndex);
  }
 else   if (comp < 0) {
    tree.pageIndex=0;
    return null;
  }
 else {
    comp=iKey.compareTo(getKeyAt(size - 1));
    if (comp > 0) {
      tree.pageIndex=size;
      return null;
    }
  }
  if (size < BINARY_SEARCH_THRESHOLD)   return linearSearch(iKey);
 else   return binarySearch(iKey);
}
