{
  removeAllCollectionChangeListeners();
  _fieldValues=null;
  _fieldTypes=null;
  _fieldOriginalValues=null;
  _fieldChangeListeners=null;
  _fieldCollectionChangeTimeLines=null;
  _contentChanged=false;
  _schema=null;
  fetchSchemaIfCan();
  super.fromStream(iRecordBuffer);
  if (!_lazyLoad) {
    checkForFields();
    checkForLoading();
  }
  return this;
}
