{
  if (record != null && !record.isDirty()) {
    for (    OTreeMapEntryPersistent<K,V> e : pTree.entryPoints)     if (e == this) {
      return;
    }
    if (parent != null) {
      if (parent.left == this)       parent.left=null;
 else       parent.right=null;
      parent.clear();
      parent=null;
    }
    if (left != null) {
      left.parent=null;
      left.clear();
      left=null;
    }
    if (right != null) {
      right.parent=null;
      right.clear();
      right=null;
    }
    keys=null;
    values=null;
    serializedKeys=null;
    serializedValues=null;
    record=null;
  }
}
