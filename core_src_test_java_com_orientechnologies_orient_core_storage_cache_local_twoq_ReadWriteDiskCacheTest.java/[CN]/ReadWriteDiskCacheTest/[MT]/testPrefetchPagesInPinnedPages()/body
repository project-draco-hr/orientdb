{
  final long fileId=readBuffer.addFile(fileName,writeBuffer);
  Assert.assertEquals(readBuffer.getMaxSize(),4);
  for (int i=0; i < 8; i++) {
    OCacheEntry cacheEntry=readBuffer.allocateNewPage(fileId,writeBuffer,storagePerformanceStatistic);
    cacheEntry.acquireExclusiveLock();
    try {
      byte[] userData=new byte[userDataSize];
      for (int n=0; n < userData.length; n++) {
        userData[n]=(byte)(i + 1);
      }
      final ByteBuffer buffer=cacheEntry.getCachePointer().getBuffer();
      buffer.position(OWOWCache.PAGE_PADDING + systemOffset);
      buffer.put(userData);
      setLsn(buffer,new OLogSequenceNumber(1,i));
      cacheEntry.markDirty();
    }
  finally {
      cacheEntry.releaseExclusiveLock();
      readBuffer.release(cacheEntry,writeBuffer,storagePerformanceStatistic);
    }
  }
  readBuffer.clear();
  writeBuffer.flush();
  for (int i=0; i < 8; i++) {
    byte[] userData=new byte[userDataSize];
    for (int n=0; n < userData.length; n++) {
      userData[n]=(byte)(i + 1);
    }
    assertFile(i,userData,new OLogSequenceNumber(1,i));
  }
  LRUList am=readBuffer.getAm();
  LRUList a1in=readBuffer.getA1in();
  LRUList a1out=readBuffer.getA1out();
  Assert.assertEquals(am.size(),0);
  Assert.assertEquals(a1out.size(),0);
  Assert.assertEquals(a1in.size(),0);
  for (int i=0; i < 7; i++) {
    OCacheEntry cacheEntry=readBuffer.load(fileId,i,false,writeBuffer,1,storagePerformanceStatistic);
    readBuffer.release(cacheEntry,writeBuffer,storagePerformanceStatistic);
  }
  Assert.assertEquals(am.size(),0);
  Assert.assertEquals(a1out.size(),2);
  Assert.assertEquals(a1in.size(),4);
  OCacheEntry cacheEntry=readBuffer.load(fileId,1,false,writeBuffer,1,storagePerformanceStatistic);
  readBuffer.release(cacheEntry,writeBuffer,storagePerformanceStatistic);
  Assert.assertEquals(am.size(),1);
  Assert.assertEquals(a1out.size(),2);
  Assert.assertEquals(a1in.size(),3);
  cacheEntry=readBuffer.load(fileId,4,false,writeBuffer,1,storagePerformanceStatistic);
  readBuffer.pinPage(cacheEntry);
  readBuffer.release(cacheEntry,writeBuffer,storagePerformanceStatistic);
  Assert.assertEquals(am.size(),1);
  Assert.assertEquals(a1out.size(),2);
  Assert.assertEquals(a1in.size(),2);
  readBuffer.changeMaximumAmountOfMemory(5 * (PAGE_SIZE + 2 * OWOWCache.PAGE_PADDING));
  cacheEntry=readBuffer.load(fileId,0,false,writeBuffer,4,storagePerformanceStatistic);
  readBuffer.release(cacheEntry,writeBuffer,storagePerformanceStatistic);
  Assert.assertEquals(am.size(),3);
  Assert.assertEquals(a1out.size(),2);
  Assert.assertEquals(a1in.size(),1);
  Assert.assertTrue(readBuffer.inPinnedPages(fileId,4));
  Assert.assertNotNull(am.get(fileId,1));
  Assert.assertNotNull(am.get(fileId,2));
  Assert.assertNotNull(am.get(fileId,3));
  Assert.assertNotNull(a1out.get(fileId,5));
  Assert.assertNotNull(a1out.get(fileId,6));
  Assert.assertNotNull(a1in.get(fileId,0));
  readBuffer.changeMaximumAmountOfMemory(4 * (PAGE_SIZE + 2 * OWOWCache.PAGE_PADDING));
}
