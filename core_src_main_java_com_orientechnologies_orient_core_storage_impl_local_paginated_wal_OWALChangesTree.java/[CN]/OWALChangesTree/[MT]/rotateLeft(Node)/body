{
  Node p=node;
  Node q=p.right;
  Node b=q.left;
  Node parent=node.parent;
  q.left=p;
  p.parent=q;
  q.parent=parent;
  if (parent != null) {
    if (parent.left == node)     parent.left=q;
 else     parent.right=q;
  }
  p.right=b;
  if (b != null)   b.parent=p;
  updateMaxEndAccordingToChildren(p);
}
