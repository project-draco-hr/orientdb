{
  long physicalRecordTobrowse=currentClusterPosition > -2 && lastClusterPosition > -1 ? lastClusterPosition - currentClusterPosition : 0;
  if (physicalRecordTobrowse == 0 && txEntries != null) {
    currentTxEntryPosition++;
    if (currentTxEntryPosition >= txEntries.size())     throw new NoSuchElementException();
 else     return (REC)txEntries.get(currentTxEntryPosition).getRecord();
  }
  return null;
}
