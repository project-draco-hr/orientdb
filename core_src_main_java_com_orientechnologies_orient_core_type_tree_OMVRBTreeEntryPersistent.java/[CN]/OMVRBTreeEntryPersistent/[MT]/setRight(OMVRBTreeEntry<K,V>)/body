{
  if (iRight != right) {
    right=(OMVRBTreeEntryPersistent<K,V>)iRight;
    if (right != null && !right.record.getIdentity().equals(rightRid))     markDirty();
    rightRid=iRight == null ? ORecordId.EMPTY_RECORD_ID : right.record.getIdentity();
    if (right != null && right.parent != this)     right.setParent(this);
    checkEntryStructure();
  }
  return right;
}
