{
  checkDirection(true);
  ORecordInternal<?> record;
  while (hasNext()) {
    if (currentRecord != null) {
      try {
        return (REC)currentRecord;
      }
  finally {
        currentRecord=null;
      }
    }
    record=getTransactionEntry();
    if (record != null)     return (REC)record;
  }
  return null;
}
