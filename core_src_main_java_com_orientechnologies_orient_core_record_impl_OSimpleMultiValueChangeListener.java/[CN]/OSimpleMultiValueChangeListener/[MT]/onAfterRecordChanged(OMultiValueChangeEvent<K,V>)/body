{
  if (this.oDocument.getInternalStatus() != STATUS.UNMARSHALLING) {
    if (event.isChangesOwnerContent())     this.oDocument.setDirty();
 else     this.oDocument.setDirtyNoChanged();
  }
  if (!(this.oDocument._trackingChanges && this.oDocument.getIdentity().isValid()) || this.oDocument.getInternalStatus() == STATUS.UNMARSHALLING)   return;
  if (this.oDocument._fieldOriginalValues != null && this.oDocument._fieldOriginalValues.containsKey(fieldName))   return;
  if (this.oDocument._fieldCollectionChangeTimeLines == null)   this.oDocument._fieldCollectionChangeTimeLines=new HashMap<String,OMultiValueChangeTimeLine<Object,Object>>();
  OMultiValueChangeTimeLine<Object,Object> timeLine=this.oDocument._fieldCollectionChangeTimeLines.get(fieldName);
  if (timeLine == null) {
    timeLine=new OMultiValueChangeTimeLine<Object,Object>();
    this.oDocument._fieldCollectionChangeTimeLines.put(fieldName,timeLine);
  }
  timeLine.addCollectionChangeEvent((OMultiValueChangeEvent<Object,Object>)event);
}
