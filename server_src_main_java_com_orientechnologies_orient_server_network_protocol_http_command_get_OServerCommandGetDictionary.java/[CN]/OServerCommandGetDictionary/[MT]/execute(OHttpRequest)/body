{
  iRequest.data.commandInfo="Dictionary lookup";
  String[] urlParts=checkSyntax(iRequest.url,3,"Syntax error: dictionary/<database>/<key>");
  ODatabaseDocumentTx db=null;
  try {
    db=getProfiledDatabaseInstance(iRequest,urlParts[1]);
    final ORecord<?> record=db.getDictionary().get(urlParts[2]);
    if (record == null)     throw new ORecordNotFoundException("Key '" + urlParts[2] + "' was not found in the database dictionary");
    sendRecordContent(iRequest,record);
  }
  finally {
    if (db != null)     OSharedDocumentDatabase.release(db);
  }
}
