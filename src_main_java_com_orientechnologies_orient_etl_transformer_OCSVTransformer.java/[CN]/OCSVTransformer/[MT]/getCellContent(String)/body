{
  if (iValue == null || iValue.isEmpty() || "NULL".equals(iValue))   return null;
  if (stringCharacter != null && iValue.length() > 1 && (iValue.charAt(0) == stringCharacter && iValue.charAt(iValue.length() - 1) == stringCharacter))   return iValue.substring(1,iValue.length() - 1);
  return iValue;
}
