{
  acquireWriteLock();
  try {
    final long filledUpTo=getFilledUpTo();
    if (iSize >= filledUpTo)     return;
    OLogManager.instance().debug(this,"Shrinking filled file from " + filledUpTo + " to "+ iSize+ " bytes. "+ toString());
    setFilledUpTo(iSize);
  }
  finally {
    releaseWriteLock();
  }
}
