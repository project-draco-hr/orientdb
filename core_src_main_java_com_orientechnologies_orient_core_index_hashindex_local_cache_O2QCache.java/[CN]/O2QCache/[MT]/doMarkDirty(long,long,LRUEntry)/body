{
  if (lruEntry.isDirty)   return;
  final int segment=OIntegerSerializer.INSTANCE.deserializeFromDirectMemory(directMemory,lruEntry.dataPointer + OLongSerializer.LONG_SIZE + OIntegerSerializer.INT_SIZE);
  final long position=OLongSerializer.INSTANCE.deserializeFromDirectMemory(directMemory,lruEntry.dataPointer + OLongSerializer.LONG_SIZE + 2 * OIntegerSerializer.INT_SIZE);
  dirtyPages.get(fileId).add(new DirtyPage(pageIndex,new OLogSequenceNumber(segment,position)));
  lruEntry.isDirty=true;
}
