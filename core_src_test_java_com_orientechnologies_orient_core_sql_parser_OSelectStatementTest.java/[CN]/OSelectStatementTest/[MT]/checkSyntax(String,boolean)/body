{
  OrientSql osql=getParserFor(query);
  try {
    SimpleNode result=osql.parse();
    if (!isCorrect) {
      fail();
    }
    System.out.println(query);
    System.out.println("->");
    StringBuilder builer=new StringBuilder();
    result.toString(null,builer);
    System.out.println(builer.toString());
    System.out.println("............");
    return result;
  }
 catch (  Exception e) {
    if (isCorrect) {
      e.printStackTrace();
      fail();
    }
  }
  return null;
}
