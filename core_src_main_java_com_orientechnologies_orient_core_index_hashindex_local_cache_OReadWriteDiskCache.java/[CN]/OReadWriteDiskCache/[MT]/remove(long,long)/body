{
  OCacheEntry cacheEntry=am.remove(fileId,pageIndex);
  if (cacheEntry != null) {
    if (cacheEntry.dataPointer.getUsagesCount() > 1)     throw new IllegalStateException("Record cannot be removed because it is used!");
    return cacheEntry;
  }
  cacheEntry=a1out.remove(fileId,pageIndex);
  if (cacheEntry != null) {
    return cacheEntry;
  }
  cacheEntry=a1in.remove(fileId,pageIndex);
  if (cacheEntry != null && cacheEntry.dataPointer.getUsagesCount() > 1)   throw new IllegalStateException("Record cannot be removed because it is used!");
  return cacheEntry;
}
