{
  final long timer=OProfiler.getInstance().startChrono();
  init();
  try {
    if (schemaClusterId == -1 || database.countClusterElements(OStorage.CLUSTER_INTERNAL_NAME) == 0)     return;
    indexManager.load();
    security.load();
    schema.load();
  }
  finally {
    OProfiler.getInstance().stopChrono("OMetadata.load",timer);
  }
}
