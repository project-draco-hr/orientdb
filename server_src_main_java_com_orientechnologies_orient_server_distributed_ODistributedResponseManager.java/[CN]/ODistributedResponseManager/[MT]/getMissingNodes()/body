{
  synchronousResponsesLock.lock();
  try {
    final List<String> missingNodes=new ArrayList<String>();
    for (    Map.Entry<String,OResponseData> entry : responses.entrySet())     if (entry.getValue() == NO_RESPONSE)     missingNodes.add(entry.getKey());
    return missingNodes;
  }
  finally {
    synchronousResponsesLock.unlock();
  }
}
