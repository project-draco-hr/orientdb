{
  if (targetClasses != null) {
    final OClass cls=targetClasses.keySet().iterator().next();
    if (iRecord instanceof ODocument && ((ODocument)iRecord).getSchemaClass() == null || !((ODocument)iRecord).getSchemaClass().isSubClassOf(cls))     return false;
  }
  if (rootCondition == null)   return true;
  return (Boolean)rootCondition.evaluate((ORecordSchemaAware<?>)iRecord);
}
