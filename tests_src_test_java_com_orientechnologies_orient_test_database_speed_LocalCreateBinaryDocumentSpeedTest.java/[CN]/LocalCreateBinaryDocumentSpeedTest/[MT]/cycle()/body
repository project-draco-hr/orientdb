{
  final OStorage storage=database.getStorage();
  ((OAbstractPaginatedStorage)storage).startGatheringPerformanceStatisticForCurrentThread();
  record=new ORecordBytes(database,payload);
  record.save();
  if (data.getCyclesDone() == data.getCycles() - 1)   database.commit();
  final OStoragePerformanceStatistic performanceStatistic=((OAbstractPaginatedStorage)storage).completeGatheringPerformanceStatisticForCurrentThread();
  System.out.println(performanceStatistic.toDocument().toJSON(""));
}
