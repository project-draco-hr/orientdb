{
  if (iElement != null && iElement instanceof ORecordId) {
    final ORecordInternal<?> record=ORecordFactory.newInstance(recordType);
    final ORecordId rid=(ORecordId)iElement;
    record.setDatabase(database);
    record.setIdentity(rid);
    record.load();
    return record;
  }
  return iElement;
}
