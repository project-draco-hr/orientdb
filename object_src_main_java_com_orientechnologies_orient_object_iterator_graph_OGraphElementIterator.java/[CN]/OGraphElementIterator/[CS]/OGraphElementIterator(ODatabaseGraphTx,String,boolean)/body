{
  database=iDatabase;
  className=iClassName;
  underlying=new ORecordIteratorClass<ODocument>((ODatabaseRecord)iDatabase.getUnderlying(),(ODatabaseRecordAbstract)((ODatabaseDocumentTx)iDatabase.getUnderlying()).getUnderlying(),iClassName,iPolymorphic);
