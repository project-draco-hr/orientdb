{
  final long timer=Orient.instance().getProfiler().startChrono();
  try {
    return OBinaryProtocol.string2bytes(toString(iRecord,new StringBuilder(),null,null,OSerializationThreadLocal.INSTANCE.get(),iOnlyDelta,true).toString());
  }
  finally {
    Orient.instance().getProfiler().stopChrono("ORecordSerializerStringAbstract.toStream",timer);
  }
}
