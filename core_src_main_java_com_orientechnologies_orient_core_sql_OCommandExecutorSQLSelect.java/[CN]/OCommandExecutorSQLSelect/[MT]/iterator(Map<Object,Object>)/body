{
  final Iterator<OIdentifiable> subIterator;
  if (target == null) {
    executeSearch(iArgs);
    applyExpand();
    handleNoTarget();
    handleGroupBy(context);
    applyOrderBy(true);
    subIterator=new ArrayList<OIdentifiable>((List<OIdentifiable>)getResult()).iterator();
    lastRecord=null;
    tempResult=null;
    groupedResult=null;
  }
 else {
    subIterator=(Iterator<OIdentifiable>)target;
  }
  return subIterator;
}
