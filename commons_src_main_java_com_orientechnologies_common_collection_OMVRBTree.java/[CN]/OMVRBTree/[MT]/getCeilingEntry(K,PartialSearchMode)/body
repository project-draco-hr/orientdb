{
  OMVRBTreeEntry<K,V> p=getEntry(key,true,partialSearchMode);
  if (p == null)   return null;
  if (pageItemFound)   return p;
 else   if (pageIndex < p.getSize()) {
    if (key instanceof OCompositeKey)     return adjustSearchResult((OCompositeKey)key,partialSearchMode,p);
 else     return p;
  }
  return null;
}
