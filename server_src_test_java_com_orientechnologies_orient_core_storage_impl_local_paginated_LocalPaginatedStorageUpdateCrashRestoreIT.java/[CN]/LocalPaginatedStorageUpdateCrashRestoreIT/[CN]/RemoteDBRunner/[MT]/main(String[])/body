{
  OServer server=OServerMain.create();
  server.startup(RemoteDBRunner.class.getResourceAsStream("/com/orientechnologies/orient/core/storage/impl/local/paginated/db-update-config.xml"));
  server.activate();
  final String mutexFile=System.getProperty("mutexFile");
  final RandomAccessFile mutex=new RandomAccessFile(mutexFile,"rw");
  mutex.seek(0);
  mutex.write(1);
  mutex.close();
}
