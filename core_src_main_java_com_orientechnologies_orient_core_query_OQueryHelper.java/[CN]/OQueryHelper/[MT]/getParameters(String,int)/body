{
  int openPos=iText.indexOf(OPEN_BRACE,iBeginPosition);
  if (openPos == -1)   return EMPTY_PARAMETERS;
  int closePos=iText.indexOf(CLOSED_BRACE,openPos + 1);
  if (closePos == -1)   return EMPTY_PARAMETERS;
  if (closePos - openPos == 1)   return EMPTY_PARAMETERS;
  String[] pars=iText.substring(openPos + 1,closePos).split(PARAMETER_SEPARATOR);
  for (int i=0; i < pars.length; ++i)   pars[i]=pars[i].trim();
  return pars;
}
