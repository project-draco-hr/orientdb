{
  database.open("admin","admin");
  record.reset();
  record=database.browseClass("AnimalType").last().previous();
  List<ORecordVObject> races=record.field("races");
  races.add((ORecordVObject)database.newInstance("AnimalRace").field("name","Egyptian"));
  record.setDirty();
  record.save();
  database.close();
}
