{
  if (iParameters.length == 1) {
    if (iParameters[0] instanceof Number)     sum((Number)iParameters[0]);
 else     if (OMultiValue.isMultiValue(iParameters[0]))     for (    Object n : OMultiValue.getMultiValueIterable(iParameters[0]))     sum((Number)n);
    return sum;
  }
 else {
    sum=null;
    for (int i=0; i < iParameters.length; ++i)     sum((Number)iParameters[i]);
    return sum;
  }
}
