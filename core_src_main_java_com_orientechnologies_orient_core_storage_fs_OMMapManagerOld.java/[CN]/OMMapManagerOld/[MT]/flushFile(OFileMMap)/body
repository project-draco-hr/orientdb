{
  lock.readLock().lock();
  try {
    final List<OMMapBufferEntry> entries=bufferPoolPerFile.get(iFile);
    boolean allFlushed=true;
    if (entries != null)     for (    OMMapBufferEntry entry : entries)     if (!entry.flush())     allFlushed=false;
    return allFlushed;
  }
  finally {
    lock.readLock().unlock();
  }
}
