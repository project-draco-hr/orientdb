{
  if (!ascSortOrder)   throw new IllegalStateException("Descending sort order is not supported");
  final OCommandRequest cmd;
  if (isInclusive)   cmd=formatCommand(QUERY_GET_VALUE_MINOR_EQUALS,name);
 else   cmd=formatCommand(QUERY_GET_VALUE_MINOR,name);
  return getDatabase().command(cmd).execute(toKey);
}
