{
  acquireWriteLock();
  try {
    close();
    if (osFile != null) {
      boolean deleted=OFileUtils.delete(osFile);
      int retryCount=0;
      while (!deleted) {
        deleted=OFileUtils.delete(osFile);
        retryCount++;
        if (retryCount > 10)         throw new IOException("Cannot delete file. Retry limit exceeded");
      }
    }
  }
  finally {
    releaseWriteLock();
  }
}
