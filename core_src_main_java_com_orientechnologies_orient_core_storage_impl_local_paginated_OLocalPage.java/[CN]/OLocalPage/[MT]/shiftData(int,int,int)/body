{
  if (walLog == null) {
    directMemory.copyData(pagePointer + from,pagePointer + to,len);
  }
 else {
    byte[] content=directMemory.get(pagePointer + from,len);
    walLog.logRecord(new OSetPageDataRecord(content,to,pageIndex,fileName));
    directMemory.copyData(pagePointer + from,pagePointer + to,len);
  }
}
