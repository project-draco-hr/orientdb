{
  OLogManager.instance().warn(this,"-> %s (%s mode) CONFLICT record %s (other RID=#%d:%d)...",this,iRequestType,iRecord.getIdentity(),iRecord.getIdentity().getClusterId(),iOtherClusterPosition);
  final ODocument doc=createConflictDocument(iOperation,iRecord);
  doc.field("otherClusterPos",iOtherClusterPosition);
  doc.save();
}
