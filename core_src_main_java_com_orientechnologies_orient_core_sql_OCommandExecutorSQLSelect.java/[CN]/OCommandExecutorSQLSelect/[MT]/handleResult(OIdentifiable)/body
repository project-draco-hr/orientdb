{
  if ((orderedFields.isEmpty() || fullySortedByIndex) && skip > 0) {
    lastRecord=null;
    skip--;
    return true;
  }
  lastRecord=iRecord;
  resultCount++;
  if (!addResult(lastRecord))   return false;
  if ((orderedFields.isEmpty() || fullySortedByIndex) && !isAnyFunctionAggregates() && fetchLimit > -1 && resultCount >= fetchLimit)   return false;
  return true;
}
