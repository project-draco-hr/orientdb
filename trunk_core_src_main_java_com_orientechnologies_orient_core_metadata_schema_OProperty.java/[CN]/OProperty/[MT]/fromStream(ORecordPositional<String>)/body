{
  name=iRecord.next();
  type=OType.getById(Integer.parseInt(iRecord.next()));
  offset=Integer.parseInt(iRecord.next());
  mandatory=Boolean.parseBoolean(iRecord.next());
  notNull=Boolean.parseBoolean(iRecord.next());
  min=iRecord.next();
  if (min.length() == 0)   min=null;
  max=iRecord.next();
  if (max.length() == 0)   max=null;
  if (type == OType.EMBEDDED || type == OType.LINK || type == OType.LINKSET)   linkedClass=owner.owner.getClass(iRecord.next());
 else   if (type == OType.EMBEDDEDSET || type == OType.EMBEDDEDLIST) {
    String value=iRecord.next();
    if (value != null && value.length() > 0)     linkedClass=owner.owner.getClass(value);
    value=iRecord.next();
    if (value != null && value.length() > 0)     linkedType=OType.getById(Integer.parseInt(value));
  }
}
