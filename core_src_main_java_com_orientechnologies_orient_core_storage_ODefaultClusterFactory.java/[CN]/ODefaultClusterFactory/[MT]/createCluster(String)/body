{
  if (type.equalsIgnoreCase(OClusterLocal.TYPE))   return new OClusterLocal();
 else   if (type.equalsIgnoreCase(OClusterMemory.TYPE))   return new OClusterMemoryArrayList();
 else   throw new OStorageException("Cluster type '" + type + "' is not supported. Supported types are: "+ Arrays.toString(TYPES));
}
