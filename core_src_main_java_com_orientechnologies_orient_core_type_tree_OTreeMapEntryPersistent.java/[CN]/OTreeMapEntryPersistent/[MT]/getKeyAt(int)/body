{
  if (keys[iIndex] == null)   try {
    OProfiler.getInstance().updateStatistic("OTreeMapEntryP.unserializeKey",1);
    keys[iIndex]=(K)pTree.keySerializer.fromStream(serializedKeys[iIndex]);
  }
 catch (  IOException e) {
    OLogManager.instance().error(this,"Can't lazy load the key #" + iIndex + " in tree node "+ this,e,OSerializationException.class);
  }
  return keys[iIndex];
}
