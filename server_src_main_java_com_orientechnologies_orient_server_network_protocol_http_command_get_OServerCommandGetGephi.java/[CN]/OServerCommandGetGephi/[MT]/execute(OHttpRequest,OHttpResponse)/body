{
  String[] urlParts=checkSyntax(iRequest.url,4,"Syntax error: gephi/<database>/sql/<query-text>[/<limit>][/<fetchPlan>].<br/>Limit is optional and is setted to 20 by default. Set expressely to 0 to have no limits.");
  final int limit=urlParts.length > 4 ? Integer.parseInt(urlParts[4]) : 20;
  final String fetchPlan=urlParts.length > 5 ? urlParts[5] : null;
  final String text=urlParts[3];
  iRequest.data.commandInfo="Gephi";
  iRequest.data.commandDetail=text;
  ODatabaseDocumentTx db=null;
  try {
    db=getProfiledDatabaseInstance(iRequest);
    final List<OIdentifiable> response=(List<OIdentifiable>)db.command(new OSQLSynchQuery<ORecordSchemaAware<?>>(text,limit).setFetchPlan(fetchPlan)).execute();
    sendRecordsContent(iRequest,iResponse,response,fetchPlan);
  }
  finally {
    if (db != null)     db.close();
  }
  return false;
}
