{
  if (!firstAttribute)   out.append(",");
  format(iIdentLevel,iNewLine);
  if (iName != null) {
    out.append(writeValue(iName,iFormat));
    out.append(":");
  }
  if (iFormat.contains("graph") && (iValue == null || iValue instanceof OIdentifiable) && (iName.startsWith("in_") || iName.startsWith("out_"))) {
    out.append("[1]");
  }
 else   out.append(writeValue(iValue,iFormat));
  firstAttribute=false;
  return this;
}
