{
  SpeedTestThread[] ts=new SpeedTestThread[threads];
  SpeedTestThread t;
  for (int i=0; i < threads; ++i)   try {
    t=threadClass.newInstance();
    ts[i]=t;
    t.setOwner(this);
    t.setCycles(threadCycles / threads);
    t.start();
  }
 catch (  Exception e) {
    e.printStackTrace();
    return;
  }
  for (int i=0; i < threads; ++i) {
    ts[i].join();
  }
}
