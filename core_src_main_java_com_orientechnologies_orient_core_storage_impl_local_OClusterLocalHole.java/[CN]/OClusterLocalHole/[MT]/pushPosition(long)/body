{
  final int position=getHoles() * RECORD_SIZE;
  file.allocateSpace(RECORD_SIZE);
  file.writeLong(position,iPosition);
  if (OLogManager.instance().isDebugEnabled())   OLogManager.instance().debug(this,"Pushed new hole %s/#%d -> #%d:%d",owner.getName(),position / RECORD_SIZE,owner.getId(),iPosition);
  return position;
}
