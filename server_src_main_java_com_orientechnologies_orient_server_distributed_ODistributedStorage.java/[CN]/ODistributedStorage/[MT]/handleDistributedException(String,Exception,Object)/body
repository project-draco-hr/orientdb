{
  OLogManager.instance().error(this,iMessage,e,iParams);
  final Throwable t=e.getCause();
  if (t != null) {
    if (t instanceof OException)     throw (OException)t;
 else     if (t.getCause() instanceof OException)     throw (OException)t.getCause();
  }
  throw new OStorageException(String.format(iMessage,iParams),e);
}
