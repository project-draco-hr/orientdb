{
  checkOpeness();
  stateLock.acquireReadLock();
  try {
    checkOpeness();
    final OCluster cluster=clusterMap.get(clusterName.toLowerCase());
    if (cluster == null)     throw new IllegalArgumentException("Cluster " + clusterName + " does not exist in database '"+ name+ "'");
    return cluster;
  }
  finally {
    stateLock.releaseReadLock();
  }
}
