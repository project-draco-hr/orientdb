{
  if (valueContainerAlgorithm == null)   valueContainerAlgorithm=NONE_VALUE_CONTAINER;
  if (version < 0)   version=getLastVersion();
  if (SBTREE_ALGORITHM.equals(algorithm))   return createSBTreeIndex(indexType,valueContainerAlgorithm,metadata,(OAbstractPaginatedStorage)database.getStorage().getUnderlying(),version);
  throw new OConfigurationException("Unsupported type : " + indexType);
}
