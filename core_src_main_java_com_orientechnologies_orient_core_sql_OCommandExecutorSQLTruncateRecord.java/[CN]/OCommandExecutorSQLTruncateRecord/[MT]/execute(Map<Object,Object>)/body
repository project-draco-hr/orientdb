{
  if (records.isEmpty())   throw new OCommandExecutionException("Can't execute the command because it hasn't been parsed yet");
  for (  String rec : records) {
    try {
      final ORecordId rid=new ORecordId(rec);
      database.getStorage().deleteRecord(rid,-1);
    }
 catch (    Throwable e) {
      throw new OCommandExecutionException("Error on executing command",e);
    }
  }
  return records.size();
}
