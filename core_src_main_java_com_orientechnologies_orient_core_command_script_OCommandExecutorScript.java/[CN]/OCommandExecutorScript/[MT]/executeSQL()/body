{
  ODatabaseRecordInternal db=ODatabaseRecordThreadLocal.INSTANCE.getIfDefined();
  if (db != null && !(db instanceof ODatabaseRecordTx))   db=db.getUnderlying();
  try {
    return executeSQLScript(db,parserText);
  }
 catch (  IOException e) {
    throw new OCommandExecutionException("Error on executing command: " + parserText,e);
  }
}
