{
  if (className != null) {
    return "class: " + className;
  }
  if (classNames != null) {
    return "classes: " + className;
  }
  if (alias != null) {
    return "as: " + alias;
  }
  if (minDepth != null) {
    return "mindepth: " + minDepth;
  }
  if (maxDepth != null) {
    return "maxdepth: " + maxDepth;
  }
  if (filter != null) {
    return "where: (" + filter + ")";
  }
  if (whileCondition != null) {
    return "while: (" + whileCondition + ")";
  }
  return null;
}
