{
  final long timer=OProfiler.getInstance().startChrono();
  boolean forceSucceed=false;
  for (int i=0; i < FORCE_RETRY; ++i) {
    try {
      iEntry.buffer.force();
      forceSucceed=true;
      break;
    }
 catch (    Exception e) {
      OLogManager.instance().debug(iEntry,"Can't write memory buffer to disk. Retrying (" + (i + 1) + "/"+ FORCE_RETRY+ ")...");
      try {
        System.gc();
        Thread.sleep(FORCE_DELAY);
      }
 catch (      InterruptedException e1) {
      }
    }
  }
  if (!forceSucceed)   OLogManager.instance().debug(iEntry,"Can't commit memory buffer to disk after %d retries",FORCE_RETRY);
 else   OProfiler.getInstance().updateCounter("OMMapManager.pagesCommitted",1);
  OProfiler.getInstance().stopChrono("OMMapManager.commitPages",timer);
  return forceSucceed;
}
