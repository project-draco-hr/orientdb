{
  Integer mainBucketOverflowInfo=mainBucketOverflowInfoByIndex.get(mainBucketIndex);
  if (mainBucketOverflowInfo == null) {
    if (val == 0)     return;
    mainBucketOverflowInfoByIndex.put(mainBucketIndex,val);
  }
 else {
    if (mainBucketOverflowInfo == val)     return;
    if (val == 0)     mainBucketOverflowInfoByIndex.remove(mainBucketIndex);
 else     mainBucketOverflowInfoByIndex.put(mainBucketIndex,val);
  }
}
