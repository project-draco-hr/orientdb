{
  int found;
  found=0;
  for (  Edge v : (Iterable<Edge>)graph.command(new OCommandSQL("select flatten( inE() ) from " + v1.getId())).execute())   found++;
  Assert.assertEquals(found,0);
  found=0;
  for (  Edge v : (Iterable<Edge>)graph.command(new OCommandSQL("select flatten( inE() ) from " + v2.getId())).execute())   found++;
  Assert.assertEquals(found,1);
  found=0;
  for (  Edge v : (Iterable<Edge>)graph.command(new OCommandSQL("select flatten( inE('SubEdge') ) from " + v2.getId())).execute())   found++;
  Assert.assertEquals(found,1);
  found=0;
  for (  Edge v : (Iterable<Edge>)graph.command(new OCommandSQL("select flatten( inE('dddd') ) from " + v2.getId())).execute())   found++;
  Assert.assertEquals(found,0);
  found=0;
  for (  Edge v : (Iterable<Edge>)graph.command(new OCommandSQL("select flatten( inE() ) from " + v3.getId())).execute())   found++;
  Assert.assertEquals(found,1);
}
