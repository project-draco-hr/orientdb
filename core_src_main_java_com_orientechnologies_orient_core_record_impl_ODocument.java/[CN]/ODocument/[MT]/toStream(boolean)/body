{
  STATUS prev=_status;
  _status=STATUS.MARSHALLING;
  try {
    if (ONetworkThreadLocalSerializer.getNetworkSerializer() != null)     return ONetworkThreadLocalSerializer.getNetworkSerializer().toStream(this,iOnlyDelta);
    if (_source == null)     _source=_recordFormat.toStream(this,iOnlyDelta);
  }
  finally {
    _status=prev;
  }
  invokeListenerEvent(ORecordListener.EVENT.MARSHALL);
  return _source;
}
