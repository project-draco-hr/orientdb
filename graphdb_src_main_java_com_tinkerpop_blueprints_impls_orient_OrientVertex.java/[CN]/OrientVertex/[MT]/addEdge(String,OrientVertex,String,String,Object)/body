{
  if (inVertex == null)   throw new IllegalArgumentException("destination vertex is null");
  final OrientBaseGraph graph=getGraph();
  if (graph != null)   return graph.addEdge(this,label,inVertex,iClassName,iClusterName,fields);
  return OrientGraphNoTx.addEdge(null,this,label,inVertex,iClassName,iClusterName,fields);
}
