{
  Token xsp;
  xsp=jj_scanpos;
  if (jj_3R_707()) {
    jj_scanpos=xsp;
    if (jj_3R_708()) {
      jj_scanpos=xsp;
      if (jj_3R_709()) {
        jj_scanpos=xsp;
        if (jj_3R_710())         return true;
      }
    }
  }
  if (jj_scan_token(COLON))   return true;
  if (jj_3R_106())   return true;
  while (true) {
    xsp=jj_scanpos;
    if (jj_3R_711()) {
      jj_scanpos=xsp;
      break;
    }
  }
  return false;
}
