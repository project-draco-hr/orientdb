{
  if (enabled) {
    if (iRecord.getIdentity().getClusterId() == excludedCluster)     return;
    if (!iRecord.getIdentity().isValid())     return;
    acquireExclusiveLock();
    try {
      if (entries.get(iRecord.getIdentity()) != iRecord)       entries.put(iRecord.getIdentity(),iRecord);
    }
  finally {
      releaseExclusiveLock();
    }
  }
  level2cache.updateRecord(iRecord);
}
