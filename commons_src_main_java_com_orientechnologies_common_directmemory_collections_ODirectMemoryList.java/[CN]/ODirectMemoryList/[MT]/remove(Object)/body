{
  if (size == 0)   return false;
  if (o == null) {
    for (int index=0; index < size; index++)     if (getData(elementData,index) == null) {
      doRemove(index);
      return true;
    }
  }
 else {
    for (int index=0; index < size; index++)     if (o.equals(getData(elementData,index))) {
      doRemove(index);
      return true;
    }
  }
  return false;
}
