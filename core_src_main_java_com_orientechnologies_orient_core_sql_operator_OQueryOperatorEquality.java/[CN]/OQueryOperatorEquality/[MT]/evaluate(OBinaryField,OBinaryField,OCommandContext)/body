{
  final Object left=ORecordSerializerBinary.INSTANCE.getCurrentSerializer().deserializeValue(iFirstField.bytes,iFirstField.type,null);
  final Object right=ORecordSerializerBinary.INSTANCE.getCurrentSerializer().deserializeValue(iSecondField.bytes,iFirstField.type,null);
  return evaluateExpression(null,null,left,right,iContext);
}
