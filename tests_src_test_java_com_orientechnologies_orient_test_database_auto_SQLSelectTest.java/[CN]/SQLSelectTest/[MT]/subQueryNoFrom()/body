{
  List<ODocument> result2=database.command(new OSQLSynchQuery<ODocument>("select $names let $names = (select EXPAND( addresses.city ) as city from Account where addresses.size() > 0 )")).execute();
  Assert.assertTrue(result2.size() != 0);
  Assert.assertTrue(result2.get(0).field("$names") instanceof Collection<?>);
  Assert.assertFalse(((Collection<?>)result2.get(0).field("$names")).isEmpty());
}
