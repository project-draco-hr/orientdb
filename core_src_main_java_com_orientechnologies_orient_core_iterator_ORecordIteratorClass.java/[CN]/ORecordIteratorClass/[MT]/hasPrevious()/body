{
  checkDirection(false);
  if (limit > -1 && browsedRecords >= limit)   return false;
  if (browsedRecords >= totalAvailableRecords)   return false;
  if (liveUpdated)   return currentClusterPosition > database.getStorage().getClusterDataRange(currentClusterId)[0];
  return currentClusterPosition > firstClusterPosition;
}
