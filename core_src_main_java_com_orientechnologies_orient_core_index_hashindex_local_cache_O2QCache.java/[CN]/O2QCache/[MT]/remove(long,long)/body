{
  LRUEntry lruEntry=am.remove(fileId,pageIndex);
  if (lruEntry != null) {
    if (lruEntry.usageCounter.get() > 1)     throw new RuntimeException("Record cannot be removed because it is used!");
    return lruEntry;
  }
  lruEntry=a1out.remove(fileId,pageIndex);
  if (lruEntry != null) {
    return lruEntry;
  }
  lruEntry=a1in.remove(fileId,pageIndex);
  if (lruEntry != null && lruEntry.usageCounter.get() > 1)   throw new RuntimeException("Record cannot be removed because it is used!");
  return lruEntry;
}
