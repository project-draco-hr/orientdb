{
  final OIdentifiable id=(OIdentifiable)iRecord;
  if (compiledFilter != null) {
    if (!(Boolean)compiledFilter.evaluate((ODocument)id.getRecord(),null,context))     return false;
  }
  if (id.getIdentity().isValid()) {
    return OGraphCommandExecutorSQLFactory.runInTx(new OGraphCommandExecutorSQLFactory.GraphCallBack<Boolean>(){
      @Override public Boolean call(      OrientBaseGraph graph){
        final OrientEdge e=graph.getEdge(id);
        if (e != null) {
          e.remove();
          removed++;
          return true;
        }
        return false;
      }
    }
);
  }
  return false;
}
