{
  OGlobalConfiguration.WAL_FUZZY_CHECKPOINT_INTERVAL.setValue(5);
  OGlobalConfiguration.RID_BAG_EMBEDDED_TO_SBTREEBONSAI_THRESHOLD.setValue(3);
  OGlobalConfiguration.FILE_LOCK.setValue(false);
  String buildDirectory=System.getProperty("buildDirectory",".");
  buildDirectory+="/indexCrashRestoreMultiValue";
  buildDir=new File(buildDirectory);
  if (buildDir.exists())   buildDir.delete();
  buildDir.mkdir();
  String javaExec=System.getProperty("java.home") + "/bin/java";
  System.setProperty("ORIENTDB_HOME",buildDirectory);
  ProcessBuilder processBuilder=new ProcessBuilder(javaExec,"-Xmx2048m","-classpath",System.getProperty("java.class.path"),"-DORIENTDB_HOME=" + buildDirectory,RemoteDBRunner.class.getName());
  processBuilder.inheritIO();
  process=processBuilder.start();
  Thread.sleep(5000);
}
