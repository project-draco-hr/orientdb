{
  database.open("reader","reader");
  try {
    new ODocument(database,"Profile").fields("nick","error","password","I don't know","lastAccessOn",new Date(),"registeredOn",new Date()).save();
  }
 catch (  OSecurityAccessException e) {
    Assert.assertTrue(true);
  }
catch (  ODatabaseException e) {
    Assert.assertTrue(e.getCause() instanceof OSecurityAccessException);
  }
 finally {
    database.close();
  }
}
