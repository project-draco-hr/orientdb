{
  if (debug)   OLogManager.instance().debug(this,"Reading string list. Reading string list items as int (4 bytes)...");
  final int items=in.readInt();
  if (debug)   OLogManager.instance().debug(this,"Read string list items: " + items);
  if (items < 0)   return null;
  List<String> result=new ArrayList<String>();
  for (int i=0; i < items; ++i)   result.add(readString());
  if (debug)   OLogManager.instance().debug(this,"Read string list with %d items: ",items);
  return result;
}
