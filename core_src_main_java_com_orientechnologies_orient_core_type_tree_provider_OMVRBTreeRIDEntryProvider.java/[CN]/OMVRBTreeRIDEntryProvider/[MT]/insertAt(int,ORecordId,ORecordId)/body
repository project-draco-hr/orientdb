{
  if (iIndex < size)   stream.move(getKeyPositionInStream(iIndex),getKeyPositionInStream(iIndex + 1));
  try {
    iKey.toStream(moveToIndex(iIndex));
  }
 catch (  IOException e) {
    throw new OSerializationException("Cannot serialize entryRID object: " + this,e);
  }
  size++;
  return setDirty();
}
