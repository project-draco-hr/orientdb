{
  if (!fileSegment.wasSoftlyClosedAtPreviousTime())   return false;
  if (!holeSegment.wasSoftlyClosedAtPreviousTime())   return false;
  final ODataLocal dataSegment=storage.getDataSegmentById(config.getDataSegmentId());
  if (!dataSegment.wasSoftlyClosedAtPreviousTime())   return false;
  if (!dataSegment.holeSegment.wasSoftlyClosedAtPreviousTime())   return false;
  return true;
}
