{
  final short srvProtocolVersion;
  try {
    srvProtocolVersion=iNetwork.readShort();
  }
 catch (  IOException e) {
    throw new ONetworkProtocolException("Cannot read data from remote server " + iNetwork.socket.getRemoteSocketAddress() + ": "+ e);
  }
  if (srvProtocolVersion != OChannelBinaryProtocol.CURRENT_PROTOCOL_VERSION) {
    iNetwork.close();
    throw new ONetworkProtocolException("Binary protocol is incompatible with the Server connected: client=" + OChannelBinaryProtocol.CURRENT_PROTOCOL_VERSION + ", server="+ srvProtocolVersion);
  }
}
