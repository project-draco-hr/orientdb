{
  if (status.compareAndSet(false,true)) {
    ODistributedServerLog.info(this,getLocalNodeName(),null,DIRECTION.NONE,"Publishing online status for database %s.%s...",getLocalNodeName(),databaseName);
    manager.setDatabaseStatus(getLocalNodeName(),databaseName,ODistributedServerManager.DB_STATUS.ONLINE);
  }
}
