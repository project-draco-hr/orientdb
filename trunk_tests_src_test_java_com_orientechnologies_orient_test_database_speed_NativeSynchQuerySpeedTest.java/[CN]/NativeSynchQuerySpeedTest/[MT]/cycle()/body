{
  result=database.query(new ONativeSynchQuery<ORecordDocument,OQueryContextNativeSchema<ORecordDocument>>("Animal",new OQueryContextNativeSchema<ORecordDocument>()){
    @Override public boolean filter(    OQueryContextNativeSchema<ORecordDocument> iRecord){
      return iRecord.column("race").like("Euro%").and().column("race").like("%an").and().column("id").toInt().eq(10).go();
    }
  }
).execute();
}
