{
  final ODocument rec=iRecord.getRecord();
  if (rec.getImmutableSchemaClass() != null)   if (rec.getImmutableSchemaClass().isSubClassOf(OrientEdgeType.CLASS_NAME)) {
    final OrientEdge edge=graph.getEdge(rec);
    if (edge != null) {
      final OrientVertex out=(OrientVertex)edge.getVertex(iDirection);
      return out;
    }
  }
  return null;
}
