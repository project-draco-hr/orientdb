{
  database.query(new ONativeAsynchQuery<ORecordDocument,OQueryContextNativeSchema<ORecordDocument>>("Animal",new OQueryContextNativeSchema<ORecordDocument>(),this){
    @Override public boolean filter(    OQueryContextNativeSchema<ORecordDocument> iRecord){
      return iRecord.column("id").toInt().minor(10).go();
    }
  }
).execute();
}
