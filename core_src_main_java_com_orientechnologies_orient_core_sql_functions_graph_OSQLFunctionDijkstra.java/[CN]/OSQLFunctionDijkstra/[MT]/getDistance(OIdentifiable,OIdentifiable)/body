{
  final Set<OIdentifiable> edges=db.getEdgesBetweenVertexes(node,target);
  if (!edges.isEmpty()) {
    final ODocument e=edges.iterator().next().getRecord();
    if (e != null) {
      final Object fieldValue=e.field(paramWeightFieldName);
      if (fieldValue != null)       if (fieldValue instanceof Float)       return (Float)fieldValue;
 else       if (fieldValue instanceof Number)       return ((Number)fieldValue).floatValue();
    }
  }
  return MIN;
}
