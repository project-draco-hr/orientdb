{
  ODatabaseDocumentTx db=(ODatabaseDocumentTx)OServerMain.server().getMemoryDatabases().get(dbName);
  if (db == null)   db=new ODatabaseDocumentTx(OServerMain.server().getStoragePath(dbName));
  if (db.isClosed())   if (db.getStorage() instanceof OStorageMemory)   db.create();
 else   db.open(iUser,iPassword);
  underlyingDatabase=((ODatabaseRaw)((ODatabaseComplex<?>)db.getUnderlying()).getUnderlying());
  return db;
}
