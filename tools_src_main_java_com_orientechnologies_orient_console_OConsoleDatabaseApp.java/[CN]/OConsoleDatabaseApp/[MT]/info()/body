{
  if (currentDatabaseName != null) {
    message("\nCurrent database: " + currentDatabaseName + " (url="+ currentDatabase.getURL()+ ")");
    final OStorage stg=currentDatabase.getStorage();
    if (stg instanceof OStorageRemote) {
      listServers();
    }
    listProperties();
    listClusters();
    listClasses();
    listIndexes();
  }
}
