{
  try {
    dgram=new DatagramPacket(discoveryPacket,discoveryPacket.length,manager.networkMulticastAddress,manager.networkMulticastPort);
    socket=new DatagramSocket();
  }
 catch (  Exception e) {
    OLogManager.instance().error(this,"Can't startup distributed server discovery signaler",e);
  }
  super.startup();
}
