{
  if (iObject == null)   return null;
  if (!isMultiValue(iObject))   return null;
  if (iObject instanceof Collection<?>)   return ((Collection<Object>)iObject);
  if (iObject instanceof Map<?,?>)   return ((Map<?,Object>)iObject).values();
  if (iObject.getClass().isArray())   return Arrays.asList(iObject);
  return null;
}
