{
  boolean containsKey=containsKey(iKey);
  final T oldValue=super.remove(iKey);
  if (oldValue instanceof ODocument)   ((ODocument)oldValue).removeOwner(this);
  if (containsKey)   fireCollectionChangedEvent(new OMultiValueChangeEvent<Object,T>(OMultiValueChangeEvent.OChangeType.REMOVE,iKey,null,oldValue));
  return oldValue;
}
