{
  if (iClass1 == iClass2)   return iClass1;
  if (iClass1 == Integer.class && (iClass2 == Long.class || iClass2 == Float.class || iClass2 == Double.class))   return iClass2;
 else   if (iClass1 == Long.class && (iClass2 == Float.class || iClass2 == Double.class))   return iClass2;
 else   if (iClass1 == Float.class && (iClass2 == Double.class))   return iClass2;
  return iClass1;
}
