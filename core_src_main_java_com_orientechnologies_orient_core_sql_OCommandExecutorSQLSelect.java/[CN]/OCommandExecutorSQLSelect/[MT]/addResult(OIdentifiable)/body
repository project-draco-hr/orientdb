{
  lastRecord=null;
  if (orderedFields == null && skip > 0) {
    skip--;
    return true;
  }
  lastRecord=iRecord instanceof ORecord<?> ? ((ORecord<?>)iRecord).copy() : iRecord.getIdentity().copy();
  lastRecord=applyProjections(lastRecord);
  resultCount++;
  if (lastRecord != null)   if (anyFunctionAggregates || orderedFields != null || flattenTarget != null) {
    if (tempResult == null)     tempResult=new ArrayList<OIdentifiable>();
    tempResult.add(lastRecord);
  }
 else   if (subIterator == null) {
    if (request.getResultListener() != null)     request.getResultListener().result(lastRecord);
  }
  if (orderedFields == null && fetchLimit > -1 && resultCount >= fetchLimit)   return false;
  return true;
}
