{
  final OStorageSynchronizer dbSynchronizer=getDatabaseSynchronizer(iTask.getDatabaseName());
  final long[] lastExecutedOperation=dbSynchronizer.getLog().getLastExecutedOperationId();
  ODistributedServerLog.debug(this,getLocalNodeId(),iTask.getNodeSource(),DIRECTION.IN,"checking operation if %d.%d > last %d.%d",iTask.getRunId(),iTask.getOperationSerial(),lastExecutedOperation[0],lastExecutedOperation[1]);
  if (iTask.getRunId() == lastExecutedOperation[0] && iTask.getOperationSerial() <= lastExecutedOperation[1]) {
    ODistributedServerLog.warn(this,getLocalNodeId(),iTask.getNodeSource(),DIRECTION.IN,"received operation %d.%d but it has already been executed: probably it's from an alignment? Ignore it.",iTask.getRunId(),iTask.getOperationSerial());
    return false;
  }
  return true;
}
