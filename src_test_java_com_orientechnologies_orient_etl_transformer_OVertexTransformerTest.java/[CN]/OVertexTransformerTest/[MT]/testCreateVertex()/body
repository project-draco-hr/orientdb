{
  process("{source: { content: { value: 'name,\nGregor' } }, extractor : { row: {} }," + " transformers: [{csv: {}}, {vertex: {class:'Person', skipDuplicates:false}}," + "], loader: { orientdb: { dbURL: 'memory:ETLBaseTest', dbType:'graph', useLightweightEdges:false } } }");
