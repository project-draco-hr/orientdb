{
  Set<OIdentifiable> indexedRIDs=get(iKey);
  if (indexedRIDs != null && !indexedRIDs.isEmpty()) {
    if (!indexedRIDs.contains(iRecord))     OLogManager.instance().exception("Found duplicated key '%s' previously assigned to the record %s",null,OIndexException.class,iKey,indexedRIDs.iterator().next());
  }
}
