{
  if (ioResult != null && operationsChain != null) {
    OSQLMethod operator=null;
    try {
      for (      OPair<OSQLMethod,Object[]> op : operationsChain) {
        operator=op.getKey();
        ioResult=operator.execute(iRecord,iContext,ioResult,op.getValue());
      }
    }
 catch (    ParseException e) {
      OLogManager.instance().exception("Error on conversion of value '%s' using field operator %s",e,OCommandExecutionException.class,ioResult,operator.getName());
    }
  }
  return ioResult;
}
