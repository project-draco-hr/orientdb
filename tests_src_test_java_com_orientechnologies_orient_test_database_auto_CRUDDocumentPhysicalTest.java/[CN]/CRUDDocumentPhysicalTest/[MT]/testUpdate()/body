{
  database=ODatabaseDocumentPool.global().acquire(url,"admin","admin");
  int i=0;
  for (  ODocument rec : database.browseCluster("Account")) {
    if (i % 2 == 0)     Assert.assertEquals(rec.field("location"),"Spain");
 else     Assert.assertEquals(rec.field("location"),"Italy");
    Assert.assertEquals(((Number)rec.field("price")).intValue(),i + 100);
    i++;
  }
  database.close();
}
