{
  for (int i=0; i < configuredParameters.length; ++i) {
    if (configuredParameters[i] instanceof OSQLFilterItemField)     runtimeParameters[i]=((OSQLFilterItemField)configuredParameters[i]).getValue(iRecord);
 else     if (configuredParameters[i] instanceof OSQLFunctionRuntime)     runtimeParameters[i]=((OSQLFunctionRuntime)configuredParameters[i]).execute(iRecord);
  }
  return function.execute(context,runtimeParameters);
}
