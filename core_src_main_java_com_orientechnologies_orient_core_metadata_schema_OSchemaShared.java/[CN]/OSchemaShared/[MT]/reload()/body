{
  getDatabase().getStorage().callInLock(new Callable<Void>(){
    public Void call() throws Exception {
      lock.acquireExclusiveLock();
      try {
        reload(null);
      }
  finally {
        lock.releaseExclusiveLock();
      }
      return null;
    }
  }
,false);
  return (RET)this;
}
