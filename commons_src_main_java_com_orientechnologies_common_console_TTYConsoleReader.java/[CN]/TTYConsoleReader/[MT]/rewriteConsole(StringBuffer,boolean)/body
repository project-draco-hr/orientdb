{
  outStream.print("\r");
  outStream.print("orientdb> ");
  if (currentPos < buffer.length() && buffer.length() > 0 && !cleaner) {
    outStream.print("\033[0m" + buffer.substring(0,currentPos) + "\033[0;30;47m"+ buffer.substring(currentPos,currentPos + 1)+ "\033[0m"+ buffer.substring(currentPos + 1)+ "\033[0m");
  }
 else {
    outStream.print(buffer);
  }
}
