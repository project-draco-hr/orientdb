{
  super(OGlobalConfiguration.ENVIRONMENT_CONCURRENT.getValueAsBoolean());
  this.type=type;
  this.keyHashFunction=new OMurmurHash3HashFunction<Object>();
  this.localHashTable=new OLocalHashTable<Object,T>(METADATA_CONFIGURATION_FILE_EXTENSION,TREE_STATE_FILE_EXTENSION,BUCKET_FILE_EXTENSION,storage,storage.getDiskCache(),keyHashFunction);
}
