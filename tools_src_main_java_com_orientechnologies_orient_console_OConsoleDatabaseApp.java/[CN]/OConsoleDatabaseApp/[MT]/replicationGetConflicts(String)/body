{
  checkForRemoteServer();
  try {
    final ODocument response=serverAdmin.getReplicationConflicts(iDatabaseName);
    currentResultSet=response.field("result");
    if (currentResultSet == null || currentResultSet.size() == 0)     message("\nThere are not replication conflicts for database '" + iDatabaseName + "'");
 else {
      new OTableFormatter(this).hideRID(true).setMaxWidthSize(Integer.parseInt(properties.get("width"))).writeRecords(currentResultSet,-1);
    }
    out.println();
  }
 catch (  Exception e) {
    printError(e);
  }
}
