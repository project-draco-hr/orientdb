{
  ODocument newDoc=new ODocument();
  newDoc.fromJSON("{\"gender\":{\"name\":\"Male\"},\"firstName\":\"Jack\",\"lastName\":\"Williams\"," + "\"phone\":\"561-401-3348\",\"email\":\"0586548571@example.com\",\"address\":{\"street1\":\"Smith Ave\"," + "\"street2\":null,\"city\":\"GORDONSVILLE\",\"state\":\"VA\",\"code\":\"22942\"},"+ "\"dob\":\"2011-11-17T03:17:04Z\"}");
  String json=newDoc.toJSON();
  ODocument loadedDoc=new ODocument().fromJSON(json);
  Assert.assertTrue(newDoc.hasSameContentOf(loadedDoc));
}
