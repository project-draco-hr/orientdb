{
  OCluster cluster=clusterMap.get(config.getName().toLowerCase());
  if (cluster != null) {
    cluster.configure(this,config);
    return -1;
  }
  if (config.getStatus() == OStorageClusterConfiguration.STATUS.ONLINE)   cluster=OPaginatedClusterFactory.INSTANCE.createCluster(configuration.version);
 else   cluster=new OOfflineCluster(this,config.getId(),config.getName());
  cluster.configure(this,config);
  return registerCluster(cluster);
}
