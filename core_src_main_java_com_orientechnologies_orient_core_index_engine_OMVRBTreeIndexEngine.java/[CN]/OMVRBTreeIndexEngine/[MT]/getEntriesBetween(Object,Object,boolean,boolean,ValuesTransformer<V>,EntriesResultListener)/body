{
  acquireExclusiveLock();
  try {
    if (ascOrder)     getEntriesBetweenAscOrder(rangeFrom,inclusive,rangeTo,inclusive,transformer,entriesResultListener);
 else     getEntriesBetweenDescOrder(rangeFrom,inclusive,rangeTo,inclusive,transformer,entriesResultListener);
  }
  finally {
    releaseExclusiveLock();
  }
}
