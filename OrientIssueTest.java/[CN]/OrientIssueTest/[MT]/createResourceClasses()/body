{
  boolean value=true;
  ODatabaseDocumentTx db=getConnection();
  try {
    if (!db.getMetadata().getSchema().existsClass(cacheClass)) {
      db.getMetadata().getSchema().createClass(cacheClass).setOverSize(2);
      db.getMetadata().getSchema().getClass(cacheClass).createProperty(ID,OType.STRING);
      db.getMetadata().getSchema().getClass(cacheClass).createProperty(VERSION,OType.LONG);
      db.getMetadata().getSchema().getClass(cacheClass).createProperty(GUID,OType.STRING);
      db.getMetadata().getSchema().getClass(cacheClass).createProperty(RECORD_COUNT,OType.LONG);
      db.getMetadata().getSchema().getClass(cacheClass).createProperty(TID,OType.STRING);
      db.getMetadata().getSchema().getClass(cacheClass).createIndex("idIndex" + cacheClass,INDEX_TYPE.NOTUNIQUE,ID);
      db.getMetadata().getSchema().getClass(cacheClass).createIndex("idIndexVersion" + cacheClass,INDEX_TYPE.UNIQUE,ID,VERSION);
      db.getMetadata().getSchema().save();
    }
  }
 catch (  Exception ex) {
    value=false;
  }
 finally {
    releaseConnection(db);
  }
  return value;
}
