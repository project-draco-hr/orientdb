{
  if (!(result instanceof OIdentifiable) && !(result instanceof OResultSet)) {
    if (!OMultiValue.isMultiValue(result)) {
      request.setRecordResultSet(false);
    }
 else     if (!(OMultiValue.getFirstValue(result) instanceof OIdentifiable)) {
      request.setRecordResultSet(false);
    }
  }
}
