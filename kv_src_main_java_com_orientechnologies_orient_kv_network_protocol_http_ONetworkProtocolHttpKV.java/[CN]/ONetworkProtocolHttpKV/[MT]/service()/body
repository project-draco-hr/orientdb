{
  if (request.method.equals(OHttpUtils.METHOD_GET))   doGet(request.url,request.content,request.channel);
 else   if (request.method.equals(OHttpUtils.METHOD_POST))   doPost(request.url,request.content,request.channel);
 else   if (request.method.equals(OHttpUtils.METHOD_PUT))   doPut(request.url,request.content,request.channel);
 else   if (request.method.equals(OHttpUtils.METHOD_DELETE))   doDelete(request.url,request.content,request.channel);
 else   OLogManager.instance().warn(this,"->" + channel.socket.getInetAddress().getHostAddress() + ": HTTP Method not supported: "+ request.method);
}
