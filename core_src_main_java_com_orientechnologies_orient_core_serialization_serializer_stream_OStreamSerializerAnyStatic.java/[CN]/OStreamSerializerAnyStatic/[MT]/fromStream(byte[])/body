{
  if (iStream == null || iStream.length == 0)   return null;
  try {
    return constructor.newInstance(iStream);
  }
 catch (  Exception e) {
    OLogManager.instance().error(this,"Error on unmarshalling content of class: " + constructor.getDeclaringClass(),e,OSerializationException.class);
  }
  return null;
}
