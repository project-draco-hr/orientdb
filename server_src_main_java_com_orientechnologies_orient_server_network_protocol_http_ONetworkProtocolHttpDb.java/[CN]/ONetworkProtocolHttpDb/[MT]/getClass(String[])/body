{
  commandType=OChannelBinaryProtocol.COMMAND;
  checkSyntax(iParts,3,"Syntax error: class/<database>/<class-name>[/<limit>]<br/>Limit is optional and is setted to 20 by default. Set expressely to 0 to have no limits.");
  ODatabaseDocumentTx db=null;
  try {
    db=OSharedDocumentDatabase.acquireDatabase(iParts[1]);
    if (db.getMetadata().getSchema().getClass(iParts[2]) == null)     throw new IllegalArgumentException("Invalid class '" + iParts[2] + "'");
    final int limit=iParts.length > 3 ? Integer.parseInt(iParts[3]) : 20;
    final List<ORecord<?>> response=new ArrayList<ORecord<?>>();
    for (    ORecord<?> rec : db.browseClass(iParts[2])) {
      if (limit > 0 && response.size() >= limit)       break;
      response.add(rec);
    }
    sendRecordsContent(response);
  }
  finally {
    if (db != null)     OSharedDocumentDatabase.releaseDatabase(db);
  }
}
