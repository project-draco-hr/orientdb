{
  OAtomicOperation atomicOperation=storage.getAtomicOperationsManager().getCurrentOperation();
  pinnedStateEntry=addPage(atomicOperation,fileId,diskCache);
  pinnedStateEntry.acquireExclusiveLock();
  try {
    OPaginatedClusterState paginatedClusterState=new OPaginatedClusterState(pinnedStateEntry,getChangesTree(atomicOperation,pinnedStateEntry));
    pinPage(atomicOperation,pinnedStateEntry,diskCache);
    paginatedClusterState.setSize(0);
    paginatedClusterState.setRecordsSize(0);
    for (int i=0; i < FREE_LIST_SIZE; i++)     paginatedClusterState.setFreeListPage(i,-1);
  }
  finally {
    pinnedStateEntry.releaseExclusiveLock();
    diskCache.release(pinnedStateEntry);
  }
}
