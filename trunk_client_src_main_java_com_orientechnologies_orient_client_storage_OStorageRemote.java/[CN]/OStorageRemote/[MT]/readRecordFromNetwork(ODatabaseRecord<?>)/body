{
  int classId=network.readShort();
  if (classId == OChannelBinaryProtocol.RECORD_NULL)   return null;
  return (ORecord<?>)((ORecordSchemaAware<?>)((ODatabaseRecordTx)iDatabase).newInstance()).fill(iDatabase,classId,network.readShort(),network.readLong(),network.readInt()).fromStream(network.readBytes());
}
