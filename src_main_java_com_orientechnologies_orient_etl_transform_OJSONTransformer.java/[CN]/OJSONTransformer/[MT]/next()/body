{
  if (!hasNext())   throw new NoSuchElementException("EOF");
  try {
    if (obj instanceof ODocument)     return obj;
 else     if (obj instanceof String)     return new ODocument((String)obj);
 else     throw new OTransformException("Unknown input '" + obj + "' of class '"+ obj.getClass()+ "'");
  }
  finally {
    obj=null;
  }
}
