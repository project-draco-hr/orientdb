{
  ODocument doc;
  if (input instanceof ODocument)   doc=(ODocument)input;
 else   if (input instanceof OrientVertex)   doc=((OrientVertex)input).getRecord();
 else   throw new IllegalArgumentException(getName() + " transformer: unsupported input object '" + input+ "' of class: "+ input.getClass());
  final Boolean result=(Boolean)sqlFilter.evaluate(doc,null,context);
  if (result)   return null;
  return input;
}
