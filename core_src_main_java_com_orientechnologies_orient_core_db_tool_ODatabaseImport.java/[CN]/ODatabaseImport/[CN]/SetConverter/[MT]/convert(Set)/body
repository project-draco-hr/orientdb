{
  boolean updated=false;
  final Set result;
  if (value instanceof OMVRBTreeRIDSet) {
    OMVRBTreeRIDSet ridSet=new OMVRBTreeRIDSet();
    ridSet.setAutoConvertToRecord(false);
    result=ridSet;
  }
 else   result=new HashSet();
  final ResultCallback callback=new ResultCallback(){
    @Override public void add(    Object item){
      result.add(item);
    }
  }
;
  for (  Object item : value)   updated=convertSingleValue(item,callback,updated);
  if (updated)   return result;
  return value;
}
