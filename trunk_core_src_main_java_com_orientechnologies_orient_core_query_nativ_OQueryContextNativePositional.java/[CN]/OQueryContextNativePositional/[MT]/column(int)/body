{
  if (result != null && result.booleanValue())   return this;
  T currentRecord=currentValue != null && currentValue instanceof ORecord<?> ? (T)currentValue : record;
  if (iIndex >= currentRecord.size())   throw new OQueryExecutionException("Column " + iIndex + " not exists. Columns range is: 0-"+ (currentRecord.size() - 1));
  currentValue=currentRecord.field(iIndex);
  return this;
}
