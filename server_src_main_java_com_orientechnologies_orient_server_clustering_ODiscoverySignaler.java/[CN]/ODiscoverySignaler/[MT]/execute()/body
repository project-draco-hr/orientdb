{
  if (dgram == null) {
    sendShutdown();
    return;
  }
  logger.log(this,Level.FINE,TYPE.CLUSTER,DIRECTION.OUT,"network ip multicast...");
  try {
    socket.send(dgram);
  }
 catch (  Throwable t) {
    sendShutdown();
    logger.log(this,Level.SEVERE,TYPE.CLUSTER,DIRECTION.OUT,"Error on sending signal for distributed server presence, probably the IP MULTICAST is disabled in current network configuration: %s",t.getMessage());
  }
 finally {
  }
}
