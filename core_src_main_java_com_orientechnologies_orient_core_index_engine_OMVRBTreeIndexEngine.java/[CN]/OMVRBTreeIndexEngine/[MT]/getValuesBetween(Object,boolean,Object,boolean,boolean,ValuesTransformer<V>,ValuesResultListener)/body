{
  acquireExclusiveLock();
  try {
    if (ascSortOrder)     getValuesBetweenAscOrder(rangeFrom,fromInclusive,rangeTo,toInclusive,transformer,valuesResultListener);
 else     getValuesBetweenDescOrder(rangeFrom,fromInclusive,rangeTo,toInclusive,transformer,valuesResultListener);
  }
  finally {
    releaseExclusiveLock();
  }
}
