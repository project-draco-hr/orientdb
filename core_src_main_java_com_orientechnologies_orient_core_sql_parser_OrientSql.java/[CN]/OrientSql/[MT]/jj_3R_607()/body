{
  if (jj_scan_token(GRANT))   return true;
  if (jj_3R_715())   return true;
  if (jj_scan_token(ON))   return true;
  if (jj_3R_716())   return true;
  Token xsp;
  while (true) {
    xsp=jj_scanpos;
    if (jj_3R_717()) {
      jj_scanpos=xsp;
      break;
    }
  }
  if (jj_scan_token(TO))   return true;
  if (jj_3R_100())   return true;
  return false;
}
