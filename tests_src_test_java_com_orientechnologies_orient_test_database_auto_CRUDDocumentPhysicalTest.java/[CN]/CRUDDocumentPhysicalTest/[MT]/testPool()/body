{
  final ODatabaseDocumentTx[] dbs=new ODatabaseDocumentTx[ODatabaseDocumentPool.global().getMaxSize()];
  for (int i=0; i < 10; ++i) {
    for (int db=0; db < dbs.length; ++db)     dbs[db]=ODatabaseDocumentPool.global().acquire(url,"admin","admin");
    for (int db=0; db < dbs.length; ++db)     dbs[db].close();
  }
}
