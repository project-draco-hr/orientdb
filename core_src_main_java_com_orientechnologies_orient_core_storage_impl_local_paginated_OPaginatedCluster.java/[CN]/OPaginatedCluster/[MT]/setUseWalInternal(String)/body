{
  if (!(stringValue.equals("true") || stringValue.equals("false")))   throw new OPaginatedClusterException("Invalid value for cluster attribute " + OCluster.ATTRIBUTES.USE_WAL + " was passed ["+ stringValue+ "].",this);
  config.useWal=Boolean.valueOf(stringValue);
  clusterPositionMap.setUseWal(config.useWal);
  storageLocal.getConfiguration().update();
}
