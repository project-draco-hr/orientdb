{
  try {
    storage=Orient.instance().getStorage(iURL);
    id=serialId++;
    status=STATUS.CLOSED;
    setProperty("fetch-max",50);
    useCache=OConfiguration.DB_USE_CACHE.getValueAsBoolean();
  }
 catch (  Throwable t) {
    throw new ODatabaseException("Error on opening database '" + iURL + "'",t);
  }
}
