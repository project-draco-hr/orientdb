{
  if (iParameters[0] == null)   return null;
  Object max=null;
  if (iParameters[0] instanceof Collection<?>) {
    for (    Object item : ((Collection<?>)iParameters[0])) {
      if (max == null || item != null && ((Comparable)item).compareTo(max) > 0)       max=item;
    }
  }
 else {
    max=(Comparable<Object>)iParameters[0];
  }
  if (aggregateResults()) {
    if (context == null)     context=(Comparable)max;
 else     if (context.compareTo((Comparable)max) < 0)     context=(Comparable)max;
    return null;
  }
  return max;
}
