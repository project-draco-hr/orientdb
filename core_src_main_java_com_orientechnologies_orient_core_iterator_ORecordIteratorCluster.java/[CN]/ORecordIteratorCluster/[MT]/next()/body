{
  checkDirection(true);
  if (currentTxEntryPosition > -1)   return (REC)txEntries.get(currentTxEntryPosition).getRecord();
  ORecordInternal<?> record=getRecord();
  while (hasNext()) {
    if ((record=readCurrentRecord(record,+1)) != null)     return (REC)record;
  }
  throw new NoSuchElementException();
}
