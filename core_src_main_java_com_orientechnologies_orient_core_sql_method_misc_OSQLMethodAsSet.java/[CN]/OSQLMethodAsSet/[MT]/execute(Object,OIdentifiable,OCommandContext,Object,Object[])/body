{
  if (ioResult instanceof Set)   return ioResult;
  if (ioResult == null)   return new HashSet<Object>();
  if (ioResult instanceof Collection<?>)   return new HashSet<Object>((Collection<Object>)ioResult);
 else   if (ioResult instanceof Iterable<?>)   ioResult=((Iterable<?>)ioResult).iterator();
  if (ioResult instanceof Iterator<?>) {
    final Set<Object> set=ioResult instanceof OSizeable ? new HashSet<Object>(((OSizeable)ioResult).size()) : new HashSet<Object>();
    for (Iterator<Object> iter=(Iterator<Object>)ioResult; iter.hasNext(); )     set.add(iter.next());
    return set;
  }
  final Set<Object> set=new HashSet<Object>();
  set.add(ioResult);
  return set;
}
