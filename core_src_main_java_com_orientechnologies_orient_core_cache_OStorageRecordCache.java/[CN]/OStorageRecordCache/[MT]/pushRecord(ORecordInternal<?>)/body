{
  if (maxSize == 0 || iRecord == null || iRecord.isDirty() || iRecord.getIdentity().isNew())   return;
  acquireExclusiveLock();
  try {
    final ORecord<?> record=entries.get(iRecord.getIdentity());
    if (record == null || iRecord.getVersion() > record.getVersion())     entries.put(iRecord.getIdentity(),iRecord);
  }
  finally {
    releaseExclusiveLock();
  }
}
