{
  if (iCurrent instanceof ODocument) {
    final ODocument current=(ODocument)iCurrent;
    if (iOther instanceof ORID) {
      if (!((ODocument)current).isDirty()) {
        if (!((ODocument)current).getIdentity().equals(iOther))         return false;
      }
 else {
        final ODocument otherDoc=(ODocument)current.getDatabase().load((ORID)iOther);
        if (!ODocumentHelper.hasSameContentOf(current,otherDoc))         return false;
      }
    }
 else     if (!ODocumentHelper.hasSameContentOf(current,(ODocument)iOther))     return false;
  }
 else   if (!iCurrent.equals(iOther))   return false;
  return true;
}
