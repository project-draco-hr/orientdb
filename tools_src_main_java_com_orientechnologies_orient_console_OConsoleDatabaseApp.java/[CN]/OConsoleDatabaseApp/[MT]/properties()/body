{
  message("\nPROPERTIES:");
  message("\n+---------------------+----------------------+");
  message("| %-30s| %-30s |\n","NAME","VALUE");
  message("\n+---------------------+----------------------+");
  for (  Entry<String,String> p : properties.entrySet()) {
    message("| %-30s= %-30s |\n",p.getKey(),p.getValue());
  }
  message("\n+---------------------+----------------------+");
}
