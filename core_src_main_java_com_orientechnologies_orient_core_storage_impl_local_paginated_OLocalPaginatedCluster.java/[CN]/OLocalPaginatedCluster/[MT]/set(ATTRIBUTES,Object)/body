{
  if (attribute == null)   throw new IllegalArgumentException("attribute is null");
  final String stringValue=value != null ? value.toString() : null;
  acquireExclusiveLock();
  try {
switch (attribute) {
case NAME:
      setNameInternal(stringValue);
    break;
}
}
  finally {
releaseExclusiveLock();
}
}
