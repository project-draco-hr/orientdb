{
  if (buffer != null) {
    buffer.close();
    buffer=null;
  }
  if (writeAheadLog != null) {
    writeAheadLog.delete();
    writeAheadLog=null;
  }
  storageLocal.delete();
  File file=new File(storageLocal.getConfiguration().getDirectory() + "/o2QCacheTest.0.tst");
  if (file.exists()) {
    Assert.assertTrue(file.delete());
    file.getParentFile().delete();
  }
}
