{
  if (subjectName == null)   throw new OCommandExecutionException("Can't execute the command because it hasn't been parsed yet");
  parameters=iArgs;
  Map<Object,Object> queryArgs=new HashMap<Object,Object>();
  for (int i=parameterCounter; parameters != null && i < parameters.size(); i++) {
    if (parameters.get(i) != null)     queryArgs.put(i - parameterCounter,parameters.get(i));
  }
  database.query(query,queryArgs);
  return recordCount;
}
