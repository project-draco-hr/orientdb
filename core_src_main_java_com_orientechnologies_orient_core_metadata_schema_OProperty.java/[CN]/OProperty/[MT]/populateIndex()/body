{
  Object fieldValue;
  ODocument document;
  index.clear();
  final int[] clusterIds=owner.getClusterIds();
  for (  int clusterId : clusterIds)   for (  Object record : database.browseCluster(database.getClusterNameById(clusterId))) {
    if (record instanceof ODocument) {
      document=(ODocument)record;
      fieldValue=document.field(name);
      if (fieldValue != null)       index.put(fieldValue.toString(),document);
    }
  }
}
