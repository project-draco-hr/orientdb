{
  StringBuilder result=new StringBuilder();
  if (alias != null) {
    result.append(alias);
    if (modifier != null) {
      result.append(modifier.toString());
    }
  }
 else   if (recordAttr != null) {
    result.append(recordAttr);
  }
 else   if (rid != null) {
    result.append(rid.toString());
  }
  if (type != null) {
    result.append(" " + type);
  }
  return result.toString();
}
