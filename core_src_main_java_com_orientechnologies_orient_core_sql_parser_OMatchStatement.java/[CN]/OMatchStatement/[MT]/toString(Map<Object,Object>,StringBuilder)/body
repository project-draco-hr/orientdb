{
  builder.append(KEYWORD_MATCH);
  builder.append(" ");
  boolean first=true;
  for (  OMatchExpression expr : this.matchExpressions) {
    if (!first) {
      builder.append(", ");
    }
    expr.toString(params,builder);
    first=false;
  }
  builder.append(" RETURN ");
  first=true;
  for (  OIdentifier expr : this.returnItems) {
    if (!first) {
      builder.append(", ");
    }
    expr.toString(params,builder);
    first=false;
  }
  if (limit != null) {
    limit.toString(params,builder);
  }
}
