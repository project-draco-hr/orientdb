{
  super(OGlobalConfiguration.ENVIRONMENT_CONCURRENT.getValueAsBoolean(),OGlobalConfiguration.MVRBTREE_TIMEOUT.getValueAsInteger(),true);
  databaseURL=ODatabaseRecordThreadLocal.INSTANCE.get().getURL();
  type=iType;
  watchDog=new Listener(){
    public void memoryUsageLow(    final long iFreeMemory,    final long iFreeMemoryPercentage){
      map.setOptimization(iFreeMemoryPercentage < 10 ? 2 : 1);
    }
  }
;
}
