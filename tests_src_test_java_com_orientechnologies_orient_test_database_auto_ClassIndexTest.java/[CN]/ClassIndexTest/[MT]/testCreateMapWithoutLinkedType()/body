{
  try {
    oClass.createIndex("ClassIndexMapWithoutLinkedTypeIndex",OClass.INDEX_TYPE.NOTUNIQUE,"fEmbeddedMapWithoutLinkedType by value");
    fail();
  }
 catch (  OIndexException e) {
    assertEquals(e.getMessage(),"Linked type was not provided. " + "You should provide linked type for embedded collections that are going to be indexed.");
  }
}
