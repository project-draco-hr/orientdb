{
  setDataCommandInfo("Remove cluster");
  checkDatabase();
  final int id=channel.readShort();
  boolean result=connection.database.dropCluster(connection.database.getClusterNameById(id));
  beginResponse();
  try {
    sendOk(clientTxId);
    channel.writeByte((byte)(result ? 1 : 0));
  }
  finally {
    endResponse();
  }
}
