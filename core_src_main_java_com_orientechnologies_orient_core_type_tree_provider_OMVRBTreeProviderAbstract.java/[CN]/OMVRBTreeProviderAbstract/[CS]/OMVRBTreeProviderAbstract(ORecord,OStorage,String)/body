{
  storage=iStorage;
  clusterName=iClusterName;
  if (storage != null) {
    if (clusterName != null)     clusterId=storage.getClusterIdByName(iClusterName);
 else     clusterId=storage.getClusterIdByName(OMetadataDefault.CLUSTER_INDEX_NAME);
  }
 else {
    clusterId=-1;
  }
  record=iRecord;
  ORecordInternal.setIdentity(record,new ORecordId());
  updateConfig();
}
