{
  final ODatabaseDocumentTx db=initDatabase();
  try {
    Thread.sleep(2000);
    db.getMetadata().getScheduler().removeEvent("test");
    db.getMetadata().getScheduler().removeEvent("test");
    Thread.sleep(3000);
    db.getMetadata().getScheduler().removeEvent("test");
    Long count=getLogCounter(db);
    Assert.assertTrue(count >= 1 && count <= 3);
  }
  finally {
    db.drop();
  }
}
