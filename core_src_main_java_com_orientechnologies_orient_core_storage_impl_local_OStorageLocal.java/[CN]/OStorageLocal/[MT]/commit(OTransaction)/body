{
  final boolean locked=lock.acquireSharedLock();
  try {
    txManager.commitAllPendingRecords((OTransactionRealAbstract)iTx);
    incrementVersion();
    if (OGlobalConfiguration.TX_COMMIT_SYNCH.getValueAsBoolean())     synch();
  }
 catch (  IOException e) {
    rollback(iTx);
  }
 finally {
    lock.releaseSharedLock(locked);
  }
}
