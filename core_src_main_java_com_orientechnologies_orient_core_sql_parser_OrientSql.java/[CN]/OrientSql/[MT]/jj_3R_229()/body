{
  if (jj_scan_token(ORDER))   return true;
  if (jj_scan_token(BY))   return true;
  Token xsp;
  xsp=jj_scanpos;
  if (jj_3R_302()) {
    jj_scanpos=xsp;
    if (jj_3R_303()) {
      jj_scanpos=xsp;
      if (jj_3R_304())       return true;
    }
  }
  xsp=jj_scanpos;
  if (jj_3R_305())   jj_scanpos=xsp;
  while (true) {
    xsp=jj_scanpos;
    if (jj_3R_306()) {
      jj_scanpos=xsp;
      break;
    }
  }
  return false;
}
