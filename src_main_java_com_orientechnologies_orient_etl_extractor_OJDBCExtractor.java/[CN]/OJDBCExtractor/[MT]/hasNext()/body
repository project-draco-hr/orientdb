{
  try {
    if (!didNext) {
      hasNext=rs.next();
      current++;
      didNext=true;
    }
    return hasNext;
  }
 catch (  SQLException e) {
    throw new OExtractorException(getName() + ": error on moving forward in resultset of query '" + query+ "'. Previous position was "+ current,e);
  }
}
