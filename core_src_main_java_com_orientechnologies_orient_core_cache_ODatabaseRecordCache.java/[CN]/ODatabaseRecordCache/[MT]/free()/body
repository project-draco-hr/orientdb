{
  acquireExclusiveLock();
  try {
    for (    ORecordInternal<?> record : entries.values()) {
      if (!record.isDirty() && !record.getIdentity().isNew()) {
        record.detach();
        level2cache.pushRecord(record);
      }
    }
    entries.clear();
  }
  finally {
    releaseExclusiveLock();
  }
}
