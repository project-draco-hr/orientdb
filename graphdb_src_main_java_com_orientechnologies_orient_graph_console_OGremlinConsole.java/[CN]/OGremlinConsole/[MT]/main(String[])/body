{
  try {
    boolean tty=false;
    try {
      if (setTerminalToCBreak())       tty=true;
    }
 catch (    Exception e) {
    }
    final OConsoleDatabaseApp console=new OGremlinConsole(args);
    if (tty)     console.setReader(new TTYConsoleReader());
    console.run();
  }
  finally {
    try {
      stty("echo");
    }
 catch (    Exception e) {
    }
  }
}
