{
  if (number.length() == 0)   number.append("0");
  if (state == 'C')   creates=Integer.parseInt(number.toString());
 else   if (state == 'R')   reads=Integer.parseInt(number.toString());
 else   if (state == 'U')   updates=Integer.parseInt(number.toString());
 else   if (state == 'D')   deletes=Integer.parseInt(number.toString());
  number.setLength(0);
  return c;
}
