{
  Token xsp;
  xsp=jj_scanpos;
  if (jj_3R_360()) {
    jj_scanpos=xsp;
    if (jj_3R_361()) {
      jj_scanpos=xsp;
      if (jj_3R_362())       return true;
    }
  }
  if (jj_scan_token(COLON))   return true;
  if (jj_3R_61())   return true;
  while (true) {
    xsp=jj_scanpos;
    if (jj_3R_363()) {
      jj_scanpos=xsp;
      break;
    }
  }
  return false;
}
