{
  parserNextWord(true);
  final String lockStrategy=parserGetLastWord();
  if (!lockStrategy.equalsIgnoreCase("NONE") && !lockStrategy.equalsIgnoreCase("RECORD"))   throwParsingException("Invalid " + KEYWORD_LOCK + " value set to '"+ lockStrategy+ "' but it should be NONE (default) or RECORD. Example: "+ KEYWORD_LOCK+ " RECORD");
  return lockStrategy;
}
