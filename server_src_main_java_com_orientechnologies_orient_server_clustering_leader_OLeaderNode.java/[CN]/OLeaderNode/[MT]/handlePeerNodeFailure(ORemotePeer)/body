{
  iNode.disconnect();
  logger.log(this,Level.WARNING,TYPE.CLUSTER,DIRECTION.NONE,"peer node %s seems down, retrying to connect...",iNode.getId());
  try {
    if (iNode.connect(manager.getConfig().networkTimeoutNode,manager.getConfig().name,manager.getConfig().securityKey))     return;
  }
 catch (  IOException e) {
    logger.log(this,Level.WARNING,TYPE.CLUSTER,DIRECTION.NONE,"remote server node %s is down, set it as DISCONNECTED and start to buffer changes",iNode.getId());
  }
  removePeer(iNode);
}
