{
  int found;
  found=0;
  for (  Vertex v : (Iterable<Vertex>)graph.command(new OCommandSQL("select flatten( out() ) from " + v1.getId())).execute())   found++;
  Assert.assertEquals(found,2);
  found=0;
  for (  Vertex v : (Iterable<Vertex>)graph.command(new OCommandSQL("select flatten( out('SubEdge') ) from " + v1.getId())).execute())   found++;
  Assert.assertEquals(found,1);
  found=0;
  for (  Vertex v : (Iterable<Vertex>)graph.command(new OCommandSQL("select flatten( out('dddd') ) from " + v1.getId())).execute())   found++;
  Assert.assertEquals(found,0);
  found=0;
  for (  Vertex v : (Iterable<Vertex>)graph.command(new OCommandSQL("select flatten( out() ) from " + v2.getId())).execute())   found++;
  Assert.assertEquals(found,0);
  found=0;
  for (  Vertex v : (Iterable<Vertex>)graph.command(new OCommandSQL("select flatten( out() ) from " + v3.getId())).execute())   found++;
  Assert.assertEquals(found,0);
}
