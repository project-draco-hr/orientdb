{
  setDataCommandInfo("Incremental backup");
  if (!isConnectionAlive())   return;
  final String path=channel.readString();
  String fileName=connection.getDatabase().incrementalBackup(path);
  beginResponse();
  try {
    sendOk(clientTxId);
    channel.writeString(fileName);
  }
  finally {
    endResponse();
  }
}
