{
  final List<OIdentifiable> entries=iDatabase.query(new OSQLSynchQuery<OIdentifiable>("select from " + DISTRIBUTED_CONFLICT_CLASS));
  final ODocument result=new ODocument().field("entries",entries);
  for (int i=0; i < entries.size(); ++i) {
    entries.set(i,entries.get(i).getRecord());
  }
  return result;
}
