{
  int found;
  found=0;
  for (  Vertex v : (Iterable<Vertex>)graph.command(new OCommandSQL("select flatten( in() ) from " + v1.getId())).execute())   found++;
  Assert.assertEquals(found,0);
  found=0;
  for (  Vertex v : (Iterable<Vertex>)graph.command(new OCommandSQL("select flatten( in() ) from " + v2.getId())).execute())   found++;
  Assert.assertEquals(found,1);
  found=0;
  for (  Vertex v : (Iterable<Vertex>)graph.command(new OCommandSQL("select flatten( in('SubEdge') ) from " + v2.getId())).execute())   found++;
  Assert.assertEquals(found,1);
  found=0;
  for (  Vertex v : (Iterable<Vertex>)graph.command(new OCommandSQL("select flatten( in('dddd') ) from " + v2.getId())).execute())   found++;
  Assert.assertEquals(found,0);
  found=0;
  for (  Vertex v : (Iterable<Vertex>)graph.command(new OCommandSQL("select flatten( in() ) from " + v3.getId())).execute())   found++;
  Assert.assertEquals(found,1);
}
