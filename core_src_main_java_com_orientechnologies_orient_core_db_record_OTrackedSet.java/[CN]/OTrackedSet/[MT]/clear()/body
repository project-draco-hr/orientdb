{
  final Set<T> origValues;
  if (changeListeners.isEmpty())   origValues=null;
 else   origValues=new HashSet<T>(this);
  super.clear();
  if (origValues != null) {
    for (    final T item : origValues)     fireCollectionChangedEvent(new OMultiValueChangeEvent<T,T>(OMultiValueChangeEvent.OChangeType.REMOVE,item,null,item));
  }
 else   setDirty();
}
