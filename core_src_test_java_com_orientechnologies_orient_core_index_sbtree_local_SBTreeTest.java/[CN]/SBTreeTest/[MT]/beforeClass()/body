{
  buildDirectory=System.getProperty("buildDirectory");
  if (buildDirectory == null)   buildDirectory=".";
  databaseDocumentTx=new ODatabaseDocumentTx("plocal:" + buildDirectory + "/localSBTreeTest");
  if (databaseDocumentTx.exists()) {
    databaseDocumentTx.open("admin","admin");
    databaseDocumentTx.drop();
  }
  databaseDocumentTx.create();
  sbTree=new OSBTree<Integer,OIdentifiable>(".sbt",false,".nbt");
  sbTree.create("sbTree",OIntegerSerializer.INSTANCE,OLinkSerializer.INSTANCE,null,(OAbstractPaginatedStorage)databaseDocumentTx.getStorage().getUnderlying(),1,false);
}
