{
  try {
    ODocument doc=new ODocument("MyTestClass");
    doc.save();
    Assert.fail();
  }
 catch (  OValidationException e) {
  }
  database.set(ODatabase.ATTRIBUTES.VALIDATION,false);
  ODocument doc=new ODocument("MyTestClass");
  doc.save();
  doc.delete();
  database.set(ODatabase.ATTRIBUTES.VALIDATION,true);
}
