{
  checkOpeness();
  final OCluster cluster=getClusterById(iRid.clusterId);
  if (txManager.isCommitting())   iRid.clusterPosition=txManager.createRecord(txManager.getCurrentTransaction().getId(),cluster,iRid,iContent,iRecordType);
 else   iRid.clusterPosition=createRecord(cluster,iContent,iRecordType);
  return iRid.clusterPosition;
}
