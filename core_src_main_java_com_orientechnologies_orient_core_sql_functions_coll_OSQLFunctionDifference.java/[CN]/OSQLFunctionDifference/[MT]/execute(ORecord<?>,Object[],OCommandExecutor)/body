{
  if (iParameters[0] == null)   return null;
  Object value=iParameters[0];
  if (iParameters.length == 1) {
    if (context == null) {
      context=new HashSet<Object>();
      if (value instanceof Collection<?>)       context.addAll((Collection<?>)value);
 else       context.add(value);
    }
 else {
      if (value instanceof Collection<?>)       context.removeAll((Collection<?>)value);
 else       context.remove(value);
    }
    return null;
  }
 else {
    if (!(value instanceof Collection<?>))     return null;
    final HashSet<Object> result=new HashSet<Object>((Collection<?>)value);
    for (int i=1; i < iParameters.length; ++i) {
      value=iParameters[i];
      result.removeAll((Collection<?>)value);
    }
    return result;
  }
}
