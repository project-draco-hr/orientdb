{
  final PoolData data=poolData.get();
  if (data.acquireCount == 0)   return;
  data.acquireCount--;
  if (data.acquireCount > 0)   return;
  ConcurrentLinkedQueue<DatabaseDocumentTxPolled> q=queue;
  queue=null;
  super.close();
  data.acquiredDatabase=null;
  q.offer(this);
  acquiredConnections.decrementAndGet();
}
