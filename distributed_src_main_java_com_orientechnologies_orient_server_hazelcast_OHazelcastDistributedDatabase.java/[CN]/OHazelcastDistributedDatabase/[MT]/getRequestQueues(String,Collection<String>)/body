{
  final IQueue<ODistributedRequest>[] queues=new IQueue[nodes.size()];
  int i=0;
  for (  String node : nodes) {
    final IQueue<ODistributedRequest> queue=msgService.getQueue(OHazelcastDistributedMessageService.getRequestQueueName(node,iDatabaseName));
    queues[i++]=queue;
  }
  return queues;
}
