{
  if (iRequest.executor.getAccount() == null) {
    if (iRequest.authorization == null) {
      sendTextContent(iRequest,OHttpUtils.STATUS_AUTH_CODE,OHttpUtils.STATUS_AUTH_DESCRIPTION,"WWW-Authenticate: Basic realm=\"Secure Area\"",OHttpUtils.CONTENT_TEXT_PLAIN,"401 Unauthorized.");
      return false;
    }
 else {
      String[] credentials=iRequest.authorization.split(":");
      iRequest.sessionId=OHttpSessionManager.getInstance().createSession();
    }
  }
  return true;
}
