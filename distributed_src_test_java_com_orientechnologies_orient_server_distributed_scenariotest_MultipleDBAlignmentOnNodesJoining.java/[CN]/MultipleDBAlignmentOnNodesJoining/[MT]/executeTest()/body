  }
 catch (  Exception e) {
    e.printStackTrace();
    fail();
  }
 finally {
    if (!dbServer3.isClosed()) {
      ODatabaseRecordThreadLocal.INSTANCE.set(dbServer3);
      dbServer3.close();
      ODatabaseRecordThreadLocal.INSTANCE.set(null);
    }
  }
}
