{
  final OScenarioThreadLocal.RUN_MODE currentRunningMode=OScenarioThreadLocal.INSTANCE.get();
  if (currentRunningMode != OScenarioThreadLocal.RUN_MODE.DEFAULT)   OScenarioThreadLocal.INSTANCE.set(OScenarioThreadLocal.RUN_MODE.DEFAULT);
  try {
    return iCall.call();
  }
  finally {
    if (currentRunningMode != OScenarioThreadLocal.RUN_MODE.DEFAULT)     OScenarioThreadLocal.INSTANCE.set(currentRunningMode);
  }
}
