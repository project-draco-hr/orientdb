{
  init();
  final OrientBaseGraph g;
  if (transactional)   g=factory.getTx();
 else   g=factory.getNoTx();
  if (conflictStrategy != null) {
    final OStorage stg=g.getRawGraph().getStorage().getUnderlying();
    if (stg instanceof OAbstractPaginatedStorage)     stg.setConflictStrategy(conflictStrategy);
  }
  return g;
}
