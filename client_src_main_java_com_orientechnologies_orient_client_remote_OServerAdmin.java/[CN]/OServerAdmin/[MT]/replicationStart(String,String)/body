{
  sendRequest(OChannelBinaryProtocol.REQUEST_REPLICATION,new ODocument().field("operation","start").field("node",iRemoteServer).field("db",iDatabaseName),"Start replication");
  OLogManager.instance().debug(this,"Started replication of database '%s' from server '%s' to '%s'",iDatabaseName,storage.getURL(),iRemoteServer);
  return this;
}
