{
  acquireExclusiveLock();
  try {
    final Number removed=getDatabase().<OCommandRequest>command(new OSQLSynchQuery<ODocument>("delete from OUser where name = '" + iUserName + "'")).execute();
    return removed != null && removed.intValue() > 0;
  }
  finally {
    releaseExclusiveLock();
  }
}
