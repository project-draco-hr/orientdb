{
  StringBuilder result=new StringBuilder();
  result.append(varName.toString());
  result.append(" = ");
  if (expression != null) {
    result.append(expression.toString());
  }
 else   if (query != null) {
    result.append("(");
    result.append(query.toString());
    result.append(")");
  }
  return result.toString();
}
