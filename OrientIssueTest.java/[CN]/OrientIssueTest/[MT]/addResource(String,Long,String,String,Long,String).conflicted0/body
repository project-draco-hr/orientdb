{
  ODatabaseDocumentTx db=getConnection();
  try {
    db.begin();
    ODocument doc=db.newInstance(cacheClass);
    doc.field(ID,id);
    doc.field(VERSION,version);
    doc.field("value",value);
    doc.field(TID,tid);
    doc.field(GUID,guid);
    doc.field(RECORD_COUNT,count);
    db.save(doc,cacheClass);
    db.commit();
  }
  finally {
    releaseConnection(db);
  }
}
