{
  try {
    final List<ORemotePeer> nodeList=leader.getPeerNodeList();
    if (nodeList == null || nodeList.size() == 0)     return;
    for (    ORemotePeer node : nodeList) {
      if (!node.sendHeartBeat(leader.getManager().getConfig().networkTimeoutLeader))       leader.handlePeerNodeFailure(node);
    }
  }
 catch (  Throwable e) {
    e.printStackTrace();
  }
}
