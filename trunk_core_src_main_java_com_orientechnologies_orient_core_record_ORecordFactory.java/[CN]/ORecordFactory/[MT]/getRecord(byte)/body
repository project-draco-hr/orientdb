{
  if (iRecordType == ORecordVObject.RECORD_TYPE)   return new ORecordVObject();
  if (iRecordType == ORecordFlat.RECORD_TYPE)   return new ORecordFlat();
  if (iRecordType == ORecordBytes.RECORD_TYPE)   return new ORecordBytes();
  if (iRecordType == ORecordCSV.RECORD_TYPE)   return new ORecordCSV();
  return null;
}
