{
  if (indexEngines.containsKey(name))   throw new IllegalArgumentException("Index engine with name " + engineData.name + " already contained in database configuration");
  final IndexEngineData oldEngine=indexEngines.putIfAbsent(name,engineData);
  if (oldEngine != null)   throw new IllegalArgumentException("Index engine with name " + engineData.name + " already contained in database configuration");
  update();
}
