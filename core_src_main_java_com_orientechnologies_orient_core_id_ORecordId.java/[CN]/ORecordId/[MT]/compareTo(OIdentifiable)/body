{
  if (iOther == this)   return 0;
  if (iOther == null)   return 1;
  ORID other=iOther.getIdentity();
  if (clusterId == other.getClusterId()) {
    if (clusterPosition == other.getClusterPosition())     return 0;
 else     if (clusterPosition > other.getClusterPosition())     return 1;
 else     if (clusterPosition < other.getClusterPosition())     return -1;
  }
 else   if (clusterId > other.getClusterId())   return 1;
  return -1;
}
