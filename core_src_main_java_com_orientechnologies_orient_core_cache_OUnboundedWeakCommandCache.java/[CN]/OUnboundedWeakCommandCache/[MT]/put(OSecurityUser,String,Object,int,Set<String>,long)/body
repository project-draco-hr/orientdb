{
  if (queryText == null || iResult == null || iInvolvedClusters == null || iInvolvedClusters.isEmpty())   return;
  if (!enable)   return;
  if (iExecutionTime < minExecutionTime)   return;
  if (iResult instanceof OResultSet && ((OResultSet)iResult).size() > maxResultsetSize)   return;
  if (strategy != STRATEGY.PER_CLUSTER)   iInvolvedClusters=null;
  final String key=getKey(iUser,queryText,iLimit);
  final WeakReference<OCachedResult> value=new WeakReference<OCachedResult>(new OCachedResult(iResult,iInvolvedClusters));
  lock.lock();
  try {
    cache.put(key,value);
  }
  finally {
    lock.unlock();
  }
}
