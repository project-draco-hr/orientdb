{
  if (iRecordType == ODocument.RECORD_TYPE)   return new ODocument();
 else   if (iRecordType == ORecordFlat.RECORD_TYPE)   return new ORecordFlat();
 else   if (iRecordType == ORecordBytes.RECORD_TYPE)   return new ORecordBytes();
 else   if (iRecordType == ORecordColumn.RECORD_TYPE)   return new ORecordColumn();
  throw new IllegalArgumentException("Unsuppurted record type" + iRecordType);
}
