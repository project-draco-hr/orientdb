{
  if (num == null && inputParam == null) {
    return "";
  }
  StringBuilder result=new StringBuilder();
  result.append(" BATCH ");
  if (num != null) {
    result.append(num);
  }
 else   if (inputFinalValue != null) {
    result.append(inputFinalValue);
  }
 else {
    result.append(inputParam);
  }
  return result.toString();
}
