{
  ((OMVRBTreeRIDProvider)dataProvider).lazyUnmarshall();
  if (hasNewItems()) {
    final Collection<ORecord<?>> v=newEntries.keySet();
    v.removeAll(c);
    if (newEntries.size() == 0)     newEntries=null;
  }
  boolean modified=false;
  for (  Object o : c)   if (remove(o) != null)   modified=true;
  return modified;
}
