{
  storage=iStorage;
  clusterName=iClusterName;
  if (storage != null) {
    if (clusterName != null)     clusterId=storage.getClusterIdByName(iClusterName);
 else     clusterId=storage.getClusterIdByName(OStorage.CLUSTER_INDEX_NAME);
  }
 else {
    clusterId=-1;
  }
  record=new ORecordBytesLazy(this);
  record.setIdentity(new ORecordId());
  keySerializer=iKeySerializer;
  valueSerializer=iValueSerializer;
  entryRecordBuffer=new OMemoryStream(getDefaultPageSize() * 15);
  updateConfig();
}
