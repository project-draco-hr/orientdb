{
  final StringBuilder buffer=new StringBuilder();
  buffer.append("id=");
  buffer.append(id);
  buffer.append(" from=");
  buffer.append(senderNodeName);
  if (task != null) {
    buffer.append(" task=");
    buffer.append(task.toString());
  }
  if (requestLoginUserName != null) {
    buffer.append(" requestLoginUserName=");
    buffer.append(requestLoginUserName);
  }
  return buffer.toString();
}
