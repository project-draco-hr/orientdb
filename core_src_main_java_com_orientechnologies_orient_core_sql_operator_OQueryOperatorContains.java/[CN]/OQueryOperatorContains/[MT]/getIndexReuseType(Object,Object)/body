{
  if (!(iLeft instanceof OSQLFilterItemField)) {
    return OIndexReuseType.NO_INDEX;
  }
  if (((OSQLFilterItemField)iLeft).hasChainOperators() || iRight instanceof OSQLFilterItemField && ((OSQLFilterItemField)iRight).hasChainOperators()) {
    return OIndexReuseType.NO_INDEX;
  }
  return OIndexReuseType.INDEX_METHOD;
}
