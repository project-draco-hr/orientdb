{
  if (jj_scan_token(ORDER))   return true;
  if (jj_scan_token(BY))   return true;
  Token xsp;
  xsp=jj_scanpos;
  if (jj_3R_248()) {
    jj_scanpos=xsp;
    if (jj_3R_249()) {
      jj_scanpos=xsp;
      if (jj_3R_250())       return true;
    }
  }
  xsp=jj_scanpos;
  if (jj_3R_251())   jj_scanpos=xsp;
  while (true) {
    xsp=jj_scanpos;
    if (jj_3R_252()) {
      jj_scanpos=xsp;
      break;
    }
  }
  return false;
}
