{
  if (iObject == null)   return null;
  if (!isMultiValue(iObject))   return null;
  if (iObject instanceof Collection<?>)   return ((Collection<Object>)iObject).iterator();
  if (iObject instanceof Map<?,?>)   return ((Map<?,Object>)iObject).values().iterator();
  if (iObject.getClass().isArray())   return Arrays.asList(iObject).iterator();
  return null;
}
