{
  if (iToNode == null)   throw new IllegalArgumentException("Missed 'to' Arc property");
  final OArc arc=new OArc(getDatabase(),this,iToNode);
  List<ODocument> arcs=field("arcs");
  if (arcs == null) {
    arcs=new ArrayList<ODocument>();
    field("arcs",arcs);
  }
  arcs.add(arc);
  return arc;
}
