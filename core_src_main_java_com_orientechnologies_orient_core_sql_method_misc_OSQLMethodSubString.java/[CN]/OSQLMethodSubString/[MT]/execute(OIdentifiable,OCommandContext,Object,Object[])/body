{
  final Object param0=OSQLHelper.getValue(iMethodParams[0].toString(),(ORecordInternal<?>)iCurrentRecord.getRecord(),iContext);
  if (param0 != null)   if (iMethodParams.length > 1) {
    final Object param1=OSQLHelper.getValue(iMethodParams[1].toString(),(ORecordInternal<?>)iCurrentRecord.getRecord(),iContext);
    ioResult=ioResult != null ? ioResult.toString().substring(Integer.parseInt(param0.toString()),Integer.parseInt(param1.toString())) : null;
  }
 else {
    ioResult=ioResult != null ? ioResult.toString().substring(Integer.parseInt(param0.toString())) : null;
  }
  return ioResult;
}
