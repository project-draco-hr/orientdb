{
  Object fieldValue;
  ODocument doc;
  index.clear();
  final int[] clusterIds=owner.getClusterIds();
  for (  int clusterId : clusterIds)   for (  Object record : document.getDatabase().browseCluster(document.getDatabase().getClusterNameById(clusterId))) {
    if (record instanceof ODocument) {
      doc=(ODocument)record;
      fieldValue=doc.field(name);
      if (fieldValue != null)       index.put(fieldValue.toString(),(ORecordId)doc.getIdentity());
    }
  }
}
