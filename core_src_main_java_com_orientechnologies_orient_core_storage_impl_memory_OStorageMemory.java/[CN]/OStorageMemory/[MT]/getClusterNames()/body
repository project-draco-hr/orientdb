{
  lock.acquireSharedLock();
  try {
    Set<String> result=new HashSet<String>();
    for (int i=0; i < clusters.size(); ++i) {
      final OCluster cluster=clusters.get(i);
      if (cluster != null)       result.add(cluster.getName());
    }
    return result;
  }
  finally {
    lock.releaseSharedLock();
  }
}
