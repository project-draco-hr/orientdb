{
  OFileUtils.checkValidName(config.getName());
  this.config=(OStoragePhysicalClusterConfigurationLocal)config;
  storageLocal=storage;
  diskCache=storageLocal.getDiskCache();
  name=config.getName();
  this.id=config.getId();
  OStorageFileConfiguration clusterStateConfiguration=new OStorageFileConfiguration(null,OStorageVariableParser.DB_PATH_VARIABLE + "/" + config.getName()+ CLUSTER_STATE_FILE_EXTENSION,OFileFactory.CLASSIC,"1024","50%");
  clusterStateHolder=new OSingleFileSegment(storage,clusterStateConfiguration);
}
