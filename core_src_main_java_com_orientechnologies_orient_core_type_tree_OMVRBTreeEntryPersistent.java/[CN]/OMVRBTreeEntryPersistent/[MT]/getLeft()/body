{
  if (left == null && leftRid != null && leftRid.isValid()) {
    try {
      left=pTree.loadEntry(this,leftRid);
      checkEntryStructure();
    }
 catch (    IOException e) {
      OLogManager.instance().error(this,"getLeft: Can't load the tree. The tree could be invalid.",e,ODatabaseException.class);
    }
  }
  return left;
}
