{
  final StringBuilder buffer=new StringBuilder();
  buffer.append("mvrb-tree entry ");
  buffer.append(record.getIdentity());
  buffer.append(" (size=");
  buffer.append(size);
  if (size > 0) {
    buffer.append(" [");
    if (size > 1) {
      buffer.append(getKeyAt(0));
      buffer.append(" ... ");
      buffer.append(getKeyAt(size - 1));
    }
 else {
      buffer.append(getKeyAt(0));
    }
    buffer.append("]");
  }
  buffer.append(")");
  return buffer.toString();
}
