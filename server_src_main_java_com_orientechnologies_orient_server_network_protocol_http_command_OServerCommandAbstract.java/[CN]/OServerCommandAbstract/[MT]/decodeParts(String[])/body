{
  try {
    if (parts == null)     return null;
    for (int i=0; i < parts.length; i++) {
      String part=parts[i];
      if (part == null)       continue;
      if (part.indexOf('%') > -1) {
        part=java.net.URLDecoder.decode(part,"UTF-8");
        parts[i]=part;
      }
    }
    return parts;
  }
 catch (  Exception ex) {
    throw new OException(ex);
  }
}
