{
  if (iValue == null)   return null;
  final String stringValue=iValue.toString();
  if (NULL_VALUE.equals(stringValue))   return null;
  if (OStringSerializerHelper.contains(stringValue,'.') || OStringSerializerHelper.contains(stringValue,','))   return (int)Float.parseFloat(stringValue);
 else   return stringValue.length() > 0 ? new Integer(stringValue) : new Integer(0);
}
