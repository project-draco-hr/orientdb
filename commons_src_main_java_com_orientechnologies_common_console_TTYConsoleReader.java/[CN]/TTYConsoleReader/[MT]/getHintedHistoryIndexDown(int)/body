{
  if (historyBuffer != null && !historyBuffer.equals("")) {
    for (int i=historyNum + 1; i < history.size(); i++) {
      if (history.get(i).startsWith(historyBuffer)) {
        return i;
      }
    }
    return history.size();
  }
  return historyNum=historyNum < history.size() ? historyNum + 1 : history.size();
}
