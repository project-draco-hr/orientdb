{
  final OClass cls=parsedTarget.getTargetClasses().keySet().iterator().next();
  if (!searchForIndexes(cls)) {
    final boolean browsingOrderAsc=!(orderedFields.size() == 1 && orderedFields.get(0).getKey().equalsIgnoreCase("@rid") && orderedFields.get(0).getValue().equalsIgnoreCase("DESC"));
    super.searchInClasses(browsingOrderAsc);
  }
}
