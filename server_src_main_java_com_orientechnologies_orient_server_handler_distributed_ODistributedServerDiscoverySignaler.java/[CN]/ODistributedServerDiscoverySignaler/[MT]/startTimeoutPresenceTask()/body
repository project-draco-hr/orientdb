{
  runningTask=new TimerTask(){
    @Override public void run(){
      try {
        if (running && !manager.isLeaderConnected())         manager.becameLeader(forceLeadership);
      }
 catch (      Exception e) {
      }
    }
  }
;
  Orient.getTimer().schedule(runningTask,manager.networkTimeoutLeader);
}
