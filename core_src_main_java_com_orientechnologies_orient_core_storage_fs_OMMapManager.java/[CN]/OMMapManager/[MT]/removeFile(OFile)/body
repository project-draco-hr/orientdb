{
  lock.writeLock().lock();
  try {
    final List<OMMapBufferEntry> entries=bufferPoolPerFile.remove(iFile);
    if (entries != null) {
      for (      OMMapBufferEntry entry : entries) {
        bufferPoolLRU.remove(entry);
        removeEntry(entry);
      }
    }
  }
  finally {
    lock.writeLock().unlock();
  }
}
