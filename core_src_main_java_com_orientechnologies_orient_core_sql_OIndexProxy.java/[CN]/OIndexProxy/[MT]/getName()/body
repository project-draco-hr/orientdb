{
  final StringBuilder res=new StringBuilder("IndexChain{");
  final List<String> indexNames=getIndexNames();
  for (int i=0; i < indexNames.size(); i++) {
    String indexName=indexNames.get(i);
    if (i > 0)     res.append(", ");
    res.append(indexName);
  }
  res.append("}");
  return res.toString();
}
