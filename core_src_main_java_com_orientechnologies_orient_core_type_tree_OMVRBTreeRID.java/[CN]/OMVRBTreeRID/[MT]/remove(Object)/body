{
  final OIdentifiable removed;
  if (hasNewItems() && newItems.containsKey(o)) {
    removed=(OIdentifiable)o;
    newItems.remove(o);
    if (newItems.size() == 0)     newItems=null;
    setDirty();
  }
 else {
    if (containsKey(o)) {
      removed=super.remove(o);
      setDirty();
    }
 else     removed=null;
  }
  return removed;
}
