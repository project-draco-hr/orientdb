{
  boolean containsKey=containsKey(iKey);
  T oldValue=super.put(iKey,iValue);
  if (containsKey && oldValue == iValue)   return oldValue;
  if (containsKey)   fireCollectionChangedEvent(new OMultiValueChangeEvent<Object,T>(OMultiValueChangeEvent.OChangeType.UPDATE,iKey,iValue,oldValue));
 else   fireCollectionChangedEvent(new OMultiValueChangeEvent<Object,T>(OMultiValueChangeEvent.OChangeType.ADD,iKey,iValue));
  return oldValue;
}
