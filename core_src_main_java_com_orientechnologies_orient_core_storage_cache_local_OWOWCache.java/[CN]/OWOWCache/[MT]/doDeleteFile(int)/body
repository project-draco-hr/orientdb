{
  final int intId=extractFileId(fileId);
  if (isOpen(fileId))   removeCachedPages(intId);
  final OFileClassic fileClassic=files.remove(fileId);
  String name=null;
  if (fileClassic != null) {
    name=fileClassic.getName();
    if (fileClassic.exists())     fileClassic.delete();
  }
  return name;
}
