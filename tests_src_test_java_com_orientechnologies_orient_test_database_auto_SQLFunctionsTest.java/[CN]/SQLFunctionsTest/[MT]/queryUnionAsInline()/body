{
  List<ODocument> result=database.command(new OSQLSynchQuery<ODocument>("select union(out, in) as edges from OGraphVertex")).execute();
  Assert.assertTrue(result.size() > 1);
  for (  ODocument d : result) {
    Assert.assertEquals(d.fieldNames().length,1);
    Assert.assertTrue(d.containsField("edges"));
  }
}
