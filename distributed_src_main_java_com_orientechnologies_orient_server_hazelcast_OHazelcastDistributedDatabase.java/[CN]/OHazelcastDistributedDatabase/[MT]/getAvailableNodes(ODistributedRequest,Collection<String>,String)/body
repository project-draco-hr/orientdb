{
  int availableNodes;
  availableNodes=0;
  int i=0;
  for (  String node : iNodes) {
    final boolean include=manager.isNodeAvailable(node,databaseName);
    if (include)     availableNodes++;
 else {
      if (ODistributedServerLog.isDebugEnabled())       ODistributedServerLog.debug(this,getLocalNodeName(),node,DIRECTION.OUT,"skip expected response from node '%s' for request %s because it's not online",node,iRequest);
    }
    ++i;
  }
  return availableNodes;
}
