{
  if (tasks.size() == 1)   return tasks.get(0).getPartitionKey();
  int partition=-1;
  for (  OAbstractRecordReplicatedTask t : tasks) {
    if (t instanceof OCreateRecordTask) {
      final int tpk=t.getPartitionKey();
      if (partition == -1)       partition=tpk;
 else       if (partition != tpk)       return -1;
    }
  }
  return partition;
}
