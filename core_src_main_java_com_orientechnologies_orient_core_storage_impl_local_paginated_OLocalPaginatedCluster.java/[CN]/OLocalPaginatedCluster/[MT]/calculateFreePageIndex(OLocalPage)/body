{
  int newFreePageIndex;
  int pageFreeSpace=localPage.getFreeSpace();
  if (pageFreeSpace == OLocalPage.MAX_RECORD_SIZE)   newFreePageIndex=freePageIndexes.length - 1;
 else {
    newFreePageIndex=(localPage.getFreeSpace() - 1023) / 1024;
    newFreePageIndex-=16;
  }
  return newFreePageIndex;
}
