{
  List<ODocument> qResult=db.command(new OCommandSQL("select friend.name as name from (match {class:Person, where:(name = 'n1'), as: me}-Friend-{}-Friend-{as:friend, where: ($matched.me != $currentMatch)} return $matches)")).execute();
  for (  ODocument doc : qResult) {
    assertNotEquals(doc.field("name"),"n1");
  }
}
