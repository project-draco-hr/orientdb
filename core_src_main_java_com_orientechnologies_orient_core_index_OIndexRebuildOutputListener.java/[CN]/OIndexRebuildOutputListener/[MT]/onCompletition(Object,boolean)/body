{
  final long idxSize=idx.getSize();
  if (idxSize > 0)   if (rebuild)   OLogManager.instance().info(this,"--> OK, indexed %,d items in %,d ms",idxSize,(System.currentTimeMillis() - startTime));
 else   OLogManager.instance().debug(this,"--> OK, indexed %,d items in %,d ms",idxSize,(System.currentTimeMillis() - startTime));
}
