{
  currentTx.rollback();
  for (  ODatabaseListener listener : underlying.getListeners())   try {
    listener.onBeforeTxBegin(underlying);
  }
 catch (  Throwable t) {
    OLogManager.instance().error(this,"Error before the transaction begin",t,OTransactionBlockedException.class);
  }
  currentTx=iTx;
  currentTx.begin();
  return this;
}
