{
  ODirtyManager real=getReal();
  if (record.getIdentity().isNew()) {
    if (real.newRecord == null)     real.newRecord=Collections.newSetFromMap(new IdentityHashMap<ORecord,Boolean>());
    real.newRecord.add(record);
  }
 else {
    if (real.updateRecord == null)     real.updateRecord=Collections.newSetFromMap(new IdentityHashMap<ORecord,Boolean>());
    real.updateRecord.add(record);
  }
}
