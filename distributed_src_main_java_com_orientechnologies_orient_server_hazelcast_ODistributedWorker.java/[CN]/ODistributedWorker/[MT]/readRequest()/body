{
  ODistributedRequest req=nextMessage();
  if (manager.isOffline())   waitNodeIsOnline();
  if (ODistributedServerLog.isDebugEnabled()) {
    final String senderNodeName=manager.getNodeNameById(req.getId().getNodeId());
    ODistributedServerLog.debug(this,manager.getLocalNodeName(),senderNodeName,DIRECTION.IN,"Processing request=%s sourceNode=%s",req,senderNodeName);
  }
  return req;
}
