{
  if (debug) {
    OLogManager.instance().debug(this,"%s - Reading byte (1 byte)...",socket.getRemoteSocketAddress());
    final byte value=in.readByte();
    OLogManager.instance().debug(this,"%s - Read byte: %d",socket.getRemoteSocketAddress(),(int)value);
    return value;
  }
  return in.readByte();
}
