{
synchronized (configuration) {
    ODocument oldConfig=configuration;
    configuration=cfg;
    configure();
    try {
      updateCfgOnDisk();
    }
 catch (    IOException e) {
      OException.wrapException(new OConfigurationException("Cannot change Command Cache Cache configuration file '" + CONFIG_FILE + "'. Command Cache will use default settings"),e);
      configuration=oldConfig;
      configure();
    }
  }
}
