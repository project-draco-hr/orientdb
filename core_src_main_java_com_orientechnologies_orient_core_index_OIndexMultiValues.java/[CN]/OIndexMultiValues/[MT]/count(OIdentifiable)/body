{
  acquireExclusiveLock();
  try {
    Set<OIdentifiable> rids;
    int tot=0;
    for (    Entry<Object,Set<OIdentifiable>> entries : map.entrySet()) {
      rids=entries.getValue();
      if (rids != null) {
        if (rids.contains(iRecord)) {
          ++tot;
        }
      }
    }
    return tot;
  }
  finally {
    releaseExclusiveLock();
  }
}
