{
  boolean removed=false;
  Iterator<OIdentifiable> all=tree.iterator();
  while (all.hasNext()) {
    OIdentifiable entry=all.next();
    try {
      if (entry == null || entry.getRecord() == null) {
        all.remove();
        removed=true;
      }
    }
 catch (    ORecordNotFoundException e) {
      all.remove();
      removed=true;
    }
  }
  return removed;
}
