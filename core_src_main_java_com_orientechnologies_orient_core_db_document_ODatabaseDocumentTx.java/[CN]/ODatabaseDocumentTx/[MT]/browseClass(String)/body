{
  if (getMetadata().getSchema().getClass(iClassName) == null)   throw new IllegalArgumentException("Class '" + iClassName + "' not found in current database");
  checkSecurity(ODatabaseSecurityResources.CLASS + "." + iClassName,ORole.CRUD_MODES.READ);
  return new ORecordIteratorMultiCluster<ODocument>(this,underlying,getMetadata().getSchema().getClass(iClassName).getClusterIds());
}
