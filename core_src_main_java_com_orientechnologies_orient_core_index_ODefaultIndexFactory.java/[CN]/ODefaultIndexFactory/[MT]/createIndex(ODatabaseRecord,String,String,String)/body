{
  if (valueContainerAlgorithm == null) {
    if (OClass.INDEX_TYPE.NOTUNIQUE.toString().equals(indexType) || OClass.INDEX_TYPE.FULLTEXT.toString().equals(indexType))     valueContainerAlgorithm=MVRBTREE_VALUE_CONTAINER;
 else     valueContainerAlgorithm=NONE_VALUE_CONTAINER;
  }
  if (SBTREE_ALGORITHM.equals(algorithm))   return createSBTreeIndex(indexType,valueContainerAlgorithm);
  if (MVRBTREE_ALGORITHM.equals(algorithm) || algorithm == null)   return createMRBTreeIndex(indexType,valueContainerAlgorithm);
  throw new OConfigurationException("Unsupported type : " + indexType);
}
