{
  checkDirection(false);
  if (limit > -1 && browsedRecords >= limit)   return false;
  if (browsedRecords >= totalAvailableRecords)   return false;
  if (liveUpdated)   return current.clusterPosition > database.getStorage().getClusterDataRange(current.clusterId)[0];
  return current.clusterPosition > firstClusterPosition;
}
