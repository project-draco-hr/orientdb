{
  checkDirection(true);
  ORecordInternal<?> record=getRecord();
  if (currentTxEntryPosition > -1)   if (currentTxEntryPosition >= txEntries.size())   throw new NoSuchElementException();
 else   return (REC)txEntries.get(currentTxEntryPosition++).getRecord();
  while (currentClusterIdx < clusterIds.length) {
    while (hasNext()) {
      if (currentTxEntryPosition > -1)       return (REC)txEntries.get(currentTxEntryPosition++).getRecord();
      if ((record=readCurrentRecord(record,+1)) != null)       return (REC)record;
    }
    currentClusterIdx++;
    if (currentClusterIdx >= clusterIds.length)     break;
    updateClusterRange();
  }
  throw new NoSuchElementException();
}
