{
  if (jj_scan_token(ORDER))   return true;
  if (jj_scan_token(BY))   return true;
  Token xsp;
  xsp=jj_scanpos;
  if (jj_3R_394()) {
    jj_scanpos=xsp;
    if (jj_3R_395())     return true;
  }
  while (true) {
    xsp=jj_scanpos;
    if (jj_3R_396()) {
      jj_scanpos=xsp;
      break;
    }
  }
  return false;
}
