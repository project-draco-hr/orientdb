{
  List<ODocument> result=database.command(new OSQLSynchQuery<ODocument>("select from Profile  where ( name = 'Giuseppe' and nick is not null ) or ( name = 'Napoleone' and nick is not null ) ")).execute();
  Assert.assertTrue(result.size() != 0);
  for (  ODocument d : result) {
    Assert.assertEquals(d.getRecordType(),ODocument.RECORD_TYPE);
  }
}
