{
  StringBuilder result=new StringBuilder();
  if (items != null && items.size() > 0) {
    result.append("ORDER BY ");
    for (int i=0; i < items.size(); i++) {
      if (i > 0) {
        result.append(", ");
      }
      result.append(items.get(i).toString());
    }
  }
  return result.toString();
}
