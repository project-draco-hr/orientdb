{
  if (skip > 0) {
    skip--;
    return true;
  }
  OIdentifiable recordCopy=iRecord instanceof ORecord<?> ? ((ORecord<?>)iRecord).copy() : iRecord.getIdentity().copy();
  recordCopy=applyProjections(recordCopy);
  resultCount++;
  if (recordCopy != null)   if (anyFunctionAggregates || orderedFields != null || flattenTarget != null) {
    if (tempResult == null)     tempResult=new ArrayList<OIdentifiable>();
    tempResult.add(recordCopy);
  }
 else {
    if (request.getResultListener() != null)     request.getResultListener().result(recordCopy);
  }
  if (fetchLimit > -1 && resultCount >= fetchLimit)   return false;
  return true;
}
