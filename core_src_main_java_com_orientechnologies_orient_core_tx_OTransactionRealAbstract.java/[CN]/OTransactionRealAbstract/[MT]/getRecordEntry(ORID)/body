{
  ORecordOperation e=allEntries.get(rid);
  if (e != null)   return e;
  if (rid.isTemporary()) {
    final ORecord record=temp2persistent.get(rid);
    if (record != null && !record.getIdentity().equals(rid))     rid=record.getIdentity();
  }
  e=recordEntries.get(rid);
  if (e != null)   return e;
  e=allEntries.get(rid);
  if (e != null)   return e;
  return null;
}
