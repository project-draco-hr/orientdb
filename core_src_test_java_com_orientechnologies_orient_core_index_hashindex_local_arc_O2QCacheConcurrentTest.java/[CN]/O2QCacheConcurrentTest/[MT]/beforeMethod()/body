{
  if (buffer != null) {
    buffer.close();
    File file=new File(storageLocal.getConfiguration().getDirectory() + "/o2QCacheTest.0.tst");
    if (file.exists()) {
      boolean delete=file.delete();
      Assert.assertTrue(delete);
    }
  }
  initBuffer();
  Random random=new Random();
  seed=(byte)(random.nextInt() & 0xFF);
}
