{
  out.println("PROPERTIES:");
  out.println("+---------------------+----------------------+");
  out.printf("| %-30s| %-30s |\n","NAME","VALUE");
  out.println("+---------------------+----------------------+");
  for (  Entry<String,String> p : properties.entrySet()) {
    out.printf("| %-30s= %-30s |\n",p.getKey(),p.getValue());
  }
  out.println("+---------------------+----------------------+");
}
