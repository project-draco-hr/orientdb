{
  boolean changed=false;
  for (  Map.Entry<Object,T> e : entrySet()) {
    if (e.getKey() instanceof ORecordElement)     if (((ORecordElement)e.getKey()).setDatabase(iDatabase))     changed=true;
    if (e.getValue() instanceof ORecordElement)     if (((ORecordElement)e.getValue()).setDatabase(iDatabase))     changed=true;
  }
  return changed;
}
