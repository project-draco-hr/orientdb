{
  try {
    tree=new OTreeMapPersistent<String,T>((ODatabaseRecord<?>)database,clusterName,OStreamSerializerString.INSTANCE,new OStreamSerializerAnyRecord((ODatabaseRecord<? extends ORecord<?>>)database));
    tree.save();
  }
 catch (  Exception e) {
    OLogManager.instance().error(this,"Can't create the local database's dictionary",e,ODatabaseException.class);
  }
}
