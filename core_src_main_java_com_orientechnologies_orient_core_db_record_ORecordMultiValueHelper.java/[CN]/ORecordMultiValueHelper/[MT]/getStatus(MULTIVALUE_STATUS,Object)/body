{
  if (iPreviousStatus == MULTIVALUE_STATUS.HYBRID) {
  }
 else   if (iPreviousStatus == MULTIVALUE_STATUS.EMPTY) {
    if (iValue instanceof ORID)     return MULTIVALUE_STATUS.ALL_RIDS;
 else     if (iValue instanceof ORecord<?>)     return MULTIVALUE_STATUS.ALL_RECORDS;
 else     return MULTIVALUE_STATUS.HYBRID;
  }
 else   if (iPreviousStatus == MULTIVALUE_STATUS.ALL_RECORDS) {
    if (iValue instanceof ORID)     return MULTIVALUE_STATUS.HYBRID;
  }
 else   if (iPreviousStatus == MULTIVALUE_STATUS.ALL_RIDS) {
    if (!(iValue instanceof ORID))     return MULTIVALUE_STATUS.HYBRID;
  }
  return iPreviousStatus;
}
