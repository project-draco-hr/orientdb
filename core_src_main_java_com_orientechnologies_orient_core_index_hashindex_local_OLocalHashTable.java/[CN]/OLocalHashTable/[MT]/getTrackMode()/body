{
  final OStorageTransaction transaction=storage.getStorageTransaction();
  if (transaction == null && !durableInNonTxMode)   return ODurablePage.TrackMode.NONE;
  final ODurablePage.TrackMode trackMode=super.getTrackMode();
  if (!trackMode.equals(ODurablePage.TrackMode.NONE))   return txTrackMode;
  return trackMode;
}
