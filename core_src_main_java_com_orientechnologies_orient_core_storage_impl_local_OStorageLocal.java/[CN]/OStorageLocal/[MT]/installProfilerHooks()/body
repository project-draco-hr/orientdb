{
  Orient.instance().getProfiler().registerHookValue(Orient.instance().getProfiler().getDatabaseMetric(name,"data.holes"),"Number of the holes in local database",METRIC_TYPE.COUNTER,new OProfilerHookValue(){
    public Object getValue(){
      return getHoles();
    }
  }
);
  Orient.instance().getProfiler().registerHookValue(Orient.instance().getProfiler().getDatabaseMetric(name,"data.holeSize"),"Size of the holes in local database",METRIC_TYPE.SIZE,new OProfilerHookValue(){
    public Object getValue(){
      return getHoleSize();
    }
  }
);
}
