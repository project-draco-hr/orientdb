{
  try {
    txManager.getTxSegment().rollback(iTx);
    if (OGlobalConfiguration.TX_COMMIT_SYNCH.getValueAsBoolean())     synch();
  }
 catch (  IOException ioe) {
    OLogManager.instance().error(this,"Error executing rollback for transaction with id '" + iTx.getId() + "' cause: "+ ioe.getMessage(),ioe);
  }
}
