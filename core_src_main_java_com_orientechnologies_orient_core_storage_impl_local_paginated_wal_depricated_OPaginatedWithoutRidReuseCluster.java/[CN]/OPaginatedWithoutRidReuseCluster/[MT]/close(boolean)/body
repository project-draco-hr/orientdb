{
  externalModificationLock.requestModificationLock();
  try {
    acquireExclusiveLock();
    try {
      if (flush)       synch();
      diskCache.closeFile(fileId,flush);
    }
  finally {
      releaseExclusiveLock();
    }
  }
  finally {
    externalModificationLock.releaseModificationLock();
  }
}
