{
  if (outEdges == null) {
    outEdges=new ArrayList<OGraphEdge>();
    List<ODocument> docs=document.field(FIELD_OUT_EDGES);
    if (docs == null) {
      docs=new ArrayList<ODocument>();
      document.field(FIELD_OUT_EDGES,docs);
    }
 else {
      for (      ODocument d : docs) {
        outEdges.add(new OGraphEdge(d));
      }
    }
  }
  return outEdges;
}
