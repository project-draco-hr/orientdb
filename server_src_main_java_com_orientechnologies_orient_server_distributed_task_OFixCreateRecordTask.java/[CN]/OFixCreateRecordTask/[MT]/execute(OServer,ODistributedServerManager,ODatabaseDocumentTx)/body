{
  ODistributedServerLog.debug(this,iManager.getLocalNodeName(),getNodeSource(),DIRECTION.IN,"fixing create record %s/%s by applying rid=%s v=%s",database.getName(),ridAssigned.toString(),this.ridToAssign.toString(),version.toString());
  ORecordInternal<?> record=Orient.instance().getRecordFactoryManager().newInstance(recordType);
  record.fill(ridAssigned,version,content,true);
  database.save(record);
  ODistributedServerLog.debug(this,iManager.getLocalNodeName(),getNodeSource(),DIRECTION.IN,"+-> fixed create record %s/%s v.%s",database.getName(),record.getIdentity().toString(),record.getRecordVersion().toString());
  return Boolean.TRUE;
}
