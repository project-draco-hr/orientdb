{
  final Object value=underlying.next();
  if (value == null)   return null;
  if (sourceRecord != null && sourceRecord.getDatabase() != null)   if (value instanceof ORecordId && convertToRecord) {
    ORecordInternal<?> record=ORecordFactory.newInstance(recordType);
    record.setDatabase(sourceRecord.getDatabase());
    record.setIdentity((ORecordId)value);
    record.load();
    return record;
  }
  return value;
}
