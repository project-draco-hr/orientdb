{
  assignTarget(iArgs);
  if (target == null) {
    if (let != null)     assignLetClauses(lastRecord != null ? lastRecord.getRecord() : null);
    return;
  }
  while (target.hasNext())   if (!executeSearchRecord(target.next()))   break;
  if (request.getResultListener() != null)   request.getResultListener().end();
}
