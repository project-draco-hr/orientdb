{
  if (status == STATUS.CLOSED)   return false;
  lock.acquireSharedLock();
  try {
    if (status == STATUS.CLOSED)     return false;
    final int remainingUsers=getUsers() > 0 ? removeUser() : 0;
    return force || (!(this instanceof OAbstractPaginatedStorage) && remainingUsers == 0);
  }
  finally {
    lock.releaseSharedLock();
  }
}
