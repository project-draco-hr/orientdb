{
  final int myVersion;
  if (isTombstone())   myVersion=-version;
 else   myVersion=version;
  final int otherVersion;
  if (o.isTombstone())   otherVersion=-o.getCounter();
 else   otherVersion=o.getCounter();
  if (myVersion == otherVersion)   return 0;
  if (myVersion < otherVersion)   return -1;
  return 1;
}
