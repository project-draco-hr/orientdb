{
  if (maxValuesToFetch < 0)   return getValuesMajor(fromKey,isInclusive,true);
  final OCommandRequest cmd;
  if (isInclusive)   cmd=formatCommand(QUERY_GET_VALUE_MAJOR_EQUALS + QUERY_GET_VALUES_LIMIT + maxValuesToFetch,name);
 else   cmd=formatCommand(QUERY_GET_VALUE_MAJOR + QUERY_GET_VALUES_LIMIT + maxValuesToFetch,name);
  return (Collection<OIdentifiable>)getDatabase().command(cmd).execute(fromKey);
}
