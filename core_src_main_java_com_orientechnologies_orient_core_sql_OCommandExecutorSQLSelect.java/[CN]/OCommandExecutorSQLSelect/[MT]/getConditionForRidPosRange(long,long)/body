{
  final OSQLFilterCondition fromCondition=new OSQLFilterCondition(new OSQLFilterItemField(null,ODocumentHelper.ATTRIBUTE_RID_POS),new OQueryOperatorMajor(),fromId);
  final OSQLFilterCondition toCondition=new OSQLFilterCondition(new OSQLFilterItemField(null,ODocumentHelper.ATTRIBUTE_RID_POS),new OQueryOperatorMinorEquals(),toId);
  return new OSQLFilterCondition(fromCondition,new OQueryOperatorAnd(),toCondition);
}
