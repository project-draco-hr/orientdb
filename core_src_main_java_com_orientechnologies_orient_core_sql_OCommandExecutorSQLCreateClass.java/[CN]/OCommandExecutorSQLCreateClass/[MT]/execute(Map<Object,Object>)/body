{
  if (className == null)   throw new OCommandExecutionException("Can't execute the command because it hasn't been parsed yet");
  OClass sourceClass=database.getMetadata().getSchema().getClass(className);
  if (sourceClass != null)   throw new OCommandExecutionException("Class " + className + " already exists");
  sourceClass=database.getMetadata().getSchema().createClass(className);
  database.getMetadata().getSchema().save();
  return sourceClass.getId();
}
