{
  latch.await();
  try {
    while (!stop) {
      for (      Thread thread : addedThreads) {
        Assert.assertSame(threadCounters.search(thread).getThread(),thread);
        if (stop)         return null;
      }
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
    throw new RuntimeException(e);
  }
  return null;
}
