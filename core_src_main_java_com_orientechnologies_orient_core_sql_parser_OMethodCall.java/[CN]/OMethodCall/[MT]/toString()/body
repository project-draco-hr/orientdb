{
  StringBuilder result=new StringBuilder();
  result.append(".");
  result.append(methodName.toString());
  result.append("(");
  boolean first=true;
  for (  OExpression param : params) {
    if (!first) {
      result.append(", ");
    }
    result.append(param.toString());
    first=false;
  }
  result.append(")");
  return result.toString();
}
