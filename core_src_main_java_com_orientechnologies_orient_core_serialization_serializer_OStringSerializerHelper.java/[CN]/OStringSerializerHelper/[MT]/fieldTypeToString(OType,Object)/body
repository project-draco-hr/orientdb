{
  if (iValue == null)   return null;
switch (iType) {
case STRING:
    return "'" + (String)iValue + "'";
case INTEGER:
case FLOAT:
case LONG:
case DOUBLE:
case SHORT:
  return String.valueOf(iValue);
case BINARY:
return new String((byte[])iValue);
case DATE:
return String.valueOf(((Date)iValue).getTime());
}
return iValue.toString();
}
