{
  if (_owners == null)   _owners=new ArrayList<WeakReference<ORecordElement>>();
  boolean found=false;
  for (  WeakReference<ORecordElement> _owner : _owners) {
    final ORecordElement e=_owner.get();
    if (e == iOwner) {
      found=true;
      break;
    }
  }
  if (!found)   this._owners.add(new WeakReference<ORecordElement>(iOwner));
}
