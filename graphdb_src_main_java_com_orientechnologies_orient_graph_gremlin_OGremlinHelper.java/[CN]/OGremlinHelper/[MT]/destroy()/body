{
  if (enginePool != null) {
    for (    ScriptEngine engine : enginePool.getResources()) {
      engine.getBindings(ScriptContext.ENGINE_SCOPE).clear();
    }
    enginePool.close();
  }
  if (graphPool != null) {
    for (    OrientGraph graph : graphPool.getResources()) {
      graph.shutdown();
    }
    graphPool.close();
  }
}
