{
  db.begin();
  ODocument doc=new ODocument("test");
  ODocument doc1=new ODocument("test");
  doc.field("ref",doc1);
  assertEquals(1,ORecordInternal.getDirtyManager(doc).getReferences().size());
  db.save(doc);
  assertEquals(1,ORecordInternal.getDirtyManager(doc).getReferences().size());
  db.commit();
  assertNull(ORecordInternal.getDirtyManager(doc).getReferences());
}
