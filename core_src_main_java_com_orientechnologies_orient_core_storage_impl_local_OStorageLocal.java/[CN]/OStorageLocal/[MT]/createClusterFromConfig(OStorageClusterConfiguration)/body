{
  OCluster cluster=clusterMap.get(iConfig.getName());
  if (cluster instanceof OClusterLocal && iConfig instanceof OStorageEHClusterConfiguration)   clusterMap.remove(iConfig.getName());
 else   if (cluster != null) {
    if (cluster instanceof OClusterLocal || cluster instanceof OClusterLocalEH) {
      cluster.configure(this,iConfig);
    }
    return -1;
  }
  cluster=Orient.instance().getClusterFactory().createCluster(iConfig);
  cluster.configure(this,iConfig);
  return registerCluster(cluster);
}
