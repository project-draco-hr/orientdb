{
  buffer.append('{');
  if (iMap != null) {
    int i=0;
    Entry<?,?> entry;
    for (Iterator<?> it=iMap.entrySet().iterator(); it.hasNext(); ++i) {
      entry=(Entry<?,?>)it.next();
      if (i > 0)       buffer.append(", ");
      buffer.append(writeValue(entry.getKey(),iFormat));
      buffer.append(": ");
      buffer.append(writeValue(entry.getValue(),iFormat));
    }
  }
  buffer.append('}');
  return buffer.toString();
}
