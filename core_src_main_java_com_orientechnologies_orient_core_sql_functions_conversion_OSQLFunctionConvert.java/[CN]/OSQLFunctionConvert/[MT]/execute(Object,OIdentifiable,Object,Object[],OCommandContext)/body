{
  if (iFuncParams[0] == null || iFuncParams[1] == null)   return null;
  final String destType=iFuncParams[1].toString();
  if (destType.contains("."))   try {
    return OType.convert(iFuncParams[0],Class.forName(destType));
  }
 catch (  ClassNotFoundException e) {
  }
 else {
    final OType orientType=OType.valueOf(destType.toUpperCase());
    if (orientType != null)     return OType.convert(iFuncParams[0],orientType.getDefaultJavaType());
  }
  return null;
}
