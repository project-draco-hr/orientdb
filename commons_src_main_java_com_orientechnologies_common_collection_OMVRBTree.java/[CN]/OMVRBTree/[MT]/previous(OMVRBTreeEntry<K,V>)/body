{
  if (t == null)   return null;
  final int index=t.getTree().getPageIndex();
  final OMVRBTreeEntry<K,V> prev;
  if (index <= 0) {
    prev=predecessor(t);
    if (prev != null)     t.tree.pageIndex=t.size - 1;
 else     t.tree.pageIndex=0;
  }
 else {
    prev=t;
    t.tree.pageIndex=index - 1;
  }
  return prev;
}
