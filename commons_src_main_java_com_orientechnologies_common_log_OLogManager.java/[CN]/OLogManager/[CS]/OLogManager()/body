{
  String configLevel=System.getProperty(SYSPROPERTY_LOG_LEVEL);
  level=configLevel != null ? Level.parse(configLevel.toUpperCase()) : Level.INFO;
  if (level.equals(Level.FINER) || level.equals(Level.FINE) || level.equals(Level.FINEST))   debug=info=warn=true;
 else   if (level.equals(Level.INFO))   info=warn=true;
 else   if (level.equals(Level.WARNING))   warn=true;
}
