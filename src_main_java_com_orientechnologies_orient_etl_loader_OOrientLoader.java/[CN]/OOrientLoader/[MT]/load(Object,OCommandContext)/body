{
  if (input == null)   return;
  if (!(input instanceof ORecord<?>))   throw new IllegalArgumentException("Expected record but received object '" + input + "' of class: "+ input.getClass());
  if (className != null && input instanceof ODocument)   ((ODocument)input).setClassName(className);
  if (clusterName != null)   ((ORecord)input).save(clusterName);
 else   ((ORecord)input).save();
  progress++;
}
