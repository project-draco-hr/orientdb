{
  g.createVertexType("V1");
  g.createVertexType("V2");
  g.createEdgeType("E1");
  g.createEdgeType("E2");
  final OrientVertex v0=g.addVertex(null);
  final OrientVertex v1=g.addVertex("class:V1");
  final OrientVertex v2=g.addVertex("class:V2");
  v0.addEdge("E",v1);
  v1.addEdge("E1",v2);
  v2.addEdge("E2",v0);
}
