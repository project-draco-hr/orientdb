{
synchronized (syncObject) {
    LRUEntry lruEntry=am.get(fileId,pageIndex);
    if (lruEntry != null) {
      lruEntry.managedExternally=false;
      return;
    }
    lruEntry=a1out.get(fileId,pageIndex);
    if (lruEntry != null) {
      lruEntry.managedExternally=false;
      return;
    }
    lruEntry=a1in.get(fileId,pageIndex);
    if (lruEntry != null) {
      lruEntry.managedExternally=false;
    }
  }
}
