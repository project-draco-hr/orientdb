{
  buildDirectory=System.getProperty("buildDirectory");
  if (buildDirectory == null)   buildDirectory=".";
  databaseDocumentTx=new ODatabaseDocumentTx("plocal:" + buildDirectory + "/localSBTreeCompositeKeyTest");
  if (databaseDocumentTx.exists()) {
    databaseDocumentTx.open("admin","admin");
    databaseDocumentTx.drop();
  }
  databaseDocumentTx.create();
  localSBTree=new OSBTree<OCompositeKey,OIdentifiable>(".sbt",2,false,".nbt");
  localSBTree.create("localSBTreeCompositeKeyTest",OCompositeKeySerializer.INSTANCE,OLinkSerializer.INSTANCE,null,(OStorageLocalAbstract)databaseDocumentTx.getStorage().getUnderlying(),false);
}
