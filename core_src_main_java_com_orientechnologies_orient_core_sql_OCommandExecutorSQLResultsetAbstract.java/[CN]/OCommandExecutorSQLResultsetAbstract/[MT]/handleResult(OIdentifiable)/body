{
  if (iRecord != null) {
    resultCount++;
    OIdentifiable recordCopy=iRecord instanceof ORecord<?> ? ((ORecord<?>)iRecord).copy() : iRecord.getIdentity().copy();
    if (recordCopy != null)     if (request.getResultListener() != null)     request.getResultListener().result(recordCopy);
    if (limit > -1 && resultCount >= limit)     return false;
  }
  return true;
}
