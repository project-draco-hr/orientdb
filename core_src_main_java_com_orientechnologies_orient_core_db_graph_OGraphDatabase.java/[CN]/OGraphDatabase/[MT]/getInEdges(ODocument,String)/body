{
  checkVertexClass(iVertex);
  final ORecordLazySet set=iVertex.field(VERTEX_FIELD_IN_EDGES);
  if (iLabel == null)   if (set != null)   return Collections.unmodifiableSet(set);
 else   return Collections.emptySet();
  final ORecordLazySet result=new ORecordLazySet(underlying);
  if (set != null)   for (  OIdentifiable item : set) {
    if (iLabel == null || iLabel.equals(((ODocument)item).field(LABEL)))     result.add(item);
  }
  return result;
}
