{
  final Class<? extends OCommandExecutor> executorClass=commandReqExecMap.get(iCommand.getClass());
  if (executorClass == null)   throw new IllegalArgumentException("Can't find a command executor for the command request: " + iCommand);
  try {
    return executorClass.newInstance();
  }
 catch (  Exception e) {
    throw new IllegalArgumentException("Can't create the command executor of class " + executorClass + " for the command request: "+ iCommand,e);
  }
}
