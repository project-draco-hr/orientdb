{
  final ORecordId fromRecordId=new ORecordId(clusterId,fromId);
  final ORecordId toRecordId=new ORecordId(clusterId,toId);
  final OSQLFilterCondition fromCondition=new OSQLFilterCondition(new OSQLFilterItemField(null,"@rid"),new OQueryOperatorMajor(),fromRecordId);
  final OSQLFilterCondition toCondition=new OSQLFilterCondition(new OSQLFilterItemField(null,"@rid"),new OQueryOperatorMinorEquals(),toRecordId);
  return new OSQLFilterCondition(fromCondition,new OQueryOperatorAnd(),toCondition);
}
