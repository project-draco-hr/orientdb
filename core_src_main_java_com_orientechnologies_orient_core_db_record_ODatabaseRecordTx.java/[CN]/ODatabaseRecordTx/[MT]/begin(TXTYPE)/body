{
  currentTx.rollback();
switch (iType) {
case NOTX:
    setDefaultTransactionMode();
  break;
case OPTIMISTIC:
currentTx=new OTransactionOptimistic<REC>(this,txSerial++);
break;
case PESSIMISTIC:
throw new UnsupportedOperationException("Pessimistic transaction");
}
currentTx.begin();
return this;
}
