{
  OIndex<?> bestIndex=null;
  for (  OIndex<?> index : involvedIndexes) {
    bestIndex=index;
    OIndexInternal<?> bestInternalIndex=index.getInternal();
    if (bestInternalIndex instanceof OIndexUnique || bestInternalIndex instanceof OIndexNotUnique) {
      return index;
    }
  }
  return bestIndex;
}
