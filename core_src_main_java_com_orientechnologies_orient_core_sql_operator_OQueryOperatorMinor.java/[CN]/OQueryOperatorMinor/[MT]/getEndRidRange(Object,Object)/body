{
  if (iLeft instanceof OSQLFilterItemField && ODocumentHelper.ATTRIBUTE_RID.equals(((OSQLFilterItemField)iLeft).getRoot()))   if (iRight instanceof ORID)   return (ORID)iRight;
 else {
    if (iRight instanceof OSQLFilterItemParameter && ((OSQLFilterItemParameter)iRight).getValue(null,null) instanceof ORID)     return (ORID)((OSQLFilterItemParameter)iRight).getValue(null,null);
  }
  return null;
}
