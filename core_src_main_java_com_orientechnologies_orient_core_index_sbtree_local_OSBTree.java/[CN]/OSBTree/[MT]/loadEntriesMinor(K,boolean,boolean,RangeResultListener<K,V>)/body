{
  acquireSharedLock();
  try {
    if (ascSortOrder)     loadEntriesMinorAscOrder(key,inclusive,listener);
 else     loadEntriesMinorDescOrder(key,inclusive,listener);
  }
 catch (  IOException ioe) {
    throw new OSBTreeException("Error during fetch of minor values for key " + key + " in sbtree "+ name);
  }
 finally {
    releaseSharedLock();
  }
}
