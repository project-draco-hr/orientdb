{
  final File importDir=new File(testPath + "/" + NEW_DB_PATH);
  if (importDir.exists())   for (  File f : importDir.listFiles())   f.delete();
 else   importDir.mkdir();
  ODatabaseDocumentTx database;
  if (url.startsWith("plocal:"))   database=new ODatabaseDocumentTx("plocal:" + testPath + "/"+ NEW_DB_URL);
 else   database=new ODatabaseDocumentTx("local:" + testPath + "/"+ NEW_DB_URL);
  database.create();
  ODatabaseImport impor=new ODatabaseImport(database,testPath + "/" + EXPORT_FILE_PATH,this);
  for (  ORecordHook hook : new ArrayList<ORecordHook>(database.getHooks())) {
    database.unregisterHook(hook);
  }
  impor.importDatabase();
  impor.close();
  database.close();
}
