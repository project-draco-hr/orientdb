{
  if (record.isDirty())   return;
  if (left != null && !left.record.isDirty())   left.clear();
  if (right != null && !right.record.isDirty())   right.clear();
  if (parent != null) {
    if (parent.left == this)     parent.left=null;
 else     parent.right=null;
    parent=null;
    serializedKeys=null;
    serializedValues=null;
    record=null;
  }
}
