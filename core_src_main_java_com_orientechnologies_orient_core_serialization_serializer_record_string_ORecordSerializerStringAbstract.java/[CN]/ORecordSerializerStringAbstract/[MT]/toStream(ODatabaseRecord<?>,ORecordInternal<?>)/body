{
  final long timer=OProfiler.getInstance().startChrono();
  try {
    return OBinaryProtocol.string2bytes(toString((ORecordSchemaAware<?>)iRecord,null,iDatabase,OSerializationThreadLocal.INSTANCE.get()));
  }
  finally {
    OProfiler.getInstance().stopChrono("ORecordSerializerStringAbstract.toStream",timer);
  }
}
