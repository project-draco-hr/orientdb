{
  ODistributedServerLog.debug(this,iManager.getLocalNodeName(),getNodeSource(),DIRECTION.IN,"fixing update record %s/%s v.%s",database.getName(),rid.toString(),version.toString());
  final ORecordInternal<?> record=rid.getRecord();
  record.fill(rid,version,content,true);
  database.save(record);
  ODistributedServerLog.debug(this,iManager.getLocalNodeName(),getNodeSource(),DIRECTION.IN,"+-> fixed update record %s/%s v.%s",database.getName(),record.getIdentity().toString(),record.getRecordVersion().toString());
  return Boolean.TRUE;
}
