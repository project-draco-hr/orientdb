{
  System.out.println("Reading from standard input...");
  OrientSql t=new OrientSql(System.in);
  try {
    OStatement n=t.parse();
    n.dump("");
    System.out.println("Thank you.");
  }
 catch (  Exception e) {
    System.out.println("Oops.");
    System.out.println(e.getMessage());
    e.printStackTrace();
  }
}
