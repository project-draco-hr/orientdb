{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  ORecordAbstract other=(ORecordAbstract)obj;
  if (database == null) {
    if (other.database != null)     return false;
  }
 else   if (!database.equals(other.database))   return false;
  if (recordId == null) {
    if (other.recordId != null)     return false;
  }
 else   if (!recordId.equals(other.recordId))   return false;
  return true;
}
