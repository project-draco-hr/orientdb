{
  _database=iDatabase;
  _recordId.clusterId=iRid.clusterId;
  _recordId.clusterPosition=iRid.clusterPosition;
  _version=iVersion;
  _status=ORecordElement.STATUS.LOADED;
  _source=iBuffer;
  _size=iBuffer != null ? iBuffer.length : 0;
  if (_source != null && _source.length > 0)   _dirty=iDirty;
  return this;
}
