{
  String key=iClassName.toLowerCase();
  if (classes.containsKey(key))   throw new OSchemaException("Class " + iClassName + " already exists in current database");
  OClass cls=new OClass(this,classes.size(),iClassName,iClusterIds,iDefaultClusterId);
  classes.put(key,cls);
  setDirty();
  return cls;
}
