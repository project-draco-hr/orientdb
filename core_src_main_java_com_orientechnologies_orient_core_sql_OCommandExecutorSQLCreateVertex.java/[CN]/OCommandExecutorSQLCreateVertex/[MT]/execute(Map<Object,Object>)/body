{
  if (clazz == null)   throw new OCommandExecutionException("Cannot execute the command because it has not been parsed yet");
  ODatabaseRecord database=getDatabase();
  if (!(database instanceof OGraphDatabase))   database=new OGraphDatabase((ODatabaseRecordTx)database);
  final ODocument vertex=((OGraphDatabase)database).createVertex(clazz.getName());
  OSQLHelper.bindParameters(vertex,fields,new OCommandParameters(iArgs));
  vertex.save(clusterName);
  return vertex;
}
