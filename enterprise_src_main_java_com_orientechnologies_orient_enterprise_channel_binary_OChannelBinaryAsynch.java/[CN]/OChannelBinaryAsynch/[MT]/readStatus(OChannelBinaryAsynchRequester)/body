{
  do {
    lockRead.lock();
    if (!channelRead) {
      currentStatus=readByte();
      currentTxId=readInt();
    }
    if (currentTxId == iRequester.getRequesterId())     break;
    lockRead.unlock();
synchronized (this) {
      try {
        wait();
      }
 catch (      InterruptedException e) {
        e.printStackTrace();
      }
    }
  }
 while (true);
  handleStatus(currentStatus,currentTxId);
  return iRequester.getRequesterId();
}
