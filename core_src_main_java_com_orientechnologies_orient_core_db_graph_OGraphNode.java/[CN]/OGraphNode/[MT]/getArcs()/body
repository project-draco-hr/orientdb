{
  if (arcs == null) {
    arcs=new ArrayList<OGraphArc>();
    List<ODocument> docs=document.field("arcs");
    if (docs == null) {
      docs=new ArrayList<ODocument>();
      document.field("arcs",docs);
    }
 else {
      for (      ODocument d : docs) {
        arcs.add(new OGraphArc(d));
      }
    }
  }
  return arcs;
}
