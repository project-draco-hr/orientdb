{
  filePath=in.readUTF();
  offset=in.readLong();
  int size=in.readInt();
  buffer=new byte[size];
  in.readFully(buffer);
  final boolean lsnNotNull=in.readBoolean();
  lsn=lsnNotNull ? new OLogSequenceNumber(in) : null;
  gzipCompressed=in.readBoolean();
  last=in.readBoolean();
  size=in.readInt();
  if (size > 0) {
    schema=new byte[size];
    in.read(schema);
  }
 else   schema=null;
}
