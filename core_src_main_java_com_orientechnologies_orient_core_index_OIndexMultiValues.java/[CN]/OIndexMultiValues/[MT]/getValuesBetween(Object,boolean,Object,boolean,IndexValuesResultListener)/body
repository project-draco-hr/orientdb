{
  checkForRebuild();
  iRangeFrom=getCollatingValue(iRangeFrom);
  iRangeTo=getCollatingValue(iRangeTo);
  acquireSharedLock();
  try {
    indexEngine.getValuesBetween(iRangeFrom,fromInclusive,iRangeTo,toInclusive,MultiValuesTransformer.INSTANCE,new OIndexEngine.ValuesResultListener(){
      @Override public boolean addResult(      OIdentifiable identifiable){
        return resultListener.addResult(identifiable);
      }
    }
);
  }
  finally {
    releaseSharedLock();
  }
}
