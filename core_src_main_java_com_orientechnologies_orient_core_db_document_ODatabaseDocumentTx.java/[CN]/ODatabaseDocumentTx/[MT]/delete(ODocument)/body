{
  if (iContent.getClassName() != null)   checkSecurity(OUser.CLASS + "." + iContent.getClassName(),OUser.DELETE);
  try {
    underlying.delete(iContent);
  }
 catch (  Throwable t) {
    OLogManager.instance().error(this,"Error on deleting record #" + iContent.getIdentity() + " of class '"+ iContent.getClassName()+ "'",t,ODatabaseException.class);
  }
  return this;
}
