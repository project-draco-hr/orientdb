{
  acquireExclusiveLock();
  try {
    if (walRecord instanceof OAddNewPageRecord)     restoreNewPage((OAddNewPageRecord)walRecord);
    if (!(walRecord instanceof OUpdatePageRecord))     throw new OStorageException("Unknown WAL record type -  " + walRecord.getClass().getName());
    restorePageData((OUpdatePageRecord)walRecord);
  }
  finally {
    releaseExclusiveLock();
  }
}
