{
  OLogManager.instance().warn(this,"DISTRIBUTED <-[%s/%s] executing group of %d commands",nodeSource,databaseName,tasks.size());
  final Object[] result=new Object[tasks.size()];
  for (int i=0; i < tasks.size(); ++i) {
    final OAbstractDistributedTask<?> task=tasks.get(i);
    result[i]=task.call();
  }
  return result;
}
