{
  if (iType.equalsIgnoreCase(OClusterLocal.TYPE))   return OGlobalConfiguration.USE_LHPEPS_CLUSTER.getValueAsBoolean() && !forceListBasedCluster ? new OClusterLocalEH() : new OClusterLocal();
 else   if (iType.equalsIgnoreCase(OClusterMemory.TYPE))   return OGlobalConfiguration.USE_LHPEPS_MEMORY_CLUSTER.getValueAsBoolean() && !forceListBasedCluster ? new OClusterMemoryHashing() : new OClusterMemoryArrayList();
 else   OLogManager.instance().exception("Cluster type '" + iType + "' is not supported. Supported types are: "+ Arrays.toString(TYPES),null,OStorageException.class);
  return null;
}
