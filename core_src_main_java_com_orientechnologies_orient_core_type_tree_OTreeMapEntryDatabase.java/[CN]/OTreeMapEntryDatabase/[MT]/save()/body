{
  if (!record.isDirty())   return this;
  assureIntegrityOfReferences();
  if (parent != null)   if (!parent.record.getIdentity().equals(parentRid))   OLogManager.instance().error(this,"checkEntryStructure: Wrong parent node loaded: " + parentRid);
  checkEntryStructure();
  if (pTree.cache.get(record.getIdentity()) != this)   pTree.cache.put(record.getIdentity(),this);
  return this;
}
