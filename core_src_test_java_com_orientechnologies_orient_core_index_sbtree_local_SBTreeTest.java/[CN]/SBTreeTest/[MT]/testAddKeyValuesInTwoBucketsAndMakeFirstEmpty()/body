{
  for (int i=0; i < 5167; i++)   sbTree.put(i,new ORecordId(i % 32000,OClusterPositionFactory.INSTANCE.valueOf(i)));
  for (int i=0; i < 3500; i++)   sbTree.remove(i);
  Assert.assertEquals((int)sbTree.firstKey(),3500);
  for (int i=0; i < 3500; i++)   Assert.assertNull(sbTree.get(i));
  for (int i=3500; i < 5167; i++)   Assert.assertEquals(sbTree.get(i),new ORecordId(i % 32000,OClusterPositionFactory.INSTANCE.valueOf(i)));
}
