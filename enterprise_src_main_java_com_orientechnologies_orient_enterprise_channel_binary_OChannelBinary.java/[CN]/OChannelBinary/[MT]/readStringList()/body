{
  if (debug)   OLogManager.instance().debug(this,"%s - Reading string list. Reading string list items as int (4 bytes)...",socket.getRemoteSocketAddress());
  final int items=in.readInt();
  if (debug)   OLogManager.instance().debug(this,"%s - Read string list items: %d",socket.getRemoteSocketAddress(),items);
  if (items < 0)   return null;
  List<String> result=new ArrayList<String>();
  for (int i=0; i < items; ++i)   result.add(readString());
  if (debug)   OLogManager.instance().debug(this,"%s - Read string list with %d items: %d",socket.getRemoteSocketAddress(),items);
  return result;
}
