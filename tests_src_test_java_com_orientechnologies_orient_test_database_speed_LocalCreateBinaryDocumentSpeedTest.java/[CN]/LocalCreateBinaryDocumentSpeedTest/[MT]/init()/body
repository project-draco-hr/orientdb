{
  OProfiler.getInstance().startRecording();
  database=new ODatabaseDocumentTx(System.getProperty("url"));
  if (database.exists()) {
    database.open("admin","admin");
    database.drop();
  }
  database.create();
  database.declareIntent(new OIntentMassiveInsert());
  database.begin(TXTYPE.NOTX);
}
