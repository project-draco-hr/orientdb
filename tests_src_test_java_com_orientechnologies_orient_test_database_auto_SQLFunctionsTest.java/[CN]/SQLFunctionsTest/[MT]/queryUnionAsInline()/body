{
  database.open("admin","admin");
  List<ODocument> result=database.command(new OSQLSynchQuery<ODocument>("select union(outEdges, inEdges) as edges from OGraphVertex")).execute();
  Assert.assertTrue(result.size() > 1);
  for (  ODocument d : result) {
    Assert.assertEquals(d.fieldNames().size(),1);
    Assert.assertTrue(d.containsField("edges"));
  }
  database.close();
}
