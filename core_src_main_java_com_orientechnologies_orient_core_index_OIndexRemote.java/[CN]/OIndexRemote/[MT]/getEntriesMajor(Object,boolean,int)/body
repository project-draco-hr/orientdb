{
  if (maxEntriesToFetch < 0)   return getEntriesMajor(fromKey,isInclusive);
  final OCommandRequest cmd;
  if (isInclusive)   cmd=formatCommand(QUERY_GET_MAJOR_EQUALS + QUERY_GET_VALUES_LIMIT + maxEntriesToFetch,name);
 else   cmd=formatCommand(QUERY_GET_MAJOR + QUERY_GET_VALUES_LIMIT + maxEntriesToFetch,name);
  return (Collection<ODocument>)getDatabase().command(cmd).execute(fromKey);
}
