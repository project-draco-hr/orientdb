{
synchronized (operationCompleted) {
    while (operationStarted.get() > operationCompleted.get()) {
      try {
        operationCompleted.wait();
      }
 catch (      InterruptedException e) {
        Thread.interrupted();
        break;
      }
    }
  }
  if (vertexCache != null)   vertexCache.clear();
  factory.close();
}
