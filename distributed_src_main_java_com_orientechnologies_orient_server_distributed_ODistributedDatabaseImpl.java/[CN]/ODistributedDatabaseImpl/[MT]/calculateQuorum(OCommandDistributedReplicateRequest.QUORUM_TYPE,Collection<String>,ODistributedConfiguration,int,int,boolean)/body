{
  final String clusterName=clusterNames == null || clusterNames.isEmpty() ? null : clusterNames.iterator().next();
  int quorum=1;
switch (quorumType) {
case NONE:
    break;
case READ:
  quorum=cfg.getReadQuorum(clusterName,allAvailableNodes);
break;
case WRITE:
quorum=cfg.getWriteQuorum(clusterName,masterAvailableNodes);
break;
case ALL:
quorum=allAvailableNodes;
break;
}
if (quorum < 0) quorum=0;
if (checkNodesAreOnline && quorum > allAvailableNodes) throw new ODistributedException("Quorum (" + quorum + ") cannot be reached because it is major than available nodes ("+ allAvailableNodes+ ")");
return quorum;
}
