{
  checkDirection(true);
  if (currentTxEntryPosition > -1)   return (REC)txEntries.get(currentTxEntryPosition).getRecord();
  while (hasNext()) {
    REC record=getRecord();
    record=readCurrentRecord(record,+1);
    if (record != null)     return record;
  }
  throw new NoSuchElementException();
}
