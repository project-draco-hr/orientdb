{
  try {
    jsonWriter.endObject(settings.indentLevel,true);
    if (!(iUserObject instanceof ODocument)) {
      collectionStack.pop();
    }
  }
 catch (  IOException e) {
    throw new OFetchException("Error writing map field " + iFieldName + " of record "+ iRootRecord.getIdentity(),e);
  }
  settings.indentLevel--;
}
