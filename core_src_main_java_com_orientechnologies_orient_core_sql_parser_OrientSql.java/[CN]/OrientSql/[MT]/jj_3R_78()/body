{
  if (jj_scan_token(CREATE))   return true;
  if (jj_scan_token(INDEX))   return true;
  if (jj_3R_220())   return true;
  Token xsp;
  xsp=jj_scanpos;
  if (jj_3_131()) {
    jj_scanpos=xsp;
    if (jj_3R_221())     return true;
  }
  xsp=jj_scanpos;
  if (jj_3_134()) {
    jj_scanpos=xsp;
    if (jj_3R_222())     return true;
  }
  return false;
}
