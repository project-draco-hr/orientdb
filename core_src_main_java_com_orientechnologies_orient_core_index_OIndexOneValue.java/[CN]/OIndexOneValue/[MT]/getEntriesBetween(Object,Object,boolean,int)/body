{
  checkForRebuild();
  if (rangeFrom.getClass() != rangeTo.getClass())   throw new IllegalArgumentException("Range from-to parameters are of different types");
  acquireSharedLock();
  try {
    return indexEngine.getEntriesBetween(rangeFrom,rangeTo,inclusive,maxEntriesToFetch,null);
  }
  finally {
    releaseSharedLock();
  }
}
