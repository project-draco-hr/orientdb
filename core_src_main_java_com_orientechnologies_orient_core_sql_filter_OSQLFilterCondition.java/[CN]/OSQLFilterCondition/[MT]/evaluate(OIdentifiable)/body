{
  Object l=evaluate(o,left);
  Object r=evaluate(o,right);
  final Object[] convertedValues=checkForConversion(o,l,r);
  if (convertedValues != null) {
    l=convertedValues[0];
    r=convertedValues[1];
  }
  if (operator == null) {
    if (l == null)     return Boolean.FALSE;
    return l;
  }
  return operator.evaluateRecord(o,this,l,r);
}
