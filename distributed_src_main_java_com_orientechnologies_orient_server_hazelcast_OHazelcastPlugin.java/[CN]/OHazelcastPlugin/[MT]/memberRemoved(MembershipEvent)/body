{
  updateLastClusterChange();
  ODistributedServerLog.warn(this,getLocalNodeName(),null,DIRECTION.NONE,"node removed id=%s name=%s",iEvent.getMember(),getNodeName(iEvent.getMember()));
  final Member member=iEvent.getMember();
  final String nodeName=getNodeName(member);
  if (nodeName != null) {
    activeNodes.remove(nodeName);
    if (messageService != null)     messageService.handleUnreachableNode(nodeName);
  }
  OClientConnectionManager.instance().pushDistribCfg2Clients(getClusterConfiguration());
}
