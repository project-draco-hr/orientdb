{
  for (  String field : indexDefinition.getFields()) {
    Object val=null;
    if (key instanceof OCompositeKey) {
      val=((OCompositeKey)key).getKeys().get(i);
      i++;
    }
 else {
      val=key;
    }
    if (val != null) {
      if (isToStore(field).equals(Field.Store.YES)) {
        doc.add(OLuceneIndexType.createField(indexContext.indexClass.getName() + "." + field+ STORED,val,Field.Store.YES,Field.Index.NOT_ANALYZED_NO_NORMS));
      }
      doc.add(OLuceneIndexType.createField(indexContext.indexClass.getName() + "." + field,val,Field.Store.NO,Field.Index.ANALYZED));
    }
  }
}
