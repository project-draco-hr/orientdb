{
  acquireSharedLock();
  try {
    OExtendibleHashingTable.Entry[] entries=content.floorEntries(OClusterPositionFactory.INSTANCE.getMaxValue());
    if (entries.length == 0)     return OClusterPosition.INVALID_POSITION;
 else     return entries[entries.length - 1].key;
  }
  finally {
    releaseSharedLock();
  }
}
