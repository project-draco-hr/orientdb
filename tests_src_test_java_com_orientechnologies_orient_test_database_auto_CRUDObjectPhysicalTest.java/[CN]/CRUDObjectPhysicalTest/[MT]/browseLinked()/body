{
  database.open("admin","admin");
  for (  Profile obj : database.browseClass(Profile.class)) {
    if (obj.getNick().equals("Neo"))     Assert.assertEquals(obj.getFollowings().size(),2);
 else     if (obj.getNick().equals("Morpheus") || obj.getNick().equals("Trinity"))     Assert.assertEquals(obj.getFollowings().size(),0);
  }
  database.close();
}
