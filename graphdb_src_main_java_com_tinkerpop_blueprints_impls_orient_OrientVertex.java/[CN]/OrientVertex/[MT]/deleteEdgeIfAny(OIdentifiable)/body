{
  if (iRecord != null) {
    final ODocument doc=iRecord.getRecord();
    if (doc != null) {
      OImmutableClass clazz=ODocumentInternal.getImmutableSchemaClass(doc);
      if (clazz != null && clazz.isEdgeType())       try {
        doc.delete();
      }
 catch (      ORecordNotFoundException e) {
      }
    }
  }
}
