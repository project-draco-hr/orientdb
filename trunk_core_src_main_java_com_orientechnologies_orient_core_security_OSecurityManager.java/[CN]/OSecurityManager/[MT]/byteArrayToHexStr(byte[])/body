{
  char[] chars=new char[data.length * 2];
  for (int i=0; i < data.length; i++) {
    byte current=data[i];
    int hi=(current & 0xF0) >> 4;
    int lo=current & 0x0F;
    chars[2 * i]=(char)(hi < 10 ? ('0' + hi) : ('A' + hi - 10));
    chars[2 * i + 1]=(char)(lo < 10 ? ('0' + lo) : ('A' + lo - 10));
  }
  return new String(chars);
}
