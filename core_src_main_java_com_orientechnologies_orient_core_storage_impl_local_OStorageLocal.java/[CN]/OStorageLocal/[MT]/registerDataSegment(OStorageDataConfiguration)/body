{
  checkOpeness();
  for (  ODataLocal data : dataSegments)   if (data != null && data.getName().equals(iConfig.name)) {
    data.config=iConfig;
    return -1;
  }
  int pos=-1;
  for (int i=0; i < dataSegments.length; ++i)   if (dataSegments[i] == null) {
    pos=i;
    break;
  }
  if (pos == -1)   pos=dataSegments.length;
  final ODataLocal segment=new ODataLocal(this,iConfig,pos);
  if (pos == dataSegments.length)   dataSegments=OArrays.copyOf(dataSegments,dataSegments.length + 1);
  dataSegments[pos]=segment;
  return pos;
}
