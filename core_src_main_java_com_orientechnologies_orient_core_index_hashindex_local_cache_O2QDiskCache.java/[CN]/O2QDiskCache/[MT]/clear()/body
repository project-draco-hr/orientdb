{
synchronized (syncObject) {
    flushBuffer();
    am.clear();
    a1in.clear();
    a1out.clear();
    for (    Set<Long> fileEntries : filePages.values())     fileEntries.clear();
    for (    SortedMap<Long,OLogSequenceNumber> fileDirtyPages : dirtyPages.values())     fileDirtyPages.clear();
  }
}
