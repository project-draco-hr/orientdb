{
  database.open("admin","admin");
  List<ODocument> result=database.command(new OSQLSynchQuery<ODocument>("select from Profile  where name = 'Giuseppe'")).execute();
  List<ODocument> resultCase=database.command(new OSQLSynchQuery<ODocument>("select from Profile  where NAME = 'Giuseppe'")).execute();
  List<ODocument> resultCase2=database.command(new OSQLSynchQuery<ODocument>("select from Profile  where NaMe = 'Giuseppe'")).execute();
  int resultSize=result.size();
  int resultCaseSize=resultCase.size();
  int resultCase2Size=resultCase2.size();
  Assert.assertTrue(resultSize != 0);
  Assert.assertTrue(resultCaseSize != 0);
  Assert.assertTrue(resultCase2Size != 0);
  Assert.assertTrue(resultSize == resultCaseSize);
  Assert.assertTrue(resultCaseSize == resultCase2Size);
  Assert.assertTrue(resultSize == resultCase2Size);
  database.close();
}
