{
  final OrientBaseGraph graph=getGraph();
  if (vOut != null)   return vOut;
  if (graph != null)   graph.setCurrentGraphInThreadLocal();
  final ODocument doc=getRecord();
  if (doc == null)   return null;
  if (settings != null && settings.keepInMemoryReferences)   return doc.rawField(OrientBaseGraph.CONNECTION_OUT);
 else   return doc.field(OrientBaseGraph.CONNECTION_OUT);
}
