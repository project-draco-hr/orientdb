{
  record.fromStream(toStream());
  if (record.getIdentity().isValid())   record.getRecordVersion().copyFrom(iStorage.updateRecord((ORecordId)record.getIdentity(),true,record.toStream(),OVersionFactory.instance().createUntrackedVersion(),ORecordInternal.getRecordType(record),(byte)0,null).getResult());
 else {
    if (record.getIdentity().getClusterId() == ORID.CLUSTER_ID_INVALID)     ((ORecordId)record.getIdentity()).clusterId=treeDataProvider.clusterId;
    final OPhysicalPosition ppos=iStorage.createRecord((ORecordId)record.getIdentity(),record.toStream(),OVersionFactory.instance().createVersion(),ORecordInternal.getRecordType(record),(byte)0,null).getResult();
    ORecordInternal.setIdentity(record,record.getIdentity().getClusterId(),ppos.clusterPosition);
    record.getRecordVersion().copyFrom(ppos.recordVersion);
  }
  ORecordInternal.unsetDirty(record);
  if (!record.getIdentity().isTemporary())   ORecordListenerManager.removeListener(record,this);
}
