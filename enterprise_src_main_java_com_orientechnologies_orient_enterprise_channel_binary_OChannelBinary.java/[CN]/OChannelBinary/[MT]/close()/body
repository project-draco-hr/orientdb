{
  try {
    acquireExclusiveLock();
  }
 catch (  InterruptedException e1) {
    return;
  }
  try {
    try {
      if (in != null)       in.close();
    }
 catch (    IOException e) {
    }
    try {
      if (out != null)       out.close();
    }
 catch (    IOException e) {
    }
    super.close();
  }
  finally {
    releaseExclusiveLock();
  }
}
