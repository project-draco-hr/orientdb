{
  boolean found=false;
  for (  int clusterId : clusterIds) {
    if (clusterId == iId) {
      found=true;
      break;
    }
  }
  if (found) {
    final int[] newClusterIds=new int[clusterIds.length - 1];
    for (int i=0, k=0; i < clusterIds.length; ++i) {
      if (clusterIds[i] == iId)       continue;
      newClusterIds[k]=clusterIds[i];
      k++;
    }
  }
  return this;
}
