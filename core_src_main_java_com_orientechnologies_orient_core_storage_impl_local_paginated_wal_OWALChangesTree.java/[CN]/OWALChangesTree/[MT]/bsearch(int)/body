{
  if (root == null)   return null;
  Node current=root;
  while (true) {
    if (start < current.start) {
      if (current.left != null)       current=current.left;
 else       return current;
    }
 else     if (start > current.start) {
      if (current.right != null)       current=current.right;
 else       return current;
    }
 else     return current;
  }
}
