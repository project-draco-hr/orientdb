{
  checkDirection(false);
  if (limit > -1 && browsedRecords >= limit)   return false;
  if (browsedRecords >= totalAvailableRecords)   return false;
  if (liveUpdated)   return current.clusterPosition > database.getStorage().getClusterDataRange(current.clusterId)[0];
  while (currentClusterIdx > -1) {
    if (current.clusterPosition > firstClusterPosition)     return true;
    currentClusterIdx--;
    updateClusterRange();
    current.clusterPosition=lastClusterPosition + 1;
  }
  return txEntries != null && txEntries.size() - (currentTxEntryPosition + 1) > 0;
}
