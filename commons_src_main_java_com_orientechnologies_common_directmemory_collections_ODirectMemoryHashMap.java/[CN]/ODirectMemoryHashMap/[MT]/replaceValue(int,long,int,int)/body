{
  final long memoryOffset=index * hashLineSize + OIntegerSerializer.INT_SIZE + OLongSerializer.LONG_SIZE * bucketSize + 2 * offset * OIntegerSerializer.INT_SIZE + OIntegerSerializer.INT_SIZE;
  final int oldValuePtr=memory.getInt(ptr,(int)memoryOffset);
  memory.free(oldValuePtr);
  memory.setInt(ptr,(int)memoryOffset,valuePtr);
  return true;
}
