{
  if (OGlobalConfiguration.LAZYSET_WORK_ON_STREAM.getValueAsBoolean() && getStreamedContent() != null) {
    if (delegate.remove(o))     return true;
  }
 else {
    lazyLoad(true);
    final int pos=indexOf((OIdentifiable)o);
    if (pos > -1) {
      delegate.remove(pos);
      return true;
    }
  }
  if (hasNewItems()) {
    final boolean removed=newItems.remove(o) != null;
    if (newItems.size() == 0)     newItems=null;
    if (removed)     setDirty();
    return removed;
  }
  return false;
}
