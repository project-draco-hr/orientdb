{
  if (iMessage != null) {
    final Logger log=iRequester != null ? Logger.getLogger(iRequester.getClass().getName()) : Logger.getLogger("");
    if (log.isLoggable(iLevel))     if (iException != null)     log.log(iLevel,iMessage,iException);
 else     log.log(iLevel,iMessage,iAdditionalArgs);
  }
}
