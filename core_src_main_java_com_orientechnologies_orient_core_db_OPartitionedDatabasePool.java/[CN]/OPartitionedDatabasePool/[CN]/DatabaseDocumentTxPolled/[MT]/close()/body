{
  if (poolData != null) {
    final PoolData data=poolData.get();
    if (data.acquireCount == 0)     return;
    data.acquireCount--;
    if (data.acquireCount > 0)     return;
    PoolPartition p=partition;
    partition=null;
    super.close();
    data.acquiredDatabase=null;
    p.queue.offer(this);
    p.acquiredConnections.decrementAndGet();
  }
 else {
    super.close();
  }
}
