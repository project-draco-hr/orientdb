{
  int nInts=shift >>> 5;
  final int nBits=shift & 0x1f;
  final int result[]=new int[CHUNKS_SIZE];
  if (nBits != 0) {
    int nBits2=32 - nBits;
    int i=nInts;
    int j=0;
    while (i < CHUNKS_SIZE - 1)     result[j++]=chunks[i++] << nBits | chunks[i] >>> nBits2;
    result[j]=chunks[i] << nBits;
  }
 else   System.arraycopy(chunks,nInts,result,0,CHUNKS_SIZE - nInts);
  return new ONodeId(result);
}
