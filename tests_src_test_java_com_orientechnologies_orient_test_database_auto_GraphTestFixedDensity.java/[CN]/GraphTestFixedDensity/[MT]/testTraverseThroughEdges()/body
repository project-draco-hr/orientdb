{
  long time=System.currentTimeMillis();
  nodeReadCounter=-1;
  readSubNodesThroughEdges(database.getRoot("LinearGraph"));
  System.out.println("Indirect traverse with hot cache of the entire graph with depth=" + nodeReadCounter + " and fixed density="+ DENSITY+ " in "+ ((System.currentTimeMillis() - time) / 1000f)+ " sec.");
  Assert.assertEquals(nodeReadCounter,nodeWrittenCounter);
}
