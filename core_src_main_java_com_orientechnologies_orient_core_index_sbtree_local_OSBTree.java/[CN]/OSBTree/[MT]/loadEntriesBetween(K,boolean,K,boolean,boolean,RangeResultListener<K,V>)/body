{
  acquireSharedLock();
  try {
    if (ascSortOrder)     loadEntriesBetweenAscOrder(keyFrom,fromInclusive,keyTo,toInclusive,listener);
 else     loadEntriesBetweenDescOrder(keyFrom,fromInclusive,keyTo,toInclusive,listener);
  }
 catch (  IOException ioe) {
    throw new OSBTreeException("Error during fetch of values between key " + keyFrom + " and key "+ keyTo+ " in sbtree "+ name);
  }
 finally {
    releaseSharedLock();
  }
}
