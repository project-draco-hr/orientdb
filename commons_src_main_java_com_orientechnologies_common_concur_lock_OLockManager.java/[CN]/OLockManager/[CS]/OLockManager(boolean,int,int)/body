{
  int cL=1;
  int sh=0;
  while (cL < concurrencyLevel) {
    cL<<=1;
    sh++;
  }
  shift=32 - sh;
  mask=cL - 1;
  map=new ConcurrentHashMap<RESOURCE_TYPE,CountableLock>(cL);
  locks=new Object[cL];
  for (int i=0; i < locks.length; i++) {
    locks[i]=new Object();
  }
  acquireTimeout=iAcquireTimeout;
  enabled=iEnabled;
}
