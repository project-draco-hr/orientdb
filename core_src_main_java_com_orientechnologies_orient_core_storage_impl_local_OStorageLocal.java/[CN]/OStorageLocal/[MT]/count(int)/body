{
  if (iClusterId == -1)   throw new OStorageException("Cluster Id " + iClusterId + " is invalid in storage '"+ name+ "'");
  checkOpeness();
  lock.acquireSharedLock();
  try {
    return clusters[iClusterId] != null ? clusters[iClusterId].getEntries() : 0l;
  }
  finally {
    lock.releaseSharedLock();
  }
}
