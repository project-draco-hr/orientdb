{
  long pointer=directMemory.allocate(OSBTreeBonsaiBucket.MAX_BUCKET_SIZE_BYTES);
  OSBTreeBonsaiBucket<Long,OIdentifiable> treeBucket=new OSBTreeBonsaiBucket<Long,OIdentifiable>(pointer,true,OLongSerializer.INSTANCE,OLinkSerializer.INSTANCE,ODurablePage.TrackMode.FULL);
  treeBucket.setLeftSibling(123);
  Assert.assertEquals(treeBucket.getLeftSibling(),123);
  directMemory.free(pointer);
}
