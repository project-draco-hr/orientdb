{
  database.open("admin","admin");
  List<ODocument> result=database.command(new OSQLSynchQuery<ODocument>("SELECT FLATTEN( outEdges ) FROM OGraphVertex WHERE outEdges TRAVERSE(1,1) (@class = 'OGraphEdge')")).execute();
  Assert.assertTrue(result.size() != 0);
  for (  ODocument d : result) {
    Assert.assertEquals(d.getClassName(),"OGraphEdge");
    Assert.assertEquals(d.getRecordType(),ODocument.RECORD_TYPE);
  }
  database.close();
}
