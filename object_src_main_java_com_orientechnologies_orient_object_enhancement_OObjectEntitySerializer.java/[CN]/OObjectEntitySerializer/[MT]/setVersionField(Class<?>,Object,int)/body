{
  if (!classes.contains(iClass)) {
    registerClass(iClass);
  }
  Field f=null;
  for (Class<?> currentClass=iClass; currentClass != null && currentClass != Object.class && !currentClass.equals(ODocument.class); ) {
    f=fieldVersions.get(currentClass);
    if (f != null)     break;
    currentClass=currentClass.getSuperclass();
  }
  if (f != null) {
    if (f.getType().equals(String.class))     setFieldValue(f,iObject,String.valueOf(iValue));
 else     if (f.getType().equals(Long.class))     setFieldValue(f,iObject,Long.valueOf(iValue));
 else     if (f.getType().equals(Object.class))     setFieldValue(f,iObject,iValue);
  }
}
