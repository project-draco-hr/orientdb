{
  ODatabaseRecord db=ODatabaseRecordThreadLocal.INSTANCE.getIfDefined();
  if (db != null && db.getTransaction().isActive())   throw new IllegalStateException("Cannot reset documents during a transaction. Create a new one each time");
  super.reset();
  internalReset();
  if (_fieldOriginalValues != null)   _fieldOriginalValues.clear();
  _owners=null;
  return this;
}
