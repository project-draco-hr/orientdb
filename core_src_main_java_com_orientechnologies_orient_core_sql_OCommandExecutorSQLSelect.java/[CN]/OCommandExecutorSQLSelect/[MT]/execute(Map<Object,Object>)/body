{
  compiledFilter.bindParameters(iArgs);
  if (compiledFilter.getTargetClasses() != null)   searchInClasses();
 else   if (compiledFilter.getTargetClusters() != null)   searchInClusters();
 else   if (compiledFilter.getTargetIndex() != null)   searchInIndex();
 else   if (compiledFilter.getTargetRecords() != null)   searchInRecords();
 else   throw new OQueryParsingException("No source found in query: specify class, clusters or single records");
  applyOrderBy();
  applyFlatten();
  return processResult();
}
