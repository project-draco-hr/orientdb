{
  final Type[] actualTypes=OReflectionHelper.getGenericTypes(serializer.getClass());
  if (actualTypes[0] instanceof Class<?>)   customSerializers.put((Class<?>)actualTypes[0],serializer);
 else   if (actualTypes[0] instanceof ParameterizedType)   customSerializers.put((Class<?>)((ParameterizedType)actualTypes[0]).getRawType(),serializer);
}
