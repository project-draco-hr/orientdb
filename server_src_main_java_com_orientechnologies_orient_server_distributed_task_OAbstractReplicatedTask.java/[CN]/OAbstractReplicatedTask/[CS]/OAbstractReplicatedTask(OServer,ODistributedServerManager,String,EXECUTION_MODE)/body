{
  super(iServer,iDistributedSrvMgr,databaseName,iMode);
  this.operationSerial=iDistributedSrvMgr.incrementDistributedSerial(databaseName);
  ODistributedServerLog.debug(this,getNodeSource(),nodeDestination,DIRECTION.OUT,"creating unique msg id %d db=%s class=%s",operationSerial,databaseName,getClass().getSimpleName());
}
