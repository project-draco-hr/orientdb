{
  List<ODocument> result=null;
  ODatabaseDocumentTx dbServer3=new ODatabaseDocumentTx(getRemoteDatabaseURL(serverInstance.get(2))).open("admin","admin");
  String dbServerUrl1=getRemoteDatabaseURL(serverInstance.get(0));
