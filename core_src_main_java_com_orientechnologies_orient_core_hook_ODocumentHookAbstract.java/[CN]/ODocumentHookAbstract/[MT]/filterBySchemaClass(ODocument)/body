{
  if (includeClasses == null && excludeClasses == null)   return true;
  final String clazz=iDocument.getClassName();
  if (clazz == null)   return false;
  if (includeClasses != null) {
    for (    String cls : includeClasses)     if (cls.equals(clazz))     return true;
    return false;
  }
  if (excludeClasses != null) {
    for (    String cls : excludeClasses)     if (cls.equals(clazz))     return false;
  }
  return true;
}
