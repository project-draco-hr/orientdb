{
  record.fromStream(toStream());
  if (record.getIdentity().isValid())   ORecordInternal.setVersion(record,iSt.updateRecord((ORecordId)record.getIdentity(),true,record.toStream(),-1,ORecordInternal.getRecordType(record),(byte)0,null).getResult());
 else {
    if (record.getIdentity().getClusterId() == ORID.CLUSTER_ID_INVALID)     ((ORecordId)record.getIdentity()).clusterId=clusterId;
    final OPhysicalPosition ppos=iSt.createRecord((ORecordId)record.getIdentity(),record.toStream(),0,ORecordInternal.getRecordType(record),(byte)0,null).getResult();
    ORecordInternal.setVersion(record,ppos.recordVersion);
  }
  ORecordInternal.unsetDirty(record);
}
