{
  checkDirection(true);
  if (limit > -1 && browsedRecords >= limit)   return false;
  if (browsedRecords >= totalAvailableRecords)   return false;
  if (liveUpdated)   lastClusterPosition=database.getStorage().getClusterDataRange(currentClusterId)[1];
  final long recordsToBrowse=currentClusterPosition > -2 && lastClusterPosition > -1 ? lastClusterPosition - currentClusterPosition : 0;
  if (recordsToBrowse <= 0)   return hasTxEntry();
  return true;
}
