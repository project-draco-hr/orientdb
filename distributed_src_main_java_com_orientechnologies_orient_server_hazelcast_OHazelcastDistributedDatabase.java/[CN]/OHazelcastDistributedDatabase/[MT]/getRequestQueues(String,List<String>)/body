{
  final IQueue<ODistributedRequest>[] queues=new IQueue[nodes.size()];
  for (int i=0; i < nodes.size(); ++i) {
    final IQueue<ODistributedRequest> queue=msgService.getQueueIfExists(OHazelcastDistributedMessageService.getRequestQueueName(nodes.get(i),iDatabaseName));
    queues[i]=queue;
  }
  return queues;
}
