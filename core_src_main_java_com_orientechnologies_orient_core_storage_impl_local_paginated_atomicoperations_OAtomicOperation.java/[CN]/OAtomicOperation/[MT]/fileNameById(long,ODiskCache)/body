{
  FileChangesContainer fileChangesContainer=fileChanges.get(fileId);
  if (fileChangesContainer != null && fileChangesContainer.fileName != null)   return fileChangesContainer.fileName;
  if (deletedFiles.contains(fileId))   throw new OStorageException("File with id " + fileId + " was deleted.");
  return diskCache.fileNameById(fileId);
}
