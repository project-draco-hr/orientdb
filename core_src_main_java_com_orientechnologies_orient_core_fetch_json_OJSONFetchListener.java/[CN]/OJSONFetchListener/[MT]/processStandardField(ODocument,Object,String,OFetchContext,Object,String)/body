{
  try {
    ((OJSONFetchContext)iContext).getJsonWriter().writeAttribute(((OJSONFetchContext)iContext).getIndentLevel(),true,iFieldName,iFieldValue);
  }
 catch (  IOException e) {
    throw OException.wrapException(new OFetchException("Error processing field '" + iFieldValue + " of record "+ iRecord.getIdentity()),e);
  }
}
