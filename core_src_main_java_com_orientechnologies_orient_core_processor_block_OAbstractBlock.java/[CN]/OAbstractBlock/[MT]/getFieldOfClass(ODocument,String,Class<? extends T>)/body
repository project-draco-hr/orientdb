{
  final Object f=iConfig.field(iFieldName);
  if (f != null)   if (!iExpectedClass.isAssignableFrom(f.getClass()))   throw new OProcessException("Block '" + getName() + "' defines the field '"+ iFieldName+ "' of typ '"+ f.getClass()+ "' that is not compatible with the expected type '"+ iExpectedClass+ "'");
  return (T)f;
}
