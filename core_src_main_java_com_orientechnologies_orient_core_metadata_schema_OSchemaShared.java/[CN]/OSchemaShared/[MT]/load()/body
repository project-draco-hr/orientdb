{
  rwSpinLock.acquireWriteLock();
  try {
    getDatabase();
    ((ORecordId)document.getIdentity()).fromString(getDatabase().getStorage().getConfiguration().schemaRecordId);
    reload("*:-1 index:0");
    snapshot=new OImmutableSchema(this);
    return this;
  }
  finally {
    rwSpinLock.releaseWriteLock();
  }
}
