{
  if (names.length == 1)   return transformValue(((ORecordSchemaAware<?>)iRecord).field(names[0]));
  Object[] values=((ORecordSchemaAware<?>)iRecord).fieldValues();
  if (hasChainOperators()) {
    for (int i=0; i < values.length; ++i)     values[i]=transformValue(values[i]);
  }
  return new OSQLRuntimeValueMulti(this,values);
}
