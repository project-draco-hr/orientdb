{
  for (  Map.Entry<Object,Object> snapshotEntry : snapshot.entrySet()) {
    Object key=snapshotEntry.getKey();
    checkForKeyType(key);
    Object snapshotValue=snapshotEntry.getValue();
    if (snapshotValue.equals(RemovedValue.INSTANCE))     indexEngine.remove(key);
 else     indexEngine.put(key,(OIdentifiable)snapshotValue);
  }
}
