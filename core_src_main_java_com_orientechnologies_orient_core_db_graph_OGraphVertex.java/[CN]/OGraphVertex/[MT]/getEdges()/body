{
  if (edges == null) {
    edges=new ArrayList<OGraphEdge>();
    List<ODocument> docs=document.field(FIELD_EDGES);
    if (docs == null) {
      docs=new ArrayList<ODocument>();
      document.field(FIELD_EDGES,docs);
    }
 else {
      for (      ODocument d : docs) {
        edges.add(new OGraphEdge(d));
      }
    }
  }
  return edges;
}
