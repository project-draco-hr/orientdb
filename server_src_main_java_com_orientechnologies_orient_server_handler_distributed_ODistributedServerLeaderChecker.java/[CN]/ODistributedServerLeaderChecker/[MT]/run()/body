{
  if (manager.getStatus() != ODistributedServerManager.STATUS.ONLINE)   return;
  final long time=System.currentTimeMillis() - manager.getLastHeartBeat();
  if (time > heartBeatDelay) {
    OLogManager.instance().warn(this,"No heartbeat message has been received from the Leader node (last was %d ms ago). Resend presence message in broadcast...",time);
    if (manager.serverElectedForLeadership) {
      cancel();
      manager.broadcastPresence(true);
    }
    return;
  }
}
