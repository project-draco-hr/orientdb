{
  String value;
  Object fieldValue;
  Object[] fieldValues=new Object[values.length];
  for (int i=0; i < values.length; ++i) {
    value=values[i];
    fieldValue=null;
    if (value != null) {
      if (value.startsWith("'") && value.endsWith("'"))       fieldValue=value.substring(1,value.length() - 1);
 else       if (value.indexOf(":") > -1)       fieldValue=new ORecordId(value);
 else {
        if (value.contains("."))         fieldValue=new Float(value);
 else         fieldValue=new Integer(value);
      }
    }
    fieldValues[i]=fieldValue;
  }
  return fieldValues;
}
