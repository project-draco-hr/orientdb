{
  if (t == null)   return null;
  OTreeMapEntry<K,V> p=null;
  if (t.getRight() != null) {
    p=t.getRight();
    while (p.getLeft() != null)     p=p.getLeft();
  }
 else {
    p=t.getParent();
    OTreeMapEntry<K,V> ch=t;
    while (p != null && ch == p.getRight()) {
      ch=p;
      p=p.getParent();
    }
  }
  return p;
}
