{
  if (query != null)   return;
  StringBuilder word=new StringBuilder();
  int pos=OSQLHelper.nextWord(text,textUpperCase,0,word,true);
  if (pos == -1 || !word.toString().equals(OSQLHelper.KEYWORD_DELETE))   throw new OCommandSQLParsingException("Keyword " + OSQLHelper.KEYWORD_DELETE + " not found",text,0);
  query=database.query(new OSQLAsynchQuery<ODocument>("select " + text.substring(pos),this));
}
