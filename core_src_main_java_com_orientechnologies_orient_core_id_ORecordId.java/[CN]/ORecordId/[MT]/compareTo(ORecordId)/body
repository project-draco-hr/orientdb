{
  if (iOther == this)   return 0;
  if (iOther == null)   return 1;
  if (clusterId == iOther.clusterId) {
    if (clusterPosition == iOther.clusterPosition)     return 0;
 else     if (clusterPosition > iOther.clusterPosition)     return 1;
 else     if (clusterPosition < iOther.clusterPosition)     return -1;
  }
 else   if (clusterId > iOther.clusterId)   return 1;
  return -1;
}
