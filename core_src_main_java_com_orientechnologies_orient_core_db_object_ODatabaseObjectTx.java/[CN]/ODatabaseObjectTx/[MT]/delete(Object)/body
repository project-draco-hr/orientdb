{
  checkOpeness();
  if (iContent == null)   return this;
  ODocument record=getRecordByUserObject(iContent,false);
  if (record == null)   record=(ODocument)underlying.load(OObjectSerializerHelper.getObjectID(this,iContent));
  underlying.delete(record);
  if (getTransaction() instanceof OTransactionNoTx)   unregisterPojo(iContent,record);
  return this;
}
