{
  checkForRemoteServer();
  try {
    final ODocument response=serverAdmin.resetReplicationJournal(iDatabaseName,iRemoteName);
    message("\nReset replication journal for database '" + iDatabaseName + "': removed "+ response.field("removedEntries")+ " entries");
  }
 catch (  Exception e) {
    printError(e);
  }
}
