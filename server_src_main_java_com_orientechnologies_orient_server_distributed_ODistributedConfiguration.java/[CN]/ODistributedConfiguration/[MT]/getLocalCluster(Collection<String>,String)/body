{
synchronized (configuration) {
    if (iClusterNames == null || iClusterNames.isEmpty())     iClusterNames=DEFAULT_CLUSTER_NAME;
    final List<String> candidates=new ArrayList<String>(5);
    for (    String p : iClusterNames) {
      final String masterServer=getLeaderServer(p);
      if (iLocalNode.equals(masterServer)) {
        if (p.endsWith(iLocalNode.toLowerCase()))         return p;
        candidates.add(p);
      }
    }
    if (!candidates.isEmpty())     return candidates.get(0);
    final String masterServer=getLeaderServer(ALL_WILDCARD);
    if (iLocalNode.equals(masterServer))     return iClusterNames.iterator().next();
    return null;
  }
}
