{
  if (isClosed())   return;
  checkOpeness();
  try {
    rollback();
  }
 catch (  Throwable t) {
  }
  try {
    ((ODatabaseRaw)((ODatabaseRecord)underlying).getUnderlying()).callOnCloseListeners();
  }
 catch (  Throwable t) {
  }
  getLevel1Cache().clear();
  ownerPool.release(this);
  ownerPool=null;
}
