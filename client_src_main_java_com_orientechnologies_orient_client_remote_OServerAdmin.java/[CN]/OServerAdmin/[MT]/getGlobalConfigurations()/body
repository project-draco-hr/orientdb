{
  storage.checkConnection();
  final Map<String,String> config=new HashMap<String,String>();
  try {
    storage.writeCommand(OChannelBinaryProtocol.REQUEST_CONFIG_LIST);
    storage.getNetwork().readStatus();
    final int num=storage.getNetwork().readShort();
    for (int i=0; i < num; ++i)     config.put(storage.getNetwork().readString(),storage.getNetwork().readString());
  }
 catch (  Exception e) {
    OLogManager.instance().error(this,"Can't retrieve the configuration list",e,OStorageException.class);
    storage.close();
  }
  return config;
}
