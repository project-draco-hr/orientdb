{
  OSBTreeBonsai<OIdentifiable,Boolean> tree=loadTree();
  try {
    if (clear)     tree.clear();
    for (    OIdentifiable identifiable : addValues)     tree.put(identifiable,true);
    for (    OIdentifiable identifiable : removedValues)     tree.remove(identifiable);
  }
  finally {
    releaseTree();
  }
}
