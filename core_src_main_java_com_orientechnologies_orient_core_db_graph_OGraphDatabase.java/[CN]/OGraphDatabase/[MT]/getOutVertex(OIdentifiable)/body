{
  final ODocument e=(ODocument)iEdge.getRecord();
  checkEdgeClass(e);
  OIdentifiable v=e.field(EDGE_FIELD_OUT);
  if (v != null && v instanceof ORID) {
    v=v.getRecord();
    final boolean wasDirty=e.isDirty();
    e.field(EDGE_FIELD_OUT,v);
    if (!wasDirty)     e.unsetDirty();
  }
  return (ODocument)v;
}
