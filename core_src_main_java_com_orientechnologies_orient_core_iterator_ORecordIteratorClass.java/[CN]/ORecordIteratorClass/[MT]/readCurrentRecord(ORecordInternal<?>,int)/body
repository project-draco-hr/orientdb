{
  if (limit > -1 && browsedRecords >= limit)   return null;
  current.clusterPosition+=iMovement;
  if (iRecord != null) {
    iRecord.setIdentity(current);
    iRecord=lowLevelDatabase.load(iRecord,fetchPlan);
  }
 else   iRecord=lowLevelDatabase.load(current,fetchPlan);
  if (iRecord != null) {
    browsedRecords++;
    return iRecord;
  }
  return null;
}
