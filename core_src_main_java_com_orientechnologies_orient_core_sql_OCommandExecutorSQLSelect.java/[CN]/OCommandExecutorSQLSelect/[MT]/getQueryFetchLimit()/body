{
  final int sqlLimit;
  final int requestLimit;
  if (orderedFields == null && limit > -1)   sqlLimit=limit;
 else   sqlLimit=-1;
  if (request.getLimit() > -1)   requestLimit=request.getLimit();
 else   requestLimit=-1;
  if (sqlLimit == -1)   return requestLimit;
  if (requestLimit == -1)   return sqlLimit;
  return Math.min(sqlLimit,requestLimit);
}
