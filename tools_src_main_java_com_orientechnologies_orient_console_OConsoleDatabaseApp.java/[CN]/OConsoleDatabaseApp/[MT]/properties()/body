{
  out.println("PROPERTIES:");
  out.println("+---------------------+----------------------+");
  out.printf("| %-20s| %-20s |\n","NAME","VALUE");
  out.println("+---------------------+----------------------+");
  for (  Entry<String,Object> p : properties.entrySet()) {
    out.printf("| %-20s= %-20s |\n",p.getKey(),p.getValue());
  }
  out.println("+---------------------+----------------------+");
}
