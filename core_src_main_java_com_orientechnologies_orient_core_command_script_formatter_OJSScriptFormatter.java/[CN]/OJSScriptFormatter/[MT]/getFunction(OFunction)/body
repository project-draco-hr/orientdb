{
  final StringBuilder fCode=new StringBuilder();
  fCode.append("function ");
  fCode.append(f.getName());
  fCode.append('(');
  int i=0;
  for (  String p : f.getParameters()) {
    if (i++ > 0)     fCode.append(',');
    fCode.append(p);
  }
  fCode.append(") {\n");
  fCode.append(f.getCode());
  fCode.append("\n}\n");
  return fCode.toString();
}
