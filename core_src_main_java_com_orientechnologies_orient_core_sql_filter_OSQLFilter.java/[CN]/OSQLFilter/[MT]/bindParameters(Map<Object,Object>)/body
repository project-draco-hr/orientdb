{
  if (parameterItems == null || iArgs == null || iArgs.size() == 0)   return;
  for (  Entry<Object,Object> entry : iArgs.entrySet()) {
    if (entry.getKey() instanceof Integer)     parameterItems.get(((Integer)entry.getKey())).setValue(entry.setValue(entry.getValue()));
 else {
      String paramName=entry.getKey().toString();
      for (      OSQLFilterItemParameter value : parameterItems) {
        if (value.getName().equalsIgnoreCase(paramName)) {
          value.setValue(entry.getValue());
          break;
        }
      }
    }
  }
}
