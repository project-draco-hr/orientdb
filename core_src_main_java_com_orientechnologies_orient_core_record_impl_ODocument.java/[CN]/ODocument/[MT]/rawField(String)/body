{
  if (iFieldName == null || iFieldName.length() == 0)   return null;
  checkForLoading();
  checkForFields();
  if (_fieldValues.size() == 0)   return null;
  if (iFieldName.charAt(0) != '@' && OStringSerializerHelper.indexOf(iFieldName,0,'.','[') == -1)   return (RET)_fieldValues.get(iFieldName);
  return (RET)ODocumentHelper.getFieldValue(this,iFieldName);
}
