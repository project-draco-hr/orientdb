{
  if (OScenarioThreadLocal.INSTANCE.get() == OScenarioThreadLocal.RUN_MODE.RUNNING_DISTRIBUTED)   return;
  if (!isRelatedToLocalServer(iDatabase))   return;
  final ODistributedConfiguration cfg=getDatabaseConfiguration(iDatabase.getName());
  if (cfg == null)   return;
  if (installClustersOfClass(iDatabase,iClass))   updateCachedDatabaseConfiguration(iDatabase.getName(),cfg.serialize(),true,true);
}
