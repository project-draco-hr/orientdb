{
  if (binarySizeKnown)   return binarySize;
  binarySizeKnown=true;
  binarySize=OClusterPositionFactory.INSTANCE.getSerializedSize() + OBinaryProtocol.SIZE_INT + OBinaryProtocol.SIZE_LONG+ OBinaryProtocol.SIZE_BYTE+ OVersionFactory.instance().getVersionSize()+ OBinaryProtocol.SIZE_INT;
  return binarySize;
}
