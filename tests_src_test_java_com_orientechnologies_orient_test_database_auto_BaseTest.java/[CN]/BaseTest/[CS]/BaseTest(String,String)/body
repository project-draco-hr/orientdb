{
  if (url == null) {
    final String buildDirectory=System.getProperty("buildDirectory",".");
    url=getStorageType() + buildDirectory + "/test-db/"+ this.getClass().getSimpleName()+ prefix;
    dropDb=true;
  }
 else   url=url + prefix;
  database=createDatabaseInstance(url);
  this.url=database.getURL();
}
