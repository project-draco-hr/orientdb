{
  if (status == MULTIVALUE_STATUS.ALL_RIDS || sourceRecord == null || database == null)   return true;
  boolean allConverted=true;
  for (int i=0; i < size(); ++i) {
    try {
      if (!convertRecord2Link(i))       allConverted=false;
    }
 catch (    ORecordNotFoundException e) {
    }
  }
  if (allConverted)   status=MULTIVALUE_STATUS.ALL_RIDS;
  return allConverted;
}
