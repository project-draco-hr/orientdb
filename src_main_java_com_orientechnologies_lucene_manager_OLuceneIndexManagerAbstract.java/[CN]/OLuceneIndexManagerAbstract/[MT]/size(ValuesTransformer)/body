{
  IndexReader reader=null;
  IndexSearcher searcher=null;
  try {
    reader=getSearcher().getIndexReader();
  }
 catch (  IOException e) {
    OLogManager.instance().error(this,"Error on getting size of Lucene index",e);
  }
 finally {
    if (searcher != null) {
      release(searcher);
    }
  }
  return reader.numDocs();
}
