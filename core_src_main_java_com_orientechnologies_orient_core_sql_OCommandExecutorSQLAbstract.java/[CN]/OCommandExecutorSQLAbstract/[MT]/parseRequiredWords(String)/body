{
  for (  String w : iWords) {
    int newPos=OSQLHelper.nextWord(text,textUpperCase,currentPos,tempParseWord,true);
    if (newPos == -1 || !tempParseWord.toString().equals(w))     throw new OCommandSQLParsingException("Keyword " + w + " not found. Use "+ getSyntax(),text,currentPos);
    currentPos=newPos;
  }
}
