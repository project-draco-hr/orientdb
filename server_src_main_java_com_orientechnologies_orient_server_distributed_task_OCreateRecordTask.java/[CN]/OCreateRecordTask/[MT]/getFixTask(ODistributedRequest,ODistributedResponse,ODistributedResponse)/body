{
  OPhysicalPosition badResult=(OPhysicalPosition)iBadResponse.getPayload();
  OPhysicalPosition goodResult=(OPhysicalPosition)iGoodResponse.getPayload();
  return new OFixCreateRecordTask(new ORecordId(rid.getClusterId(),badResult.clusterPosition),content,version,recordType,new ORecordId(rid.getClusterId(),goodResult.clusterPosition));
}
