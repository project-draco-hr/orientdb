{
  acquireSharedLock();
  try {
    className=className.toLowerCase();
    indexName=indexName.toLowerCase();
    final OIndex<?> index=indexes.get(indexName);
    if (index != null && index.getDefinition() != null && index.getDefinition().getClassName() != null && className.equals(index.getDefinition().getClassName().toLowerCase()))     return wrapInTransactional(index);
    return null;
  }
  finally {
    releaseSharedLock();
  }
}
