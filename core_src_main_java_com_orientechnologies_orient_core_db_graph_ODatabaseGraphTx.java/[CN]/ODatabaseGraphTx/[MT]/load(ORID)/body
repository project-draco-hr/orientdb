{
  if (iRecordId == null)   return null;
  final ODocument doc=underlying.load(iRecordId);
  if (doc == null)   return null;
  if (doc.getClassName().equals(OGraphNode.class.getSimpleName()))   return new OGraphNode(doc);
 else   if (doc.getClassName().equals(OGraphArc.class.getSimpleName()))   return new OGraphArc(doc);
 else   throw new IllegalArgumentException("RecordID is not of supported type. Class=" + doc.getClassName());
}
