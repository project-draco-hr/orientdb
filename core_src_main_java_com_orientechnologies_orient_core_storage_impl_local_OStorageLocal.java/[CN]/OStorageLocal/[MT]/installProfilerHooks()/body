{
  Orient.instance().getProfiler().registerHookValue("db." + name + ".data.holes",new OProfilerHookValue(){
    public Object getValue(){
      return getHoles();
    }
  }
);
  Orient.instance().getProfiler().registerHookValue("db." + name + ".data.holeSize",new OProfilerHookValue(){
    public Object getValue(){
      return getHoleSize();
    }
  }
);
}
