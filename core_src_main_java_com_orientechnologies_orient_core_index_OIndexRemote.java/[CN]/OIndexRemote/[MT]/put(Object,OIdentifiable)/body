{
  if (iKey instanceof String)   iKey="'" + iKey + "'";
  if (iValue instanceof ORecord<?> && !iValue.getIdentity().isValid())   ((ORecord<?>)iValue).save();
  final OCommandRequest cmd=formatCommand(QUERY_PUT,name,iKey,iValue.getIdentity());
  getDatabase().command(cmd).execute();
  return this;
}
