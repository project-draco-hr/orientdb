{
  try {
    createNetworkConnection();
    network.writeByte((byte)OChannelBinaryProtocol.CONNECT);
    network.flush();
    readStatus();
    sessionId=network.readInt();
  }
 catch (  Exception e) {
    OLogManager.instance().error(this,"Can't connect the remote server: " + url,e,OStorageException.class);
    close();
  }
  return this;
}
