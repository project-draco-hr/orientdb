{
  OSBTree<OIdentifiable,Boolean> tree=treeCache.get(fileName);
  if (tree != null)   return tree;
  tree=new OSBTree<OIdentifiable,Boolean>(".sbt",1,true);
  tree.create(fileName,OLinkSerializer.INSTANCE,OBooleanSerializer.INSTANCE,(OStorageLocalAbstract)ODatabaseRecordThreadLocal.INSTANCE.get().getStorage());
  return tree;
}
