{
  iRequest.authorization=authentication;
  checkSyntax(iRequest.url,3,"Syntax error: " + Arrays.toString(getNames()) + "/<nextCommand>/");
  iRequest.url=OHttpUtils.nextChainUrl(iRequest.url);
  if (iRequest.sessionId == null || OServerCommandAuthenticatedDbAbstract.SESSIONID_LOGOUT.equals(iRequest.sessionId) || iRequest.sessionId.length() > 1 && OHttpSessionManager.getInstance().getSession(iRequest.sessionId) == null)   iRequest.sessionId=OHttpSessionManager.getInstance().createSession(databaseName,userName);
  return true;
}
