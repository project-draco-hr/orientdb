{
  if (id == null)   return null;
  if (id instanceof OrientVertex)   return (Vertex)id;
  init();
  if (id instanceof OIdentifiable)   return new OrientVertex((OIdentifiable)id);
  OrientVertex v=getFromCache(id);
  if (v != null) {
    return v;
  }
  final OrientBaseGraph g=acquire();
  try {
    v=(OrientVertex)g.getVertexByKey(keyFieldName,id);
    if (v != null) {
      verticesLoaded.incrementAndGet();
    }
    return v;
  }
  finally {
    g.shutdown();
  }
}
