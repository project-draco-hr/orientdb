{
  List<ODocument> result=executeQuery("select from Profile where any() traverse(0,3,any()) ( any().indexOf('Navona') > -1 )",database);
  Assert.assertTrue(result.size() > 0);
  for (int i=0; i < result.size(); ++i) {
    record=result.get(i);
    Assert.assertTrue(record.getClassName().equalsIgnoreCase("Profile"));
  }
}
