{
  List<ODocument> result1=database.command(new OSQLSynchQuery<ODocument>("select from ( traverse V.out, E.in from " + tomCruise.getIdentity() + ") where @class = 'Movie'")).execute();
  Assert.assertTrue(result1.size() > 0);
  for (  ODocument d : result1) {
    Assert.assertEquals(d.getClassName(),"Movie");
  }
}
