{
  final OHazelcastDistributedRequest req=new OHazelcastDistributedRequest(getLocalNodeName(),iDatabaseName,iClusterName,iTask,iExecutionMode);
  final OHazelcastDistributedDatabase db=messageService.getDatabase(iDatabaseName);
  final ODistributedResponse response=db.send(req);
  if (response != null)   return response.getPayload();
  return null;
}
