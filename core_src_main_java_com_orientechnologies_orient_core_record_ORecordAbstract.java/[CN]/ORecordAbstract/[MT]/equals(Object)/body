{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  final ORecordAbstract<?> other=(ORecordAbstract<?>)obj;
  if (_database == null) {
    if (other._database != null)     return false;
  }
 else   if (!_database.equals(other._database))   return false;
  if (_recordId == null) {
    if (other._recordId != null)     return false;
  }
 else   if (!_recordId.equals(other._recordId))   return false;
  return true;
}
