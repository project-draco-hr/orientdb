{
  assertEquals(database.query(new OSQLSynchQuery<Object>("select from ouser where name = 'AdMiN'")).size(),1);
  assertEquals(database.query(new OSQLSynchQuery<Object>("select from ouser where name = 'admin'")).size(),1);
  assertEquals(database.query(new OSQLSynchQuery<Object>("select from ouser where name like 'admin'")).size(),1);
  assertEquals(database.query(new OSQLSynchQuery<Object>("select from ouser where name like 'AdMIN'")).size(),1);
}
