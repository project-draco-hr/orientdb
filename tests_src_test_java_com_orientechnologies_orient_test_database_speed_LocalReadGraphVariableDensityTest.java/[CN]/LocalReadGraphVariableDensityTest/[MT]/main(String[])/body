{
  OProfiler.getInstance().startRecording();
  database=new ODatabaseGraphTx(System.getProperty("url")).open("admin","admin");
  database.declareIntent(new OIntentMassiveInsert());
  database.begin(TXTYPE.NOTX);
  long time=System.currentTimeMillis();
  OGraphVertexOutIterator iterator=readSubNodes(database.getRoot("HighDensityGraph"));
  System.out.println("Read of the entire graph with deep=" + iterator.getMaxDeepLevel() + ". Total "+ iterator.getCount()+ " nodes in "+ ((System.currentTimeMillis() - time) / 1000f)+ " sec.");
  System.out.println("Repeating the same operation but with hot cache");
  time=System.currentTimeMillis();
  iterator=readSubNodes(database.getRoot("HighDensityGraph"));
  System.out.println("Read using the cache of the entire graph with deep=" + iterator.getMaxDeepLevel() + ". Total "+ iterator.getCount()+ " nodes in "+ ((System.currentTimeMillis() - time) / 1000f)+ " sec.");
  database.close();
}
