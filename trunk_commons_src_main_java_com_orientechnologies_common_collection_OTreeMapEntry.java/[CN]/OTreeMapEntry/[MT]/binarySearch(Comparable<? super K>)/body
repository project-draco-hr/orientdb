{
  int low=0;
  int high=size - 1;
  int mid=0;
  while (low <= high) {
    mid=(low + high) >>> 1;
    Comparable midVal=(Comparable)getKeyAt(mid);
    int cmp=midVal.compareTo(iKey);
    if (cmp == 0) {
      tree.pageItemFound=true;
      tree.pageIndex=mid;
      return getValueAt(tree.pageIndex);
    }
    if (low == high)     break;
    if (cmp < 0)     low=mid + 1;
 else     high=mid;
  }
  tree.pageIndex=mid;
  return null;
}
