{
  parserNextWord(true);
  strategy=parserGetLastWord();
  if (!strategy.equalsIgnoreCase("NONE") && !strategy.equalsIgnoreCase("LOCK"))   throwParsingException("Invalid " + KEYWORD_STRATEGY + " value set to '"+ strategy+ "' but it should be NONE, LOCK or RETRY <times> [WAIT <waitInMs>]. Example: "+ KEYWORD_STRATEGY+ " RETRY 3 WAIT 100");
}
