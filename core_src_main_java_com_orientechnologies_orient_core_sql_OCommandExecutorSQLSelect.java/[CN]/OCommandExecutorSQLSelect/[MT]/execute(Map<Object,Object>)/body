{
  fetchLimit=getQueryFetchLimit();
  if (!assignTarget(iArgs)) {
    if (compiledFilter.getTargetIndex() != null)     searchInIndex();
 else     throw new OQueryParsingException("No source found in query: specify class, cluster(s), index or single record(s). Use " + getSyntax());
  }
  executeSearch();
  applyFlatten();
  applyProjections();
  applyOrderBy();
  applyLimit();
  return handleResult();
}
