{
  if (!enabled)   return;
  acquireExclusiveLock();
  final Set<ORID> toRemove=new HashSet<ORID>();
  try {
    for (    ORID entry : entries.keySet()) {
      if (entry.getClusterId() == clusterId) {
        toRemove.add(entry);
      }
    }
    for (    ORID ridToRemove : toRemove) {
      entries.remove(ridToRemove);
    }
  }
  finally {
    toRemove.clear();
    releaseExclusiveLock();
  }
}
