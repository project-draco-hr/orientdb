{
  while (lruList.size() > openLimit) {
    Iterator<OClosableEntry<K,V>> iterator=lruList.iterator();
    boolean entryClosed=false;
    while (iterator.hasNext()) {
      OClosableEntry<K,V> entry=iterator.next();
      if (entry.makeClosed()) {
        iterator.remove();
        entryClosed=true;
        break;
      }
    }
    if (!entryClosed)     break;
  }
}
