{
  if (debug)   OLogManager.instance().info(this,"%s - Writing strings (4+%d=%d items): %s",socket.getRemoteSocketAddress(),iCollection != null ? iCollection.size() : 0,iCollection != null ? iCollection.size() + 4 : 4,iCollection != null ? iCollection.toString() : "null");
  updateMetricTransmittedBytes(OBinaryProtocol.SIZE_INT);
  if (iCollection == null)   writeInt(-1);
 else {
    writeInt(iCollection.size());
    for (    String s : iCollection)     writeString(s);
  }
  return this;
}
