{
  checkDirection(true);
  final REC record=getRecord();
  if (currentTxEntryPosition > -1)   return (REC)txEntries.get(currentTxEntryPosition).getRecord();
  while (currentClusterIdx < clusterIds.length) {
    while (hasNext()) {
      if (readCurrentRecord(record,+1) != null)       return record;
    }
    currentClusterIdx++;
  }
  throw new NoSuchElementException();
}
