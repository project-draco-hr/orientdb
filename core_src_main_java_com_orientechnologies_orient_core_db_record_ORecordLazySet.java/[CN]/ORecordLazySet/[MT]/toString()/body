{
  if (isLoaded()) {
    if (newItems == null || newItems.size() == 0)     return ORecordMultiValueHelper.toString(this);
 else {
      final StringBuilder buffer=new StringBuilder(ORecordMultiValueHelper.toString(this));
      for (      ORecord<?> item : newItems.keySet())       buffer.insert(buffer.length() - 2,item.toString());
      return buffer.toString();
    }
  }
 else {
    return "NOT LOADED: " + getStreamedContent();
  }
}
