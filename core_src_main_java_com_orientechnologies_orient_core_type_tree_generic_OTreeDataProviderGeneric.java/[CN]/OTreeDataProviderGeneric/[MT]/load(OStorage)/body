{
  if (!record.getIdentity().isValid())   return;
  ORawBuffer raw=iSt.readRecord(null,(ORecordId)record.getIdentity(),null,null);
  if (raw == null)   throw new OConfigurationException("Can't load map with id " + record.getIdentity());
  record.setVersion(raw.version);
  record.recycle(this);
  fromStream(raw.buffer);
}
