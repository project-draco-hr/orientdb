{
  try {
    return new OLocalPaginatedStorage(dbName,dbName,getMode(configuration),writeCacheIdGen.nextId(),readCache);
  }
 catch (  Throwable t) {
    OLogManager.instance().error(this,"Error on opening database: " + dbName + ". Current location is: "+ new java.io.File(".").getAbsolutePath(),t,ODatabaseException.class);
  }
  return null;
}
