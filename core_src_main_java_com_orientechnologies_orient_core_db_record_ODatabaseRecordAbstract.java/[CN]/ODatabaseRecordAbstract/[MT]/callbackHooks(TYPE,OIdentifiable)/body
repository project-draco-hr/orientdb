{
  if (!OHookThreadLocal.INSTANCE.push(iRecord))   return false;
  try {
    boolean recordChanged=false;
    for (    ORecordHook hook : hooks)     if (hook.onTrigger(iType,(ORecord<?>)iRecord))     recordChanged=true;
    return recordChanged;
  }
  finally {
    OHookThreadLocal.INSTANCE.pop(iRecord);
  }
}
