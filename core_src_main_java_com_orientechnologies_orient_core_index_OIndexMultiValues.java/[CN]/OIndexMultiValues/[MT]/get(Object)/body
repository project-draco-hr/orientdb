{
  acquireExclusiveLock();
  try {
    final ORecordLazySet values=(ORecordLazySet)map.get(iKey);
    if (values != null)     values.setDatabase(ODatabaseRecordThreadLocal.INSTANCE.get());
    if (values == null)     return ORecordLazySet.EMPTY_SET;
    return values;
  }
  finally {
    releaseExclusiveLock();
  }
}
