{
  ODocument result=new ONativeSynchQuery<ODocument,OQueryContextNativeSchema<ODocument>>((ODatabaseRecord<ODocument>)database,ORole.class.getSimpleName(),new OQueryContextNativeSchema<ODocument>()){
    @Override public boolean filter(    OQueryContextNativeSchema<ODocument> iRecord){
      return iRecord.field("name").eq(iRoleName).go();
    }
  }
.executeFirst();
  if (result != null)   return new ORole(result);
  return null;
}
