{
  this.filePages=new HashMap<Long,Set<Long>>();
  long tmpMaxSize=maxMemory / pageSize;
  if (tmpMaxSize >= Integer.MAX_VALUE) {
    maxSize=Integer.MAX_VALUE;
  }
 else {
    maxSize=(int)tmpMaxSize;
  }
  int writeMaxSize=maxSize / 16;
  maxSize=maxSize - writeMaxSize;
  this.writeCache=new OWOWCache(syncOnPageFlush,pageSize,writeGroupTTL,writeAheadLog,pageFlushInterval,writeMaxSize,storageLocal);
  K_IN=maxSize >> 2;
  K_OUT=maxSize >> 1;
  am=new LRUList();
  a1out=new LRUList();
  a1in=new LRUList();
  syncObject=new Object();
}
