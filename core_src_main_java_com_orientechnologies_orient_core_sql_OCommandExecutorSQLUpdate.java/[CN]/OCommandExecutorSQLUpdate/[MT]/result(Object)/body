{
  ORecordSchemaAware<?> record=(ORecordSchemaAware<?>)iRecord;
  Object v;
  for (  Map.Entry<String,Object> entry : setEntries.entrySet()) {
    v=entry.getValue();
    if (v instanceof OSQLFilterItem)     v=((OSQLFilterItem)v).getValue(record);
    record.field(entry.getKey(),v);
  }
  for (  String entry : removeEntries)   record.removeField(entry);
  record.save();
  recordCount++;
  return true;
}
