{
  OETLProcessor proc=getProcessor("{source: { content: { value: 'name,surname\nJay,Miner\nTest,Test' } }, extractor : { row: {} }," + " transformers: [{csv: {}}, {vertex: {class:'V'}}, {flow:{operation:'skip',if: 'name = \'Jay\''}},{field:{fieldName:'name', value:'3'}}" + "], loader: { orientdb: { dbURL: 'memory:ETLBaseTest', dbType:'graph'} } }").execute();
  assertEquals(2,graph.countVertices("V"));
  Iterator<Vertex> it=graph.getVertices().iterator();
  Vertex v1=it.next();
  Object value1=v1.getProperty("name");
  assertEquals("Jay",value1);
  Vertex v2=it.next();
  Object value2=v2.getProperty("name");
  assertEquals("3",value2);
  graph.command(new OCommandSQL("delete vertex V")).execute();
}
