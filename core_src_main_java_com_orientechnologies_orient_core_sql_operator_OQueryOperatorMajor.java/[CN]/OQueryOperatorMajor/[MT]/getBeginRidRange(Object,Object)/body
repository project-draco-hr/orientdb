{
  if (iLeft instanceof OSQLFilterItemField && ODocumentHelper.ATTRIBUTE_RID.equals(((OSQLFilterItemField)iLeft).getRoot()))   if (iRight instanceof ORID)   return new ORecordId(((ORID)iRight).next());
 else {
    if (iRight instanceof OSQLFilterItemParameter && ((OSQLFilterItemParameter)iRight).getValue(null,null) instanceof ORID)     return new ORecordId(((ORID)((OSQLFilterItemParameter)iRight).getValue(null,null)).next());
  }
  return null;
}
