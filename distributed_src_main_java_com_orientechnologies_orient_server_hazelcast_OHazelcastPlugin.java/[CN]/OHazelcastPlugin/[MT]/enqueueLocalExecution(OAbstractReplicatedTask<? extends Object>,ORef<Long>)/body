{
  final long opSerial=iTask.getOperationSerial();
  if (opSerial == -1)   try {
    return iTask.executeOnLocalNode();
  }
  finally {
    notifyQueueWaiters(iTask.getDatabaseName(),iTask.getRunId(),opSerial,false);
  }
  if (!waitForMyTurnInQueue(iTask))   return null;
  try {
    ODistributedServerLog.debug(this,iTask.getNodeSource(),iTask.getNodeDestination(),DIRECTION.IN,"local exec: pop operation=%d.%d, thread=%s",iTask.getRunId(),opSerial,Thread.currentThread().getName());
    iOperationOffset.value=logOperation2Journal(iTask.getDatabaseSynchronizer(),iTask);
    return iTask.executeOnLocalNode();
  }
  finally {
    notifyQueueWaiters(iTask.getDatabaseName(),iTask.getRunId(),opSerial,false);
  }
}
