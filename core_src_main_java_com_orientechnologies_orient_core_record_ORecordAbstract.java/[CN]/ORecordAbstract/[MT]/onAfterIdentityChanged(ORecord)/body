{
  invokeListenerEvent(ORecordListener.EVENT.IDENTITY_CHANGED);
  for (  OIdentityChangeListenerNew changeListener : newIdentityChangeListeners)   changeListener.onAfterIdentityChange(this);
  if (prevRid != null && !prevRid.equals(this._recordId)) {
    for (    OIdentityChangeListener changeListener : identityChangeListeners)     changeListener.onIdentityChanged(prevRid,this);
  }
  prevRid=null;
}
