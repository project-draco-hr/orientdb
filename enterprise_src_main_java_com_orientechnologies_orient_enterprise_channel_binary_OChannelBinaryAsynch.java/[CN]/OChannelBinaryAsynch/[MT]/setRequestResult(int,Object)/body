{
  OChannelBinaryAsynchRequester reqId;
  acquireExclusiveLock();
  try {
    for (int i=0; i < requestersQueue.size(); ++i) {
      reqId=requestersQueue.get(i);
      if (reqId.getRequesterId() == iClientTxId) {
        reqId.getRequesterResponseQueue().add(iResult);
        if (!reqId.isPermanentRequester())         requestersQueue.remove(i);
        return;
      }
    }
    OLogManager.instance().warn(this,"Request %d not found in queue. Requests in queue %d: ",iClientTxId,requestersQueue.size(),requestersQueue.toString());
  }
  finally {
    releaseExclusiveLock();
  }
}
