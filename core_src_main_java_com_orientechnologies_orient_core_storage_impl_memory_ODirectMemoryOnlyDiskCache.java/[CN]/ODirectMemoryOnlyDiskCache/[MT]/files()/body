{
  final Map<String,Long> result=new HashMap<String,Long>();
  metadataLock.lock();
  try {
    for (    Map.Entry<String,Integer> entry : fileNameIdMap.entrySet()) {
      result.put(entry.getKey(),composeFileId(id,entry.getValue()));
    }
  }
  finally {
    metadataLock.unlock();
  }
  return result;
}
