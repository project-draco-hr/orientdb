{
  final long timer=OProfiler.getInstance().startChrono();
  init();
  try {
    if (schemaClusterId == -1 || database.countClusterElements(OStorage.CLUSTER_METADATA_NAME) == 0)     return;
    loadSchema();
    loadSecurity();
  }
  finally {
    OProfiler.getInstance().stopChrono("OMetadata.load",timer);
  }
}
