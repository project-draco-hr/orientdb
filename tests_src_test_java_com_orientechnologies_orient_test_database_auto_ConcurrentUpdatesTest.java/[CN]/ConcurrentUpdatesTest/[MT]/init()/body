{
  level1CacheEnabled=OGlobalConfiguration.CACHE_LEVEL1_ENABLED.getValueAsBoolean();
  level2CacheEnabled=OGlobalConfiguration.CACHE_LEVEL2_ENABLED.getValueAsBoolean();
  mvccEnabled=OGlobalConfiguration.DB_MVCC.getValueAsBoolean();
  if (level1CacheEnabled)   OGlobalConfiguration.CACHE_LEVEL1_ENABLED.setValue(false);
  if (level2CacheEnabled)   OGlobalConfiguration.CACHE_LEVEL2_ENABLED.setValue(false);
  if (!mvccEnabled)   OGlobalConfiguration.DB_MVCC.setValue(true);
  if ("memory:test".equals(url))   new ODatabaseDocumentTx(url).create().close();
}
