{
  if (lockHolds == null)   lockHolds=new ThreadLocal<OModifiableInteger>(){
    @Override protected OModifiableInteger initialValue(){
      return new OModifiableInteger();
    }
  }
;
  if (myNode == null)   myNode=new ThreadLocal<WNode>(){
    @Override protected WNode initialValue(){
      return new WNode();
    }
  }
;
  if (predNode == null)   predNode=new ThreadLocal<WNode>();
}
