{
  if (iRecordType == ODocument.RECORD_TYPE)   return new ODocument();
  if (iRecordType == ORecordFlat.RECORD_TYPE)   return new ORecordFlat();
  if (iRecordType == ORecordBytes.RECORD_TYPE)   return new ORecordBytes();
  if (iRecordType == ORecordColumn.RECORD_TYPE)   return new ORecordColumn();
  return null;
}
