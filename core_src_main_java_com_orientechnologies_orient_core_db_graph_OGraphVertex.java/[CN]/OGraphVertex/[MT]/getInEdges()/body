{
  if (inEdges == null) {
    inEdges=new ArrayList<OGraphEdge>();
    List<ODocument> docs=document.field(FIELD_IN_EDGES);
    if (docs == null) {
      docs=new ArrayList<ODocument>();
      document.field(FIELD_IN_EDGES,docs);
    }
 else {
      for (      ODocument d : docs) {
        inEdges.add(new OGraphEdge(d));
      }
    }
  }
  return inEdges;
}
