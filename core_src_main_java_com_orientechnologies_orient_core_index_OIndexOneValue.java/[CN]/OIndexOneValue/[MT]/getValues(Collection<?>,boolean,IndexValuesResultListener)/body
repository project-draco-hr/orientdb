{
  checkForRebuild();
  final List<Object> sortedKeys=new ArrayList<Object>(keys);
  final Comparator<Object> comparator;
  if (ascSortOrder)   comparator=ODefaultComparator.INSTANCE;
 else   comparator=Collections.reverseOrder(ODefaultComparator.INSTANCE);
  Collections.sort(sortedKeys,comparator);
  acquireSharedLock();
  try {
    for (    Object key : sortedKeys) {
      key=getCollatingValue(key);
      final OIdentifiable val=indexEngine.get(key);
      if (val != null) {
        if (!resultListener.addResult(val))         return;
      }
    }
  }
  finally {
    releaseSharedLock();
  }
}
