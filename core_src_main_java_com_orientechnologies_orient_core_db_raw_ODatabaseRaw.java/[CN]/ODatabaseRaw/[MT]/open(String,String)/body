{
  try {
    if (status == STATUS.OPEN)     throw new IllegalStateException("Database " + getName() + " is already open");
    storage.open(getId(),iUserName,iUserPassword);
    for (    ODatabaseLifecycleListener listener : listeners)     try {
      listener.onOpen(this);
    }
 catch (    Throwable t) {
    }
    status=STATUS.OPEN;
  }
 catch (  ODatabaseException e) {
    throw e;
  }
catch (  Exception e) {
    throw new ODatabaseException("Can't open database",e);
  }
  return (DB)this;
}
