{
  final ODistributedConfiguration cfg=getDatabaseConfiguration(iDatabase.getName());
  if (cfg == null)   return;
  boolean cfgUpdated=false;
  for (  OClass c : iDatabase.getMetadata().getSchema().getClasses()) {
    if (!(c.getClusterSelection() instanceof OLocalClusterStrategy))     if (installClustersOfClass(iDatabase,c))     cfgUpdated=true;
  }
  if (cfgUpdated)   updateCachedDatabaseConfiguration(iDatabase.getName(),cfg.getDocument(),true,true);
}
