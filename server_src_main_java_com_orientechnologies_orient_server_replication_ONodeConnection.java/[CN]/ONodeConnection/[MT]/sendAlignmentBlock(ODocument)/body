{
  final OChannelBinaryClient network=beginRequest(OClusterProtocol.REQUEST_NODE2NODE_REPLICATION_ALIGN);
  try {
    network.writeBytes(cfg.toStream());
    network.flush();
  }
  finally {
    endRequest();
    ((ODocument)cfg.field("block")).clear();
  }
  beginResponse();
  try {
  }
  finally {
    endResponse();
  }
}
