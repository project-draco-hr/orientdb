{
  OSBTreeBonsaiLocal<OIdentifiable,Integer> tree=new OSBTreeBonsaiLocal<OIdentifiable,Integer>(DEFAULT_EXTENSION,true);
  tree.load(collectionPointer.getFileId(),collectionPointer.getRootPointer(),(OStorageLocalAbstract)ODatabaseRecordThreadLocal.INSTANCE.get().getStorage().getUnderlying());
