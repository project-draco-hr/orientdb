{
  final int queueSize=iQueue.size();
  if (queueSize > 0) {
    if (clearReqQueue) {
      ODistributedServerLog.warn(this,manager.getLocalNodeName(),null,DIRECTION.NONE,"Found %d messages in queue '%s', resetting queue...",queueSize,iQueueName);
      iQueue.clear();
    }
 else     ODistributedServerLog.warn(this,manager.getLocalNodeName(),null,DIRECTION.NONE,"Found %d messages in queue '%s'",queueSize,iQueueName);
  }
 else   ODistributedServerLog.info(this,manager.getLocalNodeName(),null,DIRECTION.NONE,"Found no previous messages in queue '%s'",iQueueName);
}
