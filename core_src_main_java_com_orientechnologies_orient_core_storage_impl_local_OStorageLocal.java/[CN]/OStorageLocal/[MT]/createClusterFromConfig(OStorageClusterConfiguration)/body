{
  if (clusterMap.containsKey(iConfig.getName())) {
    ((OClusterLocal)clusterMap.get(iConfig.getName())).config=(OStorageSegmentConfiguration)iConfig;
    return -1;
  }
  final OCluster cluster;
  if (iConfig instanceof OStoragePhysicalClusterConfiguration) {
    cluster=new OClusterLocal(this,(OStoragePhysicalClusterConfiguration)iConfig);
  }
 else   cluster=new OClusterLogical(this,(OStorageLogicalClusterConfiguration)iConfig);
  return registerCluster(cluster);
}
