{
  if (status == MULTIVALUE_STATUS.ALL_RECORDS || !autoConvertToRecord || database == null)   return;
  final HashSet<Object> copy=new HashSet<Object>();
  for (  Object k : map.keySet())   copy.add(convertLink2Record(k));
  clear();
  addAll(copy);
  copy.clear();
  status=MULTIVALUE_STATUS.ALL_RECORDS;
}
