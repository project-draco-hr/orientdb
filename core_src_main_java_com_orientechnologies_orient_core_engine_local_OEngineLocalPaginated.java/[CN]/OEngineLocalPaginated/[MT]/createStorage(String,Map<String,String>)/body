{
  if (memoryLocked.compareAndSet(false,true)) {
    lockMemory();
  }
  try {
    return new OLocalPaginatedStorage(dbName,dbName,getMode(configuration));
  }
 catch (  Throwable t) {
    OLogManager.instance().error(this,"Error on opening database: " + dbName + ". Current location is: "+ new java.io.File(".").getAbsolutePath(),t,ODatabaseException.class);
  }
  return null;
}
