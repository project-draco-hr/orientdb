{
  Token xsp;
  xsp=jj_scanpos;
  if (jj_3R_502()) {
    jj_scanpos=xsp;
    if (jj_3R_503()) {
      jj_scanpos=xsp;
      if (jj_3R_504()) {
        jj_scanpos=xsp;
        if (jj_3R_505())         return true;
      }
    }
  }
  if (jj_scan_token(COLON))   return true;
  if (jj_3R_71())   return true;
  while (true) {
    xsp=jj_scanpos;
    if (jj_3R_506()) {
      jj_scanpos=xsp;
      break;
    }
  }
  return false;
}
