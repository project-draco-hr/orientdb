{
  if (record == null || record.isDirty())   return 0;
  int freed=0;
  if (getDepthInMemory() >= iDepthLevel)   freed=disconnect(false);
 else {
    if (left != null)     freed+=left.checkToDisconnect(iDepthLevel);
    if (right != null)     freed+=right.checkToDisconnect(iDepthLevel);
  }
  return freed;
}
