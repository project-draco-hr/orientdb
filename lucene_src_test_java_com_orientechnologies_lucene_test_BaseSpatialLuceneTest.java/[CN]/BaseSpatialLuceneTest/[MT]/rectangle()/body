{
  ODocument polygon=new ODocument("OPolygon");
  polygon.field("coordinates",new ArrayList<List<List<Double>>>(){
{
      add(new ArrayList<List<Double>>(){
{
          add(Arrays.asList(-45d,-30d));
          add(Arrays.asList(-45d,30d));
          add(Arrays.asList(45d,30d));
          add(Arrays.asList(45d,30d));
          add(Arrays.asList(-45d,-30d));
        }
      }
);
    }
  }
);
  return polygon;
}
